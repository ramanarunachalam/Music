cbf\u0c9f\u0ccd\u0c9f\u0cb0\u0cc1\u0ca3 \u0cb5\u0cbf\u0cb1\u0ccd\u0caa\u0ca4\u0cbf\u0caf\u0cbf\u0ca8\u0ccd \u0cb5\u0cbf\u0ca8\u0ccd\u0ca4\u0cc1\u0ca8\u0cbe\u0ca4"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c93\u0c9a\u0cc8 \u0c9a\u0cbe\u0cb2\u0cc1\u0cae\u0cca\u0cb0\u0cc1 \u0c9a\u0ca4\u0ccd\u0ca4\u0cae\u0ca4\u0cbf \u0c95\u0caa\u0ccd\u0caa\u0c9f\u0cbf\u0c95 <br/> \u0cae\u0ccb\u0c9f\u0cc1 \u0c95\u0cc2\u0c9f\u0cbf\u0caf\u0cca\u0cb0\u0cc1 \u0cae\u0cbf\u0ca4\u0ccd\u0ca4\u0cae\u0cc1\u0ca4 \u0c9a\u0cbf\u0ca4\u0ccd\u0ca4\u0cbf\u0caf\u0cca\u0c9f\u0cc1 <br/> \u0cae\u0ccb\u0ca4\u0cc1 \u0cb5\u0cc7\u0ca4\u0c9a\u0cb0 \u0c9a\u0ca4\u0ccd\u0ca4\u0cbf\u0caf\u0c9f\u0cbf \u0caf\u0cc1\u0cb1\u0ccd\u0cb1\u0ca4\u0cbf\u0cb0\u0cc1 \u0ca8\u0ca8\u0ccd\u0ca4\u0cbf\u0caf\u0cc2\u0c9f\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c8a\u0cae\u0cc8 \u0caf\u0cc7\u0ca8\u0cc8\u0caf\u0cca\u0cb3\u0cbf\u0cb0\u0ccd \u0cb5\u0cbf\u0ca4\u0ccd\u0ca4\u0cc1\u0ca8\u0ca4\u0cc1 \u0cae\u0cc1\u0ca4\u0ccd\u0ca4\u0cbf\u0caa\u0cc6\u0cb1 <br/> \u0cae\u0cc2\u0cb2 \u0cb5\u0cbe\u0c9a\u0cb2\u0ccd\u0cb5\u0cc6\u0cb3\u0cbf \u0cb5\u0cbf\u0c9f\u0ccd\u0c9f\u0cc1\u0ca8\u0ca4\u0cc1 \u0cb0\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cca\u0cb3\u0cbf\u0cb0\u0ccd <br/> \u0caf\u0ccb\u0c95 \u0caa\u0cc7\u0ca4\u0cb5\u0c95\u0cc8 \u0caf\u0cc6\u0c9f\u0ccd\u0c9f\u0cc1\u0cae\u0cbf\u0ca4\u0cbf \u0cb2\u0cca\u0c9f\u0ccd\u0c9f\u0cc1\u0cae\u0ccd\u0cb5\u0c95\u0cc8 \u0caf\u0cbf\u0ca8\u0ccd\u0cb1\u0cc1\u0ca4\u0cbe\u0cb0\u0cbe\u0caf\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0cb5\u0cbe\u0c9a\u0cbf \u0cb5\u0cbe\u0ca3\u0cbf\u0c95\u0ca8\u0cc6 \u0ca8\u0c95\u0ccd\u0c95\u0cc1\u0ca4\u0cbf\u0cb0\u0cc8 \u0cb5\u0cbf\u0cb1\u0ccd\u0cb1\u0cc1\u0cae\u0c95\u0cbf\u0cb3\u0ccd <br/> \u0cb5\u0cbe\u0ca4 \u0cb5\u0cc2\u0cb0\u0ca8\u0c9f\u0cbf \u0cae\u0cc8\u0c95\u0ccd\u0c95\u0cca\u0cb3\u0cc1\u0c95\u0ccd\u0cb0\u0cc1 \u0caa\u0cc8\u0c95\u0ccd\u0c95\u0c9f\u0cb5\u0cc1\u0cb3\u0ccd <br/> \u0cae\u0cbe\u0cb3\u0cc8 \u0cb0\u0cc2\u0caa\u0ca8\u0ccd\u0cae\u0cc1\u0c95 \u0cae\u0ca4\u0ccd\u0ca4\u0cbf\u0c95\u0cc8\u0cb5\u0cbf \u0ca4\u0ca4\u0ccd\u0ca4\u0cb0\u0cc1\u0ca3 \u0c9a\u0cc6\u0c99\u0ccd\u0c95\u0cc8\u0caf\u0cbe\u0cb3\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0cb5\u0cbe\u0c95\u0cc1 \u0caa\u0cbe\u0ca4\u0cbf\u0caf\u0cc1\u0cb1\u0cc8 \u0c9a\u0ca4\u0ccd\u0ca4\u0cbf\u0c95\u0cb5\u0cc1 \u0cb0\u0cbf\u0c95\u0ccd\u0c95\u0cc1\u0ca4\u0cb2\u0cc8 <br/> \u0cb5\u0cbe\u0caf\u0cbf\u0ca8\u0ccd \u0cae\u0cbe\u0ca4\u0cc1\u0ca4\u0cc1\u0c95\u0cbf\u0cb0\u0ccd \u0caa\u0c9a\u0ccd\u0c9a\u0cc8\u0cb5\u0c9f\u0cbf \u0cb5\u0cbf\u0c9a\u0ccd\u0c9a\u0cbf\u0cb5\u0cc8\u0caf\u0cc6\u0ca8\u0ccd <br/> \u0cae\u0cbe\u0c9a\u0cc1 \u0c9a\u0cc7\u0cb0\u0cc6\u0cb3\u0cc1\u0caa\u0cbf \u0cb1\u0caa\u0ccd\u0caa\u0cc8\u0caf\u0cc1\u0cae \u0cb1\u0cc1\u0ca4\u0ccd\u0ca4\u0cb5\u0cc1\u0cae\u0cc8 \u0ca4\u0ca8\u0ccd\u0ca4\u0cb5\u0cbe\u0cb3\u0ccd\u0cb5\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0c95\u0cbe\u0c9a\u0cbf \u0cb0\u0cbe\u0cae\u0cc6\u0c9a\u0cc1\u0cb0\u0cae\u0ccd \u0cb0\u0ca4\u0ccd\u0ca8\u0c95\u0cbf\u0cb0\u0cbf \u0c9a\u0cb0\u0ccd\u0caa\u0ccd\u0caa\u0c95\u0cbf\u0cb0\u0cbf <br/> \u0c86\u0cb0\u0cc2\u0cb0\u0ccd \u0cb5\u0cc7\u0cb2\u0cc1\u0cb0\u0ccd \u0ca4\u0cc6\u0cb5\u0cc1\u0cb0\u0ccd \u0c95\u0c9a\u0ccd\u0c9a\u0cbf\u0cae\u0ca4\u0cc1 \u0cb0\u0cc8\u0caa\u0ccd\u0caa\u0cb1\u0cbf\u0caf\u0cb2\u0ccd <br/> \u0c95\u0cbe\u0cb5\u0cc8 \u0cae\u0cc2\u0ca4\u0cc1\u0cb0\u0cb0\u0cc1 \u0ca3\u0c95\u0ccd\u0c95\u0cbf\u0cb0\u0cbf\u0ca4\u0cbf \u0cb0\u0cc1\u0ca4\u0ccd\u0ca4\u0ca3\u0cbf\u0caf\u0cb2\u0ccd \u0c9a\u0cc6\u0ca8\u0ccd\u0ca4\u0cbf\u0cb2\u0ccd\u0ca8\u0cbe\u0c95\u0cc8"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 7", "T": "\u0c95\u0cbe\u0cb3\u0cbf \u0cb5\u0cc7\u0cb3\u0cc1\u0cb0\u0ccd\u0caa\u0cb3 \u0ca8\u0cbf\u0c95\u0ccd\u0c95\u0cbf\u0cb0\u0cbf \u0c95\u0cc1\u0cb1\u0cc1\u0c95\u0ccd\u0c95\u0cc8\u0ca4\u0cbf\u0cb0\u0cc1 <br/> \u0ca8\u0cbe\u0cb5 \u0cb2\u0cc2\u0cb0\u0ccd\u0ca4\u0cbf\u0cb0\u0cc1\u0cb5\u0cc6 \u0ca3\u0cc6\u0caf\u0ccd\u0caa\u0ccd\u0caa\u0ca4\u0cbf\u0caf\u0cbf\u0ca8\u0ccd \u0cae\u0cbf\u0c95\u0ccd\u0c95\u0ca4\u0cbf\u0c95\u0cb3\u0ccd <br/> \u0c95\u0cbe\u0ca4\u0cb2\u0ccd \u0c9a\u0ccb\u0cb2\u0cc8\u0cb5\u0cb3\u0cb0\u0ccd \u0cb5\u0cc6\u0cb1\u0ccd\u0caa\u0cbf\u0cb2\u0cc1\u0cb1\u0cc8 \u0cae\u0cc1\u0ca4\u0ccd\u0ca4\u0cb0\u0ccd\u0caa\u0cc1\u0c95\u0cb3\u0ccd \u0ca4\u0cae\u0ccd\u0caa\u0cbf\u0cb0\u0cbe\u0ca8\u0cc7."}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d06\u0d1a\u0d48 \u0d28\u0d3e\u0d32\u0d41\u0d1a\u0d24\u0d41 \u0d30\u0d15\u0d4d\u0d15\u0d2e\u0d32 \u0d2e\u0d41\u0d31\u0d4d\u0d31\u0d3f\u0d29\u0d4a\u0d33\u0d3f <br/> \u0d35\u0d40\u0d1a\u0d3f \u0d2f\u0d4b\u0d1f\u0d3f\u0d2f\u0d3f\u0d30\u0d41 \u0d2a\u0d15\u0d4d\u0d15\u0d2e\u0d4a\u0d1f\u0d41 \u0d31\u0d1a\u0d4d\u0d1a\u0d46\u0d32\u0d4d\u0d35\u0d33\u0d3f <br/> \u0d06\u0d35\u0d32\u0d4d \u0d15\u0d42\u0d30\u0d2e\u0d23\u0d4d\u0d2e\u0d41 \u0d24\u0d31\u0d4d\u0d1a\u0d32\u0d1a \u0d2a\u0d4a\u0d31\u0d4d\u0d1a\u0d2a\u0d48\u0d2f\u0d41 \u0d2e\u0d3f\u0d28\u0d4d\u0d24\u0d41\u0d35\u0d3e\u0d15\u0d48"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d06\u0d30 \u0d2e\u0d42\u0d23\u0d41\u0d2a\u0d24\u0d3f \u0d2f\u0d3f\u0d31\u0d4d\u0d15\u0d4a\u0d33\u0d28\u0d3f \u0d31\u0d41\u0d24\u0d4d\u0d24\u0d3f\u0d35\u0d46\u0d33\u0d3f <br/> \u0d2f\u0d3e\u0d30\u0d41 \u0d1a\u0d4b\u0d24\u0d3f\u0d28\u0d41\u0d31\u0d41 \u0d2a\u0d24\u0d4d\u0d24\u0d3f\u0d29\u0d41\u0d1f \u0d29\u0d46\u0d1f\u0d4d\u0d1f\u0d41\u0d07\u0d24 <br/> \u0d34\u0d3e\u0d15\u0d3f \u0d2f\u0d47\u0d34\u0d41\u0d2e\u0d33 \u0d35\u0d3f\u0d1f\u0d4d\u0d1f\u0d30\u0d41\u0d23 \u0d35\u0d3f\u0d31\u0d4d\u0d2a\u0d24\u0d3f\u0d2f\u0d3f\u0d29\u0d4d \u0d35\u0d3f\u0d28\u0d4d\u0d24\u0d41\u0d28\u0d3e\u0d24"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d13\u0d1a\u0d48 \u0d1a\u0d3e\u0d32\u0d41\u0d2e\u0d4a\u0d30\u0d41 \u0d1a\u0d24\u0d4d\u0d24\u0d2e\u0d24\u0d3f \u0d15\u0d2a\u0d4d\u0d2a\u0d1f\u0d3f\u0d15 <br/> \u0d2e\u0d4b\u0d1f\u0d41 \u0d15\u0d42\u0d1f\u0d3f\u0d2f\u0d4a\u0d30\u0d41 \u0d2e\u0d3f\u0d24\u0d4d\u0d24\u0d2e\u0d41\u0d24 \u0d1a\u0d3f\u0d24\u0d4d\u0d24\u0d3f\u0d2f\u0d4a\u0d1f\u0d41 <br/> \u0d2e\u0d4b\u0d24\u0d41 \u0d35\u0d47\u0d24\u0d1a\u0d30 \u0d1a\u0d24\u0d4d\u0d24\u0d3f\u0d2f\u0d1f\u0d3f \u0d2f\u0d41\u0d31\u0d4d\u0d31\u0d24\u0d3f\u0d30\u0d41 \u0d28\u0d28\u0d4d\u0d24\u0d3f\u0d2f\u0d42\u0d1f\u0d47"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d0a\u0d2e\u0d48 \u0d2f\u0d47\u0d29\u0d48\u0d2f\u0d4a\u0d33\u0d3f\u0d30\u0d4d \u0d35\u0d3f\u0d24\u0d4d\u0d24\u0d41\u0d29\u0d24\u0d41 \u0d2e\u0d41\u0d24\u0d4d\u0d24\u0d3f\u0d2a\u0d46\u0d31 <br/> \u0d2e\u0d42\u0d32 \u0d35\u0d3e\u0d1a\u0d32\u0d4d\u0d35\u0d46\u0d33\u0d3f \u0d35\u0d3f\u0d1f\u0d4d\u0d1f\u0d41\u0d29\u0d24\u0d41 \u0d30\u0d24\u0d4d\u0d24\u0d3f\u0d32\u0d4a\u0d33\u0d3f\u0d30\u0d4d <br/> \u0d2f\u0d4b\u0d15 \u0d2a\u0d47\u0d24\u0d35\u0d15\u0d48 \u0d2f\u0d46\u0d1f\u0d4d\u0d1f\u0d41\u0d2e\u0d3f\u0d24\u0d3f \u0d32\u0d4a\u0d1f\u0d4d\u0d1f\u0d41\u0d2e\u0d4d\u0d35\u0d15\u0d48 \u0d2f\u0d3f\u0d29\u0d4d\u0d31\u0d41\u0d24\u0d3e\u0d30\u0d3e\u0d2f\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d35\u0d3e\u0d1a\u0d3f \u0d35\u0d3e\u0d23\u0d3f\u0d15\u0d29\u0d46 \u0d29\u0d15\u0d4d\u0d15\u0d41\u0d24\u0d3f\u0d30\u0d48 \u0d35\u0d3f\u0d31\u0d4d\u0d31\u0d41\u0d2e\u0d15\u0d3f\u0d34\u0d4d <br/> \u0d35\u0d3e\u0d24 \u0d35\u0d42\u0d30\u0d29\u0d1f\u0d3f \u0d2e\u0d48\u0d15\u0d4d\u0d15\u0d4a\u0d33\u0d41\u0d15\u0d4d\u0d30\u0d41 \u0d2a\u0d48\u0d15\u0d4d\u0d15\u0d1f\u0d35\u0d41\u0d33\u0d4d <br/> \u0d2e\u0d3e\u0d34\u0d48 \u0d30\u0d42\u0d2a\u0d29\u0d4d\u0d2e\u0d41\u0d15 \u0d2e\u0d24\u0d4d\u0d24\u0d3f\u0d15\u0d48\u0d35\u0d3f \u0d24\u0d24\u0d4d\u0d24\u0d30\u0d41\u0d23 \u0d1a\u0d46\u0d19\u0d4d\u0d15\u0d48\u0d2f\u0d3e\u0d33\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 5", "T": "\u0d35\u0d3e\u0d15\u0d41 \u0d2a\u0d3e\u0d24\u0d3f\u0d2f\u0d41\u0d31\u0d48 \u0d1a\u0d24\u0d4d\u0d24\u0d3f\u0d15\u0d35\u0d41 \u0d30\u0d3f\u0d15\u0d4d\u0d15\u0d41\u0d24\u0d32\u0d48 <br/> \u0d35\u0d3e\u0d2f\u0d3f\u0d29\u0d4d \u0d2e\u0d3e\u0d24\u0d41\u0d24\u0d41\u0d15\u0d3f\u0d30\u0d4d \u0d2a\u0d1a\u0d4d\u0d1a\u0d48\u0d35\u0d1f\u0d3f \u0d35\u0d3f\u0d1a\u0d4d\u0d1a\u0d3f\u0d35\u0d48\u0d2f\u0d46\u0d29\u0d4d <br/> \u0d2e\u0d3e\u0d1a\u0d41 \u0d1a\u0d47\u0d30\u0d46\u0d34\u0d41\u0d2a\u0d3f \u0d31\u0d2a\u0d4d\u0d2a\u0d48\u0d2f\u0d41\u0d2e \u0d31\u0d41\u0d24\u0d4d\u0d24\u0d35\u0d41\u0d2e\u0d48 \u0d24\u0d28\u0d4d\u0d24\u0d35\u0d3e\u0d34\u0d4d\u0d35\u0d47"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 6", "T": "\u0d15\u0d3e\u0d1a\u0d3f \u0d30\u0d3e\u0d2e\u0d46\u0d1a\u0d41\u0d30\u0d2e\u0d4d \u0d30\u0d24\u0d4d\u0d28\u0d15\u0d3f\u0d30\u0d3f \u0d1a\u0d30\u0d4d\u0d2a\u0d4d\u0d2a\u0d15\u0d3f\u0d30\u0d3f <br/> \u0d06\u0d30\u0d42\u0d30\u0d4d \u0d35\u0d47\u0d32\u0d41\u0d30\u0d4d \u0d24\u0d46\u0d35\u0d41\u0d30\u0d4d \u0d15\u0d1a\u0d4d\u0d1a\u0d3f\u0d2e\u0d24\u0d41 \u0d30\u0d48\u0d2a\u0d4d\u0d2a\u0d31\u0d3f\u0d2f\u0d32\u0d4d <br/> \u0d15\u0d3e\u0d35\u0d48 \u0d2e\u0d42\u0d24\u0d41\u0d30\u0d30\u0d41 \u0d23\u0d15\u0d4d\u0d15\u0d3f\u0d30\u0d3f\u0d24\u0d3f \u0d30\u0d41\u0d24\u0d4d\u0d24\u0d23\u0d3f\u0d2f\u0d32\u0d4d \u0d1a\u0d46\u0d28\u0d4d\u0d24\u0d3f\u0d32\u0d4d\u0d28\u0d3e\u0d15\u0d48"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 7", "T": "\u0d15\u0d3e\u0d34\u0d3f \u0d35\u0d47\u0d33\u0d41\u0d30\u0d4d\u0d2a\u0d34 \u0d28\u0d3f\u0d15\u0d4d\u0d15\u0d3f\u0d30\u0d3f \u0d15\u0d41\u0d31\u0d41\u0d15\u0d4d\u0d15\u0d48\u0d24\u0d3f\u0d30\u0d41 <br/> \u0d28\u0d3e\u0d35 \u0d32\u0d42\u0d30\u0d4d\u0d24\u0d3f\u0d30\u0d41\u0d35\u0d46 \u0d23\u0d46\u0d2f\u0d4d\u0d2a\u0d4d\u0d2a\u0d24\u0d3f\u0d2f\u0d3f\u0d29\u0d4d \u0d2e\u0d3f\u0d15\u0d4d\u0d15\u0d24\u0d3f\u0d15\u0d34\u0d4d <br/> \u0d15\u0d3e\u0d24\u0d32\u0d4d \u0d1a\u0d4b\u0d32\u0d48\u0d35\u0d33\u0d30\u0d4d \u0d35\u0d46\u0d31\u0d4d\u0d2a\u0d3f\u0d32\u0d41\u0d31\u0d48 \u0d2e\u0d41\u0d24\u0d4d\u0d24\u0d30\u0d4d\u0d2a\u0d41\u0d15\u0d34\u0d4d \u0d24\u0d2e\u0d4d\u0d2a\u0d3f\u0d30\u0d3e\u0d29\u0d47."}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0906\u091a\u0948 \u0928\u093e\u0932\u0941\u091a\u0924\u0941 \u0930\u0915\u094d\u0915\u092e\u0932 \u0