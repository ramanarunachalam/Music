0cbe\u0cae\u0ccd\u0cb0\u0ccd\u0c97 \u0ca8\u0cbe\u0ca8\u0cbe\u0cb5\u0cbf\u0ca7 \u0cae\u0cc1\u0c95\u0ccd\u0ca4\u0cbf \u0cb5\u0cbf\u0ca7\u0cbe\u0caf\u0c95 \u0c9a\u0cb0\u0cbe\u0c9a\u0cb0\u0cbe\u0ca4\u0ccd\u0cae\u0c95 \u0cb0\u0cc2\u0caa \u0ca8\u0cae\u0ccb"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0cb5\u0cbf\u0ca6\u0cc2\u0cb0 \u0c95\u0cc1\u0caf\u0ccb\u0c97\u0cbf\u0ca8\u0cbe\u0cae\u0ccd \u0caa\u0ca6 \u0caa\u0c82\u0c95\u0c9c \u0cb5\u0cbf\u0ca8\u0cc1\u0ca4 \u0c9c\u0ca8\u0cbe\u0cb5\u0ca8 \u0cb0\u0cae\u0cbf\u0ca4 \u0caa\u0cb0\u0cae\u0cc0\u0cb8 \u0ca8\u0cae\u0ccb"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0ca6\u0cbe\u0ca8 \u0cb8\u0ccc \u0cae\u0ca8 \u0ca4\u0caa\u0ccb\u0caf \u0cb8\u0cbe\u0ca6\u0caf \u0ca4\u0ca8\u0ccd\u0ca4\u0ccd\u0cb0 \u0cae\u0ca8\u0ccd\u0ca4\u0ccd\u0cb0 \u0cab\u0cb2 \u0ca6\u0cbe\u0caf\u0c95 \u0cae\u0c82\u0c97\u0cb3 <br/> \u0c97\u0cbe\u0ca8 \u0cb8\u0cae\u0ccd\u0caa\u0ca6 \u0ca8\u0cbe\u0cb0\u0ca6\u0cbe\u0ca6\u0cbf \u0cae\u0cc1\u0ca8\u0cbf\u0c97\u0cc0\u0caf \u0cae\u0cbe\u0ca8\u0cb5\u0cb0 \u0c95\u0cc0\u0cb0\u0ccd\u0ca4\u0cbf \u0cb5\u0cbf\u0ca7\u0cbe\u0cb0\u0ca3 \u0ca8\u0cae\u0ccb"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0c95\u0ccd\u0cb7\u0cc0\u0cb0 \u0caa\u0cbe\u0cb0\u0cbe\u0cb5\u0cb0 \u0ca4\u0cb0\u0c82\u0c97 \u0cae\u0ccd\u0cb0\u0ccd\u0ca6\u0cc1 \u0ca4\u0cb0\u0cb3 \u0caa\u0c82\u0c95\u0c9c\u0cbe\u0caa\u0ca4\u0caf\u0cc7 <br/> \u0c95\u0ccd\u0cb7\u0cbf\u0ca4\u0cbf\u0c9c\u0cbe\u0caa\u0ca4\u0caf\u0cc7 \u0c95\u0ccd\u0cb7\u0cbf\u0ca4\u0cc0\u0caa\u0ca6\u0caf\u0cc7 \u0ca6\u0cbf\u0ca8\u0c95\u0cb0 \u0c9a\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0caa\u0ca6\u0caf\u0cc7 \u0caa\u0ca6\u0caf\u0cc7 <br/> \u0cb8\u0cbe\u0ca7\u0cc1 \u0c9c\u0ca8\u0cbe\u0cae\u0ccd\u0caa\u0ca6\u0caf\u0cc7 \u0cb5\u0ccd\u0cb0\u0c9c\u0caa\u0ca6\u0caf\u0cc7 \u0ca4\u0cbe\u0ca8 \u0caf\u0ccb\u0c97 \u0c9c\u0caa\u0ca4\u0caa \u0cb8\u0cbe\u0ca7\u0ca8 <br/> \u0cb8\u0c82\u0c97\u0cc0\u0ca4 \u0caa\u0cb0\u0cae\u0ccb\u0ca6 \u0cb5\u0cbf\u0ca7\u0cbe\u0caf\u0c95 \u0caa\u0ca6\u0caf\u0cc7 \u0cae\u0ca7\u0cc1 \u0cae\u0cc1\u0cb0\u0cb9\u0cb0\u0caf\u0cc7 \u0ca8\u0cae\u0ccb"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 7", "T": "\u0ca6\u0cbf\u0cb5\u0ccd\u0caf \u0cae\u0c82\u0c97\u0cb3 \u0cb5\u0cbf\u0c97\u0ccd\u0cb0\u0cb9 \u0cb6\u0ccb\u0cad\u0cae\u0cbe\u0ca8 \u0c9c\u0cb2\u0ca7 \u0cb5\u0cb0\u0ccd\u0ca3 \u0c97\u0cae\u0ccd\u0cad\u0cc0\u0cb0 \u0cb6\u0cc1\u0cac\u0cbe\u0c82\u0c97 <br/> \u0ca7\u0cc0\u0cb0 \u0ca7\u0cb0\u0ccb\u0ca8\u0ccd\u0ca8\u0ca4 \u0cb5\u0cbf\u0cb2\u0cbe\u0cb8 \u0cad\u0cbe\u0cb8\u0cc1\u0cb0 \u0ca6\u0cc7\u0cb5 \u0ca6\u0cc7\u0cb5 \u0cae\u0cb9\u0ca8\u0cc0\u0caf \u0c89\u0ca4\u0ccd\u0ca4\u0cc1\u0c82\u0c97 <br/> \u0cad\u0cb5 \u0ca4\u0cbf\u0cae\u0cbf\u0cb0 \u0c98\u0ccb\u0cb0 \u0cb9\u0cb0\u0cae\u0cbf\u0c95\u0cbf\u0cb0 \u0c95\u0ccb\u0c9f\u0cbf \u0cb5\u0cbf\u0c9c\u0cbf\u0ca4 \u0c95\u0cae\u0cb2\u0ca6\u0cb3 \u0c95\u0cb0\u0cc1\u0ca3\u0cbe \u0caa\u0cbe\u0c82\u0c97 <br/> \u0cad\u0cbe\u0cb5\u0cbf\u0ca4 \u0cb5\u0cc0\u0cb9\u0cbf\u0ca4 \u0ca8\u0cbf\u0cae\u0cbf\u0ca4\u0ccd\u0ca4 \u0cb8\u0ca6\u0ccd \u0caa\u0ccd\u0cb0\u0cc7\u0cae \u0cad\u0cbe\u0c97\u0cb5\u0ca4 \u0c9c\u0ca8 \u0cb9\u0ccd\u0cb0\u0ccd\u0ca6\u0caf\u0cbe\u0ca8\u0ccd\u0ca4 \u0cb0\u0c82\u0c97"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 8", "T": "\u0c9a\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0c9c\u0c9f\u0cbe\u0ca7\u0cb0 \u0cad\u0c97\u0cb5\u0cbe\u0ca8\u0ccd\u0ca8\u0ca4 \u0ca6\u0cc8\u0ca4\u0ccd\u0caf \u0cb5\u0cb0\u0ccd\u0caf \u0cae\u0ca8\u0cc1 \u0c95\u0cc1\u0c9f\u0cc1\u0cae\u0ccd\u0cac\u0cb5\u0cc7\u0ca8 <br/> \u0c9c\u0ca8\u0c95\u0cbe\u0c82\u0c95 \u0ca7\u0ccd\u0cb0\u0cc1\u0cb5 \u0cae\u0cc1\u0c9a\u0cc1\u0c95\u0cc1\u0ca8\u0ccd\u0ca6 \u0cb5\u0cbf\u0ca6\u0cc7\u0cb9 \u0c97\u0cbe\u0ca4\u0cbf \u0cb0\u0c98\u0cc1 \u0ca8\u0cbe\u0c95\u0cc1\u0cb7 \u0cae\u0cbe\u0ca8\u0ccd\u0ca6\u0cbe\u0ca4\u0cbe\u0ca8\u0cc1 <br/> \u0c9a\u0ca8\u0ccd\u0ca6\u0ca8\u0cc1 \u0cac\u0cb2\u0cbf \u0cb0\u0ca8\u0ccd\u0ca6\u0cbf \u0ca6\u0cc7\u0cb5 \u0caa\u0cbf\u0caa\u0ccd\u0caa\u0cbf\u0cb2\u0cbe\u0ca6 \u0cad\u0cc2\u0cb0\u0cbf\u0cb7\u0cc7\u0ca3 \u0ca6\u0cbf\u0cb2\u0cc0\u0caa <br/> \u0c89\u0ca6\u0c82\u0c97 \u0ca6\u0cc7\u0cb5\u0cb2 \u0cb8\u0cbe\u0cb0\u0cb8\u0ccd\u0cb5\u0ca4 \u0cb6\u0c95\u0cb0 \u0caa\u0cb0\u0cbe\u0cb6\u0cb0 \u0cb5\u0cbf\u0c9c\u0caf \u0cb5\u0cbf\u0ca6\u0cc1\u0cb0 \u0c85\u0ca6\u0cc2\u0cb0\u0ccd\u0ca4\u0ccd- <br/> \u0ca4\u0caf\u0cbe\u0cae\u0ccd\u0cac\u0cb0\u0cc0\u0c95\u0ccd\u0cb7 \u0cb5\u0cbf\u0cad\u0cc0\u0cb7\u0ca3 \u0c85\u0ca4\u0cbf\u0cb6\u0caf \u0cae\u0cb9\u0cbf\u0cae\u0ccb\u0ca4\u0ccd\u0ca4\u0cae \u0c9a\u0cbf\u0ca4\u0ccd\u0ca4 \u0cad\u0cbe\u0cb5 <br/> \u0cae\u0cbe\u0cb0\u0cc1\u0ca4 \u0ca4\u0ca8\u0caf \u0caa\u0ccd\u0cb0\u0cae\u0cc1\u0c96\u0cbe\u0ca6\u0cbf \u0cad\u0cbe\u0c97\u0cb5\u0ca4 \u0cb5\u0cbf\u0ca8\u0cc1\u0ca4 \u0ca8\u0cbf\u0cb0\u0ca8\u0ccd\u0ca4\u0cb0 \u0cae\u0ca8\u0ccb \u0cb0\u0cae\u0ca3 \u0ca8\u0cae\u0ccb <br/> \u0cb8\u0cbe\u0cae\u0ccb\u0ca6 \u0c97\u0ccb\u0caa\u0cc0\u0c9c\u0ca8 \u0cac\u0ccd\u0cb0\u0ccd\u0ca8\u0ccd\u0ca6\u0cbe\u0cb0\u0c95 \u0cb8\u0cb0\u0cb8\u0cbe\u0c82\u0c97 \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0 \u0cb0\u0cbe\u0ca7\u0cbe\u0caa\u0ca4\u0cc7 <br/> \u0cb8\u0cb0\u0ccd\u0cb5\u0ca6\u0cbe \u0cad\u0cb0\u0cbf\u0ca4 \u0c97\u0ccb\u0c97\u0ca3 \u0c97\u0ccb\u0cb5\u0cb0\u0ccd\u0ca7\u0ca8 \u0ca4\u0cb0\u0ca3 \u0cad\u0cc1\u0c9c\u0c82\u0c97 \u0cb6\u0cbf\u0cb0\u0cb8\u0cc0 \u0ca8\u0c9f\u0ca8"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d05\u0d17\u0d23\u0d3f\u0d24 \u0d2e\u0d39\u0d3f\u0d2e\u0d3e\u0d26\u0d4d\u0d2d\u0d41\u0d24 \u0d32\u0d40\u0d32 \u0d38\u0d26\u0d3e \u0d15\u0d30\u0d4d\u0d37\u0d3f\u0d24 \u0d30\u0d1c\u0d38\u0d3e\u0d26\u0d3f \u0d38\u0d27\u0d4d\u0d2f\u0d3e\u0d24\u0d4d\u0d2e\u0d15 <br/> \u0d2a\u0d4d\u0d30\u0d2a\u0d02\u0d1a \u0d2a\u0d30\u0d3f\u0d2a\u0d3e\u0d32 \u0d05\u0d28\u0d28\u0d4d\u0d24 \u0d2a\u0d30\u0d4d\u0d2f\u0d02\u0d17 \u0d36\u0d2f\u0d28 \u0d28\u0d2e\u0d4b \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d16\u0d17 \u0d24\u0d41\u0d30\u0d02\u0d17 \u0d15\u0d30\u0d41\u0d23\u0d3e\u0d32\u0d35\u0d3e\u0d32 \u0d2e\u0d27\u0d41\u0d15\u0d48\u0d1f\u0d2c\u0d3e\u0d26\u0d3f \u0d38\u0d41\u0d30\u0d30\u0d3f\u0d2a\u0d41 \u0d15\u0d41\u0d32\u0d15\u0d3e\u0d32"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d15\u0d2e\u0d32\u0d3e \u0d2e\u0d41\u0d16 \u0d15\u0d2e\u0d32\u0d36\u0d3f\u0d32\u0d40\u0d2e\u0d41\u0d16 \u0d38\u0d41\u0d30\u0d17\u0d23 \u0d38\u0d1c\u0d4d\u0d1c\u0d28 \u0d28\u0d24\u0d3e\u0d02\u0d17\u0d4d\u0d30\u0d3f\u0d2f\u0d41\u0d17"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d28\u0d2e\u0d4b \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47 \u0d2a\u0d41\u0d30\u0d41\u0d37\u0d4b\u0d24\u0d4d\u0d24\u0d2e \u0d28\u0d2e\u0d4b \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47 \u0d28\u0d3e\u0d30\u0d3e\u0d2f\u0d23 \u0d2a\u0d41\u0d30\u0d41\u0d37\u0d4b\u0d24\u0d4d\u0d24\u0d2e <br/> \u0d28\u0d2e\u0d4b \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47 \u0d2a\u0d41\u0d28\u0d30\u0d2a\u0d3f \u0d2a\u0d41\u0d28\u0d30\u0d2a\u0d3f \u0d28\u0d3e\u0d30\u0d3e\u0d2f\u0d23\u0d3e \u0d05\u0d28\u0d28\u0d4d\u0d24 \u0d32\u0d4b\u0d15\u0d2a\u0d24\u0d47"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d36\u0d4d\u0d30\u0d4d\u0d24\u0d1c\u0d28 \u0d15\u0d32\u0d3f \u0d15\u0d32\u0d4d\u0d2e\u0d37 \u0d39\u0d30 \u0d26\u0d3e\u0d28\u0d35\u0d15\u0d41\u0d32 \u0d2d\u0d02\u0d1c\u0d28 \u0d30\u0d2e\u0d3e \u0d30\u0d2e\u0d23 \u0d28\u0d2e\u0d4b"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d2e\u0d28\u0d38\u0d3e\u0d2e\u0d4d\u0d30\u0d4d\u0d17 \u0d28\u0d3e\u0d28\u0d3e\u0d35\u0d3f\u0d27 \u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3f \u0d35\u0d3f\u0d27\u0d3e\u0d2f\u0d15 \u0d1a\u0d30\u0d3e\u0d1a\u0d30\u0d3e\u0d24\u0d4d\u0d2e\u0d15 \u0d30\u0d42\u0d2a \u0d28\u0d2e\u0d4b"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d35\u0d3f\u0d26\u0d42\u0d30 \u0d15\u0d41\u0d2f\u0d4b\u0d17\u0d3f\u0d28\u0d3e\u0d2e\u0d4d \u0d2a\u0d26 \u0d2a\u0d02\u0d15\u0d1c \u0d35\u0d3f\u0d28\u0d41\u0d24 \u0d1c\u0d28\u0d3e\u0d35\u0d28 \u0d30\u0d2e\u0d3f\u0d24 \u0d2a\u0d30\u0d2e\u0d40\u0d38 \u0d28\u0d2e\u0d4b"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 5", "T": "\u0d26\u0d3e\u0d28 \u0d38\u0d4c \u0d2e\u0d28 \u0d24\u0d2a\u0d4b\u0d2f \u0d38\u0d3e\u0d26\u0d2f \u0d24\u0d28\u0d4d\u0d24\u0d4d\u0d30 \u0d2e\u0d28\u0d4d\u0d24\u0d4d\u0d30 \u0d2b\u0d32 \u0d26\u0d3e\u0d2f\u0d15 \u0d2e\u0d02\u0d17\u0d33 <br/> \u0d17\u0d3e\u0d28 \u0d38\u0d2e\u0d4d\u0d2a\u0d26 \u0d28\u0d3e\u0d30\u0d26\u0d3e\u0d26\u0d3f \u0d2e\u0d41\u0d28\u0d3f\u0d17\u0d40\u0d2f \u0d2e\u0d3e\u0d28\u0d35\u0d30 \u0d15\u0d40\u0d30\u0d4d\u0d24\u0d3f \u0d35\u0d3f\u0d27\u0d3e\u0d30\u0d23 \u0d28\u0d2e\u0d4b"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 6", "T": "\u0d15\u0d4d\u0d37\u0d40\u0d30 \u0d2a\u0d3e\u0d30\u0d3e\u0d35\u0d30 \u0d24\u0d30\u0d02\u0d17 \u0d2e\u0d4d\u0d30\u0d4d\u0d26\u0d41 \u0d24\u0d30\u0d33 \u0d2a\u0d02\u0d15\u0d1c\u0d3e\u0d2a\u0d24\u0d2f\u0d47 <br/> \u0d15\u0d4d\u0d37\u0d3f\u0d24\u0d3f\u0d1c\u0d3e\u0d2a\u0d24\u0d2f\u0d47 \u0d15\u0d4d\u0d37\u0d3f\u0d24\u0d40\u0d2a\u0d26\u0d2f\u0d47 \u0d26\u0d3f\u0d28\u0d15\u0d30 \u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30 \u0d2a\u0d26\u0d2f\u0d47 \u0d2a\u0d26\u0d2f\u0d47 <br/> \u0d38\u0d3e\u0d27\u0d41 \u0d1c\u0d28\u0d3e\u0d2e\u0d4d\u0d2a\u0d26\u0d2f\u0d47 \u0d35\u0d4d\u0d30\u0d1c\u0d2a\u0d26\u0d2f\u0d47 \u0d24\u0d3e\u0d28 \u0d2f\u0d4b\u0d17 \u0d1c\u0d2a\u0d24\u0d2a \u0d38\u0d3e\u0d27\u0d28 <br/> \u0d38\u0d02\u0d17\u0d40\u0d24 \u0d2a\u0d30\u0d2e\u0d4b\u0d26 \u0d35\u0d3f\u0d27\u0d3e\u0d2f\u0d15 \u0d2a\u0d26\u0d2f\u0d47 \u0d2e\u0d27\u0d41 \u0d2e\u0d41\u0d30\u0d39\u0d30\u0d2f\u0d47 \u0d28\u0d2e\u0d4b"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 7", "T": "\u0d26\u0d3f\u0d35\u0d4d\u0d2f \u0d2e\u0d02\u0d17\u0d33 \u0d35\u0d3f\u0d17\u0d4d\u0d30\u0d39 \u0d36\u0d4b\u0d2d\u0d2e\u0d3e\u0d28 \u0d1c\u0d32\u0d27 \u0d35\u0d30\u0d4d\u0d23 \u0d17\u0d2e\u0d4d\u0d2d\u0d40\u0d30 \u0d36\u0d41\u0d2c\u0d3e\u0d02\u0d17 <br/> \u0d27\u0d40\u0d30 \u0d27\u0d30\u0d4b\u0d28\u0d4d\u0d28\u0d24 \u0d35\u0d3f\u0d32\u0d3e\u0d38 \u0d2d\u0d3e\u0d38\u0d41\u0d30 \u0d26\u0d47\u0d35 \u0d26\u0d47\u0d35 \u0d2e\u0d39\u0d28\u0d40\u0d2f \u0d09\u0d24\u0d4d\u0d24\u0d41\u0d02\u0d17 <br/> \u0d2d\u0d35 \u0d24\u0d3f\u0d2e\u0d3f\u0d30 \u0d18\u0d4b\u0d30 \u0d39\u0d30\u0d2e\u0d3f\u0d15\u0d3f\u0d30 \u0d15\u0d4b\u0d1f\u0d3f \u0d35\u0d3f\u0d1c\u0d3f\u0d24 \u0d15\u0d2e\u0d32\u0d26\u0d33 \u0d15\u0d30\u0d41\u0d23\u0d3e \u0d2a\u0d3e\u0d02\u0d17 <br/> \u0d2d\u0d3e\u0d35\u0d3f\u0d24 \u0d35\u0d40\u0d39\u0d3f\u0d24 \u0d28\u0d3f\u0d2e\u0d3f\u0d24\u0d4d\u0d24 \u0d38\u0d26\u0d4d \u0d2a\u0d4d\u0d30\u0d47\u0d2e \u0d2d\u0d3e\u0d17\u0d35\u0d24 \u0d1c\u0d28 \u0d39\u0d4d\u0d30\u0d4d\u0d26\u0d2f\u0d3e\u0d28\u0d4d\u0d24 \u0d30\u0d02\u0d17"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 8", "T": "\u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30 \u0d1c\u0d1f\u0d3e\u0d27\u0d30 \u0d2d\u0d17\u0d35\u0d3e\u0d28\u0d4d\u0d28\u0d24 \u0d26\u0d48\u0d24\u0d4d\u0d2f \u0d35\u0d30\u0d4d\u0d2f \u0d2e\u0d28\u0d41 \u0d15\u0d41\u0d1f\u0d41\u0d2e\u0d4d\u0d2c\u0d35\u0d47\u0d28 <br/> \u0d1c\u0d28\u0d15\u0d3e\u0d02\u0d15 \u0d27\u0d4d\u0d30\u0d41\u0d35 \u0d2e\u0d41\u0d1a\u0d41\u0d15\u0d41\u0d28\u0d4d\u0d26 \u0d35\u0d3f\u0d26\u0d47\u0d39 \u0d17\u0d3e\u0d24\u0d3f \u0d30\u0d18\u0d41 \u0d28\u0d3e\u0d15\u0d41\u0d37 \u0d2e\u0d3e\u0d28\u0d4d\u0d26\u0d3e\u0d24\u0d3e\u0d28\u0d41 <br/> \u0d1a\u0d28\u0d4d\u0d26\u0d28\u0d41 \u0d2c\u0d32\u0d3f \u0d30\u0d28\u0d4d\u0d26\u0d3f \u0d26\u0d47\u0d35 \u0d2a\u0d3f\u0d2a\u0d4d\u0d2a\u0d3f\u0d32\u0d3e\u0d26 \u0d2d\u0d42\u0d30\u0d3f\u0d37\u0d47\u0d23 \u0d26\u0d3f\u0d32\u0d40\u0d2a <br/> \u0d09\u0d26\u0d02\u0d17 \u0d26\u0d47\u0d35\u0d32 \u0d38\u0d3e\u0d30\u0d38\u0d4d\u0d35\u0d24 \u0d36\u0d15\u0d30 \u0d2a\u0d30\u0d3e\u0d36\u0d30 \u0d35\u0d3f\u0d1c\u0d2f \u0d35\u0d3f\u0d26\u0d41\u0d30 \u0d05\u0d26\u0d42\u0d30\u0d4d\u0d24\u0d4d- <br/> \u0d24\u0d2f\u0d3e\u0d2e\u0d4d\u0d2c\u0d30\u0d40\u0d15\u0d4d\u0d37 \u0d35\u0d3f\u0d2d\u0d40\u0d37\u0d23 \u0d05\u0d24\u0d3f\u0d36\u0d2f \u0d2e\u0d39\u0d3f\u0d2e\u0d4b\u0d24\u0d4d\u0d24\u0d2e \u0d1a\u0d3f\u0d24\u0d4d\u0d24 \u0d2d\u0d3e\u0d35 <br/> \u0d2e\u0d3e\u0d30\u0d41\u0d24 \u0d24\u0d28\u0d2f \u0d2a\u0d4d\u0d30\u0d2e\u0d41\u0d16\u0d3e\u0d26\u0d3f \u0d2d\u0d3e\u0d17\u0d35\u0d24 \u0d35\u0d3f\u0d28\u0d41\u0d24 \u0d28\u0d3f\u0d30\u0d28\u0d4d\u0d24\u0d30 \u0d2e\u0d28\u0d4b \u0d30\u0d2e\u0d23 \u0d28\u0d2e\u0d4b <br/> \u0d38\u0d3e\u0d2e\u0d4b\u0d26 \u0d17\u0d4b\u0d2a\u0d40\u0d1c\u0d28 \u0d2c\u0d4d\u0d30\u0d4d\u0d28\u0d4d\u0d26\u0d3e\u0d30\u0d15 \u0d38\u0d30\u0d38\u0d3e\u0d02\u0d17 \u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30 \u0d30\u0d3e\u0d27\u0d3e\u0d2a\u0d24\u0d47 <br/> \u0d38\u0d30\u0d4d\u0d35\u0d26\u0d3e \u0d2d\u0d30\u0d3f\u0d24 \u0d17\u0d4b\u0d17\u0d23 \u0d17\u0d4b\u0d35\u0d30\u0d4d\u0d27\u0d28 \u0d24\u0d30\u0d23 \u0d2d\u0d41\u0d1c\u0d02\u0d17 \u0d36\u0d3f\u0d30\u0d38\u0d40 \u0d28\u0d1f\u0d28"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0905\u0917\u0923\u093f\u0924 \u092e\u0939\u093f\u092e\u093e\u0926\u094d\u092d\u0941\u0924 \u0932\u0940\u0932 \u0938\u0926\u093e \u0915\u0930\u094d\u0937\u093f\u0924 \u0930\u091c\u0938\u093e\u0926\u093f \u0938\u0927\u094d\u092f\u093e\u0924\u094d\u092e\u0915 <br/> \u092a\u094d\u0930\u092a\u0902\u091a \u092a\u0930\u093f\u092a\u093e\u0932 \u0905\u0928\u0928\u094d\u0924 \u092a\u0930\u094d\u092f\u0902\u0917 \u0936\u092f\u0928 \u0928\u092e\u094b \u0928\u092e\u0938\u094d\u0924\u0947"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0916\u0917 \u0924\u0941\u0930\u0902\u0917 \u0915\u0930\u0941\u0923\u093e\u0932\u0935\u093e\u0932 \u092e\u0927\u0941\u0915\u0948\u091f\u092c\u093e\u0926\u093f \u0938\u0941\u0930\u0930\u093f\u092a\u0941 \u0915\u0941\u0932\u0915\u093e\u0932"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0915\u092e\u0932\u093e \u092e\u0941\u0916 \u0915\u092e\u0932\u0936\u093f\u0932\u0940\u092e\u0941\u0916 \u0938\u0941\u0930\u0917\u0923 \u0938\u091c\u094d\u091c\u0928 \u0928\u0924\u093e\u0902\u0917\u094d\u0930\u093f\u092f\u0941\u0917"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0928\u092e\u094b \u0928\u092e\u0938\u094d\u0924\u0947 \u092a\u0941\u