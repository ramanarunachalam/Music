\u0cb0\u0ca4\u0ccd\u0ca8 \u0ca4\u0cbe\u0c9f\u0c99\u0ccd\u0c95 \u0c92\u0cb3\u0cbf \u0c89\u0cae\u0cbf\u0cb3\u0ccd \u0c85\u0cb3\u0c97\u0cbf\u0caf \u0c87\u0cb0\u0cc1 \u0cb8\u0cc6\u0cb5\u0cbf\u0caf\u0cca\u0ca1\u0cc1 \u0c95\u0cc1\u0cb3\u0cbf\u0cb0\u0ccd \u0ca8\u0c98\u0cc8 \u0ca5\u0cb5\u0cb3\u0cc1\u0cae\u0ccd \u0cae\u0ca4\u0cbf\u0cb5\u0ca6\u0ca8"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d05\u0d16\u0d3f\u0d32\u0d3e\u0d23\u0d4d\u0d21 \u0d28\u0d3e\u0d2f\u0d15\u0d3f \u0d05\u0d2e\u0d4d\u0d2c\u0d41\u0d32\u0d3f\u0d19\u0d4d\u0d17\u0d24\u0d4d-\u0d24\u0d32\u0d2e\u0d4d \u0d35\u0d33\u0d30\u0d41\u0d2e\u0d4d"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d1c\u0d17\u0d26\u0d40\u0d36\u0d28\u0d4d \u0d1c\u0d2e\u0d4d\u0d2c\u0d41\u0d15\u0d47\u0d36\u0d28\u0d4d \u0d09\u0d33\u0d2e\u0d4d \u0d15\u0d35\u0d30\u0d4d <br/> \u0d24\u0d3f\u0d30\u0d41\u0d35\u0d3e\u0d28\u0d48\u0d15\u0d4d-\u0d15\u0d3e\u0d35\u0d32\u0d4d \u0d2a\u0d24\u0d3f\u0d24\u0d28\u0d3f\u0d32\u0d4d \u0d0e\u0d34\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d41\u0d33\u0d4d \u0d1c\u0d17\u0d1c\u0d4d\u0d1c\u0d28\u0d28\u0d40"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d09\u0d2d\u0d2f \u0d15\u0d3e\u0d35\u0d47\u0d30\u0d3f \u0d2e\u0d26\u0d4d\u0d27\u0d3f\u0d2f\u0d3f\u0d32\u0d4d \u0d12\u0d30\u0d41 \u0d2f\u0d3e\u0d28\u0d48 \u0d12\u0d30\u0d41 \u0d36\u0d3f\u0d32\u0d28\u0d4d\u0d26\u0d3f\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d \u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3f\u0d2f\u0d33\u0d3f\u0d24\u0d4d\u0d24 <br/> \u0d2e\u0d39\u0d47\u0d36\u0d28\u0d48 \u0d09\u0d33\u0d4d\u0d33\u0d2e\u0d4d \u0d35\u0d3f\u0d30\u0d41\u0d2e\u0d4d\u0d2c\u0d3f \u0d24\u0d35\u0d2e\u0d4d \u0d2a\u0d41\u0d30\u0d3f\u0d28\u0d4d\u0d26\u0d3e\u0d33\u0d4d \u0d09\u0d2e\u0d48 \u0d05\u0d28\u0d4d\u0d28\u0d48\u0d2f\u0d47 \u0d28\u0d3f\u0d24\u0d4d\u0d24\u0d3f\u0d2f \u0d15\u0d28\u0d4d\u0d28\u0d3f\u0d15\u0d48\u0d2f\u0d47 <br/> \u0d05\u0d2d\u0d2f\u0d2e\u0d4d \u0d24\u0d28\u0d4d\u0d26\u0d30\u0d41\u0d33\u0d4d \u0d05\u0d21\u0d3f\u0d2e\u0d32\u0d30\u0d4d \u0d24\u0d1e\u0d4d\u0d1c\u0d2e\u0d4d \u0d05\u0d21\u0d48\u0d28\u0d4d\u0d26\u0d47\u0d28\u0d4d \u0d05\u0d21\u0d3f\u0d2f\u0d35\u0d30\u0d4d\u0d15\u0d4d\u0d15\u0d30\u0d41\u0d33\u0d4d \u0d35\u0d30\u0d26\u0d47 <br/> \u0d05\u0d28\u0d28\u0d4d\u0d24 \u0d17\u0d41\u0d23\u0d3e\u0d15\u0d30\u0d40 \u0d05\u0d2e\u0d4d\u0d2c\u0d3e \u0d36\u0d19\u0d4d\u0d15\u0d30\u0d40 \u0d06\u0d26\u0d3f \u0d05\u0d28\u0d4d\u0d24\u0d2e\u0d3f\u0d32\u0d4d \u0d24\u0d4d\u0d30\u0d3f\u0d2d\u0d41\u0d35\u0d28 \u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3f"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d39\u0d30 \u0d39\u0d30 \u0d36\u0d19\u0d4d\u0d15\u0d30 \u0d2c\u0d17\u0d35\u0d24\u0d4d \u0d2a\u0d3e\u0d26\u0d3e\u0d33\u0d4d \u0d05\u0d28\u0d4d\u0d2c\u0d3f\u0d32\u0d4d \u0d2a\u0d4d\u0d30\u0d24\u0d3f\u0d37\u0d4d\u0d1f\u0d48 \u0d36\u0d46\u0d2f\u0d4d\u0d26 \u0d05\u0d31\u0d4d\u0d2a\u0d41\u0d24 <br/> \u0d30\u0d24\u0d4d\u0d28 \u0d24\u0d3e\u0d1f\u0d19\u0d4d\u0d15 \u0d12\u0d33\u0d3f \u0d09\u0d2e\u0d3f\u0d34\u0d4d \u0d05\u0d34\u0d17\u0d3f\u0d2f \u0d07\u0d30\u0d41 \u0d38\u0d46\u0d35\u0d3f\u0d2f\u0d4a\u0d21\u0d41 \u0d15\u0d41\u0d33\u0d3f\u0d30\u0d4d \u0d28\u0d18\u0d48 \u0d25\u0d35\u0d34\u0d41\u0d2e\u0d4d \u0d2e\u0d24\u0d3f\u0d35\u0d26\u0d28"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0905\u0916\u093f\u0932\u093e\u0923\u094d\u0921 \u0928\u093e\u092f\u0915\u093f \u0905\u092e\u094d\u092c\u0941\u0932\u093f\u0919\u094d\u0917\u0924\u094d-\u0924\u0932\u092e\u094d \u0935\u0932\u0930\u0941\u092e\u094d"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u091c\u0917\u0926\u0940\u0936\u0928\u094d \u091c\u092e\u094d\u092c\u0941\u0915\u0947\u0936\u0928\u094d \u0909\u0932\u092e\u094d \u0915\u0935\u0930\u094d <br/> \u0924\u093f\u0930\u0941\u0935\u093e\u0928\u0948\u0915\u094d-\u0915\u093e\u0935\u0932\u094d \u092a\u0924\u093f\u0924\u0928\u093f\u0932\u094d \u090f\u0932\u0941\u0928\u094d\u0926\u0930\u0941\u093