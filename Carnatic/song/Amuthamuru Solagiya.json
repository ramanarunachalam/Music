u0c30 \u0c2e\u0c40\u0c24\u0c41\u0c15\u0c41 \u0c32\u0c3e\u0c35\u0c3f\u0c2f\u0c41 \u0c32\u0c3e\u0c35\u0c3f\u0c2f \u0c2a\u0c46\u0c30\u0c41\u0c2e\u0c3e\u0c33\u0c47."}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c85\u0cae\u0cc1\u0ca4 \u0cae\u0cc2\u0cb1\u0cc1\u0c9a\u0cca \u0cb2\u0cbe\u0c95\u0cbf\u0caf \u0ca4\u0ccb\u0c95\u0cc8\u0caf\u0cb0\u0ccd <br/> \u0caa\u0cca\u0cb0\u0cc1\u0cb3\u0cc1 \u0cb3\u0cbe\u0cb0\u0cc8\u0caf\u0cc6 \u0ca8\u0cbe\u0ca3\u0cc8\u0caf\u0cc1 \u0ca8\u0cbe\u0ca3\u0cc8\u0caf\u0cc6 <br/> \u0ca8\u0cb0\u0cc1\u0c95\u0cc1 \u0cb5\u0cc0\u0c9f\u0cbf\u0ca4\u0cc1 \u0ca4\u0cbe\u0ca8\u0ca4\u0cbf\u0cb2\u0ccd \u0cb5\u0cbe\u0cb0\u0cc1\u0cae\u0cc6 \u0ca8\u0cc1\u0cb0\u0cc8\u0c95\u0cc2\u0cb1\u0cc1\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c85\u0c9a\u0c9f\u0cc1 \u0cae\u0cbe\u0ca4\u0cb0\u0ccd\u0c95\u0cc1 \u0cb5\u0cbe\u0ca4\u0cc1\u0c9a\u0cca\u0cb2\u0ccd \u0c95\u0cc7\u0c9f\u0cbf\u0c95\u0cb3\u0ccd <br/> \u0ca4\u0cc6\u0cb0\u0cc1\u0cb5\u0cbf\u0ca8\u0ccd \u0cae\u0cc0\u0ca4\u0cc1\u0c95\u0cc1 \u0cb2\u0cbe\u0cb5\u0cbf\u0caf\u0cc1 \u0cb2\u0cbe\u0cb5\u0cbf\u0c95\u0cb3\u0ccd <br/> \u0c85\u0cb5\u0cb0\u0ccd\u0c95\u0cb3\u0ccd \u0cae\u0cbe\u0caf\u0cc8\u0caa \u0c9f\u0cbe\u0cae\u0cb2\u0ccd\u0c95\u0cc6 \u0c9f\u0cbe\u0cae\u0cb2\u0ccd\u0ca8\u0cbf \u0ca8\u0cb0\u0cc1\u0cb3\u0ccd\u0ca4\u0cbe\u0cb0\u0cbe\u0caf\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c95\u0cc1\u0cae\u0cb0\u0cbf \u0c95\u0cbe\u0cb3\u0cbf\u0cb5 \u0cb0\u0cbe\u0c95\u0cbf\u0cae \u0c95\u0cc7\u0c9a\u0cc1\u0cb0\u0cbf <br/> \u0c95\u0cb5\u0cc1\u0cb0\u0cbf \u0cae\u0ccb\u0c9f\u0cbf\u0c9a\u0cc1 \u0cb0\u0cbe\u0cb0\u0cbf\u0ca8\u0cbf \u0cb0\u0cbe\u0caa\u0cb0\u0cbf <br/> \u0c95\u0cca\u0c9f\u0cbf\u0caf \u0c9a\u0cc2\u0cb2\u0cbf\u0c9a\u0cc1 \u0c9f\u0cbe\u0cb0\u0ca3\u0cbf \u0caf\u0cbe\u0cae\u0cb3\u0cbf \u0cae\u0c95\u0cae\u0cbe\u0caf\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c95\u0cc1\u0cb1\u0cb3\u0cc1 \u0cb0\u0cc2\u0caa\u0cae\u0cc1 \u0cb0\u0cbe\u0cb0\u0cbf\u0c9a \u0c95\u0ccb\u0ca4\u0cb0\u0cbf <br/> \u0caf\u0cc1\u0cb2\u0c95 \u0ca4\u0cbe\u0cb0\u0cbf\u0c89\u0ca4\u0cbe\u0cb0\u0cbf\u0caa \u0cb0\u0cbe\u0caa\u0cb0\u0cbf <br/> \u0c95\u0cc1\u0cb0\u0cc1\u0caa \u0cb0\u0cbe\u0cb0\u0cbf\u0cb5\u0cbf \u0c95\u0cbe\u0cb0\u0cbf\u0ca8 \u0cae\u0ccb\u0c95\u0cb0\u0cbf \u0c85\u0caa\u0cbf\u0cb0\u0cbe\u0cae\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c9a\u0cae\u0cb0 \u0ca8\u0cc0\u0cb2\u0cbf\u0caa\u0cc1 \u0cb0\u0cbe\u0cb0\u0cbf\u0ca4 \u0ca8\u0cbe\u0caf\u0c95\u0cbf <br/> \u0cae\u0cb2\u0cc8\u0c95\u0cc1 \u0cae\u0cbe\u0cb0\u0cbf\u0c95 \u0caa\u0cbe\u0cb2\u0cbf\u0ca8 \u0ca8\u0cbe\u0cb0\u0ca3\u0cbf <br/> \u0c9a\u0cb2\u0cbf\u0cb2 \u0cae\u0cbe\u0cb0\u0cbf\u0c9a\u0cbf \u0cb5\u0cbe\u0caf\u0cae \u0ca8\u0ccb\u0c95\u0cb0\u0cbf \u0caa\u0cb0\u0cc8\u0caf\u0ccb\u0c95\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0c9a\u0cb5\u0cc1\u0cb0\u0cbf \u0cb5\u0cc0\u0cb0\u0cbf\u0cae\u0cc1 \u0ca8\u0cc0\u0cb0\u0ccd\u0cb5\u0cbf\u0c9f \u0caa\u0ccb\u0c9c\u0ca8\u0cbf <br/> \u0ca4\u0cbf\u0c95\u0cbf\u0cb0\u0cbf \u0cae\u0cc7\u0cb5\u0cc1\u0c95\u0cc8 \u0caf\u0cbe\u0cb3\u0cbf\u0c9a\u0cc6 \u0caf\u0cbe\u0cb3\u0cca\u0cb0\u0cc1 <br/> \u0c9a\u0c95\u0cb2 \u0cb5\u0cc7\u0ca4\u0cae\u0cc1 \u0cae\u0cbe\u0caf\u0cbf\u0ca8 \u0ca4\u0cbe\u0caf\u0cc1\u0cae\u0cc8 \u0caf\u0cb0\u0cc1\u0cb3\u0ccd\u0caa\u0cbe\u0cb2\u0cbe"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0ca4\u0cbf\u0cae\u0cbf\u0ca4 \u0cae\u0cbe\u0c9f\u0cc1\u0c9a\u0cc1 \u0cb0\u0cbe\u0cb0\u0cbf\u0ca8\u0cbf \u0c9a\u0cbe\u0c9a\u0cb0\u0cb0\u0ccd <br/> \u0cae\u0cc1\u0c9f\u0cbf\u0c95 \u0c9f\u0ccb\u0cb1\u0cc1\u0c95 \u0c9f\u0cbe\u0cb5\u0cbf\u0caf\u0cbf \u0c9f\u0cc7\u0caf\u0cca\u0cb0\u0cc1 <br/> \u0c9a\u0cbf\u0cb2\u0caa \u0c9a\u0cbe\u0c9a\u0cc1\u0c95\u0cc1 \u0ca3\u0cbe\u0cb2\u0cbf\u0ca8\u0cbf \u0ca3\u0cbe\u0cae\u0cc1\u0ca3 \u0cb5\u0cbf\u0c9f\u0cc1\u0cae\u0ccd\u0cb5\u0cc7\u0cb2\u0cbe"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 7", "T": "\u0ca4\u0cbf\u0cb0\u0cc1\u0cb5\u0cc1 \u0cb2\u0cbe\u0cb5\u0cc1\u0c9a\u0cca \u0ca3\u0cc7\u0c9a\u0cb0 \u0ca3\u0cbe\u0cae\u0cb2\u0cc8 <br/> \u0cae\u0cc1\u0c95\u0cbf\u0cb2\u0cc1 \u0cb2\u0cbe\u0cb5\u0cc1\u0cb5\u0cbf \u0cae\u0cbe\u0ca8\u0ca8 \u0cb5\u0ccb\u0ca8\u0cbf\u0cb2\u0cc8 <br/> \u0c9a\u0cbf\u0c95\u0cb0 \u0cae\u0cc0\u0ca4\u0cc1\u0c95\u0cc1 \u0cb2\u0cbe\u0cb5\u0cbf\u0caf\u0cc1 \u0cb2\u0cbe\u0cb5\u0cbf\u0caf \u0caa\u0cc6\u0cb0\u0cc1\u0cae\u0cbe\u0cb3\u0cc7."}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d05\u0d2e\u0d41\u0d24 \u0d2e\u0d42\u0d31\u0d41\u0d1a\u0d4a \u0d32\u0d3e\u0d15\u0d3f\u0d2f \u0d24\u0d4b\u0d15\u0d48\u0d2f\u0d30\u0d4d <br/> \u0d2a\u0d4a\u0d30\u0d41\u0d33\u0d41 \u0d33\u0d3e\u0d30\u0d48\u0d2f\u0d46 \u0d28\u0d3e\u0d23\u0d48\u0d2f\u0d41 \u0d28\u0d3e\u0d23\u0d48\u0d2f\u0d46 <br/> \u0d28\u0d30\u0d41\u0d15\u0d41 \u0d35\u0d40\u0d1f\u0d3f\u0d24\u0d41 \u0d24\u0d3e\u0d28\u0d24\u0d3f\u0d32\u0d4d \u0d35\u0d3e\u0d30\u0d41\u0d2e\u0d46 \u0d28\u0d41\u0d30\u0d48\u0d15\u0d42\u0d31\u0d41\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d05\u0d1a\u0d1f\u0d41 \u0d2e\u0d3e\u0d24\u0d30\u0d4d\u0d15\u0d41 \u0d35\u0d3e\u0d24\u0d41\u0d1a\u0d4a\u0d32\u0d4d \u0d15\u0d47\u0d1f\u0d3f\u0d15\u0d33\u0d4d <br/> \u0d24\u0d46\u0d30\u0d41\u0d35\u0d3f\u0d28\u0d4d \u0d2e\u0d40\u0d24\u0d41\u0d15\u0d41 \u0d32\u0d3e\u0d35\u0d3f\u0d2f\u0d41 \u0d32\u0d3e\u0d35\u0d3f\u0d15\u0d33\u0d4d <br/> \u0d05\u0d35\u0d30\u0d4d\u0d15\u0d33\u0d4d \u0d2e\u0d3e\u0d2f\u0d48\u0d2a \u0d1f\u0d3e\u0d2e\u0d32\u0d4d\u0d15\u0d46 \u0d1f\u0d3e\u0d2e\u0d32\u0d4d\u0d28\u0d3f \u0d28\u0d30\u0d41\u0d33\u0d4d\u0d24\u0d3e\u0d30\u0d3e\u0d2f\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d15\u0d41\u0d2e\u0d30\u0d3f \u0d15\u0d3e\u0d33\u0d3f\u0d35 \u0d30\u0d3e\u0d15\u0d3f\u0d2e \u0d15\u0d47\u0d1a\u0d41\u0d30\u0d3f <br/> \u0d15\u0d35\u0d41\u0d30\u0d3f \u0d2e\u0d4b\u0d1f\u0d3f\u0d1a\u0d41 \u0d30\u0d3e\u0d30\u0d3f\u0d28\u0d3f \u0d30\u0d3e\u0d2a\u0d30\u0d3f <br/> \u0d15\u0d4a\u0d1f\u0d3f\u0d2f \u0d1a\u0d42\u0d32\u0d3f\u0d1a\u0d41 \u0d1f\u0d3e\u0d30\u0d23\u