c4d \u0c35\u0c3e\u0c1f\u0c4d\u0c15\u0c23\u0c4d \u0c28\u0c19\u0c4d\u0c15\u0c48\u0c2f\u0c41\u0c2e\u0c4d, <br/> \u0c07\u0c30\u0c41\u0c35\u0c30\u0c41\u0c2e\u0c4d \u0c2e\u0c3e\u0c31\u0c3f\u0c2a\u0c4d \u0c2a\u0c41\u0c15\u0c4d\u0c15\u0c41, \u0c07\u0c24\u0c2f\u0c2e\u0c4d \u0c0e\u0c2f\u0c4d\u0c24\u0c3f\u0c28\u0c3e\u0c30\u0c4d."}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 4", "T": "\u0c2e\u0c30\u0c41\u0c19\u0c4d\u0c15\u0c41 \u0c07\u0c32\u0c3e \u0c28\u0c19\u0c4d\u0c15\u0c48\u0c2f\u0c41\u0c2e\u0c4d, \u0c35\u0c1a\u0c48 \u0c07\u0c32\u0c4d \u0c10\u0c2f\u0c28\u0c41\u0c2e\u0c4d, <br/> \u0c12\u0c30\u0c41\u0c19\u0c4d\u0c15\u0c3f\u0c2f \u0c07\u0c30\u0c23\u0c4d\u0c1f\u0c41 \u0c09\u0c1f\u0c31\u0c4d\u0c15\u0c41 \u0c09\u0c2f\u0c3f\u0c30\u0c4d \u0c12\u0c28\u0c4d\u0c31\u0c41 \u0c06\u0c2f\u0c3f\u0c28\u0c3e\u0c30\u0c4d - <br/> \u0c15\u0c30\u0c41\u0c19\u0c4d\u0c15\u0c1f\u0c32\u0c4d \u0c2a\u0c33\u0c4d\u0c33\u0c3f\u0c2f\u0c3f\u0c32\u0c4d \u0c15\u0c32\u0c35\u0c3f \u0c28\u0c40\u0c19\u0c4d\u0c15\u0c3f\u0c2a\u0c4d \u0c2a\u0c4b\u0c2f\u0c4d\u0c2a\u0c4d <br/> \u0c2a\u0c3f\u0c30\u0c3f\u0c28\u0c4d\u0c24\u0c35\u0c30\u0c4d \u0c15\u0c42\u0c1f\u0c3f\u0c28\u0c3e\u0c32\u0c4d, \u0c2a\u0c47\u0c1a\u0c32\u0c4d \u0c35\u0c47\u0c23\u0c4d\u0c1f\u0c41\u0c2e\u0c4b?"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c85\u0ca3\u0ccd\u0ca3\u0cb2\u0cc1\u0cae\u0ccd \u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cbf\u0ca8\u0cbe\u0ca8\u0ccd \u0c85\u0cb5\u0cb3\u0cc1\u0cae\u0ccd \u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cbf\u0ca8\u0cbe\u0cb3\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c8e\u0ca3\u0ccd\u0ca3 \u0c85\u0cb0\u0cc1 \u0ca8\u0cb2\u0ca4\u0ccd\u0ca4\u0cbf\u0ca8\u0cbe\u0cb3\u0ccd \u0c87\u0ca8\u0cc8\u0caf\u0cb3\u0ccd \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cc1\u0cb3\u0cbf, <br/> \u0c95\u0ca3\u0ccd\u0ca3\u0cca\u0c9f\u0cc1 \u0c95\u0ca3\u0ccd \u0c87\u0ca3\u0cc8 \u0c95\u0cb5\u0ccd\u0cb5\u0cbf, \u0c92\u0ca8\u0ccd\u0cb1\u0cc8 \u0c92\u0ca8\u0ccd\u0cb1\u0cc1 <br/> \u0c89\u0ca3\u0ccd\u0ca3\u0cb5\u0cc1\u0cae\u0ccd, \u0ca8\u0cbf\u0cb2\u0cc8\u0caa\u0cc6\u0cb1\u0cbe\u0ca4\u0cc1 \u0c89\u0ca3\u0cb0\u0ccd\u0cb5\u0cc1\u0cae\u0ccd \u0c92\u0ca8\u0ccd\u0cb1\u0cbf\u0c9f, <br/> \u0c85\u0ca3\u0ccd\u0ca3\u0cb2\u0cc1\u0cae\u0ccd \u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cbf\u0ca8\u0cbe\u0ca8\u0ccd \u0c85\u0cb5\u0cb3\u0cc1\u0cae\u0ccd \u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cbf\u0ca8\u0cbe\u0cb3\u0ccd."}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cbf\u0caf \u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cc1 \u0c8e\u0ca8\u0cc1\u0cae\u0ccd \u0ca8\u0cc1\u0ca4\u0cbf \u0c95\u0cca\u0cb3\u0ccd \u0cb5\u0cc7\u0cb2\u0ccd \u0c87\u0ca3\u0cc8 <br/> \u0c86\u0c95\u0ccd\u0c95\u0cbf\u0caf \u0cae\u0ca4\u0cc1\u0c95\u0cc8\u0caf\u0cbe\u0ca8\u0ccd \u0ca4\u0ccb\u0cb3\u0cbf\u0ca8\u0ccd \u0c86\u0cb3\u0ccd\u0ca8\u0ccd\u0ca4\u0ca8 <br/> \u0cb5\u0cc0\u0c95\u0ccd\u0c95\u0cbf\u0caf \u0c95\u0ca8\u0cc8 \u0c95\u0cb3\u0cb2\u0ccd \u0cb5\u0cc0\u0cb0\u0ca8\u0ccd \u0c9a\u0cc6\u0c99\u0ccd\u0c95\u0ca3\u0cc1\u0cae\u0ccd <br/> \u0ca4\u0cbe\u0c95\u0ccd\u0c95\u0cc1 \u0c85\u0ca3\u0c99\u0ccd\u0c95\u0cc1 \u0c85\u0ca8\u0cc8\u0caf\u0cb5\u0cb3\u0ccd \u0ca4\u0ca8\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0ccd \u0ca4\u0cc8\u0ca4\u0ccd\u0ca4\u0cb5\u0cc7."}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0caa\u0cb0\u0cc1\u0c95\u0cbf\u0caf \u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cc1 \u0c8e\u0ca8\u0cc1\u0cae\u0ccd \u0caa\u0cbe\u0c9a\u0ca4\u0ccd\u0ca4\u0cbe\u0cb2\u0ccd \u0caa\u0cbf\u0ca3\u0cbf\u0ca4\u0ccd\u0ca4\u0cc1, <br/> \u0c92\u0cb0\u0cc1\u0cb5\u0cb0\u0cc8 \u0c92\u0cb0\u0cc1\u0cb5\u0cb0\u0ccd\u0ca4\u0cae\u0ccd \u0c89\u0cb3\u0ccd\u0cb3\u0cae\u0ccd \u0c88\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0cb2\u0cbe\u0cb2\u0ccd, <br/> \u0cb5\u0cb0\u0cbf \u0c9a\u0cbf\u0cb2\u0cc8 \u0c85\u0ca3\u0ccd\u0ca3\u0cb2\u0cc1\u0cae\u0ccd \u0cb5\u0cbe\u0c9f\u0ccd\u0c95\u0ca3\u0ccd \u0ca8\u0c99\u0ccd\u0c95\u0cc8\u0caf\u0cc1\u0cae\u0ccd, <br/> \u0c87\u0cb0\u0cc1\u0cb5\u0cb0\u0cc1\u0cae\u0ccd \u0cae\u0cbe\u0cb1\u0cbf\u0caa\u0ccd \u0caa\u0cc1\u0c95\u0ccd\u0c95\u0cc1, \u0c87\u0ca4\u0caf\u0cae\u0ccd \u0c8e\u0caf\u0ccd\u0ca4\u0cbf\u0ca8\u0cbe\u0cb0\u0ccd."}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0cae\u0cb0\u0cc1\u0c99\u0ccd\u0c95\u0cc1 \u0c87\u0cb2\u0cbe \u0ca8\u0c99\u0ccd\u0c95\u0cc8\u0caf\u0cc1\u0cae\u0ccd, \u0cb5\u0c9a\u0cc8 \u0c87\u0cb2\u0ccd \u0c90\u0caf\u0ca8\u0cc1\u0cae\u0ccd, <br/> \u0c92\u0cb0\u0cc1\u0c99\u0ccd\u0c95\u0cbf\u0caf \u0c87\u0cb0\u0ca3\u0ccd\u0c9f\u0cc1 \u0c89\u0c9f\u0cb1\u0ccd\u0c95\u0cc1 \u0c89\u0caf\u0cbf\u0cb0\u0ccd \u0c92\u0ca8\u0ccd\u0cb1\u0cc1 \u0c86\u0caf\u0cbf\u0ca8\u0cbe\u0cb0\u0ccd - <br/> \u0c95\u0cb0\u0cc1\u0c99\u0ccd\u0c95\u0c9f\u0cb2\u0ccd \u0caa\u0cb3\u0ccd\u0cb3\u0cbf\u0caf\u0cbf\u0cb2\u0ccd \u0c95\u0cb2\u0cb5\u0cbf \u0ca8\u0cc0\u0c99\u0ccd\u0c95\u0cbf\u0caa\u0ccd \u0caa\u0ccb\u0caf\u0ccd\u0caa\u0ccd <br/> \u0caa\u0cbf\u0cb0\u0cbf\u0ca8\u0ccd\u0ca4\u0cb5\u0cb0\u0ccd \u0c95\u0cc2\u0c9f\u0cbf\u0ca8\u0cbe\u0cb2\u0ccd, \u0caa\u0cc7\u0c9a\u0cb2\u0ccd \u0cb5\u0cc7\u0ca3\u0ccd\u0c9f\u0cc1\u0cae\u0ccb?"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d05\u0d23\u0d4d\u0d23\u0d32\u0d41\u0d2e\u0d4d \u0d28\u0d4b\u0d15\u0d4d\u0d15\u0d3f\u0d28\u0d3e\u0d28\u0d4d \u0d05\u0d35\u0d33\u0d41\u0d2e\u0d4d \u0d28\u0d4b\u0d15\u0d4d\u0d15\u0d3f\u0d28\u0d3e\u0d33\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d0e\u0d23\u0d4d\u0d23 \u0d05\u0d30\u0d41 \u0d28\u0d32\u0d24\u0d4d\u0d24\u0d3f\u0d28\u0d3e\u0d33\u0d4d \u0d07\u0d28\u0d48\u0d2f\u0d33\u0d4d \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d41\u0d34\u0d3f, <br/> \u0d15\u0d23\u0d4d\u0d23\u0d4a\u0d1f\u0d41 \u0d15\u0d23\u0d4d \u0d07\u0d23\u0d48 \u0d15\u0d35\u0d4d\u0d35\u0d3f, \u0d12\u0d28\u0d4d\u0d31\u0d48 \u0d12\u0d28\u0d4d\u0d31\u0d41 <br/> \u0d09\u0d23\u0d4d\u0d23\u0d35\u0d41\u0d2e\u0d4d, \u0d28\u0d3f\u0d32\u0d48\u0d2a\u0d46\u0d31\u0d3e\u0d24\u0d41 \u0d09\u0d23\u0d30\u0d4d\u0d35\u0d41\u0d2e\u0d4d \u0d12\u0d28\u0d4d\u0d31\u0d3f\u0d1f, <br/> \u0d05\u0d23\u0d4d\u0d23\u0d32\u0d41\u0d2e\u0d4d \u0d28\u0d4b\u0d15\u0d4d\u0d15\u0d3f\u0d28\u0d3e\u0d28\u0d4d \u0d05\u0d35\u0d33\u0d41\u0d2e\u0d4d \u0d28\u0d4b\u0d15\u0d4d\u0d15\u0d3f\u0d28\u0d3e\u0d33\u0d4d."}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d28\u0d4b\u0d15\u0d4d\u0d15\u0d3f\u0d2f \u0d28\u0d4b\u0d15\u0d4d\u0d15\u0d41 \u0d0e\u0d28\u0d41\u0d2e\u0d4d \u0d28\u0d41\u0d24\u0d3f \u0d15\u0d4a\u0d33\u0d4d \u0d35\u0d47\u0d32\u0d4d \u0d07\u0d23\u0d48 <br/> \u0d06\u0d15\u0d4d\u0d15\u0d3f\u0d2f \u0d2e\u0d24\u0d41\u0d15\u0d48\u0d2f\u0d3e\u0d28\u0d4d \u0d24\u0d4b\u0d33\u0d3f\u0d28\u0d4d \u0d06\u0d34\u0d4d\u0d28\u0d4d\u0d24\u0d28 <br/> \u0d35\u0d40\u0d15\u0d4d\u0d15\u0d3f\u0d2f \u0d15\u0d28\u0d48 \u0d15\u0d34\u0d32\u0d4d \u0d35\u0d40\u0d30\u0d28\u0d4d \u0d1a\u0d46\u0d19\u0d4d\u0d15\u0d23\u0d41\u0d2e\u0d4d <br/> \u0d24\u0d3e\u0d15\u0d4d\u0d15\u0d41 \u0d05\u0d23\u0d19\u0d4d\u0d15\u0d41 \u0d05\u0d28\u0d48\u0d2f\u0d35\u0d33\u0d4d \u0d24\u0d28\u0d24\u0d4d\u0d24\u0d3f\u0d32\u0d4d \u0d24\u0d48\u0d24\u0d4d\u0d24\u0d35\u0d47."}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d2a\u0d30\u0d41\u0d15\u0d3f\u0d2f \u0d28\u0d4b\u0d15\u0d4d\u0d15\u0d41 \u0d0e\u0d28\u0d41\u0d2e\u0d4d \u0d2a\u0d3e\u0d1a\u0d24\u0d4d\u0d24\u0d3e\u0d32\u0d4d \u0d2a\u0d3f\u0d23\u0d3f\u0d24\u0d4d\u0d24\u0d41, <br/> \u0d12\u0d30\u0d41\u0d35\u0d30\u0d48 \u0d12\u0d30\u0d41\u0d35\u0d30\u0d4d\u0d24\u0d2e\u0d4d \u0d09\u0d33\u0d4d\u0d33\u0d2e\u0d4d \u0d08\u0d30\u0d4d\u0d24\u0d4d\u0d24\u0d32\u0d3e\u0d32\u0d4d, <br/> \u0d35\u0d30\u0d3f \u0d1a\u0d3f\u0d32\u0d48 \u0d05\u0d23\u0d4d\u0d23\u0d32\u0d41\u0d2e\u0d4d \u0d35\u0d3e\u0d1f\u0d4d\u0d15\u0d23\u0d4d \u0d28\u0d19\u0d4d\u0d15\u0d48\u0d2f\u0d41\u0d2e\u0d4d, <br/> \u0d07\u0d30\u0d41\u0d35\u0d30\u0d41\u0d2e\u0d4d \u0d2e\u0d3e\u0d31\u0d3f\u0d2a\u0d4d \u0d2a\u0d41\u0d15\u0d4d\u0d15\u0d41, \u0d07\u0d24\u0d2f\u0d2e\u0d4d \u0d0e\u0d2f\u0d4d\u0d24\u0d3f\u0d28\u0d3e\u0d30\u0d4d."}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d2e\u0d30\u0d41\u0d19\u0d4d\u0d15\u0d41 \u0d07\u0d32\u0d3e \u0d28\u0d19\u0d4d\u0d15\u0d48\u0d2f\u0d41\u0d2e\u0d4d, \u0d35\u0d1a\u0d48 \u0d07\u0d32\u0d4d \u0d10\u0d2f\u0d28\u0d41\u0d2e\u0d4d, <br/> \u0d12\u0d30\u0d41\u0d19\u0d4d\u0d15\u0d3f\u0d2f \u0d07\u0d30\u0d23\u0d4d\u0d1f\u0d41 \u0d09\u0d1f\u0d31\u0d4d\u0d15\u0d41 \u0d09\u0d2f\u0d3f\u0d30\u0d4d \u0d12\u0d28\u0d4d\u0d31\u0d41 \u0d06\u0d2f\u0d3f\u0d28\u0d3e\u0d30\u0d4d - <br/> \u0d15\u0d30\u0d41\u0d19\u0d4d\u0d15\u0d1f\u0d32\u0d4d \u0d2a\u0d33\u0d4d\u0d33\u0d3f\u0d2f\u0d3f\u0d32\u0d4d \u0d15\u0d32\u0d35\u0d3f \u0d28\u0d40\u0d19\u0d4d\u0d15\u0d3f\u0d2a\u0d4d \u0d2a\u0d4b\u0d2f\u0d4d\u0d2a\u0d4d <br/> \u0d2a\u0d3f\u0d30\u0d3f\u0d28\u0d4d\u0d24\u0d35\u0d30\u0d4d \u0d15\u0d42\u0d1f\u0d3f\u0d28\u0d3e\u0d32\u0d4d, \u0d2a\u0d47\u0d1a\u0d32\u0d4d \u0d35\u0d47\u0d23\u0d4d\u0d1f\u0d41\u0d2e\u0d4b?"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0905\u0923\u094d\u0923\u0932\u0941\u092e\u094d \u0928\u094b\u0915\u094d\u0915\u093f\u0928\u093e\u0928\u094d \u0905\u0935\u0932\u0941\u092e\u094d \u0928\u094b\u0915\u094d\u0915\u093f\u0928\u093e\u0932\u094d"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u090f\u0923\u094d\u0923 \u0905\u0930\u0941 \u0928\u0932\u0924\u094d\u0924\u093f\u0928\u093e\u0932\u094d \u0907\u0928\u0948\u092f\u0932\u094d \u0928\u093f\u0928\u094d\u0930\u0941\u0932\u093f, <br/> \u0915\u0923\u094d\u0923\u094b\u091f\u0941 \u0915\u0923\u094d \u0907\u0923\u0948 \u0915\u0935\u094d\u0935\u093f, \u0913\u0928\u094d\u0930\u0948 \u0913\u0928\u094d\u0930\u0941 <br/> \u0909\u0923\u094d\u0923\u0935\u0941\u092e\u094d, \u0928\u093f\u0932\u0948\u092a\u0947\u0930\u093e\u0924\u0941 \u0909\u0923\u0930\u094d\u0935\u0941\u092e\u094d \u0913\u0928\u094d\u0930\u093f\u091f, <br/> \u0905\u0923\u094d\u0923\u0932\u0941\u092e\u094d \u0928\u094b\u0915\u094d\u0915\u093f\u0928\u093e\u0928\u094d \u0905\u0935\u0932\u0941\u092e\u094d \u0928\u094b\u0915\u094d\u0915\u093f\u0928\u093e\u0932\u094d."}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u0928\u094b\u0915\u094d\u0915\u093f\u092f \u0928\u094b\u0915\u094d\u0915\u0941 \u090f\u0928\u0941\u092e\u094d \u0928\u0941\u0924\u093f \u0915\u094b\u0932\u094d \u0935\u0947\u0932\u094d \u0907\u0923\u0948 <br/> \u0906\u0915\u094d\u0915\u093f\u092f \u092e\u0924\u0941\u0915\u0948\u092f\u093e\u0928\u094d \u0924\u094b\u0932\u093f\u0928\u094d \u0906\u0932\u094d\u0928\u094d\u0924\u0928 <br/> \u0935\u0940\u0915\u094d\u0915\u093f\u092f \u0915\u0928\u0948 \u0915\u0932\u0932\u094d \u0935\u0940\u0930\u0928\u094d \u091a\u0947\u0919\u094d\u0915\u0923\u0941\u092e\u094d <br/> \u0924\u093e\u0915\u094d\u0915\u0941 \u0905\u0923\u0919\u094d\u0915\u0941 \u0905\u0928\u0948\u092f\u0935\u0932\u094d \u0924\u0928\u0924\u094d\u0924\u093f\u0932\u094d \u0924\u0948\u0924\u094d\u0924\u0935\u0947."}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u092a\u0930\u0941\u0915\u093f\u092f \u0928\u094b\u0915\u094d\u0915\u0941 \u090f\u0928\u0941\u092e\u094d \u092a\u093e\u091a\u0924\u094d\u0924\u093e\u0932\u094d \u092a\u093f\u0923\u093f\u0924\u094d\u0924\u0941, <br/> \u0913\u0930\u0941\u0935\u0930\u0948 \u0913\u0930\u0941\u0935\u0930\u094d\u0924\u092e\u094d \u0909\u0932\u094d\u0932\u092e\u094d \u0908\u0930\u094d\u0924\u094d\u0924\u0932\u093e\u0932\u094d, <br/> \u0935\u0930\u093f \u091a\u093f\u0932\u0948 \u0905\u0923\u094d\u0923\u0932\u0941\u092e\u094d \u0935\u093e\u091f\u094d\u0915\u0923\u094d \u0928\u0919\u094d\u0915\u0948\u092f\u0941\u092e\u094d, <br/> \u0907\u0930\u0941\u0935\u0930\u0941\u092e\u094d \u092e\u093e\u0930\u093f\u092a\u094d \u092a\u0941\u0915\u094d\u0915\u0941, \u0907\u0924\u092f\u092e\u094d \u090f\u092f\u094d\u0924\u093f\u0928\u093e\u0930\u094d."}, {"M": "\u091a\u0930\u0923\u092e\u094d 4", "T": "\u092e\u0930\u0941\u0919\u094d\u0915\u0941 \u0907\u0932\u093e \u0928\u0919\u094d\u0915\u0948\u092f\u0941\u092e\u094d, \u0935\u091a\u0948 \u0907\u0932\u094d \u0910\u092f\u0928\u0941\u092e\u094d, <br/> \u0913\u0930\u0941\u0919\u094d\u0915\u093f\u092f \u0907\u0930\u0923\u094d\u091f\u0941 \u0909\u091f\u0930\u094d\u0915\u0941 \u0909\u092f\u093f\u0930\u094d \u0913\u0928\u094d\u0930\u0941 \u0906\u092f\u093f\u0928\u093e\u0930\u094d - <br/> \u0915\u0930\u0941\u0919\u094d\u0915\u091f\u0932\u094d \u092a\u0932\u094d\u0932\u093f\u092f\u093f\u0932\u094d \u0915\u0932\u0935\u093f \u0928\u0940\u0919\u094d\u0915\u093f\u092a\u094d \u092a\u094b\u092f\u094d\u092a\u094d <br/> \u092a\u093f\u0930\u093f\u0928\u094d\u0924\u0935\u0930\u094d \u0915\u0942\u091f\u093f\u0928\u093e\u0932\u094d, \u092a\u0947\u091a\u0932\u094d \u0935\u0947\u0923\u094d\u091f\u0941\u092e\u094b?"}]}], "L": "l", "lyricsref": [{"links": [{"N": "www.chennailibrary.com", "L": "https://www.chennailibrary.com/kambar/bala/mithilaikatchippadalam.html"}]}], "songs": [{"I": "tXawlcpdwfM", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 66, "D": "10:23", "V": "3,297", "J": "m"}, {"I": "q6tjf8292Oc", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 66, "D": "6:18", "V": "134", "J": "m"}, {"I": "yvKieWCgym0", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 462, "D": "11:02", "V": "714", "J": "m"}, {"I": "uwNIwqd7ZrI", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 462, "D": "6:35", "V": "440", "J": "m"}, {"I": "nAH1WlN6QqA&start=0&end=5", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 462, "D": "0:00", "V": "0", "J": "m"}, {"I": "M2oCQF3BsF4&start=1492&end=2992", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 128, "D": "25:00", "V": "18,027", "J": "h"}, {"I": "jdcD447xc2A&start=8916&end=9384", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 128, "D": "7:48", "V": "10,084", "J": "s"}, {"I": "VZHk4cAjcbQ", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 128, "D": "5:23", "V": "458", "J": "m"}, {"I": "RhJfY97Y_9E&start=0&end=4", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 128, "D": "0:00", "V": "0", "J": "m"}, {"I": "CqX1gbmXv84", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 13, "D": "5:01", "V": "1,472", "J": "m"}, {"I": "WTNxFekrDxE", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 168, "D": "4:17", "V": "211", "J": "m"}, {"I": "HFDXvbyvIS4", "T": 0, "S": 4291, "R": 63, "C": 166, "A": 88, "D": "10:20", "V": "614", "J": "m"}, {"I": "X1ygPBKfrq8", "T": 0, "S": 4291, "R": 