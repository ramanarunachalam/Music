31\u0c35\u0c30\u0c4d\u0c15\u0c4d\u0c15\u0c41 \u0c07\u0c2a\u0c4d\u0c2a\u0c30\u0c4d\u0c35\u0c48 \u0c15\u0c3f\u0c21\u0c48\u0c2f\u0c3e\u0c26\u0c41 \u0c24\u0c3f\u0c30\u0c41 <br/> \u0c28\u0c1f\u0c28\u0c2e\u0c4d \u0c06\u0c21\u0c41\u0c35\u0c3e\u0c30\u0c4d \u0c24\u0c3e\u0c33\u0c2e\u0c4d \u0c2a\u0c4b\u0c21\u0c42\u0c35\u0c3e\u0c30\u0c4d \u0c05\u0c28\u0c4d\u0c2c\u0c30\u0c4d \u0c15\u0c42\u0c21\u0c41\u0c35\u0c3e\u0c30\u0c4d \u0c07\u0c36\u0c48 \u0c2a\u0c3e\u0c21\u0c41\u0c35\u0c3e\u0c30\u0c4d \u0c07\u0c26\u0c48 <br/> \u0c15\u0c23\u0c4d\u0c21\u0c3e\u0c30\u0c41\u0c2e\u0c4d \u0c15\u0c3f\u0c21\u0c48\u0c2f\u0c3e\u0c26\u0c41 \u0c35\u0c3f\u0c23\u0c4d\u0c21\u0c3e\u0c30\u0c41\u0c2e\u0c4d \u0c36\u0c4a\u0c28\u0c4d\u0c28\u0c26\u0c3f\u0c32\u0c4d\u0c32\u0c48 \u0c06\u0c23\u0c4d\u0c21\u0c3e\u0c23\u0c4d\u0c21 \u0c15\u0c4b\u0c1f\u0c3f\u0c2f\u0c46\u0c32\u0c4d\u0c32\u0c3e\u0c2e\u0c4d \u0c12\u0c28\u0c4d\u0c31\u0c3e\u0c2f\u0c4d <br/> \u0c36\u0c2e\u0c48\u0c28\u0c4d\u0c26\u0c3f\u0c30\u0c41\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d \u0c05\u0c32\u0c4d\u0c32\u0c35\u0c4b \u0c2a\u0c30\u0c48\u0c2f\u0c28\u0c4d \u0c36\u0c4a\u0c32\u0c4d\u0c32\u0c35\u0c4b \u0c05\u0c19\u0c4d\u0c17\u0c47 \u0c36\u0c46\u0c32\u0c4d\u0c32\u0c35\u0c4b \u0c28\u0c47\u0c30\u0c2e\u0c3e\u0c17\u0c41\u0c26\u0c32\u0c4d\u0c32\u0c35\u0c4b"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 5", "T": "\u0c2c\u0c3e\u0c32\u0c15\u0c3f\u0c30\u0c41\u0c37\u0c4d\u0c23\u0c28\u0c4d \u0c2a\u0c23\u0c3f\u0c28\u0c4d\u0c26\u0c3f\u0c21\u0c41\u0c2e\u0c4d \u0c36\u0c40\u0c32 \u0c17\u0c41\u0c30\u0c41 \u0c1a\u0c3f\u0c26\u0c2e\u0c4d\u0c2c\u0c30\u0c2e\u0c4d \u0c2e\u0c47\u0c32\u0c47 \u0c35\u0c48\u0c24\u0c4d\u0c24\u0c35\u0c3e\u0c38\u0c48\u0c2f\u0c3e\u0c32\u0c47 <br/> \u0c15\u0c3e\u0c32\u0c28\u0c4d\u0c31\u0c30\u0c41 \u0c2a\u0c4b\u0c35\u0c47\u0c28\u0c4d \u0c0e\u0c28\u0c4d\u0c31\u0c41 \u0c38\u0c3e\u0c24\u0c4d\u0c24\u0c3f\u0c30\u0c2e\u0c4d \u0c28\u0c32\u0c4d\u0c32 \u0c15\u0c4d\u0c37\u0c47\u0c24\u0c4d\u0c24\u0c3f\u0c30\u0c2e\u0c4d \u0c38\u0c30\u0c4d\u0c2a\u0c3e\u0c24\u0c4d\u0c24\u0c3f\u0c30\u0c2e\u0c4d \u0c1c\u0c4d\u0c1e\u0c3e\u0c28 \u0c28\u0c47\u0c24\u0c4d\u0c24\u0c3f\u0c30\u0c2e\u0c4d \u0c15\u0c4a\u0c23\u0c4d\u0c21\u0c41"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c85\u0caf\u0ccd\u0caf\u0cc7 \u0cae\u0cc6\u0ca4\u0ccd\u0ca4\u0c95\u0ccd\u0c95\u0ca1\u0cbf\u0ca8\u0cae\u0ccd \u0c89\u0cae\u0c95\u0ccd\u0c95\u0ca1\u0cbf\u0cae\u0cc8 \u0c85\u0caf\u0ccd\u0caf\u0cc7 \u0cae\u0cc6\u0ca4\u0ccd\u0ca4\u0c95\u0ccd\u0c95\u0ca1\u0cbf\u0ca8\u0cae\u0ccd"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0caa\u0cca\u0caf\u0ccd\u0caf\u0cbe\u0ca6 \u0caa\u0cca\u0ca8\u0ccd\u0ca8\u0cae\u0ccd\u0cac\u0cb2\u0ca4\u0ccd\u0ca4\u0cc8\u0caf\u0cbe-\u0caf\u0cbf\u0cb0\u0cc1\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0cbf\u0ca1\u0cae\u0ccd <br/> \u0ca8\u0cc8\u0caf\u0cbe\u0ca6 \u0cae\u0ca8\u0cbf\u0ca6\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc1 \u0c89\u0caf\u0ccd\u0caf\u0cbe\u0ca6\u0cc1 \u0c95\u0ca3\u0ccd\u0ca1\u0cc1 \u0c95\u0cca\u0cb3\u0ccd\u0cb3\u0cc1\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cb5\u0cbe\u0cb6\u0cbf\u0caf\u0cbe\u0cb2\u0cc7 \u0cae\u0cc2\u0cb2\u0c95\u0ccd\u0c95\u0ca8\u0cb2\u0ccd \u0cb5\u0cc0\u0cb6\u0cbf\u0caf\u0cc7 \u0cb6\u0cc1\u0cb3\u0ca8\u0ccd\u0cb1\u0cc1 \u0cb5\u0cb0\u0caa\u0ccd-\u0caa\u0cc2\u0c9c\u0cc8 \u0caa\u0ca3\u0ccd\u0ca3\u0cc0\u0caa\u0ccd-\u0caa\u0ca3\u0cbf\u0ca8\u0ccd\u0ca6\u0cbf\u0ca1\u0cc1 <br/> \u0cae\u0cbe\u0cb6\u0cb0\u0c95\u0ccd-\u0c95\u0cc1\u0ca3\u0ccd\u0ca1\u0cb2\u0cbf\u0caf\u0cc8 \u0cb5\u0cbf\u0c9f\u0ccd\u0c9f\u0cc1 \u0cae\u0ca8\u0cae\u0cc2\u0c9f\u0ccd\u0c9f\u0cc1\u0cae\u0cc7 \u0cae\u0cc7\u0cb2\u0ccb\u0c9f\u0ccd\u0c9f\u0cc1\u0cae\u0cc7 \u0cb5\u0cb3\u0cbf \u0c95\u0cbe\u0c9f\u0ccd\u0c9f\u0cc1\u0cae\u0cc7 \u0c87\u0ca8\u0ccd\u0ca6 <br/> \u0cae\u0cbe\u0ca8\u0cbe\u0cad\u0cbf\u0cae\u0cbe\u0ca8\u0cae\u0ccd \u0cb5\u0cbf\u0c9f\u0ccd\u0c9f\u0cc1 \u0ca4\u0cbe\u0ca8\u0cbe\u0c97\u0cbf \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cb5\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc1 \u0cb8\u0cc7\u0ca8\u0cbe\u0ca7\u0cbf\u0caa\u0ca4\u0cbf \u0caa\u0ccb\u0cb2\u0cc7 \u0c9c\u0ccd\u0c9e\u0cbe\u0ca8\u0cbe\u0ca7\u0cbf\u0caa\u0ca4\u0cbf\u0caf\u0cc1\u0ca3\u0ccd\u0ca1\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0caa\u0cbe\u0cb0\u0cc1\u0cae\u0cc7 \u0c95\u0c9f\u0ccd\u0c9f\u0cbf\u0c95\u0ccd\u0c95\u0cbe\u0cb0\u0cc1\u0cae\u0cc7 \u0c89\u0cb3\u0ccd\u0cb3\u0cc7 \u0cb6\u0cc7\u0cb0\u0cc1\u0cae\u0cc7 \u0c85\u0ca6\u0cc1 \u0caa\u0cc2\u0cb0\u0cc1\u0cae\u0cc7 \u0c85\u0c99\u0ccd\u0c97\u0cc7 \u0cb6\u0c99\u0ccd\u0c97\u0cc8\u0caf\u0cb0\u0cb5\u0cc7 <br/> \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cc1 \u0caa\u0cca\u0c99\u0ccd\u0c97\u0cbf \u0cb5\u0cb0\u0cc1\u0cae\u0ccd \u0caa\u0cbe\u0cb2\u0cc1\u0ca3\u0ccd\u0ca1\u0cc1 \u0c85\u0c99\u0ccd\u0c97\u0cae\u0ccd \u0c87\u0cb3\u0cc8\u0caa\u0ccd\u0caa\u0cbe\u0cb0\u0cbf\u0c95\u0ccd\u0c95\u0ccb\u0ca3\u0ccd\u0ca1\u0cc1 \u0ca4\u0c99\u0ccd\u0c97 \u0cac\u0cca\u0cae\u0ccd\u0cae\u0cc8 \u0caa\u0ccb\u0cb2\u0cb5\u0cc7 <br/> \u0ca8\u0cbf\u0cb2\u0ccd\u0cb2\u0cc1\u0cae\u0cc7 \u0c8f\u0ca6\u0cc1\u0cae\u0ccd \u0cb6\u0cca\u0cb2\u0ccd\u0cb2\u0cc1\u0cae\u0cc7 \u0c9c\u0ccd\u0c9e\u0cbe\u0ca8\u0cae\u0ccd \u0cb6\u0cca\u0cb2\u0ccd\u0cb2\u0cc1\u0cae\u0cc7 \u0caf\u0cbe\u0ca6\u0cc1\u0cae\u0ccd \u0cb5\u0cc6\u0cb2\u0ccd\u0cb2\u0cc1\u0cae\u0cc7 \u0c87\u0ca8\u0ccd\u0ca6"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c85\u0c9f\u0ccd\u0c9f\u0cbe\u0c99\u0ccd\u0c97\u0cae\u0ccd \u0caa\u0ca3\u0ccd\u0ca3\u0cbf\u0ca8\u0cbe\u0cb2\u0cc1\u0cae\u0ccd \u0ca8\u0cc6\u0c9f\u0ccd\u0c9f\u0cbe\u0c99\u0ccd\u0c97\u0cc1 \u0caa\u0ca3\u0ccd\u0ca3\u0cbf\u0caf\u0ca6\u0cc1 \u0c95\u0cbf\u0c9f\u0ccd\u0c9f\u0cbe\u0ca6\u0cc1 \u0c95\u0cbf\u0c9f\u0ccd\u0c9f\u0cc0 <br/> \u0cb5\u0cb0\u0cb5\u0cca\u0c9f\u0ccd\u0c9f\u0cbe\u0ca6\u0cc1 \u0cae\u0cc1\u0c9f\u0ccd\u0c9f\u0cbf\u0caf\u0ca6\u0cc1 \u0caa\u0cbe\u0caf\u0cc1\u0cae\u0cc7 \u0cae\u0cc1\u0ca8\u0cc8 \u0ca4\u0cc7\u0caf\u0cc1\u0cae\u0cc7 \u0c85\u0ca6\u0cc1 \u0cb5\u0ccb\u0caf\u0cc1\u0cae\u0cc7 \u0c89\u0cb3\u0ccd\u0cb3\u0cc7 \u0ca4\u0ccb\u0caf\u0cc1\u0cae\u0cc7 <br/> \u0cb5\u0cc7\u0ca6 \u0cae\u0ca8\u0ccd\u0ca4\u0cbf\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cc7 \u0caa\u0ccb\u0c9f\u0ccd\u0c9f\u0cc1 \u0caf\u0ca8\u0ccd\u0ca4\u0cbf\u0cb0\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cc7 \u0caa\u0cbe\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd \u0ca4\u0ca8\u0ccd\u0ca4\u0cbf\u0cb0\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cc7\u0caf\u0cc1\u0cae\u0ccd \u0c87\u0cb2\u0ccd\u0cb2\u0cc8 \u0c85\u0ca8\u0ccd\u0ca6\u0cb0\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cc7 <br/> \u0c85\u0cb5\u0ca6\u0cbe\u0ca8\u0cae\u0cc7 \u0c85\u0ca6\u0cc1 \u0ca6\u0cbe\u0ca8\u0cae\u0cc7 \u0cac\u0cb2\u0cb9\u0cc0\u0ca8\u0cae\u0cc7 \u0caa\u0cc7\u0cb6\u0cbe \u0cae\u0ccb\u0ca8\u0cae\u0cc7 \u0c85\u0ca8\u0ccd\u0ca6"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0cae\u0cc1\u0caa\u0ccd\u0caa\u0cb3\u0cc1\u0cae\u0ccd \u0ca4\u0cbe\u0ca3\u0ccd\u0ca1\u0cbf \u0cb5\u0ca8\u0ccd\u0ca6\u0cc1 \u0c85\u0caa\u0ccd\u0caa\u0cbe\u0cb2\u0cc7 \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cb5\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc1 \u0c87\u0caa\u0ccd\u0caa\u0cb0\u0ccd\u0cb5\u0cc8 \u0c95\u0cbf\u0ca1\u0cc8\u0caf\u0cbe\u0ca6\u0cc1 \u0ca4\u0cbf\u0cb0\u0cc1 <br/> \u0ca8\u0c9f\u0ca8\u0cae\u0ccd \u0c86\u0ca1\u0cc1\u0cb5\u0cbe\u0cb0\u0ccd \u0ca4\u0cbe\u0cb3\u0cae\u0ccd \u0caa\u0ccb\u0ca1\u0cc2\u0cb5\u0cbe\u0cb0\u0ccd \u0c85\u0ca8\u0ccd\u0cac\u0cb0\u0ccd \u0c95\u0cc2\u0ca1\u0cc1\u0cb5\u0cbe\u0cb0\u0ccd \u0c87\u0cb6\u0cc8 \u0caa\u0cbe\u0ca1\u0cc1\u0cb5\u0cbe\u0cb0\u0ccd \u0c87\u0ca6\u0cc8 <br/> \u0c95\u0ca3\u0ccd\u0ca1\u0cbe\u0cb0\u0cc1\u0cae\u0ccd \u0c95\u0cbf\u0ca1\u0cc8\u0caf\u0cbe\u0ca6\u0cc1 \u0cb5\u0cbf\u0ca3\u0ccd\u0ca1\u0cbe\u0cb0\u0cc1\u0cae\u0ccd \u0cb6\u0cca\u0ca8\u0ccd\u0ca8\u0ca6\u0cbf\u0cb2\u0ccd\u0cb2\u0cc8 \u0c86\u0ca3\u0ccd\u0ca1\u0cbe\u0ca3\u0ccd\u0ca1 \u0c95\u0ccb\u0c9f\u0cbf\u0caf\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0c92\u0ca8\u0ccd\u0cb1\u0cbe\u0caf\u0ccd <br/> \u0cb6\u0cae\u0cc8\u0ca8\u0ccd\u0ca6\u0cbf\u0cb0\u0cc1\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd \u0c85\u0cb2\u0ccd\u0cb2\u0cb5\u0ccb \u0caa\u0cb0\u0cc8\u0caf\u0ca8\u0ccd \u0cb6\u0cca\u0cb2\u0ccd\u0