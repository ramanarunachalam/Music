af\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6 \u0c95\u0ca8\u0ccd\u0ca6 \u0cae\u0cc1\u0c95\u0cc1\u0ca8\u0ccd\u0ca6\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0c95\u0cae\u0cb2\u0cb2\u0ccb\u0c9a\u0ca8\u0cae\u0ccd \u0c95\u0cb0\u0ccd\u0cae \u0cae\u0ccb\u0c9a\u0ca8\u0cae\u0ccd <br/> \u0c95\u0caa\u0c9f \u0c97\u0ccb\u0caa\u0cbf\u0c95\u0cbe \u0cb5\u0cb8\u0ca8\u0ccd\u0ca4\u0cae\u0ccd <br/> \u0c85\u0cae\u0cb0\u0cbe\u0cb0\u0ccd\u0c9a\u0ccd\u0c9a\u0cbf\u0ca4 \u0c9a\u0cb0\u0ca3\u0cae\u0ccd \u0cad\u0cb5\u0ca4\u0cb0\u0ca3\u0cae\u0ccd <br/> \u0c85\u0cb0\u0ccd\u0c9c\u0cc1\u0ca8 \u0cb8\u0cbe\u0cb0\u0ca5\u0cbf\u0cae\u0ccd \u0c95\u0cb0\u0cc1\u0ca3\u0cbe\u0ca8\u0cbf\u0ca7\u0cbf\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cae\u0cae\u0ca4\u0cbe \u0cb0\u0cb9\u0cbf\u0ca4\u0cae\u0ccd \u0c97\u0cc1\u0cb0\u0cc1\u0c97\u0cc1\u0cb9 \u0cb5\u0cbf\u0cb9\u0cbf\u0ca4\u0cae\u0ccd <br/> \u0cae\u0cbe\u0ca7\u0cb5\u0cae\u0ccd \u0cb8\u0ca4\u0ccd\u0caf \u0cad\u0cbe\u0cae\u0cbe \u0ca7\u0cb5\u0cae\u0ccd <br/> \u0c95\u0cae\u0cb2\u0cc7\u0cb6\u0cae\u0ccd \u0c97\u0ccb\u0c95\u0cc1\u0cb2 \u0caa\u0ccd\u0cb0\u0cb5\u0cc7\u0cb6\u0cae\u0ccd <br/> \u0c95\u0cae\u0ccd\u0cb8 \u0cad\u0c82\u0c9c\u0ca8\u0cae\u0ccd \u0cad\u0c95\u0ccd\u0ca4 \u0cb0\u0c82\u0c9c\u0ca8\u0cae\u0ccd"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2c\u0d3e\u0d32\u0d15\u0d43\u0d37\u0d4d\u0d23\u0d2e\u0d4d \u0d2d\u0d3e\u0d35\u0d2f\u0d3e\u0d2e\u0d3f <br/> \u0d2c\u0d32 \u0d30\u0d3e\u0d2e\u0d3e\u0d28\u0d41\u0d1c\u0d2e\u0d4d \u0d35\u0d38\u0d41\u0d26\u0d47\u0d35\u0d1c\u0d2e\u0d4d"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d28\u0d40\u0d32 \u0d2e\u0d47\u0d18 \u0d17\u0d3e\u0d24\u0d4d\u0d30\u0d2e\u0d4d \u0d38\u0d4d\u0d24\u0d41\u0d24\u0d3f\u0d2a\u0d3e\u0d24\u0d4d\u0d30\u0d2e\u0d4d <br/> \u0d28\u0d3f\u0d24\u0d4d\u0d2f\u0d3e\u0d28\u0d28\u0d4d\u0d26 \u0d15\u0d28\u0d4d\u0d26 \u0d2e\u0d41\u0d15\u0d41\u0d28\u0d4d\u0d26\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d15\u0d2e\u0d32\u0d32\u0d4b\u0d1a\u0d28\u0d2e\u0d4d \u0d15\u0d30\u0d4d\u0d2e \u0d2e\u0d4b\u0d1a\u0d28\u0d2e\u0d4d <br/> \u0d15\u0d2a\u0d1f \u0d17\u0d4b\u0d2a\u0d3f\u0d15\u0d3e \u0d35\u0d38\u0d28\u0d4d\u0d24\u0d2e\u0d4d <br/> \u0d05\u0d2e\u0d30\u0d3e\u0d30\u0d4d\u0d1a\u0d4d\u0d1a\u0d3f\u0d24 \u0d1a\u0d30\u0d23\u0d2e\u0d4d \u0d2d\u0d35\u0d24\u0d30\u0d23\u0d2e\u0d4d <br/> \u0d05\u0d30\u0d4d\u0d1c\u0d41\u0d28 \u0d38\u0d3e\u0d30\u0d25\u0d3f\u0d2e\u0d4d \u0d15\u0d30\u0d41\u0d23\u0d3e\u0d28\u0d3f\u0d27\u0d3f\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d2e\u0d2e\u0d24\u0d3e \u0d30\u0d39\u0d3f\u0d24\u0d2e\u0d4d \u0d17\u0d41\u0d30\u0d41\u0d17\u0d41\u0d39 \u0d35\u0d3f\u0d39\u0d3f\u0d24\u0d2e\u0d4d <br/> \u0d2e\u0d3e\u0d27\u0d35\u0d2e\u0d4d \u0d38\u0d24\u0d4d\u0d2f \u0d2d\u0d3e\u0d2e\u0d3e \u0d27\u0d35\u0d2e\u0d4d <br/> \u0d15\u0d2e\u0d32\u0d47\u0d36\u0d2e\u0d4d \u0d17\u0d4b\u0d15\u0d41\u0d32 \u0d2a\u0d4d\u0d30\u0d35\u0d47\u0d36\u0d2e\u0d4d <br/> \u0d15\u0d2e\u0d4d\u0d38 \u0d2d\u0d02\u0d1c\u0d28\u0d2e\u0d4d \u0d2d\u0d15\u0d4d\u0d24 \u0d30\u0d02\u0d1c\u0d28\u0d2e\u0d4d"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092c\u093e\u0932\u0915\u0943\u0937\u094d\u0923\u092e\u094d \u092d\u093e\u0935\u092f\u093e\u092e\u093f <br/> \u092c\u0932 \u0930\u093e\u092e\u093e\u0928\u0941\u091c\u092e\u094d \u0935\u0938\u0941\u0926\u0947\u0935\u091c\u092e\u094d"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0928\u0940\u0932 \u092e\u0947\u0918 \u0917\u093e\u0924\u094d\u0930\u092e\u094d \u0938\u094d\u0924\u0941\u0924\u093f\u092a\u093e\u0924\u094d\u0930\u092e\u094d <br/> \u0928\u093f\u0924\u094d\u092f\u093e\u0928\u0928\u094d\u0926 \u0915\u0928\u094d\u0926 \u092e\u0941\u0915\u0941\u0928\u094d\u0926\u092e\u094d"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0915\u092e\u0932\u0932\u094b\u091a\u0928\u092e\u094d \u0915\u0930\u094d\u092e \u092e\u094b\u091a\u0928\u092e\u094d <br/> \u0915\u092a\u091f \u0917\u094b\u092a\u093f\u0915\u093e \u0935\u0938\u0928\u094d\u0924\u092e\u094d <br/> \u0905\u092e\u0930\u093e\u0930\u094d\u091a\u094d\u091a\u093f\u0924 \u091a\u0930\u0923\u092e\u094d \u092d\u0935\u0924\u0930\u0923\u092e\u094d <br/> \u0905\u0930\u094d\u091c\u0941\u0928 \u0938\u093e\u0930\u0925\u093f\u092e\u094d \u0915\u0930\u0941\u0923\u093e\u0928\u093f\u0927\u093f\u092e\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u092e\u092e\u0924\u093e \u0930\u0939\u093f\u0924\u092e\u094d \u0917\u0941\u0930\u0941\u0917\u0941\u0939 \u0935\u093f\u0939\u093f\u0924\u092e\u094d <br/> \u092e\u093e\u0927\u0935\u092e\u094d \u0938\u0924\u094d\u092f \u092d\u093e\u092e\u093e \u0927\u0935\u092e\u094d <br/> \u0915\u092e\u0932\u0947\u0936\u092e\u094d \u0917\u094b\u0915\u0941\u0932 \u092a\u094d\u0930\u0935\u0947\u0936\u092e\u094d <br/> \u0915\u092e\u094d\u0938 \u092d\u0902\u091c\u0928\u092e\u094d \u092d\u0915\u094d\u0924 \u0930\u0902\u091c\u0928\u092e\u094d"}]}], "L": "l", "lyricsref": [{"links": [{"N": "www.medieval.org", "L": "http://www.medieval.org/music/world/carnatic/lyrics/TKG/balakrishnam.html"}, {"N": "guru-guha.blogspot.com", "L": "http://guru-guha.blogspot.com/2008/02/dikshitar-kriti-balakrishnam-bhavayami.html"}]}]}