a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2c\u0d3e\u0d32\u0d15\u0d3f\u0d30\u0d41\u0d37\u0d4d\u0d23\u0d28\u0d4d \u0d2a\u0d3e\u0d26\u0d2e\u0d32\u0d30\u0d4d \u0d2a\u0d23\u0d3f\u0d35\u0d4b\u0d30\u0d4d\u0d15\u0d4d\u0d15\u0d3f\u0d21\u0d30\u0d3f\u0d32\u0d48 \u0d35\u0d30 \u0d17\u0d41\u0d23"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d28\u0d40\u0d32 \u0d2e\u0d41\u0d15\u0d3f\u0d32\u0d4d \u0d2a\u0d4b\u0d32\u0d4d \u0d05\u0d34\u0d15\u0d28\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d28\u0d3f\u0d31\u0d48\u0d2e\u0d24\u0d3f \u0d35\u0d24\u0d28\u0d2e\u0d4d \u0d05\u0d24\u0d28\u0d3f\u0d32\u0d4d \u0d07\u0d33 \u0d28\u0d15\u0d48 \u0d28\u0d3f\u0d32\u0d35 \u0d05\u0d30\u0d41\u0d33\u0d4a\u0d33\u0d3f \u0d24\u0d35\u0d34\u0d41\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d15\u0d4b\u0d15\u0d41\u0d32\u0d2e\u0d4d \u0d2a\u0d4d\u0d30\u0d41\u0d28\u0d4d\u0d24\u0d3e\u0d35\u0d28\u0d2e\u0d4d \u0d2f\u0d2e\u0d41\u0d28\u0d3e \u0d35\u0d3f\u0d39\u0d3e\u0d30\u0d40 \u0d15\u0d4b\u0d2a\u0d3e\u0d32\u0d28\u0d4d <br/> \u0d15\u0d4b\u0d2a\u0d40 \u0d1c\u0d28 \u0d2e\u0d28 \u0d2e\u0d4b\u0d39\u0d28 \u0d2e\u0d41\u0d30\u0d33\u0d40 \u0d15\u0d3e\u0d28 \u0d35\u0d3f\u0d32\u0d4b\u0d32\u0d28\u0d4d\u0d35\u0d4d\u0d2f\u0d3e\u0d15\u0d41\u0d32\u0d2e\u0d4d <br/> \u0d24\u0d35\u0d3f\u0d30\u0d4d\u0d24\u0d4d\u0d24\u0d28\u0d4d\u0d2a\u0d30\u0d4d \u0d2e\u0d28\u0d24\u0d3f\u0d32\u0d4d \u0d35\u0d3e\u0d34\u0d4d \u0d15\u0d30\u0d41\u0d23\u0d3e\u0d32 \u0d35\u0d3e\u0d32\u0d28\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d2e\u0d34\u0d48 \u0d24\u0d1f\u0d41\u0d15\u0d4d\u0d15 \u0d15\u0d4b\u0d35\u0d30\u0d4d\u0d24\u0d28 \u0d2e\u0d32\u0d48 \u0d0e\u0d1f\u0d41\u0d24\u0d4d\u0d24 \u0d24\u0d3f\u0d23\u0d4d\u0d24\u0d4b\u0d33\u0d28\u0d4d <br/> \u0d2a\u0d3f\u0d34\u0d48 \u0d2a\u0d4a\u0d30\u0d41\u0d24\u0d4d\u0d24\u0d30\u0d41\u0d33\u0d4d \u0d24\u0d2f\u0d3e\u0d33\u0d28\u0d4d \u0d2a\u0d3f\u0d30\u0d2e\u0d28\u0d4d \u0d2a\u0d23\u0d3f \u0d2e\u0d32\u0d30\u0d4d \u0d24\u0d3e\u0d33\u0d28\u0d4d"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092c\u093e\u0932\u0915\u093f\u0930\u0941\u0937\u094d\u0923\u0928\u094d \u092a\u093e\u0926\u092e\u0932\u0930\u094d \u092a\u0923\u093f\u0935\u094b\u0930\u094d\u0915\u094d\u0915\u093f\u0921\u0930\u093f\u0932\u0948 \u0935\u0930 \u0917\u0941\u0923"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0928\u0940\u0932 \u092e\u0941\u0915\u093f\u0932\u094d \u092a\u094b\u0932\u094d \u0905\u0932\u0915\u0928\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0928\u093f\u0930\u0948\u092e\u0924\u093f \u0935\u0924\u0928\u092e\u094d \u0905\u0924\u0928\u093f\u0932\u094d \u0907\u0932 \u0928\u0915\u0948 \u0928\u093f\u0932\u0935 \u0905\u0930\u0941\u0932\u094b\u0932\u093f \u0924\u0935\u0932\u0941\u092e\u094d"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0915\u094b\u0915\u0941\u0932\u092e\u094d \u092a\u094d\u0930\u0941\u0928\u094d\u0924\u093e\u0935\u0928\u092e\u094d \u092f\u092e\u0941\u0928\u093e \u0935\u093f\u0939\u093e\u0930\u0940 \u0915\u094b\u092a\u093e\u0932\u0928\u094d <br/> \u0915\u094b\u092a\u0940 \u091c\u0928 \u092e\u0928 \u092e\u094b\u0939\u0928 \u092e\u0941\u0930\u0932\u0940 \u0915\u093e\u0928 \u0935\u093f\u0932\u094b\u0932\u0928\u094d\u0935\u094d\u092f\u093e\u0915\u0941\u0932\u092e\u094d <br/> \u0924\u0935\u093f\u0930\u094d\u0924\u094d\u0924\u0928\u094d\u092a\u0930\u094d \u092e\u0928\u0924\u093f\u0932\u094d \u0935\u093e\u0932\u094d \u0915\u0930\u0941\u0923\u093e\u0932 \u0935\u093e\u0932\u0928\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u092e\u0932\u0948 \u0924\u091f\u0941\u0915\u094d\u0915 \u0915\u094b\u0935\u0930\u094d\u0924\u0928 \u092e\u0932\u0948 \u090f\u091f\u0941\u0924\u094d\u0924 \u0924\u093f\u0923\u094d\u0924\u094b\u0932\u0928\u094d <br/> \u092a\u093f\u0932\u0948 \u092a\u094b\u0930\u0941\u0924\u094d\u0924\u0930\u0941\u0932\u094d \u0924\u092f\u093e\u0932\u0928\u094d \u092a\u093f\u0930\u092e\u0928\u094d \u092a\u0923\u093f \u092e\u0932\u0930\u094d \u0924\u093e\u0932\u0928\u094d"}]}], "L": "l", "lyricsref": [{"links": [{"N": "periscope-narada.blogspot.com", "L": "http://periscope-narada.blogspot.com/2018/04/balakrishnan-pada-malar.html"}]}]}