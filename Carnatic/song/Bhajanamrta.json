u0ba4 \u0baa\u0b99\u0bcd\u0b95\u0b9c \u0b9a\u0bae \u0b85\u0ba9\u0bc1\u0b95\u0bae\u0bcd\u0baa\u0bbf\u0ba4 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0bbf \u0bb8\u0bcd\u0bae\u0bb0 \u0b9a\u0bae\u0bcd\u0baa\u0bb5 \u0ba8\u0bbf\u0b9c"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0ba8\u0bbe\u0bb0\u0bbe\u0baf\u0ba3 \u0ba8\u0bbe\u0bae \u0bb0\u0b9a\u0ba9 \u0baa\u0bb0\u0bae\u0bbe\u0ba9\u0ba8\u0bcd\u0ba4\u0baf\u0bc1\u0ba4 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0baf \u0b9c\u0bbf\u0ba4\u0b95\u0bb2\u0bcd\u0bae\u0bb7 \u0ba8\u0bb0 <br/> \u0b9a\u0bbf\u0bae\u0bcd\u0bb9 \u0ba4\u0baf\u0bbe \u0ba8\u0bbf\u0ba4\u0bbf\u0bae\u0bbf\u0bb5 \u0ba4\u0bb0\u0ba3 \u0b9a\u0ba4\u0bbe \u0b95\u0ba9\u0b95\u0b95\u0b9a\u0bbf\u0baa\u0bc1 \u0b9a\u0bc1\u0ba4 \u0b86\u0ba4\u0baf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0baa\u0bb0\u0bb9\u0bbf\u0ba4 \u0b95\u0bb0\u0bc1\u0ba3\u0bbe\u0b95\u0bb0 \u0bb5\u0bb0\u0bb8\u0bcd\u0bae\u0bbf\u0ba4 \u0bae\u0bc1\u0b95 \u0baa\u0bb5 \u0baa\u0bb0\u0bbe\u0baa\u0bb5 \u0baa\u0bb0\u0bae \u0baa\u0bbe\u0bb5\u0ba9\u0b9a\u0bb0 <br/> \u0b9a\u0bbf\u0b9c\u0baa\u0bb5 \u0b9a\u0bc1\u0b95\u0bc1\u0bae\u0bbe\u0bb0 \u0b9a\u0ba9\u0b95 \u0b9a\u0ba9\u0ba8\u0bcd\u0ba4\u0ba9 \u0b9a\u0ba9\u0bbe\u0ba4\u0ba9 \u0ba8\u0bbe\u0bb0\u0ba4 \u0b9a\u0bc1\u0b95 \u0b85\u0ba4\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0ba4\u0bbe\u0bb0\u0b95 \u0bae\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 \u0b9a\u0ba4\u0bbe\u0baa\u0b9f\u0ba9\u0bae\u0bcd \u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0ba4\u0bb0\u0b95\u0bb0\u0ba4\u0bb2 \u0b9a\u0b9e\u0bcd\u0b9c\u0bc0\u0bb5\u0bbf \u0b95\u0bbf\u0bb0\u0bbf\u0bae\u0bcd \u0b95\u0bcd\u0bb0\u0bcd\u0ba4 <br/> \u0ba4\u0b9a\u0bb0\u0ba4 \u0bb0\u0bbe\u0b9c\u0b95\u0bc1\u0bae\u0bbe\u0bb0 \u0bb9\u0bbf\u0ba4\u0bae\u0bcd \u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0b9a\u0ba4\u0bbe \u0b95\u0ba4\u0bbf \u0baa\u0bb5\u0ba9 \u0b95\u0bc1\u0bae\u0bbe\u0bb0\u0bbe\u0ba4\u0bbf \u0bb9\u0bb0\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0ba4\u0b9a\u0bae\u0bc1\u0b95 \u0b9a\u0bcb\u0ba4\u0bb0 \u0b95\u0bc1\u0bb9 \u0b9a\u0baa\u0bb0\u0bc0 \u0b9a\u0bc1\u0b95\u0bcd\u0bb0\u0bc0\u0bb5\u0bbe\u0b99\u0bcd\u0b95\u0ba4 \u0b9a\u0bc1\u0b9a\u0bb0\u0bbf\u0ba4 \u0bae\u0bb9\u0bbe\u0baa\u0bbe\u0b95\u0bcd\u0baf <br/> \u0bb0\u0bbe\u0bae\u0bc7\u0ba3 \u0baa\u0bb9\u0bc1\u0baa\u0bbe\u0bb7\u0bbf\u0ba4 \u0baa\u0bbe\u0bb5\u0bbf\u0ba4 \u0bb5\u0bc0\u0b95\u0bcd\u0bb7\u0bbf\u0ba4 \u0bb0\u0b95\u0bcd\u0bb7\u0bbf\u0ba4 \u0bae\u0bbf\u0ba4\u0bcd\u0bb0 \u0baa\u0bb5\u0bbf\u0ba4\u0bcd\u0bb0\u0bbf\u0ba4"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0baa\u0bbe\u0b95\u0bb5\u0ba4 \u0b95\u0ba4\u0bbe\u0bb9 \u0baa\u0b9f\u0ba9 \u0bb6\u0bcd\u0bb0\u0bb5\u0ba3 \u0b9a\u0ba4\u0bbe \u0bb8\u0bcd\u0bae\u0bb0\u0ba3 \u0baa\u0b9c\u0ba9 \u0b95\u0bc1\u0ba3 \u0b95\u0bc0\u0bb0\u0bcd\u0ba4\u0ba9\u0baf <br/> \u0ba4\u0bbe \u0b95\u0ba4\u0bbf\u0ba4 \u0b95\u0bcd\u0bb0\u0bcd\u0bb7\u0bcd\u0ba3 \u0b9a\u0bb0\u0bbf\u0ba4 \u0bb5\u0bb0\u0bcd\u0ba3\u0ba9\u0bae\u0bcd \u0ba4\u0ba4\u0bbe\u0ba9\u0bc1\u0b95\u0bc2\u0bb2 \u0baa\u0bcd\u0bb0\u0b95\u0b9f\u0ba9 \u0bb5\u0bbf\u0ba4\u0bb0\u0ba3 \u0bb9\u0bb0\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 6", "T": "\u0ba8\u0ba8\u0bcd\u0ba4\u0bc0\u0b9a \u0bae\u0ba4\u0b99\u0bcd\u0b95 \u0bb5\u0bcd\u0baf\u0bbe\u0b95\u0bcd\u0bb0\u0baa\u0bbe\u0ba4 \u0b9a\u0bb9\u0bb8\u0bcd\u0bb0\u0baa\u0ba9\u0bbe \u0bae\u0b95\u0bc1\u0b9f \u0ba4\u0bb0 \u0ba4\u0bc1\u0bae\u0bcd\u0baa\u0bc1\u0bb0\u0bc1 <br/> \u0b9a\u0ba3\u0bcd\u0b9f\u0bc0\u0b9a \u0bae\u0bcd\u0bb0\u0bcd\u0b95\u0ba3\u0bcd\u0ba4\u0bc1 \u0b9a\u0bc1\u0ba4 \u0bae\u0bc1\u0ba9\u0bbf\u0b95\u0ba3 \u0b9a\u0ba8\u0bcd\u0ba4 \u0ba4\u0ba9\u0bc1\u0b9c \u0bb0\u0b9c\u0ba4 \u0b9a\u0bc8\u0bb2\u0bb8\u0bcd\u0ba4\u0bbf\u0ba4 \u0b9a\u0bbf\u0bb5"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 7", "T": "\u0b9a\u0bbf\u0bb5\u0bb9\u0bb0\u0bbf \u0b9a\u0bb0\u0bb5\u0ba3\u0baa\u0bb5 \u0b95\u0bc1\u0bb9 \u0baa\u0b9c\u0ba9 \u0ba8\u0bbf\u0bb0\u0ba8\u0bcd\u0ba4\u0bb0 \u0bae\u0bbe\u0bb2\u0bbe\u0bb2\u0b99\u0bcd\u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0b9a\u0bcb\u0baa <br/> \u0bb5\u0bbe\u0b95\u0bc0\u0b9a \u0b9a\u0bbf\u0bb5 \u0baa\u0bbe\u0ba4 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0baf \u0b9a\u0bc1\u0ba4 \u0bae\u0ba9\u0bbf\u0bb5\u0bbe\u0b9a\u0b95 \u0b9a\u0bc1\u0ba8\u0bcd\u0ba4\u0bb0\u0b9f\u0bbf\u0ba3\u0bcd\u0b9f\u0bbf\u0bae \u0b95\u0bb5\u0bbf\u0bb0\u0bbe\u0b9c <br/> \u0bae\u0ba4\u0bc1\u0bb0\u0b95\u0bb5\u0bbf \u0bb0\u0bbe\u0b9c \u0bb0\u0bbe\u0bae\u0bbe\u0ba9\u0bc1\u0b9c \u0b95\u0bc1\u0bb2\u0b9a\u0bc7\u0b95\u0bb0 \u0bb5\u0bbf\u0bb7\u0bcd\u0ba3\u0bc1\u0b9a\u0bbf\u0ba4\u0bcd\u0ba4\u0baa\u0bb0 \u0b95\u0bbe\u0bb2 <br/> \u0baa\u0bc1\u0bb0\u0ba8\u0bcd\u0ba4\u0bb0 \u0ba4\u0bc1\u0bb3\u0b9a\u0bbf\u0ba4\u0bbe\u0b9a \u0b9a\u0bb0\u0ba3\u0bbe\u0bb0\u0bb5\u0bbf\u0ba8\u0bcd\u0ba4 \u0ba4\u0bc2\u0bb3\u0bbf \u0bb9\u0bb0\u0bbf \u0b9a\u0bbf\u0bb5 \u0b95\u0bc1\u0bb9"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 8", "T": "\u0b95\u0bcd\u0bb0\u0bcd\u0bb7\u0bcd\u0ba3 \u0b95\u0ba4\u0bbe \u0b95\u0bb0\u0bcd\u0ba3\u0ba9\u0bb5\u0bbf\u0ba4 \u0b9c\u0baa\u0ba4\u0baa \u0bb8\u0bcd\u0ba4\u0bcb\u0ba4\u0bcd\u0bb0 \u0b95\u0bcd\u0bb5\u0ba3\u0bbf\u0ba4\u0bbe\u0bb0\u0bcd\u0b9a\u0ba9 \u0baf\u0bcb\u0b95 <br/> \u0bb0\u0bbe\u0b9a \u0bae\u0bb9\u0bcb\u0ba4\u0bcd\u0b9a\u0bb5 \u0bb5\u0bbf\u0baa\u0bb5 \u0baa\u0bbe\u0bb5 \u0baa\u0bb0\u0bae\u0ba4\u0bcd\u0baa\u0bc1\u0ba4 \u0ba8\u0bb0\u0bcd\u0ba4\u0ba9 \u0bb5\u0bb0 \u0ba8\u0bcd\u0bb0\u0bcd\u0ba4\u0bcd\u0baf \u0b9a\u0ba4\u0bc1\u0bb0 <br/> \u0b85\u0b95\u0ba3\u0bbf\u0ba4 \u0bb0\u0bbe\u0b95 \u0ba8\u0bb5 \u0bb5\u0bbf\u0ba4 \u0ba4\u0bbe\u0bb3 \u0b95\u0bcd\u0bb0\u0bae\u0bb2\u0baf \u0b95\u0ba4\u0bc0\u0bb6\u0bcd\u0bb5\u0bb0 \u0ba4\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0\u0bbf\u0b9a\u0bae\u0ba9\u0bcd\u0bb5\u0bbf\u0ba4 <br/> \u0b86\u0ba9\u0ba8\u0bcd\u0ba4\u0bbe\u0ba4\u0bbf\u0b9a\u0baf \u0b9a\u0bc1\u0b95 \u0ba8\u0bbf\u0bae\u0b95\u0bcd\u0ba9 \u0b85\u0ba9\u0ba8\u0bcd\u0ba4 \u0bae\u0bb9\u0bbe\u0ba8\u0bcd\u0ba4 \u0b9a\u0bb0\u0ba3\u0bbe\u0bb0\u0bb5\u0bbf\u0ba8\u0bcd\u0ba4"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 9", "T": "\u0bb0\u0bbe\u0bae \u0bb0\u0bbe\u0b95\u0bb5 \u0b95\u0bc7\u0b9a\u0bb5 \u0bb0\u0b95\u0bc1\u0b95\u0bc1\u0bb2 \u0ba8\u0ba8\u0bcd\u0ba4\u0ba9 \u0bae\u0bc8\u0ba4\u0bbf\u0bb2\u0bc0 \u0bb0\u0bae\u0ba9\u0bbe <br/> \u0b95\u0bcd\u0bb0\u0bcd\u0bb7\u0bcd\u0ba3 \u0b95\u0bcb\u0b95\u0bc1\u0bb2 \u0bb5\u0bc8\u0baa\u0bb5 \u0ba8\u0ba8\u0bcd\u0ba4 \u0ba8\u0ba8\u0bcd\u0ba4\u0ba9 \u0b95\u0bbe\u0bb3\u0bc0\u0baf \u0baa\u0ba3 \u0ba8\u0b9f\u0ba9\u0bbe <br/> \u0b9a\u0bcb\u0bae\u0b9a\u0bc7\u0b95\u0bb0 \u0b9a\u0bc1\u0ba8\u0bcd\u0ba4\u0bb0 \u0ba8\u0b9f\u0ba9 \u0b9a\u0bbf\u0ba4\u0bae\u0bcd\u0baa\u0bb0 \u0b9a\u0bc8\u0bb2\u0b9c\u0bbe \u0bb0\u0bae\u0bcd\u0ba3\u0bbe <br/> \u0baa\u0bbe\u0bb9\u0bc1\u0bb2\u0bc7\u0baf \u0b9a\u0bbf\u0b95\u0bbf\u0bb5\u0bbe\u0bb9 \u0ba4\u0baf\u0bbe\u0baa\u0bb0 \u0baa\u0bbe\u0bb9\u0bbf \u0b87\u0ba4\u0bbf\u0bb5\u0bbf\u0ba4 \u0ba8\u0bbe\u0bae \u0ba8\u0bbf\u0b9c \u0bb9\u0bb0\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 10", "T": "\u0b95\u0bcd\u0bb7\u0ba3\u0bae\u0baa\u0bbf \u0bb9\u0bb0\u0bbf\u0b95\u0bc1\u0ba3 \u0b95\u0ba4\u0bbf\u0ba4 \u0bae\u0bb9\u0bcb\u0ba4\u0bcd\u0ba4\u0bae \u0b95\u0bcd\u0bb7\u0ba3\u0bae\u0baa\u0bbf \u0bb6\u0bcd\u0bb0\u0bb5\u0ba3 \u0b9a\u0bc1\u0b95\u0bcd\u0bb0\u0bcd\u0ba4 <br/> \u0b95\u0bb0\u0bae\u0bcb\u0ba4\u0bcd\u0ba4\u0bae \u0b95\u0bcd\u0bb7\u0ba3\u0bae\u0baa\u0bbf \u0ba4\u0bb0\u0bcd\u0b9a\u0bbf\u0ba4 \u0b9a\u0ba4\u0bcd\u0b9a\u0b99\u0bcd\u0b95 \u0baa\u0bb0\u0bcb\u0ba4\u0bcd\u0ba4\u0bae \u0b9c\u0ba9\u0bcd\u0bae \u0b9c\u0ba9\u0bcd\u0bae\u0bae\u0baa\u0bbf <br/> \u0baa\u0b99\u0bcd\u0b95\u0b9c \u0baa\u0ba4\u0bae\u0bbf\u0bb9 \u0b85\u0ba8\u0bcd\u0ba4\u0b95\u0bb0\u0ba3 \u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0b85\u0ba8\u0bcd\u0ba4\u0bb0\u0b99\u0bcd\u0b95 \u0bb9\u0bb0\u0bbf"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2d\u0c1c\u0c28\u0c3e\u0c2e\u0c4d\u0c30\u0c4d\u0c24 \u0c2a\u0c30\u0c2e\u0c3e\u0c28\u0c28\u0c4d\u0c26 \u0c2d\u0c3e\u0c17\u0c35\u0c24 \u0c38\u0c28\u0c4d\u0c24 \u0c1a\u0c30\u0c23\u0c30\u0c47\u0c28\u0c41\u0c2e\u0c4d \u0c28\u0c3f\u0c30\u0c28\u0c4d\u0c24\u0c30\u0c2e\u0c4d \u0c35\u0c39\u0c3e\u0c2e\u0c4d\u0c2f\u0c39\u0c2e\u0c4d \u0c39\u0c30\u0c3f\u0c28\u0c3f\u0c1c"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c35\u0c4d\u0c30\u0c1c \u0c30\u0c3e\u0c1c\u0c15\u0c41\u0c2e\u0c3e\u0c30 \u0c2e\u0c28\u0c4b\u0c39\u0c30 \u0c35\u0c47\u0c23\u0c41\u0c17\u0c3e\u0c28 \u0c36\u0c4d\u0c30\u0c35\u0c23\u0c3e\u0c28\u0c28\u0c4d\u0c26 \u0c35\u0c4d\u0c30\u0c1c \u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30\u0c3f <br/> \u0c1c\u0c28 \u0c2a\u0c26 \u0c2a\u0c02\u0c15\u0c1c \u0c38\u0c2e \u0c05\u0c28\u0c41\u0c15\u0c2e\u0c4d\u0c2a\u0c3f\u0c24 \u0c39\u0c4d\u0c30\u0c4d\u0c26\u0c3f \u0c38\u0c4d\u0c2e\u0c30 \u0c38\u0c2e\u0c4d\u0c2d\u0c35 \u0c28\u0c3f\u0c1c"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c28\u0c3e\u0c30\u0c3e\u0c2f\u0c23 \u0c28\u0c3e\u0c2e \u0c30\u0c38\u0c28 \u0c2a\u0c30\u0c2e\u0c3e\u0c28\u0c28\u0c4d\u0c26\u0c2f\u0c41\u0c24 \u0c39\u0c4d\u0c30\u0c4d\u0c26\u0c2f \u0c1c\u0c3f\u0c24\u0c15\u0c32\u0c4d\u0c2e\u0c37 \u0c28\u0c30 <br/> \u0c38\u0c3f\u0c2e\u0c4d\u0c39 \u0c26\u0c2f\u0c3e \u0c28\u0c3f\u0c24\u0c3f\u0c2e\u0c3f\u0c35 \u0c27\u0c30\u0c23 \u0c38\u0c26\u0c3e \u0c15\u0c28\u0c15\u0c15\u0c36\u0c3f\u0c2a\u0c41 \u0c38\u0c41\u0c24 \u0c06\u0c26\u0c2f"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c2a\u0c30\u0c39\u0c3f\u0c24 \u0c15\u0c30\u0c41\u0c23\u0c3e\u0c15\u0c30 \u0c35\u0c30\u0c38\u0c4d\u0c2e\u0c3f\u0c24 \u0c2e\u0c41\u0c16 \u0c2d\u0c35 \u0c2a\u0c30\u0c3e\u0c2d\u0c35 \u0c2a\u0c30\u0c2e \u0c2a\u0c3e\u0c35\u0c28\u0c38\u0c30 <br/> \u0c38\u0c3f\u0c1c\u0c2d\u0c35 \u0c38\u0c41\u0c15\u0c41\u0c2e\u0c3e\u0c30 \u0c38\u0c28\u0c15 \u0c38\u0c28\u0c28\u0c4d\u0c26\u0c28 \u0c38\u0c28\u0c3e\u0c24\u0c28 \u0c28\u0c3e\u0c30\u0c26 \u0c38\u0c41\u0c16 \u0c05\u0c24\u0c3f"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c24\u0c3e\u0c30\u0c15 \u0c2e\u0c28\u0c4d\u0c24\u0c4d\u0c30 \u0c38\u0c26\u0c3e\u0c2a\u0c1f\u0c28\u0c2e\u0c4d \u0c15\u0c4d\u0c30\u0c4d\u0c24 \u0c24\u0c30\u0c15\u0c30\u0c26\u0c32 \u0c38\u0c02\u0c1c\u0c40\u0c35\u0c3f \u0c17\u0c3f\u0c30\u0c3f\u0c2e\u0c4d \u0c15\u0c4d\u0c30\u0c4d\u0c24 <br/> \u0c26\u0c36\u0c30\u0c