d\u0cb5\u0cbe\u0cb0\u0cbf\u0cb5\u0cbe\u0c95\u0ccd\u0caa\u0ccd\u0cb0\u0ca4\u0cbf\u0caa\u0cbe\u0ca6\u0cbf\u0ca4\u0c82 \u0cb8\u0cbf\u0c82\u0ca7\u0cc1\u0cb0\u0cbe\u0cae\u0c95\u0ccd\u0cb0\u0cbf\u0caf\u0cbe\u0ca4\u0cbf\u0cb9\u0cbf\u0ca4\u0c82 ||"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d1b\u0d3f\u0d02\u0d24\u0d2f \u0d38\u0d26\u0d3e \u0d35\u0d3e\u0d30\u0d23 \u0d35\u0d26\u0d28\u0d02 <br/> \u0d1b\u0d3f\u0d24\u0d4d\u0d24\u0d2a\u0d30\u0d3e\u0d36\u0d15\u0d4d\u0d24\u0d3f \u0d38\u0d39\u0d3f\u0d24\u0d2e\u0d47\u0d15\u0d26\u0d02\u0d24\u0d02"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d24\u0d24\u0d02 \u0d2e\u0d39\u0d3e\u0d2e\u0d41\u0d28\u0d3f\u0d28\u0d41\u0d24 \u0d1b\u0d30\u0d23\u0d02"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d1b\u0d3f\u0d02\u0d24\u0d3f\u0d24\u0d3e\u0d30\u0d4d\u0d25 \u0d2b\u0d32\u0d26\u0d3e\u0d2f\u0d15 \u0d1b\u0d3f\u0d26\u0d3e\u0d28\u0d02\u0d26\u0d02 <br/> \u0d1b\u0d3f\u0d24\u0d4d\u0d2a\u0d4d\u0d30\u0d15\u0d3e\u0d36 \u0d15\u0d4d\u0d30\u0d41\u0d24\u0d3f \u0d28\u0d3f\u0d2a\u0d41\u0d23\u0d02"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d05\u0d28\u0d02\u0d24\u0d3e\u0d28\u0d02\u0d24 \u0d38\u0d41\u0d17\u0d41\u0d23 \u0d38\u0d3e\u0d02\u0d26\u0d4d\u0d30\u0d02 <br/> \u0d05\u0d28\u0d3e\u0d26\u0d3f\u0d36\u0d4d\u0d30\u0d40\u0d17\u0d41\u0d30\u0d41 \u0d38\u0d4d\u0d35\u0d30\u0d42\u0d2a\u0d27\u0d30\u0d02 <br/> \u0d05\u0d28\u0d3f\u0d24\u0d4d\u0d2f\u0d35\u0d4d\u0d2f\u0d35\u0d39\u0d3e\u0d30 \u0d2c\u0d02\u0d27\u0d28\u0d28\u0d3f\u0d35\u0d3e\u0d30\u0d15\u0d02 <br/> \u0d06\u0d28\u0d02\u0d26\u0d15\u0d30 \u0d2e\u0d4b\u0d15\u0d4d\u0d36\u0d2a\u0d26\u0d26\u0d3e\u0d2f\u0d15\u0d02  \u0d2e\u0d4b\u0d15\u0d4d\u0d36\u0d2a\u0d4d\u0d30\u0d26\u0d3e\u0d28\u0d28\u0d3f\u0d2a\u0d41\u0d23\u0d02"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d2a\u0d30\u0d24\u0d2