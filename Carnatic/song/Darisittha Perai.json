\u0cbe\u0c98 \u0c9a\u0cbf\u0ca6\u0cae\u0ccd\u0cac\u0cb0\u0cae\u0ccd \u0c85\u0ca8\u0ccd\u0cb1\u0cbf\u0caf\u0cc1\u0ca3\u0ccd\u0ca1\u0ccb"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb5\u0cbf\u0cb0\u0cbf\u0cb5\u0c9f\u0ccd\u0cb1 \u0cad\u0cc2\u0cb2\u0ccb\u0c95 \u0c95\u0caf\u0cbf\u0cb2\u0cc8\u0c8e\u0ca8\u0ccd\u0cb1\u0cc1\u0cb3 \u0c92\u0cb0\u0cc1 \u0cae\u0cbf\u0ca4\u0ccd\u0ca4\u0cc1 \u0ca8\u0c9f\u0cae\u0ccd \u0c8e\u0ca6\u0cbf\u0cb0\u0ccd \u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cc1 <br/> \u0cb5\u0cc7\u0cb0\u0cc1\u0cb5\u0ccb\u0ca4\u0ccd\u0ca4\u0ca8\u0cc8 \u0cb5\u0cbf\u0cb0\u0ca3\u0ccd\u0ca1\u0ccb\u0ca1\u0cbf\u0cb0\u0ca3\u0ccd\u0ca1\u0cc1 \u0c97\u0ccb\u0caa\u0cc1\u0cb0\u0ca4\u0ccd\u0ca4\u0cc8\u0c95\u0ccd-\u0c95\u0cbe\u0ca3\u0ccd\u0ca3\u0cbe\u0cb0\u0cb5\u0cc7 \u0c95\u0ca3\u0ccd\u0ca1\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cae\u0cc7\u0cb2\u0cc8\u0ca6\u0ccd-\u0ca6\u0cbf\u0cb6\u0cc8 \u0c97\u0ccb\u0caa\u0cc1\u0cb0\u0ca4\u0ccd\u0ca4\u0cbf\u0ca8\u0cbf\u0cb0\u0ccd\u0c95\u0ccd-\u0c95\u0cb1\u0ccd\u0caa\u0c97 \u0cb5\u0cc7\\\u0cb3 \u0cae\u0cc1\u0c96\u0ca8\u0ccd\u0cb1\u0ca8\u0cc8 \u0cb5\u0cbe\u0cb3\u0ccd\u0ca4\u0ccd\u0ca4\u0cbf \u0ca8\u0cbe\u0cb2\u0cbe \u0ca8\u0cc0\u0cb2\u0ca4\u0ccd- <br/> \u0ca4\u0cbf\u0cb0\u0cc1\u0cae\u0caf\u0cbf\u0cb2\u0cc7\u0cb0\u0cc1\u0cae\u0ccd \u0c95\u0cc1\u0cae\u0cb0\u0ca8\u0ccd \u0cae\u0cc1\u0ca8\u0ccd \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cc1 \u0ca4\u0cca\u0cb3\u0cc1\u0ca6\u0ca1\u0cbf \u0caa\u0ccb\u0c9f\u0ccd\u0cb1\u0cbf \u0c85\u0ca8\u0cc1\u0c95\u0cc2\u0cb2 \u0cb6\u0cbf\u0cb5\u0c97\u0ca8\u0ccd\u0c99\u0ccd\u0c97\u0cc8 \u0cae\u0cc2\u0cb3\u0ccd\u0c97\u0cbf\u0caf <br/> \u0ca8\u0cc0\u0cb0\u0cc8 \u0c89\u0c9f\u0ccd\u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cc1 \u0cae\u0ca8\u0c95\u0ccd-\u0c95\u0cc1\u0cb1\u0cc8\u0caf\u0cbe\u0c9f\u0ccd\u0cb1\u0cbf \u0c87\u0ca8\u0ccd\u0ca6 \u0c9c\u0ccd\u0c9e\u0cbe\u0cb2\u0ca4\u0ccd\u0ca4\u0cc8 \u0c88\u0ca8\u0ccd\u0cb1 \u0cb6\u0cbf\u0cb5\u0c95\u0cbe\u0cae \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf \u0caa\u0cbe\u0cb2\u0cc2\u0c9f\u0ccd\u0cb1\u0cbf\u0cb0\u0cc1- <br/> \u0cb5\u0cbf\u0ca8\u0cc8 \u0cae\u0cbe\u0c9f\u0ccd\u0cb1\u0cbf \u0cb5\u0cc6\u0c98\u0cc1 \u0cb6\u0cc0\u0cb2\u0ca4\u0ccd\u0ca4\u0cc1\u0ca1\u0ca8\u0ccd \u0cae\u0cb2\u0cb0\u0ccd \u0cb6\u0cbe\u0ca4\u0ccd\u0ca4\u0cbf \u0c85\u0cb0\u0ccd\u0ca7 \u0c9c\u0cbe\u0cae\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cc7 \u0ca4\u0cca\u0cb3\u0cc1\u0ca6\u0cc7\u0ca4\u0ccd\u0ca4\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0ca8\u0cae\u0ccd\u0cac\u0cbf\u0ca4\u0ccd-\u0ca4\u0cbf\u0cb0\u0cc1\u0cae\u0cc2\u0cb2 \u0cb8\u0ccd\u0ca5\u0cbe\u0ca8\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cc7 \u0ca4\u0cc1\u0ca6\u0cbf \u0ca8\u0cbe\u0ca4\u0ccd\u0ca4\u0cae\u0cc1\u0cae\u0ccd \u0caa\u0cc7\u0cb0\u0caa\u0ccd-\u0caa\u0caf\u0cbf\u0ca8\u0ccd\u0cb1\u0cc1 \u0c89\u0cae\u0cc8 <br/> \u0c85\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cc8\u0ca4\u0ccd-\u0ca4\u0cbe\u0cb3\u0cc8\u0caa\u0ccd-\u0caa\u0caf\u0cbf\u0ca8\u0ccd\u0cb1\u0cc1 \u0cae\u0cb2\u0cbe\u0ca6\u0cbf\u0caf\u0ccb\u0caf\u0ca1\u0cc8\u0ca8\u0ccd\u0ca6\u0cc1 \u0caa\u0cc1\u0cb2\u0cbe\u0ca6\u0cbf\u0caf\u0cc8\u0caf\u0cc6\u0ca8\u0ccd\u0cb1\u0cc1 \u0ca8\u0cbf\u0ca6\u0cae\u0ccd \u0c89\u0cae\u0ccd\u0cac\u0cb0\u0ccd <br/> \u0cb5\u0ca3\u0c99\u0ccd\u0c97\u0cc1\u0cae\u0ccd \u0caa\u0cc7\u0cb0\u0cae\u0ccd\u0cac\u0cb2\u0ca4\u0ccd\u0ca4\u0cbe\u0ca8\u0cc8\u0c95\u0ccd-\u0c95\u0ca3\u0ccd\u0ca1\u0cc1\u0ca3\u0ccd\u0cae\u0cc8\u0caf\u0cbe\u0caf\u0ccd \u0cb8\u0cc7\u0cb5\u0cbf\u0ca4\u0ccd\u0ca4 \u0c95\u0ca8\u0ccd\u0cb1\u0cc1 \u0c95\u0cb3\u0ccd\u0cb3 <br/> \u0cb5\u0cc6\u0cae\u0ccd\u0caa\u0cbf\u0cb0\u0cb5\u0cbf\u0caa\u0ccd-\u0caa\u0cbf\u0ca3\u0cbf\u0cb5\u0cc7\u0cb0\u0cc8\u0caa\u0ccd-\u0caa\u0cca\u0ca1\u0cbf \u0cb6\u0cc6\u0caf\u0ccd\u0caf\u0cc1\u0cae\u0ccd \u0cb6\u0cc6\u0cae\u0ccd \u0caa\u0cca\u0cb0\u0ccd-\u0cb8\u0cad\u0cc8 \u0ca8\u0cbe\u0ca1\u0cbf \u0cb6\u0cc6\u0ca8\u0ccd\u0cb1\u0cc1 <br/> \u0c95\u0cca\u0ca1\u0cbf\u0c95\u0ccd\u0c95\u0cae\u0ccd\u0cac\u0ca4\u0ccd-\u0ca4\u0cb0\u0cc1\u0c97\u0cbf\u0ca8\u0cbf\u0cb2\u0ccd \u0ca8\u0ca8\u0ccd\u0cb1\u0cbf\u0cb0\u0cc1 \u0c95\u0cc8\u0caf\u0cbe\u0cb2\u0ccd \u0c95\u0cc1\u0cae\u0ccd\u0cac\u0cbf\u0c9f\u0ccd\u0c9f\u0cc1 \u0cb9\u0cb0\u0cb9\u0cb0\u0cbe\u0cb5\u0cc6\u0ca8\u0ccd\u0cb1\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c9c\u0ccd\u0c9e\u0cbe\u0ca8 \u0cb8\u0cad\u0cc8 \u0ca4\u0ca8\u0ccd\u0ca8\u0cc8 \u0cb8\u0cc7\u0cb5\u0cbf\u0ca4\u0ccd\u0ca4\u0c99\u0ccd\u0c97\u0cc1\u0cb3\u0ccd\u0cb3\u0cbe\u0c95\u0ccd\u0c95\u0cbf\u0caf\u0ccd\u0caf \u0c89\u0ca3\u0ccd\u0cae\u0cc8\u0caf\u0cc8 \u0ca8\u0cbe\u0ca1\u0cbf \u0c92\u0cb3\u0cbf\u0caf\u0cbe\u0ca8\u0ca4\u0ccd\u0ca4\u0cc8 <br/> \u0c85\u0cb1\u0ccd\u0caa\u0cc1\u0ca4 \u0c9c\u0ccd\u0c9e\u0cbe\u0ca8\u0c95\u0ccd\u0c95\u0ca3\u0ccd\u0ca3\u0cbe\u0cb2\u0ccd \u0c85\u0cb1\u0cbf\u0ca8\u0ccd\u0ca6\u0ca6\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6 \u0cb5\u0cbe\u0cb0\u0cbf\u0caf\u0cb2\u0cbe\u0ca1\u0cbf\u0ca4\u0ccd-\u0ca4\u0cb5 \u0cae\u0ccb\u0ca8\u0ca4\u0ccd\u0ca4\u0cc1\u0ca1\u0ca8\u0ccd \u0c85\u0c99\u0ccd\u0c97\u0cbf\u0cb0\u0cc1\u0ca8\u0ccd\u0ca6\u0cc1 <br/> \u0c9c\u0caa\u0cae\u0ccd \u0c95\u0ca3\u0ccd \u0cae\u0cc1\u0cb3\u0cc1\u0ca6\u0cc1\u0cae\u0ccd \u0c9c\u0caa\u0cbf\u0ca4\u0ccd-\u0ca4\u0ca8\u0ccd\u0cac\u0cc1 \u0ca8\u0cc0\u0ca1\u0cbf \u0ca8\u0cbe\u0cb3\u0cc1\u0cae\u0ccd \u0cb5\u0cbe\u0ca8\u0ca4\u0ccd\u0ca4\u0cb5\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd \u0c95\u0cbf\u0ca1\u0cc8\u0caf\u0cbe\u0ca6 \u0cae\u0cc1\u0c95\u0ccd\u0ca4\u0cbf <br/> \u0ca8\u0cbf\u0ca7\u0ccd\u0ca8\u0ca4\u0ccd\u0ca4\u0cbf\u0cb0\u0ccd-\u0cb8\u0cc7\u0cb5\u0ca1\u0cbf \u0ca4\u0cc7\u0ca1\u0cbf \u0cae\u0cc6\u0caf\u0ccd \u0ca7\u0ccd\u0caf\u0cbe\u0ca8\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cc7\u0ca6\u0cc1\u0cae\u0ccd \u0caa\u0cbe\u0ca1\u0cbf \u0c9c\u0ccd\u0c9e\u0cbe\u0ca8\u0ca4\u0ccd\u0ca4\u0cc8 \u0cb5\u0cc7\u0cb0\u0cb0\u0c9a\u0ccd\u0c9a\u0cbe\u0ca1\u0cbf"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d26\u0d30\u0d3f\u0d36\u0d3f\u0d24\u0d4d\u0d24 \u0d2a\u0d47\u0d30\u0d48\u0d2a\u0d4d-\u0d2a\u0d30\u0d3f\u0d36\u0d41\u0d26\u0d4d\u0d27\u0d30\u0d3e\u0d18 \u0d1a\u0d3f\u0d26\u0d2e\u0d4d\u0d2c\u0d30\u0d2e\u0d4d \u0d05\u0d28\u0d4d\u0d31\u0d3f\u0d2f\u0d41\u0d23\u0d4d\u0d21\u0d4b"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d35\u0d3f\u0d30\u0d3f\u0d35\u0d1f\u0d4d\u0d31 \u0d2d\u0d42\u0d32\u0d4b\u0d15 \u0d15\u0d2f\u0d3f\u0d32\u0d48\u0d0e\u0d28\u0d4d\u0d31\u0d41\u0d33 \u0d12\u0d30\u0d41 \u0d2e\u0d3f\u0d24\u0d4d\u0d24\u0d41 \u0d28\u0d1f\u0d2e\u0d4d \u0d0e\u0d26\u0d3f\u0d30\u0d4d \u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d41 <br/> \u0d35\u0d47\u0d30\u0d41\u0d35\u0d4b\u0d24\u0d4d\u0d24\u0d28\u0d48 \u0d35\u0d3f\u0d30\u0d23\u0d4d\u0d21\u0d4b\u0d21\u0d3f\u0d30\u0d23\u0d4d\u0d21\u0d41 \u0d17\u0d4b\u0d2a\u0d41\u0d30\u0d24\u0d4d\u0d24\u0d48\u0d15\u0d4d-\u0d15\u0d3e\u0d23\u0d4d\u0d23\u0d3e\u0d30\u0d35\u0d47 \u0d15\u0d23\u0d4d\u0d21\u0d41"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d2e\u0d47\u0d32\u0d48\u0d26\u0d4d-\u0d26\u0d3f\u0d36\u0d48 \u0d17\u0d4b\u0d2a\u0d41\u0d30\u0d24\u0d4d\u0d24\u0d3f\u0d28\u0d3f\u0d30\u0d4d\u0d15\u0d4d-\u0d15\u0d31\u0d4d\u0d2a\u0d17 \u0d35\u0d47\\\u0d34 \u0d2e\u0d41\u0d16\u0d28\u0d4d\u0d31\u0d28\u0d48 \u0d35\u0d3e\u0d34\u0d4d\u0d24\u0d4d\u0d24\u0d3f \u0d28\u0d3e\u0d32\u0d3e \u0d28\u0d40\u0d32\u0d24\u0d4d- <br/> \u0d24\u0d3f\u0d30\u0d41\u0d2e\u0d2f\u0d3f\u0d32\u0d47\u0d30\u0d41\u0d2e\u0d4d \u0d15\u0d41\u0d2e\u0d30\u0d28\u0d4d \u0d2e\u0d41\u0d28\u0d4d \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d41 \u0d24\u0d4a\u0d34\u0d41\u0d26\u0d21\u0d3f \u0d2a\u0d4b\u0d1f\u0d4d\u0d31\u0d3f \u0d05\u0d28\u0d41\u0d15\u0d42\u0d32 \u0d36\u0d3f\u0d35\u0d17\u0d28\u0d4d\u0d19\u0d4d\u0d17\u0d48 \u0d2e\u0d42\u0d34\u0d4d\u0d17\u0d3f\u0d2f <br/> \u0d28\u0d40\u0d30\u0d48 \u0d09\u0d1f\u0d4d\u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d41 \u0d2e\u0d28\u0d15\u0d4d-\u0d15\u0d41\u0d31\u0d48\u0d2f\u0d3e\u0d1f\u0d4d\u0d31\u0d3f \u0d07\u0d28\u0d4d\u0d26 \u0d1c\u0d4d\u0d1e\u0d3e\u0d32\u0d24\u0d4d\u0d24\u0d48 \u0d08\u0d28\u0d4d\u0d31 \u0d36\u0d3f\u0d35\u0d15\u0d3e\u0d2e \u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3f \u0d2a\u0d3e\u0d32\u0d42\u0d1f\u0d4d\u0d31\u0d3f\u0d30\u0d41- <br/> \u0d35\u0d3f\u0d28\u0d48 \u0d2e\u0d3e\u0d1f\u0d4d\u0d31\u0d3f \u0d35\u0d46\u0d18\u0d41 \u0d36\u0d40\u0d32\u0d24\u0d4d\u0d24\u0d41\u0d21\u0d28\u0d4d \u0d2e\u0d32\u0d30\u0d4d \u0d36\u0d3e\u0d24\u0d4d\u0d24\u0d3f \u0d05\u0d30\u0d4d\u0d27 \u0d1c\u0d3e\u0d2e\u0d24\u0d4d\u0d24\u0d3f\u0d32\u0d47 \u0d24\u0d4a\u0d34\u0d41\u0d26\u0d47\u0d24\u0d4d\u0d24\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d28\u0d2e\u0d4d\u0d2c\u0d3f\u0d24\u0d4d-\u0d24\u0d3f\u0d30\u0d41\u0d2e\u0d42\u0d32 \u0d38\u0d4d\u0d25\u0d3e\u0d28\u0d24\u0d4d\u0d24\u0d3f\u0d32\u0d47 \u0d24\u0d41\u0d26\u0d3f \u0d28\u0d3e\u0d24\u0d4d\u0d24\u0d2e\u0d41\u0d2e\u0d4d \u0d2a\u0d47\u0d30\u0d2a\u0d4d-\u0d2a\u0d2f\u0d3f\u0d28\u0d4d\u0d31\u0d41 \u0d09\u0d2e\u0d48 <br/> \u0d05\u0d2e\u0d4d\u0d2c\u0d3f\u0d15\u0d48\u0d24\u0d4d-\u0d24\u0d3e\u0d33\u0d48\u0d2a\u0d4d-\u0d2a\u0d2f\u0d3f\u0d28\u0d4d\u0d31\u0d41 \u0d2e\u0d32\u0d3e\u0d26\u0d3f\u0d2f\u0d4b\u0d2f\u0d21\u0d48\u0d28\u0d4d\u0d26\u0d41 \u0d2a\u0d41\u0d32\u0d3e\u0d26\u0d3f\u0d2f\u0d48\u0d2f\u0d46\u0d28\u0d4d\u0d31\u0d41 \u0d28\u0d3f\u0d26\u0d2e\u0d4d \u0d09\u0d2e\u0d4d\u0d2c\u0d30\u0d4d <br/> \u0d35\u0d23\u0d19\u0d4d\u0d17\u0d41\u0d2e\u0d4d \u0d2a\u0d47\u0d30\u0d2e\u0d4d\u0d2c\u0d32\u0d24\u0d4d\u0d24\u0d3e\u0d28\u0d48\u0d15\u0d4d-\u0d15\u0d23\u0d4d\u0d21\u0d41\u0d23\u0d4d\u0d2e\u0d48\u0d2f\u0d3e\u0d2f\u0d4d \u0d38\u0d47\u0d35\u0d3f\u0d24\u0d4d\u0d24 \u0d15\u0d28\u0d4d\u0d31\u0d41 \u0d15\u0d33\u0d4d\u0d33 <br/> \u0d35\u0d46\u0d2e\u0d4d\u0d2a\u0d3f\u0d30\u0d35\u0d3f\u0d2a\u0d4d-\u0d2a\u0d3f\u0d23\u0d3f\u0d35\u0d47\u0d30\u0d48\u0d2a\u0d4d-\u0d2a\u0d4a\u0d21\u0d3f \u0d36\u0d46\u0d2f\u0d4d\u0d2f\u0d41\u0d2e\u0d4d \u0d36\u0d46\u0d2e\u0d4d \u0d2a\u0d4a\u0d30\u0d4d-\u0d38\u0d2d\u0d48 \u0d28\u0d3e\u0d21\u0d3f \u0d36\u0d46\u0d28\u0d4d\u0d31\u0d41 <br/> \u0d15\u0d4a\u0d21\u0d3f\u0d15\u0d4d\u0d15\u0d2e\u0d4d\u0d2c\u0d24\u0d4d-\u0d24\u0d30\u0d41\u0d17\u0d3f\u0d28\u0d3f\u0d32\u0d4d \u0d28\u0d28\u0d4d\u0d31\u0d3f\u0d30\u0d41 \u0d15\u0d48\u0d2f\u0d3e\u0d32\u0d4d \u0d15\u0d41\u0d2e\u0d4d\u0d2c\u0d3f\u0d1f\u0d4d\u0d1f\u0d41 \u0d39\u0d30\u0d39\u0d30\u0d3e\u0d35\u0d46\u0d28\u0d4d\u0d31\u0d41"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d1c\u0d4d\u0d1e\u0d3e\u0d28 \u0d38\u0d2d\u0d48 \u0d24\u0d28\u0d4d\u0d28\u0d48 \u0d38\u0d47\u0d35\u0d3f\u0d24\u0d4d\u0d24\u0d19\u0d4d\u0d17\u0d41\u0d33\u0d4d\u0d33\u0d3e\u0d15\u0d4d\u0d15\u0d3f\u0d2f\u0d4d\u0d2f \u0d09\u0d23\u0d4d\u0d2e\u0d48\u0d2f\u0d48 \u0d28\u0d3e\u0d21\u0d3f \u0d12\u0d33\u0d3f\u0d2f\u0d3e\u0d28\u0d24\u0d4d\u0d24\u0d48 <br/> \u0d05\u0d31\u0d4d\u0d2a\u0d41\u0d24 \u0d1c\u0d4d\u0d1e\u0d3e\u0d28\u0d15\u0d4d\u0d15\u0d23\u0d4d\u0d23\u0d3e\u0d32\u0d4d \u0d05\u0d31\u0d3f\u0d28\u0d4d\u0d26\u0d26\u0d3e\u0d28\u0d28\u0d4d\u0d26 \u0d35\u0d3e\u0d30\u0d3f\u0d2f\u0d32\u0d3e\u0d21\u0d3f\u0d24\u0d4d-\u0d24\u0d35 \u0d2e\u0d4b\u0d28\u0d24\u0d4d\u0d24\u0d41\u0d21\u0d28\u0d4d \u0d05\u0d19\u0d4d\u0d17\u0d3f\u0d30\u0d41\u0d28\u0d4d\u0d26\u0d41 <br/> \u0d1c\u0d2a\u0d2e\u0d4d \u0d15\u0d23\u0d4d \u0d2e\u0d41\u0d34\u0d41\u0d26\u0d41\u0d2e\u0d4d \u0d1c\u0d2a\u0d3f\u0d24\u0d4d-\u0d24\u0d28\u0d4d\u0d2c\u0d41 \u0d28\u0d40\u0d21\u0d3f \u0d28\u0d3e\u0d33\u0d41\u0d2e\u0d4d \u0d35\u0d3e\u0d28\u0d24\u0d4d\u0d24\u0d35\u0d30\u0d4d\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d \u0d15\u0d3f\u0d21\u0d48\u0d2f\u0d3e\u0d26 \u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3f <br/> \u0d28\u0d3f\u0d27\u0d4d\u0d28\u0d24\u0d4d\u0d24\u0d3f\u0d30\u0d4d-\u0d38\u0d47\u0d35\u0d21\u0d3f \u0d24\u0d47\u0d21\u0d3f \u0d2e\u0d46\u0d2f\u0d4d \u0d27\u0d4d\u0d2f\u0d3e\u0d28\u0d24\u0d4d\u0d24\u0d3f\u0d32\u0d47\u0d26\u0d41\u0d2e\u0d4d \u0d2a\u0d3e\u0d21\u0d3f \u0d1c\u0d4d\u0d1e\u0d3e\u0d28\u0d24\u0d4d\u0d24\u0d48 \u0d35\u0d47\u0d30\u0d30\u0d1a\u0d4d\u0d1a\u0d3e\u0d21\u0d3f"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0926\u0930\u093f\u0936\u093f\u0924\u094d\u0924 \u092a\u0947\u0930\u0948\u092a\u094d-\u092a\u0930\u093f\u0936\u0941\u0926\u094d\u0927\u0930\u093e\u0918 \u091a\u093f\u0926\u092e\u094d\u092c\u0930\u092e\u094d \u0905\u0928\u094d\u0930\u093f\u092f\u0941\u0923\u094d\u0921\u094b"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0935\u093f\u0930\u093f\u0935\u091f\u094d\u0930 \u092d\u0942\u0932\u094b\u0915 \u0915\u092f\u093f\u0932\u0948\u090f\u0928\u094d\u0930\u0941\u0932 \u0913\u0930\u0941 \u092e\u093f\u0924\u094d\u0924\u0941 \u0928\u091f\u092e\u094d \u090f\u0926\u093f\u0930\u094d \u0915\u094b\u0923\u094d\u0921\u0941 <br/> \u0935\u0947\u0930\u0941\u0935\u094b\u0924\u094d\u0924\u0928\u0948 \u0935\u093f\u0930\u0923\u094d\u0921\u094b\u0921\u093f\u0930\u0923\u094d\u0921\u0941 \u0917\u094b\u092a\u0941\u0930\u0924\u094d\u0924\u0948\u0915\u094d-\u0915\u093e\u0923\u094d\u0923\u093e\u0930\u0935\u0947 \u0915\u0923\u094d\u0921\u0941"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u092e\u0947\u0932\u0948\u0926\u094d-\u0926\u093f\u0936\u0948 \u0917\u094b\u092a\u0941\u0930\u0924\u094d\u0924\u093f\u0928\u093f\u0930\u094d\u0915\u094d-\u0915\u0930\u094d\u092a\u0917 \u0935\u0947\\\u0932 \u092e\u0941\u0916\u0928\u094d\u0930\u0928\u0948 \u0935\u093e\u0932\u094d\u0924\u094d\u0924\u093f \u0928\u093e\u0932\u093e \u0928\u0940\u0932\u0924\u094d- <br/> \u0924\u093f\u0930\u0941\u092e\u092f\u093f\u0932\u0947\u0930\u0941\u092e\u094d \u0915\u0941\u092e\u0930\u0928\u094d \u092e\u0941\u0928\u094d \u0928\u093f\u0928\u094d\u0930\u0941 \u0924\u094b\u0932\u0941\u0926\u0921\u093f \u092a\u094b\u091f\u094d\u0930\u093f \u0905\u0928\u0941\u0915\u0942\u0932 \u0936\u093f\u0935\u0917\u0928\u094d\u0919\u094d\u0917\u0948 \u092e\u0942\u0932\u094d\u0917\u093f\u092f <br/> \u0928\u0940\u0930\u0948 \u0909\u091f\u094d\u0915\u094b\u0923\u094d\u0921\u0941 \u092e\u0928\u0915\u094d-\u0915\u0941\u0930\u0948\u092f\u093e\u091f\u094d\u0930\u093f \u0907\u0928\u094d\u0926 \u091c\u094d\u091e\u093e\u0932\u0924\u094d\u0924\u0948 \u0908\u0928\u094d\u0930 \u0936\u093f\u0935\u0915\u093e\u092e \u0938\u0941\u0928\u094d\u0926\u0930\u093f \u092a\u093e\u0932\u0942\u091f\u094d\u0930\u093f\u0930\u0941- <br/> \u0935\u093f\u0928\u0948 \u092e\u093e\u091f\u094d\u0930\u093f \u0935\u0947\u0918\u0941 \u0936\u0940\u0932\u0924\u094d\u0924\u0941\u0921\u0928\u094d \u092e\u0932\u0930\u094d \u0936\u093e\u0924\u094d\u0924\u093f \u0905\u0930\u094d\u0927 \u091c\u093e\u092e\u0924\u094d\u0924\u093f\u0932\u0947 \u0924\u094b\u0932\u0941\u0926\u0947\u0924\u094d\u0924\u093f"}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u0928\u092e\u094d\u092c\u093f\u0924\u094d-\u0924\u093f\u0930\u0941\u092e\u0942\u0932 \u0938\u094d\u0925\u093e\u0928\u0924\u094d\u0924\u093f\u0932\u0947 \u0924\u0941\u0926\u093f \u0928\u093e\u0924\u094d\u0924\u092e\u0941\u092e\u094d \u092a\u0947\u0930\u092a\u094d-\u092a\u092f\u093f\u0928\u094d\u0930\u0941 \u0909\u092e\u0948 <br/> \u0905\u092e\u094d\u092c\u093f\u0915\u0948\u0924\u094d-\u0924\u093e\u0932\u0948\u092a\u094d-\u092a\u092f\u093f\u0928\u094d\u0930\u0941 \u092e\u0932\u093e\u0926\u093f\u092f\u094b\u092f\u0921\u0948\u0928\u094d\u0926\u0941 \u092a\u0941\u0932\u093e\u0926\u093f\u092f\u0948\u092f\u0947\u0928\u094d\u0930\u0941 \u0928\u093f\u0926\u092e\u094d \u0909\u092e\u094d\u092c\u0930\u094d <br/> \u0935\u0923\u0919\u094d\u0917\u0941\u092e\u094d \u092a\u0947\u0930\u092e\u094d\u092c\u0932\u0924\u094d\u0924\u093e\u0928\u0948\u0915\u094d-\u0915\u0923\u094d\u0921\u0941\u0923\u094d\u092e\u0948\u092f\u093e\u092f\u094d \u0938\u0947\u0935\u093f\u0924\u094d\u0924 \u0915\u0928\u094d\u0930\u0941 \u0915\u0932\u094d\u0932 <br/> \u0935\u0947\u092e\u094d\u092a\u093f\u0930\u0935\u093f\u092a\u094d-\u092a\u093f\u0923\u093f\u0935\u0947\u0930\u0948\u092a\u094d-\u092a\u094b\u0921\u093f \u0936\u0947\u092f\u094d\u092f\u0941\u092e\u094d \u0936\u0947\u092e\u094d \u092a\u094b\u0930\u094d-\u0938\u092d\u0948 \u0928\u093e\u0921\u093f \u0936\u0947\u0928\u094d\u0930\u0941 <br/> \u0915\u094b\u0921\u093f\u0915\u094d\u0915\u092e\u094d\u092c\u0924\u094d-\u0924\u0930\u0941\u0917\u093f\u0928\u093f\u0932\u094d \u0928\u0928\u094d\u0930\u093f\u0930\u0941 \u0915\u0948\u092f\u093e\u0932\u094d \u0915\u0