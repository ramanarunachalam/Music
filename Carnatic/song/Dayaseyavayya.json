cae\u0cc7\u0ca8\u0cc1 <br/> \u0caa\u0cc6\u0c9e\u0ccd\u0c9a\u0cbf \u0c95\u0ca8\u0cbf\u0caa\u0cbf\u0c9e\u0ccd\u0c9a\u0cbf\u0ca8\u0c9f\u0ccd\u0cb2 \u0ca6\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c8e\u0c9f\u0ccd\u0c9f\u0cbf \u0cb5\u0cbe\u0cb0\u0cbf\u0c95\u0cc8\u0ca8 \u0ca6\u0cca\u0cb0\u0c95\u0ca8\u0ccd- <br/> \u0c85\u0c9f\u0ccd\u0c9f\u0cbf \u0c9c\u0ca8\u0c95\u0c9c\u0ca8\u0cc1 \u0cac\u0cca\u0c9f\u0ccd\u0c9f\u0cc1 <br/> \u0c95\u0c9f\u0ccd\u0c9f\u0cbf \u0c95\u0cb0\u0cae\u0cc1 \u0caa\u0c9f\u0ccd\u0c9f\u0cc1 \u0cb5\u0cc7\u0cb3 <br/> \u0c95\u0cbe\u0ca8\u0ccd\u0ca4\u0c95\u0cc1\u0ca8\u0ccd\u0ca8 \u0cb8\u0cc1\u0c96\u0cae\u0cc1 \u0ca4\u0ca8\u0c95\u0cc1 \u0ca6\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0c95\u0ccb\u0cb0\u0cbf\u0ca8 \u0cb5\u0cbe\u0cb0\u0cb2\u0ca8\u0cc1 \u0ca8\u0cc7\u0ca8\u0ccd- <br/> \u0c88 \u0cb0\u0cc0\u0ca4\u0cbf \u0cac\u0ccd\u0cb0\u0ccb\u0ca4\u0cc1\u0ca8\u0ca8\u0cbf \u0cb8\u0cad <br/> \u0cb5\u0cbe\u0cb0\u0cb2\u0c95\u0cc1 \u0ca4\u0cc6\u0cb2\u0cbf\u0caf \u0c9c\u0cc7\u0caf\u0cc1 <br/> \u0ca6\u0cbe\u0cb0\u0cbf \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c\u0cc1\u0ca8\u0c95\u0cc1 \u0ca6\u0caf"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d26\u0d2f \u0d38\u0d47\u0d2f\u0d35\u0d2f\u0d4d\u0d2f\u0d3e \u0d38\u0d26\u0d2f \u0d30\u0d3e\u0d2e \u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d26\u0d2f\u0d28\u0d41 \u0d15\u0d4a\u0d1e\u0d4d\u0d1a\u0d46\u0d2e\u0d48\u0d28 \u0d28\u0d3e\u0d21\u0d41 <br/> \u0d24\u0d32\u0d1a\u0d41\u0d1a\u0d41\u0d28\u0d4d\u0d28 \u0d38\u0d40\u0d24\u0d3e \u0d38\u0d41\u0d16\u0d2e\u0d41 \u0d26\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d15\u0d4d\u0d36\u0d3f\u0d24\u0d3f \u0d28\u0d3e\u0d25\u0d41\u0d32 \u0d30\u0d3e\u0d15\u0d2f\u0d41 \u0d38\u0d2e\u0d4d\u0d2e\u0d24\u0d3f <br/> \u0d32\u0d47\u0d15 \u0d36\u0d4d\u0d30\u0d40 \u0d38\u0d3e\u0d15\u0d47\u0d24 <br/> \u0d2a\u0d24\u0d3f \u0d30\u0d3e\u0d21\u0d47\u0d2f\