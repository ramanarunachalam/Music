0c2c\u0c3f\u0c30\u0c41\u0c26\u0c41\u0c23\u0c4d\u0c1f\u0c3e\u0c17\u0c3f"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c8e\u0cb2\u0ccd\u0cb2\u0cbf\u0cb0\u0cc1\u0cb5\u0ca8\u0ccb \u0cb0\u0c99\u0ccd\u0c97 \u0c8e\u0cae\u0ccd\u0cac \u0cb8\u0cae\u0ccd\u0cb6\u0caf \u0cac\u0cc7\u0ca1 \u0c8e\u0cb2\u0ccd\u0cb2\u0cbf \u0cad\u0c95\u0ccd\u0ca4\u0cb0\u0cc1 \u0c95\u0cb0\u0cc6\u0caf\u0cc6 \u0c85\u0cb2\u0ccd\u0cb2\u0cbf \u0cac\u0ca8\u0ccd\u0ca6\u0ccb\u0ca6\u0c97\u0cc1\u0cb5\u0ca8\u0cc1"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0ca4\u0cb0\u0cb2 \u0caa\u0ccd\u0cb0\u0cb9\u0ccd\u0cb2\u0cbe\u0ca6 \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb9\u0cb0\u0cbf \u0cb5\u0cbf\u0cb6\u0ccd\u0cb5\u0cae\u0caf\u0ca8\u0cc6\u0ca8\u0ccd\u0ca6\u0cc1 \u0cad\u0cb0\u0ca6\u0ccd\u0caf\u0cca\u0ca6\u0cc6\u0caf\u0cb2\u0cb5\u0ca8\u0caa\u0cbf\u0ca4 \u0c95\u0ccb\u0caa\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6 <br/> \u0cb8\u0ccd\u0ca5\u0cbf\u0cb0\u0cb5\u0cbe\u0ca6\u0ca1\u0cbf \u0c95\u0cae\u0ccd\u0cac\u0ca6\u0cca\u0cb3\u0cc1 \u0ca4\u0ccb\u0cb0\u0cc1 \u0ca4\u0ccb\u0cb0\u0cc6\u0ca8\u0cb2\u0cc1 \u0cad\u0cb0\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6 \u0cac\u0cb0\u0cb2\u0ca6\u0c95\u0cc7 \u0cb5\u0cc8\u0c95\u0cc1\u0ca3\u0ccd\u0ca0 \u0ca8\u0cc6\u0cb0\u0cc6\u0cae\u0ca8\u0cc6\u0caf\u0cc6"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0caa\u0cbe\u0caa \u0c95\u0cb0\u0ccd\u0cae\u0cb5 \u0cae\u0cbe\u0ca1\u0cbf\u0ca6\u0c9c\u0cae\u0cbf\u0cb3\u0ca8 \u0caf\u0caeB\u0cb9\u0c9f\u0cb0\u0cc1 \u0c95\u0ccb\u0caa\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6\u0cc6\u0cb3\u0cc6\u0caf\u0cc1\u0ca4\u0cbf\u0cb0\u0cc6 B\u0cb9\u0cc0\u0ca4\u0cbf\u0caf\u0cbf\u0ca8\u0ccd\u0ca6 <br/> \u0ca4\u0cbe \u0caa\u0cc1\u0ca4\u0ccd\u0cb0\u0ca8\u0ca8\u0cc1 \u0c95\u0cb0\u0cc6\u0caf\u0cc6 \u0c95\u0cc7\u0cb3\u0cbf \u0cb0\u0c95\u0ccd\u0cb6\u0cbf\u0cb8\u0cc6 \u0cb6\u0ccd\u0cb5\u0cc7\u0ca4 \u0ca6\u0ccd\u0cb5\u0cc0\u0caa\u0cb5\u0cc0 \u0ca2\u0cb0\u0cc6\u0c97\u0cc6 \u0cb8\u0cae\u0cc0\u0caa\u0ca6\u0cb2\u0ccd\u0cb2\u0cbf\u0cb9\u0cc1\u0ca6\u0cc6"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c95\u0cb0\u0cbf\u0cb0\u0cbe\u0c9c\u0ca8\u0ca8\u0cc1 \u0ca8\u0cc6\u0c97\u0cb3\u0cc1 \u0ca8\u0cc1\u0c99\u0ccd\u0c97\u0cb2\u0cbf\u0cb0\u0cc6 B\u0cb9\u0caf\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6 \u0cae\u0cca\u0cb0\u0cc6\u0caf\u0cbf\u0ca1\u0cb2\u0cc1 \u0c95\u0cc7\u0cb3\u0cbf \u0ca4\u0ccd\u0cb5\u0cb0\u0cbf\u0ca4\u0ca6\u0cb2\u0cbf \u0cac\u0ca8\u0ccd\u0ca6\u0cc1 <br/> \u0c95\u0cb0\u0cc1\u0ca3\u0ca6\u0cbf \u0cac\u0ca8\u0ccd\u0ca2\u0ca8\u0cb5 \u0caa\u0cb0\u0cbf\u0cb9\u0cb0\u0cbf\u0cb8\u0cc6 \u0c97\u0c9c\u0cb0\u0cbe\u0c9c\u0ca8\u0cbf\u0cb0\u0cc1\u0cb5 \u0cb8\u0cb0\u0cb8\u0cbf\u0caf\u0cc1 \u0c85\u0ca8\u0ca8\u0ccd\u0ca4\u0cbe\u0cb8\u0ca8\u0c95\u0cc6 \u0cae\u0cc1\u0cae\u0ccd\u0cae\u0ca8\u0cc6\u0caf\u0cc6"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c95\u0cc1\u0cb0\u0cc1\u0caa\u0ca4\u0cbf\u0caf\u0cc1 \u0ca6\u0ccd\u0cb0\u0ccc\u0caa\u0ca6\u0cbf\u0caf \u0cb8\u0cc0\u0cb0\u0cc6\u0caf\u0ca8\u0cc1 \u0cb8\u0cc6\u0cb3\u0cc6\u0caf\u0cc1\u0ca4\u0cbf\u0cb0\u0cc6 \u0ca4\u0cb0\u0cc1\u0ca3\u0cbf \u0cb9\u0cbe \u0c95\u0ccd\u0cb0\u0cbf\u0cb6\u0ccd\u0ca3\u0cbe \u0c8e\u0ca8\u0ccd\u0ca6\u0cca\u0ca6\u0cb0\u0cc6 \u0c95\u0cc7\u0cb3\u0ccd\u0ca6\u0cc1 <br/> B\u0cb9\u0cb0\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6 \u0c85\u0c95\u0ccd\u0cb6\u0caf\u0cbe\u0cae\u0ccd\u0cac\u0cb0\u0cb5\u0cc0\u0caf\u0cc6 \u0cb9\u0cb8\u0ccd\u0ca4\u0cbf\u0ca8\u0cbe \u0caa\u0cc1\u0cb0\u0cbf\u0c97\u0cc6 \u0ca6\u0ccd\u0cb5\u0cbe\u0cb0\u0cbe\u0cb5\u0ca4\u0cbf\u0caf\u0cc1 \u0c95\u0cc2\u0c97\u0cb3\u0ca4\u0cc6\u0caf\u0cc6"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c85\u0ca3\u0cc1\u0cae\u0cb9\u0ca4\u0ccd\u0ca4\u0cbf\u0ca8\u0cca\u0cb3\u0cc6\u0cb2\u0ccd\u0cb2 \u0caa\u0cb0\u0cbf\u0caa\u0cc2\u0cb0\u0ccd\u0ca3 \u0cb5\u0cbf\u0cb6\u0ccd\u0cb5\u0cae\u0caf \u0c97\u0ca3\u0ca8\u0cc6\u0caf\u0cbf\u0cb2\u0ccd\u0cb2\u0ca6 \u0cae\u0cb9\u0cbe \u0cae\u0cb9\u0cbf\u0cae\u0ca8\u0cc6\u0ca8\u0cbf\u0caa <br/> \u0c98\u0ca8 \u0c95\u0ccd\u0cb0\u0ccd\u0caa\u0cbe\u0ca8\u0cbf\u0ca7\u0cbf\u0c95\u0cbe\u0c97\u0cbf \u0ca8\u0cc6\u0cb2\u0cc6\u0caf\u0ca6\u0cbf \u0c95\u0cc7\u0cb6\u0cb5\u0ca8\u0cc1 \u0ca8\u0cc6\u0ca8\u0cc6\u0ca6\u0cb5\u0cb0 \u0cae\u0ca8\u0ca6\u0cca\u0cb2\u0cbf\u0cb9\u0ca8\u0cc6\u0cae\u0ccd\u0cac \u0cac\u0cbf\u0cb0\u0cc1\u0ca6\u0cc1\u0ca3\u0ccd\u0c9f\u0cbe\u0c97\u0cbf"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d0e\u0d32\u0d4d\u0d32\u0d3f\u0d30\u0d41\u0d35\u0d28\u0d4b \u0d30\u0d19\u0d4d\u0d17 \u0d0e\u0d2e\u0d4d\u0d2c \u0d38\u0d2e\u0d4d\u0d36\u0d2f \u0d2c\u0d47\u0d21 \u0d0e\u0d32\u0d4d\u0d32\u0d3f \u0d2d\u0d15\u0d4d\u0d24\u0d30\u0d41 \u0d15\u0d30\u0d46\u0d2f\u0d46 \u0d05\u0d32\u0d4d\u0d32\u0d3f \u0d2c\u0d28\u0d4d\u0d26\u0d4b\u0d26\u0d17\u0d41\u0d35\u0d28\u0d41"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d24\u0d30\u0d32 \u0d2a\u0d4d\u0d30\u0d39\u0d4d\u0d32\u0d3e\u0d26 \u0d36\u0d4d\u0d30\u0d40 \u0d39\u0d30\u0d3f \u0d35\u0d3f\u0d36\u0d4d\u0d35\u0d2e\u0d2f\u0d28\u0d46\u0d28\u0d4d\u0d26\u0d41 \u0d2d\u0d30\u0d26\u0d4d\u0d2f\u0d4a\u0d26\u0d46\u0d2f\u0d32\u0d35\u0d28\u0d2a\u0d3f\u0d24 \u0d15\u0d4b\u0d2a\u0d26\u0d3f\u0d28\u0d4d\u0d26 <br/> \u0d38\u0d4d\u0d25\u0d3f\u0d30\u0d35\u0d3e\u0d26\u0d21\u0d3f \u0d15\u0d2e\u0d4d\u0d2c\u0d26\u0d4a\u0d33\u0d41 \u0d24\u0d4b\u0d30\u0d41 \u0d24\u0d4b\u0d30\u0d46\u0d28\u0d32\u0d41 \u0d2d\u0d30\u0d26\u0d3f\u0d28\u0d4d\u0d26 \u0d2c\u0d30\u0d32\u0d26\u0d15\u0d47 \u0d35\u0d48\u0d15\u0d41\u0d23\u0d4d\u0d20 \u0d28\u0d46\u0d30\u0d46\u0d2e\u0d28\u0d46\u0d2f\u0d46"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d2a\u0d3e\u0d2a \u0d15\u0d30\u0d4d\u0d2e\u0d35 \u0d2e\u0d3e\u0d21\u0d3f\u0d26\u0d1c\u0d2e\u0d3f\u0d33\u0d28 \u0d2f\u0d2eB\u0d39\u0d1f\u0d30\u0d41 \u0d15\u0d4b\u0d2a\u0d26\u0d3f\u0d28\u0d4d\u0d26\u0d46\u0d33\u0d46\u0d2f\u0d41\u0d24\u0d3f\u0d30\u0d46 B\u0d39\u0d40\u0d24\u0d3f\u0d2f\u0d3f\u0d28\u0d4d\u0d26 <br/> \u0d24\u0d3e \u0d2a\u0d41\u0d24\u0d4d\u0d30\u0d28\u0d28\u0d41 \u0d15\u0d30\u0d46\u0d2f\u0d46 \u0d15\u0d47\u0d33\u0d3f \u0d30\u0d15\u0d4d\u0d36\u0d3f\u0d38\u0d46 \u0d36\u0d4d\u0