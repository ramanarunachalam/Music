\u0ca8\u0cc1 \u0cb9\u0cc3\u0ca6\u0caf\u0cae\u0cc1\u0cb2\u0ccb <br/> \u0ca8\u0cbf\u0cb2\u0cc1\u0caa\u0cbf\u0ca8 \u0cb2\u0ccb\u0c95\u0cc1\u0cb2\u0cc1 \u0ca7\u0ca8\u0ccd\u0caf\u0cc1\u0cb2\u0cc8\u0cb0\u0cbf\u0c97\u0cbe \u0ca8\u0cb2\u0cc1\u0c97\u0cc1\u0cb0\u0cbf\u0cb2\u0ccb <br/> \u0cb8\u0cbe\u0cb0\u0cae\u0ca4\u0cc0 \u0ca8\u0ca8\u0cc1 \u0ca6\u0caf\u0ca4\u0ccb \u0c97\u0cbe\u0ca8\u0ccd\u0c9a\u0cbf \u0c95\u0cb0\u0cc1\u0ca3\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1\u0cae\u0cc1 \u0ca4\u0cb2\u0ccd\u0cb2\u0cbf \u0ca8\u0cc6\u0cb0 \u0ca8\u0cae\u0ccd\u0cae\u0cbf\u0ca4\u0cbf <br/> \u0c9a\u0cbe\u0cb2\u0cbe \u0cae\u0cb9\u0cbe \u0cb2\u0cc0\u0cb2 \u0c97\u0cb2\u0cbf\u0c97\u0cbf\u0ca8 \u0cb6\u0c95\u0ccd\u0ca4\u0cbf \u0cb8\u0ca8\u0ccd\u0ca4\u0ca4\u0cae\u0cc1 \u0ca8\u0cc0\u0cb5\u0cc7 \u0cb8\u0ca8\u0ccd\u0ca4\u0ccb\u0cb7\u0cb5\u0ca4\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c93 \u0c9c\u0ca8\u0ca8\u0cbf \u0c95\u0cb0\u0cc1\u0ca3\u0cbf \u0cad\u0cb5 \u0caa\u0ccd\u0cb0\u0cbf\u0caf\u0cbe \u0cb5\u0cbf\u0ca8\u0cc1\u0cae\u0cb5\u0ca8\u0cbf <br/> \u0c93\u0c82 \u0c85\u0ca8\u0cbf\u0ca8 \u0c9c\u0ca8\u0ccd\u0cae \u0cb8\u0cbe\u0cab\u0cb2\u0ccd\u0caf\u0cae\u0cbe\u0caf\u0cc7 \u0ca8\u0cc0\u0ca6\u0cc1 \u0c95\u0ca5\u0cb2\u0ca8\u0cc1 \u0cb5\u0cbf\u0ca8\u0cbf <br/> \u0c93 \u0cae\u0ccb\u0cb9\u0cbe\u0cb5\u0cc3\u0ca4\u0cb2\u0cc8\u0caf\u0cc1\u0ca8\u0ccd\u0ca8 \u0c9c\u0ca8\u0cc1\u0cb2\u0ca8\u0cc1 \u0ca4\u0cb2\u0ccd\u0cb2\u0cbf \u0c87\u0caa\u0cc1\u0ca1\u0cc1 \u0cac\u0ccd\u0cb0\u0ccb\u0cb5\u0cc1\u0cae\u0cc1 <br/> \u0c93 \u0cb0\u0cbe\u0c9c\u0cbe\u0ca7\u0cbf \u0cb0\u0cbe\u0c9c\u0cc7\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0cae\u0c95\u0cc1\u0c9f \u0caa\u0c9f\u0cb2\u0cbf \u0cae\u0ca3\u0cbf \u0cb5\u0cbf\u0cb0\u0c9a\u0cbf\u0ca4 \u0caa\u0ca6\u0cbe"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c95\u0ca8\u0ccd\u0c9c\u0ca8\u0ca6\u0cbe\u0ca8\u0ccd\u0ca4\u0cc1\u0ca8\u0cbf \u0c95\u0cbe\u0cae\u0cbf\u0ca4\u0cbe \u0cb6\u0cc1\u0cad \u0c9a\u0cb0\u0cbf\u0ca4\u0cbe \u0caa\u0ccd\u0cb0\u0cb8\u0ca8\u0ccd\u0ca8 \u0cb5\u0ca6\u0ca8\u0cbe <