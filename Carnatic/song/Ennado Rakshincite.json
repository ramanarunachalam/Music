0bbe\u0b95\u0bb0\u0bbe\u0b9c\u0bc1\u0ba9\u0bbf \u0b8e\u0ba9\u0bcd\u0ba9\u0b9f\u0bcb"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c0e\u0c28\u0c4d\u0c28\u0c21\u0c4b \u0c30\u0c15\u0c4d\u0c37\u0c3f\u0c28\u0c4d\u0c1a\u0c3f\u0c24\u0c47 \u0c28\u0c40\u0c35\u0c3f\u0c2a\u0c41\u0c21\u0c40 <br/> \u0c35\u0c17\u0c28\u0c41 \u0c09\u0c23\u0c4d\u0c21 \u0c35\u0c32\u0c46\u0c28\u0c3e"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2a\u0c28\u0c4d\u0c28\u0c17 \u0c36\u0c2f\u0c28 \u0c2a\u0c46\u0c26\u0c4d\u0c26\u0c32 \u0c38\u0c4d\u0c35- <br/> \u0c2d\u0c3e\u0c35\u0c2e\u0c3f\u0c1f\u0c4d\u0c32\u0c41\u0c23\u0c4d\u0c21\u0c17 \u0c2c\u0c3e\u0c17\u0c41\u0c17 \u0c28\u0c28\u0c4d\u0c28\u0c47 \u0c28\u0c40\u0c35\u0c46\u0c28\u0c4d\u0c28\u0c21\u0c4b"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c1a\u0c40\u0c15\u0c1f\u0c3f \u0c1a\u0c47\u0c24\u0c28\u0c41 \u0c17\u0c3e\u0c38\u0c3f \u0c1c\u0c46\u0c28\u0c4d\u0c26\u0c3f\u0c2f\u0c40 <br/> \u0c32\u0c4b\u0c15\u0c41\u0c32\u0c41 \u0c38\u0c15\u0c32 \u0c15\u0c30\u0c4d\u0c2e\u0c2e\u0c41\u0c32\u0c15\u0c48 \u0c2e\u0c28 <br/> \u0c30\u0c3e\u0c15 \u0c15\u0c4a\u0c30\u0c15\u0c41 \u0c1c\u0c42\u0c1a\u0c46\u0c26\u0c30\u0c28\u0c3f \u0c24\u0c46\u0c32\u0c3f\u0c38\u0c3f <br/> \u0c35\u0c3f\u0c2d\u0c3e\u0c15\u0c30\u0c41\u0c21\u0c41 \u0c2e\u0c26\u0c3f\u0c28\u0c3f <br/> \u0c2a\u0c4d\u0c30\u0c3e\u0c15\u0c1f\u0c2e\u0c41\u0c17 \u0c24\u0c3e \u0c30\u0c3e\u0c28\u0c41 \u0c1a\u0c46\u0c32\u0c4d\u0c32\u0c41\u0c28\u0c28\u0c3f <br/> \u0c35\u0c4d\u0c2f\u0c3e\u0c15\u0c41\u0c32\u0c41\u0c21\u0c48 \u0c05\u0c24\u0c3f \u0c35\u0c47\u0c17\u0c2e\u0c41\u0c28\u0c28\u0c41 <br/> \u0c35\u0c47\u0c15\u0c41\u0c35 \u0c1c\u0c3e\u0c2e\u0c41\u0c28 \u0c24\u0c4b\u0c37\u0c2e\u0c41 \u0c15\u0c32\u0c17\u0c28\u0c41 <br/> \u0c24\u0c3e \u0c15\u0c30\u0c41\u0c23\u0c28\u0c30\u0c41\u0c23\u0c41\u0c28\u0c3f \u0c2a\u0c28\u0c41\u0c2a \u0c32\u0c47\u0c26\u0c3e \u0c0e\u0c28\u0c4d\u0c28\u0c21\u0c4b"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c28\u0c40\u0c30\u0c41 \u0c32\u0c47\u0c15 \u0c38\u0c1c\u0c4d\u0c1c\u0c28 \u0c17\u0c23\u0c2e\u0c41\u0c32\u0c41 \u0c2e\u0c28 <br/> \u0c26\u0c3e\u0c30\u0c3f\u0c28\u0c3f \u0c1c\u0c42\u0c1a\u0c47\u0c30\u0c28\u0c41\u0c1a\u0c41 \u0c24\u0c46\u0c32\u0c3f\u0c38\u0c3f <br/> \u0c15\u0c3e\u0c35\u0c47\u0c30\u0c3f \u0c24\u0c3e\u0c28\u0c41 \u0c30\u0c3e\u0c28\u0c41 \u0c1a\u0c46\u0c32\u0c4d\u0c32\u0c41\u0c28\u0c28\u0c41\u0c1a\u0c41 <br/> \u0c35\u0c3f\u0c1a\u0c3e\u0c30 \u0c39\u0c43\u0c26\u0c2f\u0c41\u0c30\u0c3e\u0c32\u0c48 <br/> \u0c38\u0c3e\u0c30\u0c46\u0c15\u0c41 \u0c35\u0c1a\u0c4d\u0c1a\u0c41\u0c28\u0c28\u0c41\u0c1a\u0c41 \u0c38\u0c28\u0c4d\u0c24\u0c4b\u0c37\u0c2e\u0c41 <br/> \u0c35\u0c40\u0c30\u0c41\u0c32\u0c15\u0c46\u0c32\u0c4d\u0c32\u0c28\u0c41 \u0c15\u0c32\u0c4d\u0c17 \u0c2a\u0c21\u0c2e\u0c1f\u0c3f <br/> \u0c2e\u0c3e\u0c30\u0c41\u0c24 \u0c1a\u0c28\u0c4d\u0c1a\u0c32\u0c2e\u0c41\u0c32 \u0c2a\u0c28\u0c3f\u0c2a\u0c3f <br/> \u0c1a\u0c46\u0c32\u0c2e\u0c32\u0c42\u0c30 \u0c1a\u0c47\u0c2f \u0c32\u0c47\u0c26\u0c3e \u0c30\u0c3e\u0c2e \u0c0e\u0c28\u0c4d\u0c28\u0c21\u0c4b"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c17\u0c30\u0c4d\u0c35\u0c3f\u0c28\u0c4d\u0c1a\u0c3f\u0c28 \u0c06 \u0c30\u0c3e\u0c35\u0c23 \u0c2c\u0c3e\u0c27\u0c32\u0c28\u0c4d- <br/> \u0c13\u0c30\u0c4d\u0c35 \u0c32\u0c47\u0c15\u0c28\u0c41 \u0c35\u0c3f\u0c2d\u0c40\u0c37\u0c23\u0c41\u0c21\u0c41 \u0c28\u0c3f\u0c28\u0c4d\u0c28\u0c41 <br/> \u0c38\u0c30\u0c4d\u0c35 \u0c32\u0c4b\u0c15 \u0c36\u0c30\u0c23\u0c4d\u0c2f\u0c3e \u0c35\u0c28\u0c4d\u0c26\u0c28\u0c2e\u0c28 <br/> \u0c28\u0c3f\u0c30\u0c4d\u0c35\u0c3f\u0c15\u0c3e\u0c30\u0c41\u0c21\u0c48 \u0c28\u0c40\u0c35\u0c41 <br/> \u0c38\u0c30\u0c4d\u0c35\u0c26\u0c3e \u0c30\u0c15\u0c4d\u0c37\u0c3f\u0c28\u0c4d\u0c1a\u0c46\u0c26\u0c28\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c41 <br/> \u0c38\u0c3e\u0c30\u0c4d\u0c35\u0c2d\u0c4c\u0c2e \u0c32\u0c28\u0c4d\u0c15\u0c3e\u0c27\u0c3f\u0c2a\u0c24\u0c4d\u0c2f\u0c2e\u0c41\u0c28 <br/> \u0c38\u0c30\u0c4d\u0c35\u0c1c\u0c4d\u0c1e\u0c41\u0c28\u0c3f \u0c32\u0c15\u0c4d\u0c37\u0c4d\u0c2e\u0c23\u0c41\u0c28\u0c3f\u0c2e\u0c4d\u0c2e\u0c28\u0c17\u0c3e <br/> \u0c38\u0c30\u0c4d\u0c35 \u0c38\u0c28\u0c4d\u0c28\u0c41\u0c24 \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c\u0c41\u0c28\u0c3f \u0c0e\u0c28\u0c4d\u0c28\u0c21\u0c4b"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c8e\u0ca8\u0ccd\u0ca8\u0ca1\u0ccb \u0cb0\u0c95\u0ccd\u0cb7\u0cbf\u0ca8\u0ccd\u0c9a\u0cbf\u0ca4\u0cc7 \u0ca8\u0cc0\u0cb5\u0cbf\u0caa\u0cc1\u0ca1\u0cc0 <br/> \u0cb5\u0c97\u0ca8\u0cc1 \u0c89\u0ca3\u0ccd\u0ca1 \u0cb5\u0cb2\u0cc6\u0ca8\u0cbe"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0caa\u0ca8\u0ccd\u0ca8\u0c97 \u0cb6\u0caf\u0ca8 \u0caa\u0cc6\u0ca6\u0ccd\u0ca6\u0cb2 \u0cb8\u0ccd\u0cb5- <br/> \u0cad\u0cbe\u0cb5\u0cae\u0cbf\u0c9f\u0ccd\u0cb2\u0cc1\u0ca3\u0ccd\u0ca1\u0c97 \u0cac\u0cbe\u0c97\u0cc1\u0c97 \u0ca8\u0ca8\u0ccd\u0ca8\u0cc7 \u0ca8\u0cc0\u0cb5\u0cc6\u0ca8\u0ccd\u0ca8\u0ca1\u0ccb"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c9a\u0cc0\u0c95\u0c9f\u0cbf \u0c9a\u0cc7\u0ca4\u0ca8\u0cc1 \u0c97\u0cbe\u0cb8\u0cbf \u0c9c\u0cc6\u0ca8\u0ccd\u0ca6\u0cbf\u0caf\u0cc0 <br/> \u0cb2\u0ccb\u0c95\u0cc1\u0cb2\u0cc1 \u0cb8\u0c95\u0cb2 \u0c95\u0cb0\u0ccd\u0cae\u0cae\u0cc1\u0cb2\u0c95\u0cc8 \u0cae\u0ca8 <br/> \u0cb0\u0cbe\u0c95 \u0c95\u0cca\u0cb0\u0c95\u0cc1 \u0c9c\u0cc2\u0c9a\u0cc6\u0ca6\u0cb0\u0ca8\u0cbf \u0ca4\u0cc6\u0cb2\u0cbf\u0cb8\u0cbf <br/> \u0cb5\u0cbf\u0cad\u0cbe\u0c95\u0cb0\u0cc1\u0ca1\u0cc1 \u0cae\u0ca6\u0cbf\u0ca8\u0cbf <br/> \u0caa\u0ccd\u0cb0\u0cbe\u0c95\u0c9f\u0cae\u0cc1\u0c97 \u0ca4\u0cbe \u0cb0\u0cbe\u0ca8\u0cc1 \u0c9a\u0cc6\u0cb2\u0ccd\u0cb2\u0cc1\u0ca8\u0ca8\u0cbf <br/> \u0cb5\u0ccd\u0caf\u0cbe\u0c95\u0cc1\u0cb2\u0cc1\u0ca1\u0cc8 \u0c85\u0ca4\u0cbf \u0cb5\u0cc7\u0c97\u0cae\u0cc1\u0ca8\u0ca8\u0cc1 <br/> \u0cb5\u0cc7\u0c95\u0cc1\u0cb5 \u0c9c\u0cbe\u0cae\u0cc1\u0ca8 \u0ca4\u0ccb\u0cb7\u0cae\u0cc1 \u0c95\u0cb2\u0c97\u0ca8\u0cc1 <br/> \u0ca4\u0cbe \u0c95\u0cb0\u0cc1\u0ca3\u0ca8\u0cb0\u0cc1\u0ca3\u0cc1\u0ca8\u0cbf \u0caa\u0ca8\u0cc1\u0caa \u0cb2\u0cc7\u0ca6\u0cbe \u0c8e\u0ca8\u0ccd\u0ca8\u0ca1\u0ccb"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0ca8\u0cc0\u0cb0\u0cc1 \u0cb2\u0cc7\u0c95 \u0cb8\u0c9c\u0ccd\u0c9c\u0ca8 \u0c97\u0ca3\u0cae\u0cc1\u0cb2\u0cc1 \u0cae\u0ca8 <br/> \u0ca6\u0cbe\u0cb0\u0cbf\u0ca8\u0cbf \u0c9c\u0cc2\u0c9a\u0cc7\u0cb0\u0ca8\u0cc1\u0c9a\u0cc1 \u0ca4\u0cc6\u0cb2\u0cbf\u0cb8\u0cbf <br/> \u0c95\u0cbe\u0cb5\u0cc7\u0cb0\u0cbf \u0ca4\u0cbe\u0ca8\u0cc1 \u0cb0\u0cbe\u0ca8\u0cc1 \u0c9a\u0cc6\u0cb2\u0ccd\u0cb2\u0cc1\u0ca8\u0ca8\u0cc1\u0c9a\u0cc1 <br/> \u0cb5\u0cbf\u0c9a\u0cbe\u0cb0 \u0cb9\u0cc3\u0ca6\u0caf\u0cc1\u0cb0\u0cbe\u0cb2\u0cc8 <br/> \u0cb8\u0cbe\u0cb0\u0cc6\u0c95\u0cc1 \u0cb5\u0c9a\u0ccd\u0c9a\u0cc1\u0ca8\u0ca8\u0cc1\u0c9a\u0cc1 \u0cb8\u0ca8\u0ccd\u0ca4\u0ccb\u0cb7\u0cae\u0cc1 <br/> \u0cb5\u0cc0\u0cb0\u0cc1\u0cb2\u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0ca8\u0cc1 \u0c95\u0cb2\u0ccd\u0c97 \u0caa\u0ca1\u0cae\u0c9f\u0cbf <br/> \u0cae\u0cbe\u0cb0\u0cc1\u0ca4 \u0c9a\u0ca8\u0ccd\u0c9a\u0cb2\u0cae\u0cc1\u0cb2 \u0caa\u0ca8\u0cbf\u0caa\u0cbf <br/> \u0c9a\u0cc6\u0cb2\u0cae\u0cb2\u0cc2\u0cb0 \u0c9a\u0cc7\u0caf \u0cb2\u0cc7\u0ca6\u0cbe \u0cb0\u0cbe\u0cae \u0c8e\u0ca8\u0ccd\u0ca8\u0ca1\u0ccb"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c97\u0cb0\u0ccd\u0cb5\u0cbf\u0ca8\u0ccd\u0c9a\u0cbf\u0ca8 \u0c86 \u0cb0\u0cbe\u0cb5\u0ca3 \u0cac\u0cbe\u0ca7\u0cb2\u0ca8\u0ccd- <br/> \u0c93\u0cb0\u0ccd\u0cb5 \u0cb2\u0cc7\u0c95\u0ca8\u0cc1 \u0cb5\u0cbf\u0cad\u0cc0\u0cb7\u0ca3\u0cc1\u0ca1\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc1 <br/> \u0cb8\u0cb0\u0ccd\u0cb5 \u0cb2\u0ccb\u0c95 \u0cb6\u0cb0\u0ca3\u0ccd\u0caf\u0cbe \u0cb5\u0ca8\u0ccd\u0ca6\u0ca8\u0cae\u0ca8 <br/> \u0ca8\u0cbf\u0cb0\u0ccd\u0cb5\u0cbf\u0c95\u0cbe\u0cb0\u0cc1\u0ca1\u0cc8 \u0ca8\u0cc0\u0cb5\u0cc1 <br/> \u0cb8\u0cb0\u0ccd\u0cb5\u0ca6\u0cbe \u0cb0\u0c95\u0ccd\u0cb7\u0cbf\u0ca8\u0ccd\u0c9a\u0cc6\u0ca6\u0ca8\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0cc1 <br/> \u0cb8\u0cbe\u0cb0\u0ccd\u0cb5\u0cad\u0ccc\u0cae \u0cb2\u0ca8\u0ccd\u0c95\u0cbe\u0ca7\u0cbf\u0caa\u0ca4\u0ccd\u0caf\u0cae\u0cc1\u0ca8 <br/> \u0cb8\u0cb0\u0ccd\u0cb5\u0c9c\u0ccd\u0c9e\u0cc1\u0ca8\u0cbf \u0cb2\u0c95\u0ccd\u0cb7\u0ccd\u0cae\u0ca3\u0cc1\u0ca8\u0cbf\u0cae\u0ccd\u0cae\u0ca8\u0c97\u0cbe <br/> \u0cb8\u0cb0\u0ccd\u0cb5 \u0cb8\u0ca8\u0ccd\u0ca8\u0cc1\u0ca4 \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c\u0cc1\u0ca8\u0cbf \u0c8e\u0ca8\u0ccd\u0ca8\u0ca1\u0ccb"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d0e\u0d28\u0d4d\u0d28\u0d21\u0d4b \u0d30\u0d15\u0d4d\u0d37\u0d3f\u0d28\u0d4d\u0d1a\u0d3f\u0d24\u0d47 \u0d28\u0d40\u0d35\u0d3f\u0d2a\u0d41\u0d21\u0d40 <br/> \u0d35\u0d17\u0d28\u0d41 \u0d09\u0d23\u0d4d\u0d21 \u0d35\u0d32\u0d46\u0d28\u0d3e"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2a\u0d28\u0d4d\u0d28\u0d17 \u0d36\u0d2f\u0d28 \u0d2a\u0d46\u0d26\u0d4d\u0d26\u0d32 \u0d38\u0d4d\u0d35- <br/> \u0d2d\u0d3e\u0d35\u0d2e\u0d3f\u0d1f\u0d4d\u0d32\u0d41\u0d23\u0d4d\u0d21\u0d17 \u0d2c\u0d3e\u0d17\u0d41\u0d17 \u0d28\u0d28\u0d4d\u0d28\u0d47 \u0d28\u0d40\u0d35\u0d46\u0d28\u0d4d\u0d28\u0d21\u0d4b"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d1a\u0d40\u0d15\u0d1f\u0d3f \u0d1a\u0d47\u0d24\u0d28\u0d41 \u0d17\u0d3e\u0d38\u0d3f \u0d1c\u0d46\u0d28\u0d4d\u0d26\u0d3f\u0d2f\u0d40 <br/> \u0d32\u0d4b\u0d15\u0d41\u0d32\u0d41 \u0d38\u0d15\u0d32 \u0d15\u0d30\u0d4d\u0d2e\u0d2e\u0d41\u0d32\u0d15\u0d48 \u0d2e\u0d28 <br/> \u0d30\u0d3e\u0d15 \u0d15\u0d4a\u0d30\u0d15\u0d41 \u0d1c\u0d42\u0d1a\u0d46\u0d26\u0d30\u0d28\u0d3f \u0d24\u0d46\u0d32\u0d3f\u0d38\u0d3f <br/> \u0d35\u0d3f\u0d2d\u0d3e\u0d15\u0d30\u0d41\u0d21\u0d41 \u0d2e\u0d26\u0d3f\u0d28\u0d3f <br/> \u0d2a\u0d4d\u0d30\u0d3e\u0d15\u0d1f\u0d2e\u0d41\u0d17 \u0d24\u0d3e \u0d30\u0d3e\u0d28\u0d41 \u0d1a\u0d46\u0d32\u0d4d\u0d32\u0d41\u0d28\u0d28\u0d3f <br/> \u0d35\u0d4d\u0d2f\u0d3e\u0d15\u0d41\u0d32\u0d41\u0d21\u0d48 \u0d05\u0d24\u0d3f \u0d35\u0d47\u0d17\u0d2e\u0d41\u0d28\u0d28\u0d41 <br/> \u0d35\u0d47\u0d15\u0d41\u0d35 \u0d1c\u0d3e\u0d2e\