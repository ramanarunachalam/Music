\u0c3f\u0c32\u0c4d\u0c32\u0c3e\u0c26\u0c41 <br/> \u0c24\u0c23\u0c4d\u0c23\u0c42\u0c32\u0c35\u0c3f\u0c2f \u0c05\u0c32\u0c4d\u0c32\u0c3f \u0c24\u0c3f\u0c30\u0c33\u0c3e\u0c2f\u0c4d\u0c2a\u0c4d-\u0c2a\u0c42\u0c24\u0c4d\u0c24\u0c3e\u0c32\u0c41\u0c2e\u0c4d \u0c12\u0c30\u0c41 \u0c24\u0c3e\u0c2e\u0c30\u0c48\u0c15\u0c4d-\u0c15\u0c4a\u0c35\u0c4d\u0c35\u0c3e\u0c26\u0c41"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 6", "T": "\u0c2e\u0c23\u0c4d\u0c23\u0c41\u0c32\u0c17\u0c24\u0c4d\u0c24\u0c3f\u0c32\u0c41\u0c33\u0c4d\u0c33 \u0c24\u0c30\u0c41\u0c15\u0c4d\u0c15\u0c33\u0c28\u0c48\u0c24\u0c4d\u0c24\u0c41\u0c2e\u0c4d \u0c15\u0c42\u0c21\u0c3f\u0c2e\u0c30\u0c41\u0c35\u0c41\u0c32\u0c35\u0c41\u0c2e\u0c4d \u0c15\u0c31\u0c4d\u0c2a\u0c17\u0c24\u0c4d- <br/> \u0c24\u0c30\u0c41\u0c35\u0c41\u0c15\u0c4d\u0c15\u0c3f\u0c23\u0c48 \u0c35\u0c30\u0c3e\u0c26\u0c41 <br/> \u0c2a\u0c41\u0c23\u0c4d\u0c23\u0c3f\u0c2f \u0c38\u0c4d\u0c25\u0c32\u0c19\u0c4d\u0c17\u0c33\u0c4d \u0c2a\u0c32\u0c35\u0c3f\u0c30\u0c41\u0c28\u0c4d\u0c26\u0c41 \u0c28\u0c1f\u0c47\u0c38\u0c28\u0c4d \u0c35\u0c3e\u0c34\u0c41\u0c2e\u0c4d \u0c2a\u0c41\u0c23\u0c4d\u0c21\u0c30\u0c40\u0c15\u0c2a\u0c41\u0c30\u0c2e\u0c4d \u0c2a\u0c4b\u0c32\u0c4d <br/> \u0c15\u0c23\u0c4d\u0c21\u0c41 \u0c36\u0c4a\u0c32\u0c4d\u0c32 \u0c35\u0c47\u0c31\u0c47\u0c26\u0c41"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c87\u0ca8\u0ccd\u0ca8\u0cae\u0cc1\u0cae\u0ccd \u0c92\u0cb0\u0cc1 \u0cb8\u0ccd\u0ca5\u0cb2\u0cae\u0ccd \u0c87\u0cb0\u0cc1\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0cc6\u0ca8\u0ccd\u0cb1\u0cca\u0cb0\u0cc1\u0c95\u0ccd\u0c95\u0cbe\u0cb2\u0cc7 \u0c8f\u0ca8\u0ccd \u0cae\u0cb2\u0cc8\u0c95\u0ccd\u0c95\u0cbf\u0cb0\u0cbe\u0caf\u0ccd \u0cae\u0ca8\u0ca6\u0cc7"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0cca\u0ca8\u0ccd\u0ca8 \u0cb6\u0cca\u0ca8\u0ccd\u0ca8 \u0cb8\u0ccd\u0ca4\u0cb2\u0c99\u0ccd\u0c97\u0cb3\u0ccd \u0c8e\u0c99\u0ccd\u0c97\u0cc1\u0cae\u0ccd \u0c93\u0ca1\u0cbf\u0c95\u0ccd-\u0c95\u0cb3\u0cc8\u0ca4\u0c