u0ca8\u0cbf\u0cb5\u0cb0\u0cc7\u0ca1\u0cbf\u0ca4 \u0cb5\u0cbf\u0cae\u0cb2 \u0c9a\u0cb0\u0cbf\u0ca4\u0cc7 \u0cae\u0ccb\u0cb9\u0ca8\u0cc0\u0caf \u0c97\u0cc1\u0ca3\u0ccc\u0c96 \u0cad\u0cb0\u0cbf\u0ca4\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca4\u0cb0\u0cc1\u0ca3 \u0cb5\u0cbe\u0cb0\u0cbf\u0ca6 \u0ca8\u0cbf\u0cad \u0cb5\u0cc7\u0ca3\u0cbf \u0ca6\u0cc7\u0cb5\u0ca4\u0cb0\u0cc1 \u0c95\u0cbf\u0cb8\u0cb2\u0caf\u0ccb\u0caa\u0cae \u0caa\u0cbe\u0ca3\u0cbf <br/> \u0c95\u0cb2\u0cbf\u0ca4 \u0cb5\u0cb0\u0ca6\u0cbe \u0cad\u0cc0\u0ca4\u0cbf\u0cae\u0cc1\u0ca6\u0cb0\u0cc7 \u0c95\u0cb2\u0ccd\u0caf\u0cbe\u0ca3\u0cbf \u0caa\u0cc2\u0cb0\u0ccd\u0ca3 \u0cb6\u0cb0\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6\u0cc1 \u0cb8\u0cae \u0c95\u0cbe\u0ca8\u0ccd\u0ca4\u0cc7 \u0cb5\u0cbe\u0ca3\u0cbf <br/> \u0cb8\u0cc1\u0cb0\u0cc1\u0c9a\u0cbf\u0cb0 \u0ca8\u0caf\u0ca8 \u0c9c\u0cbf\u0ca4\u0cc8\u0ca3\u0cbf \u0caa\u0cb0\u0cae \u0c95\u0cb0\u0cc1\u0ca3\u0cbe\u0cb0\u0cb8 \u0cb6\u0cbf\u0cb6\u0cbf\u0cb0 \u0cb5\u0cc7\u0ca3\u0cbf <br/> \u0c9a\u0cb0\u0ca8 \u0c97\u0ca4 \u0c9c\u0ca8 \u0cad\u0cb0\u0ca3 \u0ca8\u0cbf\u0caa\u0cc1\u0ca3\u0cc7 \u0caa\u0cb0\u0cae\u0cbe\u0cae\u0ccd\u0cb0\u0ccd\u0ca4 \u0cb8\u0cc1-\u0cae\u0ca7\u0cc1\u0cb0 \u0cad\u0cbe\u0cb7\u0cbf\u0ca3\u0cc0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c98\u0ca8 \u0cb8\u0cbe\u0cb0 \u0ca4\u0cbf\u0cb2\u0c95\u0cbe\u0c99\u0ccd\u0c95\u0cbf\u0ca4 \u0cab\u0cbe\u0cb2\u0cc7 \u0c85\u0ca4\u0cbf \u0c95\u0cae\u0ca8\u0cc0\u0caf \u0cb5\u0cbf\u0cb6\u0ca6\u0ca6\u0cc1\u0c95\u0cc2\u0cb2\u0cc7 \u0cb5\u0cbf\u0ca8\u0ca4 <br/> \u0c9c\u0ca8 \u0cb5\u0cbf\u0ca6\u0ccd\u0caf\u0cbe \u0cb5\u0cbf\u0ca4\u0cb0\u0ca3 \u0cb2\u0ccb\u0cb2\u0cc7 \u0cb8\u0cbe\u0ca7\u0cc1 \u0cb8\u0ccd\u0cae\u0cb0\u0ca3\u0cc0\u0caf\u0ca4\u0cae \u0caa\u0cbe\u0ca6 \u0cae\u0cc2\u0cb2\u0cc7 <br/> \u0c95\u0ca8\u0c95 \u0cad\u0cc2\u0cb7\u0ca3\u0cc7 \u0cb6\u0cc1\u0cad \u0cb6\u0cc0\u0cb2\u0cc7 \u0cb8\u0cb0\u0ccd\u0cb5\u0cbe\u0c97\u0cae\u0cae\u0caf\u0cbf \u0cb8\u0cc1\u0c9c\u0ca8\u0cbe\u0ca8\u0cc1\u0c95\u0cc2\u0cb2\u0cc7 <br/> \u0ca8\u0cbe\u0ca8\u0cbe \u0cae\u0cc1\u0ca8\u0cbf \u0cae\u0ca8\u0ccb\u0cae\u0caf \u0cb5\u0ca8\u0c9c \u0ca8\u0cbf\u0cb2\u0caf\u0cc7 \u0cb5\u0cbf\u0ca8\u0cbf\u0cb9\u0ca4\u0cbe\u0cb6\u0ccd\u0cb0\u0ccd\u0ca4\u0cb5\u0cbf\u0cb5\u0cbf\u0ca7 \u0cb6\u0cae\u0cb2\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0caa\u0cb0\u0cbf\u0cb9\u0ccd\u0cb0\u0ccd\u0ca4 \u0c98\u0ca8\u0cbe\u0c9c\u0ccd\u0c9e\u0cbe\u0ca8 \u0c96\u0cc7\u0ca6\u0cc7 \u0c95\u0ccd\u0cb0\u0ccd\u0ca4 \u0caa\u0ca6\u0ccd\u0cae\u0ca8\u0cbe\u0cad \u0cb8\u0cc7\u0cb5\u0c95\u0cae\u0ccb\u0ca6\u0cc7 <br/> \u0cb8\u0ca4\u0ca4 \u0caa\u0cb0\u0cbf\u0c9a\u0cbf\u0ca4 \u0cb5\u0cc0\u0ca3\u0cbe \u0ca8\u0cbf\u0ca8\u0cbe\u0ca6\u0cc7 \u0cae\u0cae \u0c95\u0cc1\u0cb0\u0cc1 \u0cae\u0ca4\u0cbf\u0cae\u0caf\u0cbf \u0ca4\u0cb5 \u0caa\u0cbe\u0ca6\u0cc7 <br/> \u0cb8\u0cc1\u0cb0\u0c97\u0ca3 \u0cae\u0cb9\u0cbf\u0ca4 \u0cb5\u0cbf\u0ca8\u0ccb\u0ca6\u0cc7 \u0cb8\u0cc0\u0ca4\u0c95\u0cb0 \u0caa\u0ccb\u0ca4\u0ca7\u0cb0\u0cc7 \u0c97\u0ca4\u0cad\u0cc7\u0ca6\u0cc7 <br/> \u0cb8\u0ca6\u0cbe \u0cb8\u0cb0\u0cb8\u0cc0\u0cb0\u0cc1\u0cb9\u0cae\u0cc1\u0c96\u0cbf \u0cb8\u0caa\u0ca6\u0cbf \u0cb5\u0ca6\u0ca8\u0cc7 \u0cb8\u0cb0\u0cb8\u0cae\u0cbf\u0cb9 \u0cb5\u0cb8 \u0cb8\u0c95\u0cb2 \u0cb5\u0cb0\u0ca6\u0cc7"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d1c\u0d28\u0d28\u0d3f \u0d2e\u0d3e\u0d2e\u0d35\u0d2e\u0d47\u0d2f\u0d46 \u0d2d\u0d3e\u0d30\u0d24\u0d3f \u0d1c\u0d2f \u0d38\u0d30\u0d38\u0d3f\u0d1c\u0d3e\u0d38\u0d28 \u0d1c\u0d3e\u0d2f\u0d47"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d05\u0d28\u0d41\u0d2a\u0d2e\u0d3f\u0d24 \u0d15\u0d2e\u0d32\u0d3e \u0d35\u0d3e\u0d38\u0d47 \u0d1a\u0d3e\u0d30\u0d41 \u0d39\u0d38\u0d3f\u0d24 \u0d15\u0d4d\u0d30\u0d4d\u0d24 \u0d15\u0d41\u0d28\u0d4d\u0d26 \u0d28\u0d3f\u0d30\u0d3e\u0d38\u0d47 \u0d26\u0d47\u0d35\u0d3f \u0d2e\u0d41\u0d28\u0d3f\u0d35\u0d30\u0d47\u0d21\u0d3f\u0d24 \u0d35\u0d3f\u0d2e\u0d32 \u0d1a\u0d30\u0d3f\u0d24\u0d47 \u0d2e\u0d4b\u0d39\u0d28\u0d40\u0d2f \u0d17\u0d41\u0d23\u0d4c\u0d16 \u0d2d\u0d30\u0d3f\u0d24\u0d47"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d24\u0d30\u0d41\u0d23 \u0d35\u0d3e\u0d30\u0d3f\u0d26 \u0d28\u0d3f\u0d2d \u0d35\u0d47\u0d23\u0d3f \u0d26\u0d47\u0d35\u0d24\u0d30\u0d41 \u0d15\u0d3f\u0d38\u0d32\u0d2f\u0d4b\u0d2a\u0d2e \u0d2a\u0d3e\u0d23\u0d3f <br/> \u0d15\u0d32\u0d3f\u0d24 \u0d35\u0d30\u0d26\u0d3e \u0d2d\u0d40\u0d24\u0d3f\u0d2e\u0d41\u0d26\u0d30\u0d47 \u0d15\u0d32\u0d4d\u0d2f\u0d3e\u0d23\u0d3f \u0d2a\u0d42\u0d30\u0d4d\u0d23 \u0d36\u0d30\u0d26\u0d3f\u0d28\u0d4d\u0d26\u0d41 \u0d38\u0d2e \u0d15\u0d3e\u0d28\u0d4d\u0d24\u0d47 \u0d35\u0d3e\u0d23\u0d3f <br/> \u0d38\u0d41\u0d30\u0d41\u0d1a\u0d3f\u0d30 \u0d28\u0d2f\u0d28 \u0d1c\u0d3f\u0d24\u0d48\u0d23\u0d3f \u0d2a\u0d30\u0d2e \u0d15\u0d30\u0d41\u0d23\u0d3e\u0d30\u0d38 \u0d36\u0d3f\u0d36\u0d3f\u0d30 \u0d35\u0d47\u0d23\u0d3f <br/> \u0d1a\u0d30\u0d28 \u0d17\u0d24 \u0d1c\u0d28 \u0d2d\u0d30\u0d23 \u0d28\u0d3f\u0d2a\u0d41\u0d23\u0d47 \u0d2a\u0d30\u0d2e\u0d3e\u0d2e\u0d4d\u0d30\u0d4d\u0d24 \u0d38\u0d41-\u0d2e\u0d27\u0d41\u0d30 \u0d2d\u0d3e\u0d37\u0d3f\u0d23\u0d40"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d18\u0d28 \u0d38\u0d3e\u0d30 \u0d24\u0d3f\u0d32\u0d15\u0d3e\u0d19\u0d4d\u0d15\u0d3f\u0d24 \u0d2b\u0d3e\u0d32\u0d47 \u0d05\u0d24\u0d3f \u0d15\u0d2e\u0d28\u0d40\u0d2f \u0d35\u0d3f\u0d36\u0d26\u0d26\u0d41\u0d15\u0d42\u0d32\u0d47 \u0d35\u0d3f\u0d28\u0d24 <br/> \u0d1c\u0d28 \u0d35\u0d3f\u0d26\u0d4d\u0d2f\u0d3e \u0d35\u0d3f\u0d24\u0d30\u0d23 \u0d32\u0d4b\u0d32\u0d47 \u0d38\u0d3e\u0d27\u0d41 \u0d38\u0d4d\u0d2e\u0d30\u0d23\u0d40\u0d2f\u0d24\u0d2e \u0d2a\u0d3e\u0d26 \u0d2e\u0d42\u0d32\u0d47 <br/> \u0d15\u0d28\u0d15 \u0d2d\u0d42\u0d37\u0d23\u0d47 \u0d36\u0d41\u0d2d \u0d36\u0d40\u0d32\u0d47 \u0d38\u0d30\u0d4d\u0d35\u0d3e\u0d17\u0d2e\u0d2e\u0d2f\u0d3f \u0d38\u0d41\u0d1c\u0d28\u0d3e\u0d28\u0d41\u0d15\u0d42\u0d32\u0d47 <br/> \u0d28\u0d3e\u0d28\u0d3e \u0d2e\u0d41\u0d28\u0d3f \u0d2e\u0d28\u0d4b\u0d2e\u0d2f \u0d35\u0d28\u0d1c \u0d28\u0d3f\u0d32\