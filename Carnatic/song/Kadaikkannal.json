u0c2a\u0c4b\u0c1f\u0c4d\u0c31\u0c41\u0c2e\u0c4d \u0c2a\u0c4a\u0c31\u0c4d\u0c2a\u0c4d-\u0c2a\u0c3e\u0c26\u0c3e \u0c05\u0c30\u0c41\u0c2e\u0c4d \u0c2e\u0c3e\u0c27\u0c35 \u0c1c\u0c4d\u0c1e\u0c3e\u0c28 \u0c2a\u0c4d\u0c30\u0c38\u0c3e\u0c26\u0c3e"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0ca1\u0cc8\u0c95\u0ccd\u0c95\u0ca3\u0ccd\u0ca3\u0cbe\u0cb2\u0ccd \u0c8e\u0ca8\u0cc8\u0caa\u0ccd-\u0caa\u0cbe\u0cb0\u0cbe\u0caf\u0ccd \u0c89\u0ca8\u0ca6\u0ca1\u0cbf\u0c95\u0ccd\u0c95\u0cb2\u0cae\u0ccd \u0cb5\u0cbf\u0ca8\u0cc8 \u0ca4\u0cc0\u0cb0\u0cbe\u0caf\u0ccd"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0cbf\u0ca1\u0cc8\u0c95\u0ccd\u0c95\u0cca\u0ca3\u0ccd\u0ca3\u0cbe\u0caa\u0ccd-\u0caa\u0ca6\u0cae\u0ccd \u0ca4\u0cbe\u0cb0\u0cbe\u0caf\u0ccd \u0ca8\u0cb2\u0cae\u0ccd \u0caa\u0ca1\u0cc8\u0c95\u0ccd\u0c95\u0cb5\u0cc7 \u0c85\u0cb0\u0cc1\u0cb3\u0ccd \u0c95\u0cc2\u0cb0\u0cbe\u0caf\u0ccd <br/> \u0c85\u0cb0\u0cc1\u0c9f\u0ccd-\u0c95\u0cca\u0ca1\u0cc8\u0c95\u0ccd\u0c95\u0cc1 \u0ca8\u0cbe\u0ca8\u0cc6\u0ca8\u0ccd\u0ca8\u0cb5\u0cc7 \u0c85\u0cb5\u0ca4\u0cb0\u0cbf\u0ca4\u0ccd\u0ca4 \u0c95\u0ccb\u0cb2\u0cbe\u0cb9\u0cb2 \u0cae\u0cc2\u0cb2\u0cbe \u0c85\u0ca8\u0cc1\u0c95\u0cc2\u0cb2\u0cbe \u0c97\u0cc1\u0ca3\u0cb6\u0cc0\u0cb2\u0cbe \u0ca8\u0cbf\u0ca6\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cb5\u0c9e\u0ccd\u0c9a\u0c95 \u0cae\u0cbe\u0caf\u0cc8\u0caf\u0cbf\u0cb2\u0ccd \u0c86\u0cb3\u0ccd\u0ca8\u0ccd\u0ca6\u0cc7\u0ca8\u0ccd \u0c89\u0cb0\u0cc1 \u0ca4\u0c9e\u0ccd\u0c9c\u0cae\u0ccd \u0c8e\u0ca8\u0ccd\u0cb1\u0cc1\u0ca8\u0ccd\u0ca8\u0ca1\u0cbf \u0ca4\u0cbe\u0cb3\u0ccd\u0ca8\u0ccd\u0ca6\u0cc7\u0ca8\u0ccd <br/> \u0c85\u0c9e\u0ccd\u0c9c\u0cb2\u0ccd \u0c8e\u0ca8\u0ccd\u0cb1\u0cc7 \u0c8e\u0ca8\u0cc8 \u0c86\u0ca6\u0cb0\u0cbf\u0ca4\u0ccd\u0ca4\u0cb0\u0cc1\u0cb3\u0ccd\u0cb5\u0cbe\u0caf\u0ccd \u0c85\u0caf\u0ccd\u0caf\u0cbe \u0cae\u0cbf\u0c98\u0cc1 \u0caa\u0cca\u0caf\u0ccd\u0caf\u0cbe\u0cb2\u0ccd \u0cae\u0ca8\u0cae\u0ccd \u0ca8\u0cc8\u0caf\u0cbe\u0ca6\u0cb0\u0cc1\u0cb3\u0ccd \u0cae\u0cc6\u0caf\u0ccd\u0caf\u0cbe \u0c92\u0cb0\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c86\u0caf\u0ccd\u0caf\u0ca8\u0cc7 \u0ca4\u0cc0\u0cb0\u0cbe \u0c8e\u0ca8\u0ccd \u0caa\u0cbe\u0cb5\u0cae\u0ccd \u0c85\u0cb0\u0cc1\u0cb3\u0ccd \u0cb6\u0cc6\u0caf\u0ccd\u0caf \u0c89\u0ca8\u0c95\u0ccd\u0c95\u0cc6\u0ca8\u0cbe \u0cb2\u0ccb\u0cad\u0cae\u0ccd <br/> \u0cb5\u0cc8\u0caf\u0c97\u0cae\u0ccd \u0c86\u0ca6\u0cbf \u0cb5\u0cb3\u0cb0\u0ccd \u0c85\u0ca3\u0ccd\u0ca1\u0cae\u0ccd \u0caf\u0cbe\u0cb5\u0cbf\u0ca8\u0cc1\u0cae\u0ccd \u0cb5\u0cbe\u0cb8\u0cbe \u0cad\u0cc1\u0cb5\u0ca8\u0cc7\u0cb6\u0cbe \u0cae\u0cb0 \u0ca8\u0cbe\u0cb6\u0cbe \u0caa\u0ccd\u0cb0\u0c95\u0cbe\u0cb6\u0cbe \u0c92\u0cb0\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c85\u0c9e\u0ccd\u0c9c\u0cb2\u0cc6\u0ca8\u0cbf\u0cb2\u0ccd \u0c89\u0ca8\u0ccd \u0cb5\u0cbe\u0caf\u0ccd \u0ca8\u0ccb\u0cae\u0ccb \u0c8f\u0cb3\u0cc8 \u0ca8\u0cc6\u0c9e\u0ccd\u0c9c\u0cae\u0ccd \u0cb5\u0cb0\u0cc1\u0ca8\u0ccd\u0ca6\u0cbf\u0ca1\u0cb2\u0cbe\u0cae\u0ccb <br/> \u0caa\u0c9e\u0ccd\u0c9c\u0cae\u0ccb \u0c95\u0cc8\u0caa\u0ccd-\u0caa\u0cca\u0cb0\u0cc1\u0cb3\u0ccd \u0c95\u0cca\u0c9e\u0ccd\u0c9c\u0cae\u0ccb \u0cad\u0cc2 \u0caa\u0cb0\u0cbf\u0caa\u0cbe\u0cb2\u0cbe \u0c85\u0ca8\u0cc1\u0c95\u0cc2\u0cb2\u0cbe \u0c92\u0cb0\u0cc1 \u0cae\u0cc7\u0cb2\u0cbe \u0cae\u0cc1\u0ca6\u0cb2\u0ccd \u0ca8\u0cc2\u0cb2\u0cbe \u0c92\u0cb0\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c87\u0ca4\u0ccd\u0ca4\u0cb0\u0cc8 \u0cae\u0cc0\u0ca6\u0cbf\u0ca8\u0cbf\u0cb2\u0ccd \u0ca8\u0cc0\u0caf\u0cc7 \u0ca8\u0cb2\u0ccd\u0cb2 \u0cad\u0c95\u0ccd\u0ca4\u0cb0\u0cc8\u0c95\u0ccd-\u0c95\u0cbe\u0c95\u0ccd\u0c95 \u0cb5\u0ca8\u0ccd\u0ca6\u0cbe\u0caf\u0cc7 <br/> \u0c9a\u0cbf\u0ca4\u0ccd\u0ca4\u0cae\u0ccd \u0c89\u0cb5\u0ca8\u0ccd\u0ca6\u0cc6\u0ca8\u0cc8 \u0ca8\u0cbf\u0ca4\u0ccd\u0ca4\u0cae\u0cc1\u0cae\u0ccd \u0c86\u0cb2\u0cca\u0cb3\u0cbf\u0cb0\u0ccd \u0ca6\u0cc7\u0cb9\u0cbe \u0c98\u0ca8 \u0caf\u0ccb\u0c97\u0cbe \u0c85\u0ca4\u0cbf \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cbe \u0ca7\u0ca8\u0cbf \u0c8f\u0c95\u0cbe \u0c95\u0cc1\u0cb3\u0cbf\u0cb0\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0cad\u0cc2\u0ca4\u0cb2\u0cae\u0ccd \u0caa\u0ccb\u0c9f\u0ccd\u0cb1\u0cc1\u0cae\u0ccd \u0caa\u0cca\u0cb1\u0ccd\u0caa\u0ccd-\u0caa\u0cbe\u0ca6\u0cbe \u0c85\u0cb0\u0cc1\u0cae\u0ccd \u0cae\u0cbe\u0ca7\u0cb5 \u0c9c\u0ccd\u0c9e\u0cbe\u0ca8 \u0caa\u0ccd\u0cb0\u0cb8\u0cbe\u0ca6\u0cbe"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d15\u0d21\u0d48\u0d15\u0d4d\u0d15\u0d23\u0d4d\u0d23\u0d3e\u0d32\u0d4d \u0d0e\u0d28\u0d48\u0d2a\u0d4d-\u0d2a\u0d3e\u0d30\u0d3e\u0d2f\u0d4d \u0d09\u0d28\u0d26\u0d21\u0d3f\u0d15\u0d4d\u0d15\u0d32\u0d2e\u0d4d \u0d35\u0d3f\u0d28\u0d48 \u0d24\u0d40\u0d30\u0d3e\u0d2f\u0d4d"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d15\u0d3f\u0d21\u0d48\u0d15\u0d4d\u0d15\u0d4a\u0d23\u0d4d\u0d23\u0d3e\u0d2a\u0d4d-\u0d2a\u0d26\u0d2e\u0d4d \u0d24\u0d3e\u0d30\u0d3e\u0d2f\u0d4d \u0d28\u0d32\u0d2e\u0d4d \u0d2a\u0d21\u0d48\u0d15\u0d4d\u0d15\u0d35\u0d47 \u0d05\u0d30\u0d41\u0d33\u0d4d \u0d15\u0d42\u0d30\u0d3e\u0d2f\u0d4d <br/> \u0d05\u0d30\u0d41\u0d1f\u0d4d-\u0d15\u0d4a\u0d21\u0d48\u0d15\u0d4d\u0d15\u0d41 \u0d28\u0d3e\u0d28\u0d46\u0d28\u0d4d\u0d28\u0d35\u0d47 \u0d05\u0d35\u0d24\u0d30\u0d3f\u0d24\u0d4d\u0d24 \u0d15\u0d4b\u0d32\u0d3e\u0d39\u0d32 \u0d2e\u0d42\u0d32\u0d3e \u0d05\u0d28\u0d41\u0d15\u0d42\u0d32\u0d3e \u0d17\u0d41\u0d23\u0d36\u0d40\u0d32\u0d3e \u0d28\u0d3f\u0d26\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d35\u0d1e\u0d4d\u0d1a\u0d15 \u0d2e\u0d3e\u0d2f\u0d48\u0d2f\u0d3f\u0d32\u0d4d \u0d06\u0d34\u0d4d\u0d28\u0d4d\u0d26\u0d47\u0d28\u0d4d \u0d09\u0d30\u0d41 \u0d24\u0d1e\u0d4d\u0d1c\u0d2e\u0d4d \u0d0e\u0d28\u0d4d\u0d31\u0d41\u0d28\u0d4d\u0d28\u0d21\u0d3f \u0d24\u0d3e\u0d34\u0d4d\u0d28\u0d4d\u0d26\u0d47\u0d28\u0d4d <br/> \u0d05\u0d1e\u0d4d\u0d1c\u0d32\u0d4d \u0d0e\u0d28\u0d4d\u0d31\u0d47 \u0d0e