bc1\u0baa\u0bb2 \u0ba8\u0bc0\u0bb2\u0bae\u0bcd \u0bb5\u0bb0\u0bbf\u0b9c\u0ba4 \u0ba8\u0baf\u0ba9\u0bae\u0bcd"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0b9a\u0bb0\u0ba9\u0b95\u0ba4 \u0baa\u0bb0\u0ba9\u0bae\u0bcd \u0b9c\u0ba9\u0b95 \u0b9a\u0bc1\u0ba4 \u0b9c\u0bbe\u0ba9\u0bbf\u0bae\u0bae\u0bbf\u0ba4 \u0b95\u0bb0\u0bc1\u0ba9\u0bae\u0bcd <br/> \u0bb9\u0bb0\u0bbf \u0bb5\u0bc0\u0bb0\u0bb5\u0bcd\u0bb0\u0bcd\u0baf\u0bca\u0baa\u0ba8\u0bcd\u0ba4\u0bae\u0bae\u0bcd\u0baa\u0bc1\u0ba4\u0bbf \u0bae\u0ba4\u0bb9\u0bb0\u0bae\u0bb0\u0bc1\u0ba9 \u0baa\u0bbf\u0bae\u0bcd\u0baa \u0bb5\u0bbf\u0bb0\u0b9c\u0ba4\u0ba4\u0bb0\u0bae\u0bcd \u0b9c\u0ba9\u0bc6\u0b9a\u0bae\u0bcd <br/> \u0ba4\u0bb0\u0bbf\u0ba4 \u0ba4\u0bb0\u0bc1\u0ba9 \u0bb0\u0bb5\u0ba9 \u0bae\u0ba4\u0bbf\u0bae\u0bcd \u0bae\u0bcd\u0bb0\u0bcd\u0ba4\u0bc1\u0ba4\u0bb0 \u0bb9\u0b9a\u0bae\u0bae\u0bbf\u0bb9"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b95\u0bae\u0ba9\u0bc0\u0baf\u0ba4\u0bb0 \u0bb0\u0bc2\u0baa\u0bae\u0bcd \u0b9c\u0bb2\u0ba4\u0bbf \u0b9a\u0bae \u0b95\u0bae\u0bcd\u0baa\u0bc0\u0bb0\u0bcd\u0baf\u0bae\u0bc1\u0bb0\u0bc1 \u0baa\u0bcd\u0bb0\u0ba4\u0bbe\u0baa\u0bae\u0bcd <br/> \u0bb5\u0bbf\u0bae\u0bb2 \u0baf\u0bcb\u0b95\u0bbf \u0bae\u0bbe\u0ba9\u0b9a \u0bb5\u0bbf\u0bb9\u0bbf\u0ba4 \u0ba8\u0bbf\u0b9c \u0bb5\u0b9a\u0ba4\u0bbf\u0bae\u0bcd <br/> \u0b85\u0bae\u0bb0 \u0ba8\u0bbf\u0b95\u0bb0 \u0ba4\u0bbe\u0baa \u0b9a\u0bae\u0ba9 \u0baa\u0bb0\u0bbe\u0baf\u0ba3\u0bae\u0bcd <br/> \u0b9a\u0bbe\u0bae\u0b9c \u0baa\u0bc1\u0b99\u0bcd\u0b95\u0bb5 \u0b9a\u0bbe\u0bb0\u0bbf\u0bae \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0bbf\u0b9a\u0ba3 \u0ba8\u0bbf\u0b9c \u0b95\u0ba4\u0bbf\u0bae\u0bb9\u0bae\u0bbf\u0bb9"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0ba8\u0ba4 \u0baa\u0bb5\u0b95\u0ba4 \u0bb9\u0bbe\u0bb0\u0bbf\u0ba3\u0bae\u0bcd \u0b9a\u0bb0\u0b9a\u0bc0\u0bb0\u0bc1\u0bb9 \u0ba8\u0bbe\u0baa\u0bae\u0bcd \u0b85\u0bae\u0bb2\u0bb9\u0bbe\u0b87\u0ba3\u0bae\u0bcd <br/> \u0b9a\u0ba4\u0bae\u0ba9\u0bcd\u0baf\u0bc1 \u0bae\u0bc1\u0b95\u0ba9\u0bc1\u0ba4 \u0b9a\u0b99\u0bcd\u0b95\u0bb0\u0bae\u0bcd-\u0b85\u0ba9\u0bc1\u0b9c \u0b9a\u0bae\u0bcd\u0baf\u0bc1\u0ba4\u0bae\u0bcd \u0b86\u0ba4\u0bcd\u0bae \u0ba8\u0bbf\u0bb0\u0bc0\u0b95\u0bcd\u0bb7\u0ba3\u0bae\u0bcd <br/> \u0bb9\u0ba4 \u0baa\u0bbe\u0baa \u0b9a\u0bae\u0bc1\u0ba4\u0bbe\u0baf\u0bae\u0bbe\u0ba4\u0bc1\u0bb0 \u0b9a\u0bbe\u0ba4\u0bc1\u0b9c\u0ba9\u0bbe\u0bb5\u0ba9 \u0baa\u0b9f\u0bc1\u0ba4\u0bae \u0b95\u0bc1\u0ba3\u0b95\u0ba3\u0bae\u0bb9\u0bae\u0bbf\u0bb9"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c15\u0c32\u0c2f\u0c2e\u0c3f \u0c36\u0c4d\u0c30\u0c40 \u0c30\u0c2e\u0c2e\u0c4d"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c15\u0c32\u0c26 \u0c28\u0c35\u0c28\u0c36\u0c28\u0c2e\u0c4d\u0c15\u0c30\u0c4d\u0c2e\u0c41\u0c16 \u0c36\u0c30\u0c27\u0c30\u0c2e\u0c4d \u0c2c\u0c32\u0c2d\u0c3f\u0c26\u0c41\u0c2c\u0c32 \u0c28\u0c40\u0c32\u0c2e\u0c4d \u0c35\u0c30\u0c3f\u0c1c\u0c24 \u0c28\u0c2f\u0c28\u0c2e\u0c4d"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c36\u0c30\u0c28\u0c17\u0c24 \u0c2d\u0c30\u0c28\u0c2e\u0c4d \u0c1c\u0c28\u0c15 \u0c38\u0c41\u0c24 \u0c1c\u0c3e\u0c28\u0c3f\u0c2e\u0c2e\u0c3f\u0c24 \u0c15\u0c30\u0c41\u0c28\u0c2e\u0c4d <br/> \u0c39\u0c30\u0c3f \u0c35\u0c40\u0c30\u0c35\u0c4d\u0c30\u0c4d\u0c2f\u0c4a\u0c2a\u0c28\u0c4d\u0c24\u0c2e\u0c2e\u0c4d\u0c2c\u0c41\u0c27\u0c3f \u0c2e\u0c26\u0c39\u0c30\u0c2e\u0c30\u0c41\u0c28 \u0c2c\u0c3f\u0c2e\u0c4d\u0c2c \u0c35\u0c3f\u0c30\u0c1c\u0c24\u0c27\u0c30\u0c2e\u0c4d \u0c1c\u0c28\u0c46\u0c36\u0c2e\u0c4d <br/> \u0c26\u0c30\u0c3f\u0c24 \u0c24\u0c30\u0c41\u0c28 \u0c30\u0c35\u0c28 \u0c2e\u0c24\u0c3f\u0c2e\u0c4d \u0c2e\u0c4d\u0c30\u0c4d\u0c26\u0c41\u0c24\u0c30 \u0c39\u0c38\u0c2e\u0c2e\u0c3f\u0c39"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c15\u0c2e\u0c28\u0c40\u0c2f\u0c24\u0c30 \u0c30\u0c42\u0c2a\u0c2e\u0c4d \u0c1c\u0c32\u0c27\u0c3f \u0c38\u0c2e \u0c17\u0c2e\u0c4d\u0c2d\u0c40\u0c30\u0c4d\u0c2f\u0c2e\u0c41\u0c30\u0c41 \u0c2a\u0c4d\u0c30\u0c24\u0c3e\u0c2a\u0c2e\u0c