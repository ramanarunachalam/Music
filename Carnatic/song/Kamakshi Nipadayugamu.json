c2f\u0c2e\u0c4d 5", "T": "\u0c2e\u0c3e\u0c28\u0c35\u0c24\u0c40 \u0c28\u0c3f\u0c28\u0c41 \u0c38\u0c26\u0c3e \u0c26\u0c32\u0c1a\u0c3f\u0c28 <br/> \u0c2e\u0c3e\u0c28\u0c35\u0c41\u0c32\u0c15\u0c46\u0c32\u0c4d\u0c32 \u0c2b\u0c32\u0c2e\u0c4a\u0c38\u0c17\u0c47 <br/> \u0c2c\u0c3f\u0c30\u0c41\u0c26\u0c41 \u0c17\u0c32 \u0c26\u0c47\u0c35\u0c24\u0c2f\u0c28\u0c3f \u0c28\u0c47 <br/> \u0c35\u0c3f\u0c28\u0c2c\u0c21\u0c3f \u0c28\u0c40\u0c35\u0c47 \u0c17\u0c24\u0c3f\u0c2f\u0c28\u0c41\u0c1a\u0c41"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 6", "T": "\u0c2a\u0c3e\u0c35\u0c28\u0c40 \u0c2a\u0c41\u0c30 \u0c39\u0c30\u0c41\u0c28\u0c3f \u0c30\u0c2e\u0c23\u0c40 <br/> \u0c2a\u0c3e\u0c30\u0c4d\u0c35\u0c24\u0c40 \u0c38\u0c15\u0c32 \u0c1c\u0c28\u0c28\u0c40 <br/> \u0c2a\u0c3e\u0c24\u0c15\u0c2e\u0c41\u0c32\u0c28\u0c41 \u0c35\u0c21\u0c3f\u0c17\u0c3e \u0c26\u0c40\u0c30\u0c4d\u0c1a\u0c3f <br/> \u0c35\u0c30\u0c2e\u0c4a\u0c38\u0c17\u0c41\u0c2e\u0c41"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 7", "T": "\u0c15\u0c28\u0c15 \u0c17\u0c3f\u0c30\u0c3f \u0c38\u0c26\u0c28 \u0c28\u0c3f\u0c28\u0c41 \u0c17\u0c4a\u0c32\u0c3f\u0c1a\u0c3f\u0c28 <br/> \u0c1c\u0c28\u0c2e\u0c41\u0c32\u0c15\u0c41 \u0c26\u0c3f\u0c28\u0c26\u0c3f\u0c28\u0c2e\u0c41 \u0c36\u0c41\u0c2d\u0c2e\u0c4a\u0c38\u0c17\u0c47\u0c35\u0c28\u0c3f <br/> \u0c36\u0c4d\u0c30\u0c41\u0c24\u0c41\u0c32\u0c41 \u0c2e\u0c4a\u0c30\u0c32\u0c3f\u0c21\u0c17\u0c3e \u0c2e\u0c4a\u0c30\u0c32\u0c41 \u0c35\u0c3f\u0c28\u0c3f \u0c35\u0c3f\u0c28\u0c3f \u0c35\u0c3f\u0c28\u0c3f"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 8", "T": "\u0c2c\u0c3e\u0c32 \u0c15\u0c3f\u0c38\u0c32\u0c2f \u0c1a\u0c30\u0c23\u0c3e \u0c28\u0c3f\u0c2e\u0c3f\u0c37\u0c2e\u0c41 <br/> \u0c24\u0c3e\u0c33\u0c28\u0c3f\u0c15 \u0c35\u0c3f\u0c28\u0c41 \u0c2e\u0c26 \u0c17\u0c1c \u0c17\u0c2e\u0c28 <br/> \u0c24\u0c3e\u0c2e\u0c38\u0c2e\u0c41 \u0c38\u0c47\u0c2f\u0c15\u0c28\u0c47 \u0c28\u0c28\u0c4d\u0c28\u0c3f\u0c2a\u0c41\u0c21\u0c41 <br/> \u0c2c\u0c4d\u0c30\u0c4b\u0c35\u0c41\u0c2e\u0c41 \u0c2a\u0c30\u0c3e\u0c24\u0c4d\u0c2a\u0c30\u0c40"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 9", "T": "\u0c15\u0c41\u0c28\u0c4d\u0c26 \u0c2e\u0c41\u0c15\u0c41\u0c33 \u0c30\u0c26\u0c3e \u0c38\u0c41\u0c30 <br/> \u0c2c\u0c43\u0c28\u0c4d\u0c26 \u0c35\u0c3f\u0c28\u0c41\u0c24 \u0c2a\u0c26\u0c3e \u0c2d\u0c41\u0c35\u0c3f\u0c32\u0c4b <br/> \u0c35\u0c30 \u0c26\u0c3e\u0c2f\u0c15\u0c3f \u0c17\u0c26\u0c3e \u0c28\u0c3e \u0c2e\u0c4a\u0c30\u0c32\u0c41 <br/> \u0c1a\u0c46\u0c35\u0c41\u0c32\u0c15\u0c41 \u0c35\u0c3f\u0c28\u0c26\u0c3e \u0c17\u0c3f\u0c30\u0c3f \u0c38\u0c41\u0c24\u0c3e"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 10", "T": "\u0c28\u0c40 \u0c35\u0c32\u0c46\u0c28\u0c47 \u0c17\u0c32\u0c26\u0c3e \u0c28\u0c46\u0c30 \u0c26\u0c3e\u0c24\u0c35\u0c41 \u0c08 \u0c1c\u0c17\u0c24\u0c3f\u0c32\u0c4b <br/> \u0c28\u0c40\u0c26\u0c41 \u0c2a\u0c26 \u0c38\u0c3e\u0c30\u0c38\u0c2e\u0c41\u0c32 \u0c08 \u0c2d\u0c35 <br/> \u0c1c\u0c32\u0c27\u0c3f\u0c15\u0c3f \u0c24\u0c30\u0c3f\u0c2f\u0c28\u0c41\u0c1a\u0c41 \u0c2e\u0c3f\u0c17\u0c41\u0c32"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 11", "T": "\u0c15\u0c2e\u0c32 \u0c38\u0c2e\u0c4d\u0c2d\u0c35 \u0c38\u0c41\u0c30 \u0c2e\u0c41\u0c28\u0c40\u0c28\u0c4d\u0c26\u0c4d\u0c30\u0c41\u0c32 <br/> \u0c1a\u0c47\u0c24\u0c28\u0c41 \u0c28\u0c3f\u0c28\u0c41 \u0c2a\u0c4a\u0c17\u0c21\u0c41\u0c1f\u0c15\u0c41 \u0c24\u0c30\u0c2e\u0c3e\u0c2e\u0c4d\u0c2e\u0c3e <br/> \u0c36\u0c41\u0c2d\u0c2e\u0c3f\u0c2e\u0c4d\u0c2e\u0c3e \u0c28\u0c3f\u0c28\u0c41 \u0c28\u0c2e\u0c4d\u0c2e\u0c3f\u0c24\u0c3f\u0c28\u0c3f \u0c36\u0c4d\u0c2f\u0c3e\u0c2e <br/> \u0c15\u0c43\u0c37\u0c4d\u0c23 \u0c38\u0c4b\u0c26\u0c30\u0c40 \u0c26\u0c41\u0c30\u0c2e\u0c41\u0c17\u0c28\u0c41 <br/> \u0c15\u0c30\u0c41\u0c23 \u0c38\u0c32\u0c41\u0c2a\u0c41\u0c2e\u0c3f\u0c15\u0c28\u0c41"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0cbe\u0cae\u0cbe\u0c95\u0ccd\u0cb7\u0cbf \u0ca8\u0cc0 \u0caa\u0ca6 \u0caf\u0cc1\u0c97\u0cae\u0cc1 \u0cb8\u0ccd\u0ca5\u0cbf\u0cb0\u0cae\u0ca8\u0cbf <br/> \u0ca8\u0cc7 \u0ca8\u0cae\u0ccd\u0cae\u0cbf\u0caf\u0cc1\u0ca8\u0ccd\u0ca8\u0cbe\u0ca8\u0cc1 \u0ca8\u0cbe \u0c9a\u0cbf\u0ca8\u0ccd\u0ca4\u0cb2\u0cc6\u0cb2\u0ccd\u0cb2\u0ca8\u0cc1 \u0ca6\u0cc0\u0cb0\u0ccd\u0c9a\u0cae\u0ccd\u0cae\u0cbe"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 1", "T": "\u0c85\u0cae\u0ccd\u0cac \u0ca8\u0ca8\u0cc1 \u0cac\u0ccd\u0cb0\u0ccb\u0cb5 \u0cb8\u0cae\u0caf\u0cae\u0cc1 <br/> \u0cb5\u0cbf\u0ca8\u0cc1\u0cae\u0cbe \u0caa\u0ca4\u0cbf\u0ca4 \u0caa\u0cbe\u0cb5\u0ca8\u0cbf\u0c97\u0cbe"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 2", "T": "\u0c85\u0ca8\u0cc1\u0ca6\u0cbf\u0ca8\u0cae\u0cc1 \u0cb6\u0cb0\u0ca3\u0cae\u0ca8\u0cbf \u0ca8\u0cbf\u0ca8\u0cc1 <br/> \u0cb5\u0cc7\u0ca1\u0cc1\u0c95\u0cca\u0ca8\u0cbf\u0caf\u0cc1\u0ca8\u0ccd\u0ca8 \u0cb8\u0cc1\u0ca4\u0cc1\u0ca1\u0cae\u0ccd\u0cae\u0cbe \u0cb8\u0ca6\u0caf"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 3", "T": "\u0cb8\u0cb0\u0cb8\u0cbf\u0c9c\u0cbe\u0cb8\u0ca8 \u0cb9\u0cb0\u0cc0\u0cb6 \u0cb5\u0cbf\u0ca8\u0cc1\u0ca4 \u0caa\u0cbe\u0ca6\u0cbe <br/> \u0ca8\u0cbe\u0ca4\u0ccb \u0cb5\u0cbe\u0ca6\u0cbe"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 4", "T": "\u0c95\u0cae\u0cb2 \u0ca6\u0cb3 \u0cb8\u0cae \u0ca8\u0caf\u0ca8 <br/> \u0c95\u0c9a \u0c9c\u0cbf\u0ca4 \u0c98\u0ca8\u0cbe \u0cb6\u0cb6\u0cbf \u0ca7\u0cb0 \u0ca8\u0cbf\u0cad \u0cb5\u0ca6\u0ca8"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 5", "T": "\u0cae\u0cbe\u0ca8\u0cb5\u0ca4\u0cc0 \u0ca8\u0cbf\u0ca8\u0cc1 \u0cb8\u0ca6\u0cbe \u0ca6\u0cb2\u0c9a\u0cbf\u0ca8 <br/> \u0cae\u0cbe\u0ca8\u0cb5\u0cc1\u0cb2\u0c95\u0cc6\u0cb2\u0ccd\u0cb2 \u0cab\u0cb2\u0cae\u0cca\u0cb8\u0c97\u0cc7 <br/> \u0cac\u0cbf\u0cb0\u0cc1\u0ca6\u0cc1 \u0c97\u0cb2 \u0ca6\u0cc7\u0cb5\u0ca4\u0caf\u0ca8\u0cbf \u0ca8\u0cc7 <br/> \u0cb5\u0cbf\u0ca8\u0cac\u0ca1\u0cbf \u0ca8\u0cc0\u0cb5\u0cc7 \u0c97\u0ca4\u0cbf\u0caf\u0ca8\u0cc1\u0c9a\u0cc1"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 6", "T": "\u0caa\u0cbe\u0cb5\u0ca8\u0cc0 \u0caa\u0cc1\u0cb0 \u0cb9\u0cb0\u0cc1\u0ca8\u0cbf \u0cb0\u0cae\u0ca3\u0cc0 <br/> \u0caa\u0cbe\u0cb0\u0ccd\u0cb5\u0ca4\u0cc0 \u0cb8\u0c95\u0cb2 \u0c9c\u0ca8\u0ca8\u0cc0 <br/> \u0caa\u0cbe\u0ca4\u0c95\u0cae\u0cc1\u0cb2\u0ca8\u0cc1 \u0cb5\u0ca1\u0cbf\u0c97\u0cbe \u0ca6\u0cc0\u0cb0\u0ccd\u0c9a\u0cbf <br/> \u0cb5\u0cb0\u0cae\u0cca\u0cb8\u0c97\u0cc1\u0cae\u0cc1"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 7", "T": "\u0c95\u0ca8\u0c95 \u0c97\u0cbf\u0cb0\u0cbf \u0cb8\u0ca6\u0ca8 \u0ca8\u0cbf\u0ca8\u0cc1 \u0c97\u0cca\u0cb2\u0cbf\u0c9a\u0cbf\u0ca8 <br/> \u0c9c\u0ca8\u0cae\u0cc1\u0cb2\u0c95\u0cc1 \u0ca6\u0cbf\u0ca8\u0ca6\u0cbf\u0ca8\u0cae\u0cc1 \u0cb6\u0cc1\u0cad\u0cae\u0cca\u0cb8\u0c97\u0cc7\u0cb5\u0ca8\u0cbf <br/> \u0cb6\u0ccd\u0cb0\u0cc1\u0ca4\u0cc1\u0cb2\u0cc1 \u0cae\u0cca\u0cb0\u0cb2\u0cbf\u0ca1\u0c97\u0cbe \u0cae\u0cca\u0cb0\u0cb2\u0cc1 \u0cb5\u0cbf\u0ca8\u0cbf \u0cb5\u0cbf\u0ca8\u0cbf \u0cb5\u0cbf\u0ca8\u0cbf"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 8", "T": "\u0cac\u0cbe\u0cb2 \u0c95\u0cbf\u0cb8\u0cb2\u0caf \u0c9a\u0cb0\u0ca3\u0cbe \u0ca8\u0cbf\u0cae\u0cbf\u0cb7\u0cae\u0cc1 <br/> \u0ca4\u0cbe\u0cb3\u0ca8\u0cbf\u0c95 \u0cb5\u0cbf\u0ca8\u0cc1 \u0cae\u0ca6 \u0c97\u0c9c \u0c97\u0cae\u0ca8 <br/> \u0ca4\u0cbe\u0cae\u0cb8\u0cae\u0cc1 \u0cb8\u0cc7\u0caf\u0c95\u0ca8\u0cc7 \u0ca8\u0ca8\u0ccd\u0ca8\u0cbf\u0caa\u0cc1\u0ca1\u0cc1 <br/> \u0cac\u0ccd\u0cb0\u0ccb\u0cb5\u0cc1\u0cae\u0cc1 \u0caa\u0cb0\u0cbe\u0ca4\u0ccd\u0caa\u0cb0\u0cc0"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 9", "T": "\u0c95\u0cc1\u0ca8\u0ccd\u0ca6 \u0cae\u0cc1\u0c95\u0cc1\u0cb3 \u0cb0\u0ca6\u0cbe \u0cb8\u0cc1\u0cb0 <br/> \u0cac\u0cc3\u0ca8\u0ccd\u0ca6 \u0cb5\u0cbf\u0ca8\u0cc1\u0ca4 \u0caa\u0ca6\u0cbe \u0cad\u0cc1\u0cb5\u0cbf\u0cb2\u0ccb <br/> \u0cb5\u0cb0 \u0ca6\u0cbe\u0caf\u0c95\u0cbf \u0c97\u0ca6\u0cbe \u0ca8\u0cbe \u0cae\u0cca\u0cb0\u0cb2\u0cc1 <br/> \u0c9a\u0cc6\u0cb5\u0cc1\u0cb2\u0c95\u0cc1 \u0cb5\u0cbf\u0ca8\u0ca6\u0cbe \u0c97\u0cbf\u0cb0\u0cbf \u0cb8\u0cc1\u0ca4\u0cbe"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 10", "T": "\u0ca8\u0cc0 \u0cb5\u0cb2\u0cc6\u0ca8\u0cc7 \u0c97\u0cb2\u0ca6\u0cbe \u0ca8\u0cc6\u0cb0 \u0ca6\u0cbe\u0ca4\u0cb5\u0cc1 \u0c88 \u0c9c\u0c97\u0ca4\u0cbf\u0cb2\u0ccb <br/> \u0ca8\u0cc0\u0ca6\u0cc1 \u0caa\u0ca6 \u0cb8\u0cbe\u0cb0\u0cb8\u0cae\u0cc1\u0cb2 \u0c88 \u0cad\u0cb5 <br/> \u0c9c\u0cb2\u0ca7\u0cbf\u0c95\u0cbf \u0ca4\u0cb0\u0cbf\u0caf\u0ca8\u0cc1\u0c9a\u0cc1 \u0cae\u0cbf\u0c97\u0cc1\u0cb2"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 11", "T": "\u0c95\u0cae\u0cb2 \u0cb8\u0cae\u0ccd\u0cad\u0cb5 \u0cb8\u0cc1\u0cb0 \u0cae\u0cc1\u0ca8\u0cc0\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0\u0cc1\u0cb2 <br/> \u0c9a\u0cc7\u0ca4\u0ca8\u0cc1 \u0ca8\u0cbf\u0ca8\u0cc1 \u0caa\u0cca\u0c97\u0ca1\u0cc1\u0c9f\u0c95\u0cc1 \u0ca4\u0cb0\u0cae\u0cbe\u0cae\u0ccd\u0cae\u0cbe <br/> \u0cb6\u0cc1\u0cad\u0cae\u0cbf\u0cae\u0ccd\u0cae\u0cbe \u0ca8\u0cbf\u0ca8\u0cc1 \u0ca8\u0cae\u0ccd\u0cae\u0cbf\u0ca4\u0cbf\u0ca8\u0cbf \u0cb6\u0ccd\u0caf\u0cbe\u0cae <br/> \u0c95\u0cc3\u0cb7\u0ccd\u0ca3 \u0cb8\u0ccb\u0ca6\u0cb0\u0cc0 \u0ca6\u0cc1\u0cb0\u0cae\u0cc1\u0c97\u0ca8\u0cc1 <br/> \u0c95\u0cb0\u0cc1\u0ca3 \u0cb8\u0cb2\u0cc1\u0caa\u0cc1\u0cae\u0cbf\u0c95\u0ca8\u0cc1"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d15\u0d3e\u0d2e\u0d3e\u0d15\u0d4d\u0d37\u0d3f \u0d28\u0d40 \u0d2a\u0d26 \u0d2f\u0d41\u0d17\u0d2e\u0d41 \u0d38\u0d4d\u0d25\u0d3f\u0d30\u0d2e\u0d28\u0d3f <br/> \u0d28\u0d47 \u0d28\u0d2e\u0d4d\u0d2e\u0d3f\u0d2f\u0d41\u0d28\u0d4d\u0d28\u0d3e\u0d28\u0d41 \u0d28\u0d3e \u0d1a\u0d3f\u0d28\u0d4d\u0d24\u0d32\u0d46\u0d32\u0d4d\u0d32\u0d28\u0d41 \u0d26\u0d40\u0d30\u0d4d\u0d1a\u0d2e\u0d4d\u0d2e\u0d3e"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 1", "T": "\u0d05\u0d2e\u0d4d\u0d2c \u0d28\u0d28\u0d41 \u0d2c\u0d4d\u0d30\u0d4b\u0d35 \u0d38\u0d2e\u0d2f\u0d2e\u0d41 <br/> \u0d35\u0d3f\u0d28\u0d41\u0d2e\u0d3e \u0d2a\u0d24\u0d3f\u0d24 \u0d2a\u0d3e\u0d35\u0d28\u0d3f\u0d17\u0d3e"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 2", "T": "\u0d05\u0d28\u0d41\u0d26\u0d3f\u0d28\u0d2e\u0d41 \u0d36\u0d30\u0d23\u0d2e\u0d28\u0d3f \u0d28\u0d3f\u0d28\u0d41 <br/> \u0d35\u0d47\u0d21\u0d41\u0d15\u0d4a\u0d28\u0d3f\u0d2f\u0d41\u0d28\u0d4d\u0d28 \u0d38\u0d41\u0d24\u0d41\u0d21\u0d2e\u0d4d\u0d2e\u0d3e \u0d38\u0d26\u0d2f"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 3", "T": "\u0d38\u0d30\u0d38\u0d3f\u0d1c\u0d3e\u0d38\u0d28 \u0d39\u0d30\u0d40\u0d36 \u0d35\u0d3f\u0d28\u0d41\u0d24 \u0d2a\u0d3e\u0d26\u0d3e <br/> \u0d28\u0d3e\u0d24\u0d4b \u0d35\u0d3e\u0d26\u0d3e"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 4", "T": "\u0d15\u0d2e\u0d32 \u0d26\u0d33 \u0d38\u0d2e \u0d28\u0d2f\u0d28 <br/> \u0d15\u0d1a \u0d1c\u0d3f\u0d24 \u0d18\u0d28\u0d3e \u0d36\u0d36\u0d3f \u0d27\u0d30 \u0d28\u0d3f\u0d2d \u0d35\u0d26\u0d28"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 5", "T": "\u0d2e\u0d3e\u0d28\u0d35\u0d24\u0d40 \u0d28\u0d3f\u0d28\u0d41 \u0d38\u0d26\u0d3e \u0d26\u0d32\u0d1a\u0d3f\u0d28 <br/> \u0d2e\u0d3e\u0d28\u0d35\u0d41\u0d32\u0d15\u0d46\u0d32\u0d4d\u0d32 \u0d2b\u0d32\u0d2e\u0d4a\u0d38\u0d17\u0d47 <br/> \u0d2c\u0d3f\u0d30\u0d41\u0d26\u0d41 \u0d17\u0d32 \u0d26\u0d47\u0d35\u0d24\u0d2f\u0d28\u0d3f \u0d28\u0d47 <br/> \u0d35\u0d3f\u0d28\u0d2c\u0d21\u0d3f \u0d28\u0d40\u0d35\u0d47 \u0d17\u0d24\u0d3f\u0d2f\u0d28\u0d41\u0d1a\u0d41"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 6", "T": "\u0d2a\u0d3e\u0d35\u0d28\u0d40 \u0d2a\u0d41\u0d30 \u0d39\u0d30\u0d41\u0d28\u0d3f \u0d30\u0d2e\u0d23\u0d40 <br/> \u0d2a\u0d3e\u0d30\u0d4d\u0d35\u0d24\u0d40 \u0d38\u0d15\u0d32 \u0d1c\u0d28\u0d28\u0d40 <br/> \u0d2a\u0d3e\u0d24\u0d15\u0d2e\u0d41\u0d32\u0d28\u0d41 \u0d35\u0d21\u0d3f\u0d17\u0d3e \u0d26\u0d40\u0d30\u0d4d\u0d1a\u0d3f <br/> \u0d35\u0d30\u0d2e\u0d4a\u0d38\u0d17\u0d41\u0d2e\u0d41"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 7", "T": "\u0d15\u0d28\u0d15 \u0d17\u0d3f\u0d30\u0d3f \u0d38\u0d26\u0d28 \u0d28\u0d3f\u0d28\u0d41 \u0d17\u0d4a\u0d32\u0d3f\u0d1a\u0d3f\u0d28 <br/> \u0d1c\u0d28\u0d2e\u0d41\u0d32\u0d15\u0d41 \u0d26\u0d3f\u0d28\u0d26\u0d3f\u0d28\u0d2e\u0d41 \u0d36\u0d41\u0d2d\u0d2e\u0d4a\u0d38\u0d17\u0d47\u0d35\u0d28\u0d3f <br/> \u0d36\u0d4d\u0d30\u0d41\u0d24\u0d41\u0d32\u0d41 \u0d2e\u0d4a\u0d30\u0d32\u0d3f\u0d21\u0d17\u0d3e \u0d2e\u0d4a\u0d30\u0d32\u0d41 \u0d35\u0d3f\u0d28\u0d3f \u0d35\u0d3f\u0d28\u0d3f \u0d35\u0d3f\u0d28\u0d3f"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 8", "T": "\u0d2c\u0d3e\u0d32 \u0d15\u0d3f\u0d38\u0d32\u0d2f \u0d1a\u0d30\u0d23\u0d3e \u0d28\u0d3f\u0d2e\u0d3f\u0d37\u0d2e\u0d41 <br/> \u0d24\u0d3e\u0d33\u0d28\u0d3f\u0d15 \u0d35\u0d3f\u0d28\u0d41 \u0d2e\u0d26 \u0d17\u0d1c \u0d17\u0d2e\u0d28 <br/> \u0d24\u0d3e\u0d2e\u0d38\u0d2e\u0d41 \u0d38\u0d47\u0d2f\u0d15\u0d28\u0d47 \u0d28\u0d28\u0d4d\u0d28\u0d3f\u0d2a\u0d41\u0d21\u0d41 <br/> \u0d2c\u0d4d\u0d30\u0d4b\u0d35\u0d41\u0d2e\u0d41 \u0d2a\u0d30\u0d3e\u0d24\u0d4d\u0d2a\u0d30\u0d40"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 9", "T": "\u0d15\u0d41\u0d28\u0d4d\u0d26 \u0d2e\u0d41\u0d15\u0d41\u0d33 \u0d30\u0d26\u0d3e \u0d38\u0d41\u0d30 <br/> \u0d2c\u0d43\u0d28\u0d4d\u0d26 \u0d35\u0