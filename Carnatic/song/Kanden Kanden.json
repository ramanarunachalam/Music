u0c17\u0c15\u0c4d-\u0c15\u0c34\u0c3f\u0c24\u0c4d\u0c24\u0c3e\u0c33\u0c47 \u0c2a\u0c3f\u0c30\u0c2f\u0c3e\u0c36\u0c3f <br/> \u0c28\u0c3f\u0c28\u0c48\u0c24\u0c4d\u0c24\u0c19\u0c4d\u0c17\u0c47 \u0c30\u0c3e\u0c35\u0c23\u0c28\u0c4d \u0c05\u0c28\u0c4d\u0c26\u0c3e\u0c28\u0c4d \u0c35\u0c30\u0c1a\u0c4d-\u0c1b\u0c40\u0c1a\u0c3f \u0c28\u0c3f\u0c32\u0c4d\u0c32\u0c21\u0c3e \u0c1a\u0c23\u0c4d\u0c21\u0c3e\u0c33\u0c3e \u0c0e\u0c28\u0c4d\u0c31\u0c47\u0c36\u0c3f \u0c24\u0c28\u0c3f\u0c24\u0c4d\u0c24\u0c41\u0c24\u0c4d\u0c24\u0c28\u0c4d <br/> \u0c09\u0c2f\u0c3f\u0c30\u0c4d\u0c24\u0c28\u0c48\u0c24\u0c4d-\u0c24\u0c3e\u0c28\u0c4d \u0c35\u0c3f\u0c21 \u0c2e\u0c39\u0c30\u0c3e\u0c1c\u0c3f \u0c36\u0c3e\u0c30\u0c41\u0c2e\u0c4d \u0c2a\u0c4a\u0c34\u0c41\u0c26\u0c41 \u0c15\u0c3e\u0c23\u0c41\u0c2e\u0c4d \u0c38\u0c2e\u0c2f\u0c2e\u0c3f\u0c26\u0c41\u0c35\u0c47 \u0c35\u0c3e\u0c36\u0c3f\u0c2f\u0c4d\u0c28\u0c3f\u0c24\u0c4d- <br/> \u0c24\u0c3e\u0c2e\u0c26\u0c2e\u0c4d \u0c36\u0c46\u0c2f\u0c4d\u0c2f\u0c32\u0c4d \u0c06\u0c18\u0c3e\u0c26\u0c46\u0c28\u0c4d\u0c31\u0c3f\u0c21\u0c30\u0c4d \u0c35\u0c40\u0c36\u0c3f \u0c30\u0c3e\u0c2e \u0c30\u0c3e\u0c2e \u0c30\u0c3e\u0c2e \u0c30\u0c3e\u0c2e \u0c0e\u0c28\u0c4d\u0c31\u0c46\u0c26\u0c3f\u0c30\u0c4d \u0c2a\u0c47\u0c36\u0c3f (\u0c15\u0c23\u0c4d\u0c21\u0c47\u0c28\u0c4d)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c05\u0c21\u0c32\u0c4d \u0c36\u0c47\u0c30\u0c41\u0c2e\u0c4d \u0c35\u0c3e\u0c32\u0c3f\u0c2f\u0c48 \u0c35\u0c3e\u0c28\u0c41\u0c32\u0c17\u0c3f\u0c32\u0c47 \u0c15\u0c42\u0c1f\u0c4d\u0c1f\u0c3f \u0c05\u0c35\u0c28\u0c3f\u0c2f\u0c48 \u0c38\u0c41\u0c17\u0c4d\u0c30\u0c40\u0c35\u0c28\u0c4d \u0c06\u0c32 \u0c2e\u0c41\u0c21\u0c3f \u0c36\u0c42\u0c1f\u0c4d\u0c1f\u0c3f <br/> \u0c09\u0c21\u0c28\u0c47 \u0c28\u0c40 \u0c24\u0c42\u0c26\u0c41 \u0c2a\u0c4b \u0c0e\u0c28\u0c4d\u0c31 \u0c36\u0c4a\u0c32\u0c4d \u0c05\u0c2e\u0c41\u0c26\u0c42\u0c1f\u0c4d\u0c1f\u0c3f \u0c09\u0c28\u0c4d \u0c0e\u0c34\u0c3f\u0c32\u0c4d \u0c2a\u0c3e\u0c30\u0c3e\u0c1f\u0c4d\u0c1f\u0c3f <br/> \u0c35\u0c3f\u0c21\u0c35\u0c28\u0c4d\u0c26 \u0c05\u0c28\u0c41\u0c2e\u0c28\u0c4d\u0c24\u0c28\u0c4d \u0c28\u0c3e\u0c28\u0c4d \u0c0e\u0c28\u0c4d\u0c31\u0c41 \u0c36\u0c40\u0c30\u0c3e\u0c1f\u0c4d\u0c1f\u0c3f \u0c35\u0c3f\u0c35\u0c30\u0c2e\u0c4d \u0c36\u0c4a\u0c32\u0c4d\u0c32 \u0c09\u0c2f\u0c3f\u0c30\u0c4d \u0c15\u0c4a\u0c23\u0c4d\u0c21\u0c3f\u0c30\u0c41\u0c15\u0c4d\u0c15\u0c3f\u0c30\u0c3e\u0c33\u0c4d \u0c36\u0c40\u0c2e\u0c3e\u0c1f\u0c4d\u0c1f\u0c3f <br/> \u0c26\u0c3f\u0c21\u0c2e\u0c3e \u0c32\u0c15\u0c4d\u0c37\u0c4d\u0c2e\u0c23\u0c28\u0c4d \u0c36\u0c46\u0c2f\u0c4d\u0c26 \u0c2a\u0c30\u0c4d\u0c23\u0c36\u0c3e\u0c32\u0c48 \u0c35\u0c40\u0c1f\u0c4d\u0c1f\u0c3f\u0c32\u0c4d \u0c26\u0c47\u0c35\u0c3e\u0c26\u0c3f \u0c26\u0c47\u0c35\u0c3e \u0c09\u0c28\u0c4d \u0c24\u0c3f\u0c30\u0c41\u0c35\u0c3e\u0c34\u0c3f\u0c24\u0c28\u0c3f\u0c15\u0c4d-\u0c15\u0c3e\u0c1f\u0c4d\u0c1f\u0c3f (\u0c15\u0c23\u0c4d\u0c21\u0c47\u0c28\u0c4d)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0ca3\u0ccd\u0ca1\u0cc7\u0ca8\u0ccd \u0c95\u0ca3\u0ccd\u0ca1\u0cc7\u0ca8\u0ccd \u0cb8\u0cc0\u0ca4\u0cc8\u0caf\u0cc8\u0c95\u0ccd-\u0c95\u0ca3\u0ccd\u0ca1\u0cc7\u0ca8\u0ccd \u0cb0\u0cbe\u0c98\u0cb5\u0cbe"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c85\u0ca3\u0ccd\u0ca1\u0cb0\u0cc1\u0c99\u0ccd\u0c97\u0ccd-\u0c95\u0cbe\u0ca3\u0cbe\u0ca6 \u0cb2\u0c99\u0ccd\u0c95\u0cbe\u0caa\u0cc1\u0cb0\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0cc7 \u0c85\u0cb0\u0cb5\u0cbf\u0ca8\u0ccd\u0ca6 \u0cb5\u0cc7\u0ca6\u0cbe\u0cb5\u0cc8\u0ca4\u0ccd-\u0ca4\u0cb0\u0cb5\u0ca8\u0ccd\u0ca6 \u0cae\u0cbe\u0ca4\u0cbe\u0cb5\u0cc8 (\u0c95\u0ca3\u0ccd\u0ca1\u0cc7\u0ca8\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c95\u0cbe\u0cb5\u0cbf \u0cb5\u0cbf\u0cb3\u0cbf\u0c97\u0cb3\u0cbf\u0cb2\u0ccd \u0c89\u0ca8\u0ccd \u0c89\u0cb0\u0cc1\u0cb5\u0cc7\u0cb3\u0cbf \u0cae\u0cbf\u0ca8\u0ccd\u0ca8\u0c95\u0ccd-\u0c95\u0ca8\u0cbf\u0cb5\u0cbe\u0caf\u0ccd \u0ca4\u0ca8\u0cbf\u0cb2\u0cc7 \u0c89\u0ca8\u0ccd \u0ca4\u0cbf\u0cb0\u0cc1.\u0ca3\u0cbe\u0cae\u0cae\u0cc7 \u0caa\u0ca8\u0ccd\u0ca8 <br/> \u0c86\u0cb5\u0cbf\u0ca4\u0ccd\u0ca4\u0cc1\u0ca3\u0cc8\u0caf\u0cc8\u0caa\u0ccd-\u0caa\u0cbf\u0cb0\u0cbf\u0ca8\u0ccd\u0ca6 \u0cae\u0ca1 \u0c85\u0ca8\u0ccd\u0ca8\u0cae\u0ccd \u0c85\u0ca8\u0cbe\u0cb3\u0ccd \u0ca8\u0cbe\u0ca8\u0ccd \u0cb6\u0cca\u0cb2\u0ccd\u0cb2\u0cc1\u0cb5\u0ca6\u0cc6\u0ca8\u0ccd\u0ca8 <br/> \u0caa\u0cc2\u0cb5\u0cc8\u0ca4\u0ccd-\u0ca4\u0cbf\u0cb0\u0cbf\u0cb6\u0ca1\u0cc8 \u0ca8\u0cbf\u0ca4\u0ccd\u0ca4\u0cae\u0ccd \u0ca8\u0cbf\u0ca4\u0ccd\u0ca4\u0cae\u0ccd \u0cb6\u0cca\u0ca8\u0ccd\u0ca8 \u0cac\u0cc1\u0ca6\u0ccd\u0ca7\u0cbf \u0cb5\u0cb3\u0cbf\u0caf\u0cc7 \u0ca4\u0ca8\u0ccd \u0cac\u0cc1\u0ca7\u0cbf \u0ca8\u0cbf\u0cb2\u0cc8 \u0cae\u0ca8\u0ccd\u0ca8\u0caa\u0ccd- <br/> \u0caa\u0cbe\u0cb5\u0cbf \u0c85\u0cb0\u0c95\u0ccd\u0c95\u0cbf\u0caf\u0cb0\u0ccd \u0c95\u0cbe\u0cb5\u0cb2\u0ccd \u0cb6\u0cbf\u0cb0\u0cc8\u0ca6\u0cc1\u0ca8\u0ccd\u0ca8\u0caa\u0ccd-\u0caa\u0c9e\u0ccd\u0c9c\u0cc1 \u0caa\u0ca1\u0cbf\u0ca8\u0ccd\u0ca6 \u0caa\u0cb3\u0ca8\u0ccd-\u0c9a\u0cbf\u0ca4\u0ccd\u0cb0\u0cae\u0ccd \u0c8e\u0ca8\u0ccd\u0ca8 (\u0c95\u0ca3\u0ccd\u0ca1\u0cc7\u0ca8\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0caa\u0ca8\u0cbf\u0c95\u0ccd\u0c95\u0cbe\u0cb2 \u0cb5\u0cbe\u0cb0\u0cbf\u0c9c\u0cae\u0ccd \u0caa\u0ccb\u0cb2 \u0ca8\u0cbf\u0cb0\u0c99\u0ccd\u0c97\u0cc2\u0cb6\u0cbf\u0caa\u0ccd-\u0caa\u0c98\u0cb2\u0ccd \u0c92\u0cb0\u0cc1 <br/> \u0caf\u0cc1\u0c97\u0cae\u0cbe\u0c97\u0c95\u0ccd-\u0c95\u0cb3\u0cbf\u0ca4\u0ccd\u0ca4\u0cbe\u0cb3\u0cc7 \u0caa\u0cbf\u0cb0\u0caf\u0cbe\u0cb6\u0cbf <br/> \u0ca8\u0cbf\u0ca8\u0cc8\u0ca4\u0ccd\u0ca4\u0c99\u0ccd\u0c97\u0cc7 \u0cb0\u0cbe\u0cb5\u0ca3\u0ca8\u0ccd \u0c85\u0ca8\u0ccd\u0ca6\u0cbe\u0ca8\u0ccd \u0cb5\u0cb0\u0c9a\u0ccd-\u0c9b\u0cc0\u0c9a\u0cbf \u0ca8\u0cbf\u0cb2\u0ccd\u0cb2\u0ca1\u0cbe \u0c9a\u0ca3\u0ccd\u0ca1\u0cbe\u0cb3\u0cbe \u0c8e\u0ca8\u0ccd\u0cb1\u0cc7\u0cb6\u0cbf \u0ca4\u0ca8\u0cbf\u0ca4\u0ccd\u0ca4\u0cc1\u0ca4\u0ccd\u0ca4\u0ca8\u0ccd <br/> \u0c89\u0caf\u0cbf\u0cb0\u0ccd\u0ca4\u0ca8\u0cc8\u0ca4\u0ccd-\u0ca4\u0cbe\u0ca8\u0ccd \u0cb5\u0cbf\u0ca1 \u0cae\u0cb9\u0cb0\u0cbe\u0c9c\u0cbf \u0cb6\u0cbe\u0cb0\u0cc1\u0cae\u0ccd \u0caa\u0cca\u0cb3\u0cc1\u0ca6\u0cc1 \u0c95\u0cbe\u0ca3\u0cc1\u0cae\u0ccd \u0cb8\u0cae\u0caf\u0cae\u0cbf\u0ca6\u0cc1\u0cb5\u0cc7 \u0cb5\u0cbe\u0cb6\u0cbf\u0caf\u0ccd\u0ca8\u0cbf\u0ca4\u0ccd- <br/> \u0ca4\u0cbe\u0cae\u0ca6\u0cae\u0ccd \u0cb6\u0cc6\u0caf\u0ccd\u0caf\u0cb2\u0ccd \u0c86\u0c98\u0cbe\u0ca6\u0cc6\u0ca8\u0ccd\u0cb1\u0cbf\u0ca1\u0cb0\u0ccd \u0cb5\u0cc0\u0cb6\u0cbf \u0cb0\u0cbe\u0cae \u0cb0\u0cbe\u0cae \u0cb0\u0cbe\u0cae \u0cb0\u0cbe\u0cae \u0c8e\u0ca8\u0ccd\u0cb1\u0cc6\u0ca6\u0cbf\u0cb0\u0ccd \u0caa\u0cc7\u0cb6\u0cbf (\u0c95\u0ca3\u0ccd\u0ca1\u0cc7\u0ca8\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c85\u0ca1\u0cb2\u0ccd \u0cb6\u0cc7\u0cb0\u0cc1\u0cae\u0ccd \u0cb5\u0cbe\u0cb2\u0cbf\u0caf\u0cc8 \u0cb5\u0cbe\u0ca8\u0cc1\u0cb2\u0c97\u0cbf\u0cb2\u0cc7 \u0c95\u0cc2\u0c9f\u0ccd\u0c9f\u0cbf \u0c85\u0cb5\u0ca8\u0cbf\u0caf\u0cc8 \u0cb8\u0cc1\u0c97\u0ccd\u0cb0\u0cc0\u0cb5\u0ca8\u0ccd \u0c86\u0cb2 \u0cae\u0cc1\u0ca1\u0cbf \u0cb6\u0cc2\u0c9f\u0ccd\u0c9f\u0cbf <br/> \u0c89\u0ca1\u0ca8\u0cc7 \u0ca8\u0cc0 \u0ca4\u0cc2\u0ca6\u0cc1 \u0caa\u0ccb \u0c8e\u0ca8\u0ccd\u0cb1 \u0cb6\u0cca\u0cb2\u0ccd \u0c85\u0cae\u0cc1\u0ca6\u0cc2\u0c9f\u0ccd\u0c9f\u0cbf \u0c89\u0ca8\u0ccd \u0c8e\u0cb3\u0cbf\u0cb2\u0ccd \u0caa\u0cbe\u0cb0\u0cbe\u0c9f\u0ccd\u0c9f\u0cbf <br/> \u0cb5\u0cbf\u0ca1\u0cb5\u0ca8\u0ccd\u0ca6 \u0c85\u0ca8\u0cc1\u0cae\u0ca8\u0ccd\u0ca4\u0ca8\u0ccd \u0ca8\u0cbe\u0ca8\u0ccd \u0c8e\u0ca8\u0ccd\u0cb1\u0cc1 \u0cb6\u0cc0\u0cb0\u0cbe\u0c9f\u0ccd\u0c9f\u0cbf \u0cb5\u0cbf\u0cb5\u0cb0\u0cae\u0ccd \u0cb6\u0cca\u0cb2\u0ccd\u0cb2 \u0c89\u0caf\u0cbf\u0cb0\u0ccd \u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cbf\u0cb0\u0cc1\u0c95\u0ccd\u0c95\u0cbf\u0cb0\u0cbe\u0cb3\u0ccd \u0cb6\u0cc0\u0cae\u0cbe\u0c9f\u0ccd\u0c9f\u0cbf <br/> \u0ca6\u0cbf\u0ca1\u0cae\u0cbe \u0cb2\u0c95\u0ccd\u0cb7\u0ccd\u0cae\u0ca3\u0ca8\u0ccd \u0cb6\u0cc6\u0caf\u0ccd\u0ca6 \u0caa\u0cb0\u0ccd\u0ca3\u0cb6\u0cbe\u0cb2\u0cc8 \u0cb5\u0cc0\u0c9f\u0ccd\u0c9f\u0cbf\u0cb2\u0ccd \u0ca6\u0cc7\u0cb5\u0cbe\u0ca6\u0cbf \u0ca6\u0cc7\u0cb5\u0cbe \u0c89\u0ca8\u0ccd \u0ca4\u0cbf\u0cb0\u0cc1\u0cb5\u0cbe\u0cb3\u0cbf\u0ca4\u0ca8\u0cbf\u0c95\u0ccd-\u0c95\u0cbe\u0c9f\u0ccd\u0c9f\u0cbf (\u0c95\u0ca3\u0ccd\u0ca1\u0cc7\u0ca8\u0ccd)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d15\u0d23\u0d4d\u0d21\u0d47\u0d28\u0d4d \u0d15\u0d23\u0d4d\u0d21\u0d47\u0d28\u0d4d \u0d38\u0d40\u0d24\u0d48\u0d2f\u0d48\u0d15\u0d4d-\u0d15\u0d23\u0d4d\u0d21\u0d47\u0d28\u0d4d \u0d30\u0d3e\u0d18\u0d35\u0d3e"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d05\u0d23\u0d4d\u0d21\u0d30\u0d41\u0d19\u0d4d\u0d17\u0d4d-\u0d15\u0d3e\u0d23\u0d3e\u0d26 \u0d32\u0d19\u0d4d\u0d15\u0d3e\u0d2a\u0d41\u0d30\u0d24\u0d4d\u0d24\u0d3f\u0d32\u0d47 \u0d05\u0d30\u0d35\u0d3f\u0d28\u0d4d\u0d26 \u0d35\u0d47\u0d26\u0d3e\u0d35\u0d48\u0d24\u0d4d-\u0d24\u0d30\u0d35\u0d28\u0d4d\u0d26 \u0d2e\u0d3e\u0d24\u0d3e\u0d35\u0d48 (\u0d15\u0d23\u0d4d\u0d21\u0d47\u0d28\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d15\u0d3e\u0d35\u0d3f \u0d35\u0d3f\u0d34\u0d3f\u0d17\u0d33\u0d3f\u0d32\u0d4d \u0d09\u0d28\u0d4d \u0d09\u0d30\u0d41\u0d35\u0d47\u0d33\u0d3f \u0d2e\u0d3f\u0d28\u0d4d\u0d28\u0d15\u0d4d-\u0d15\u0d28\u0d3f\u0d35\u0d3e\u0d2f\u0d4d \u0d24\u0d28\u0d3f\u0d32\u0d47 \u0d09\u0d28\u0d4d \u0d24\u0d3f\u0d30\u0d41.\u0d23\u0d3e\u0d2e\u0d2e\u0d47 \u0d2a\u0d28\u0d4d\u0d28 <br/> \u0d06\u0d35\u0d3f\u0d24\u0d4d\u0d24\u0d41\u0d23\u0d48\u0d2f\u0d48\u0d2a\u0d4d-\u0d2a\u0d3f\u0d30\u0d3f\u0d28\u0d4d\u0d26 \u0d2e\u0d21 \u0d05\u0d28\u0d4d\u0d28\u0d2e\u0d4d \u0d05\u0d28\u0d3e\u0d33\u0d4d \u0d28\u0d3e\u0d28\u0d4d \u0d36\u0d4a\u0d32\u0d4d\u0d32\u0d41\u0d35\u0d26\u0d46\u0d28\u0d4d\u0d28 <br/> \u0d2a\u0d42\u0d35\u0d48\u0d24\u0d4d-\u0d24\u0d3f\u0d30\u0d3f\u0d36\u0d21\u0d48 \u0d28\u0d3f\u0d24\u0d4d\u0d24\u0d2e\u0d4d \u0d28\u0d3f\u0d24\u0d4d\u0d24\u0d2e\u0d4d \u0d36\u0d4a\u0d28\u0d4d\u0d28 \u0d2c\u0d41\u0d26\u0d4d\u0d27\u0d3f \u0d35\u0d34\u0d3f\u0d2f\u0d47 \u0d24\u0d28\u0d4d \u0d2c\u0d41\u0d27\u0d3f \u0d28\u0d3f\u0d32\u0d48 \u0d2e\u0d28\u0d4d\u0d28\u0d2a\u0d4d- <br/> \u0d2a\u0d3e\u0d35\u0d3f \u0d05\u0d30\u0d15\u0d4d\u0d15\u0d3f\u0d2f\u0d30\u0d4d \u0d15\u0d3e\u0d35\u0d32\u0d4d \u0d36\u0d3f\u0d30\u0d48\u0d26\u0d41\u0d28\u0d4d\u0d28\u0d2a\u0d4d-\u0d2a\u0d1e\u0d4d\u0d1c\u0d41 \u0d2a\u0d21\u0d3f\u0d28\u0d4d\u0d26 \u0d2a\u0d34\u0d28\u0d4d-\u0d1a\u0d3f\u0d24\u0d4d\u0d30\u0d2e\u0d4d \u0d0e\u0d28\u0d4d\u0d28 (\u0d15\u0d23\u0d4d\u0d21\u0d47\u0d28\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d2a\u0d28\u0d3f\u0d15\u0d4d\u0d15\u0d3e\u0d32 \u0d35\u0d3e\u0d30\u0d3f\u0d1c\u0d2e\u0d4d \u0d2a\u0d4b\u0d32 \u0d28\u0d3f\u0d30\u0d19\u0d4d\u0d17\u0d42\u0d36\u0d3f\u0d2a\u0d4d-\u0d2a\u0d18\u0d32\u0d4d \u0d12\u0d30\u0d41 <br/> \u0d2f\u0d41\u0d17\u0d2e\u0d3e\u0d17\u0d15\u0d4d-\u0d15\u0d34\u0d3f\u0d24\u0d4d\u0d24\u0d3e\u0d33\u0d47 \u0d2a\u0d3f\u0d30\u0d2f\u0d3e\u0d36\u0d3f <br/> \u0d28\u0d3f\u0d28\u0d48\u0d24\u0d4d\u0d24\u0d19\u0d4d\u0d17\u0d47 \u0d30\u0d3e\u0d35\u0d23\u0d28\u0d4d \u0d05\u0d28\u0d4d\u0d26\u0d3e\u0d28\u0d4d \u0d35\u0d30\u0d1a\u0d4d-\u0d1b\u0d40\u0d1a\u0d3f \u0d28\u0d3f\u0d32\u0d4d\u0d32\u0d21\u0d3e \u0d1a\u0d23\u0d4d\u0d21\u0d3e\u0d33\u0d3e \u0d0e\u0d28\u0d4d\u0d31\u0d47\u0d36\u0d3f \u0d24\u0d28\u0d3f\u0d24\u0d4d\u0d24\u0d41\u0d24\u0d4d\u0d24\u0d28\u0d4d <br/> \u0d09\u0d2f\u0d3f\u0d30\u0d4d\u0d24\u0d28\u0d48\u0d24\u0d4d-\u0d24\u0d3e\u0d28\u0d4d \u0d35\u0d3f\u0d21 \u0d2e\u0d39\u0d30\u0d3e\u0d1c\u0d3f \u0d36\u0d3e\u0d30\u0d41\u0d2e\u0d4d \u0d2a\u0d4a\u0d34\u0d41\u0d26\u0d41 \u0d15\u0d3e\u0d23\u0d41\u0d2e\u0d4d \u0d38\u0d2e\u0d2f\u0d2e\u0d3f\u0d26\u0d41\u0d35\u0d47 \u0d35\u0d3e\u0d36\u0d3f\u0d2f\u0d4d\u0d28\u0d3f\u0d24\u0d4d- <br/> \u0d24\u0d3e\u0d2e\u0d26\u0d2e\u0d4d \u0d36\u0d46\u0d2f\u0d4d\u0d2f\u0d32\u0d4d \u0d06\u0d18\u0d3e\u0d26\u0d46\u0d28\u0d4d\u0d31\u0d3f\u0d21\u0d30\u0d4d \u0d35\u0d40\u0d36\u0d3f \u0d30\u0d3e\u0d2e \u0d30\u0d3e\u0d2e \u0d30\u0d3e\u0d2e \u0d30\u0d3e\u0d2e \u0d0e\u0d28\u0d4d\u0d31\u0d46\u0d26\u0d3f\u0d30\u0d4d \u0d2a\u0d47\u0d36\u0d3f (\u0d15\u0d23\u0d4d\u0d21\u0d47\u0d28\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d05\u0d21\u0d32\u0d4d \u0d36\u0d47\u0d30\u0d41\u0d2e\u0d4d \u0d35\u0d3e\u0d32\u0d3f\u0d2f\u0d48 \u0d35\u0d3e\u0d28\u0d41\u0d32\u0d17\u0d3f\u0d32\u0d47 \u0d15\u0d42\u0d1f\u0d4d\u0d1f\u0d3f \u0d05\u0d35\u0d28\u0d3f\u0d2f\u0d48 \u0d38\u0d41\u0d17\u0d4d\u0d30\u0d40\u0d35\u0d28\u0d4d \u0d06\u0d32 \u0d2e\u0d41\u0d21\u0d3f \u0d36\u0d42\u0d1f\u0d4d\u0d1f\u0d3f <br/> \u0d09\u0d21\u0d28\u0d47 \u0d28\u0d40 \u0d24\u0d42\u0d26\u0d41 \u0d2a\u0d4b \u0d0e\u0d28\u0d4d\u0d31 \u0d36\u0d4a\u0d32\u0d4d \u0d05\u0d2e\u0d41\u0d26\u0d42\u0d1f\u0d4d\u0d1f\u0d3f \u0d09\u0d28\u0d4d \u0d0e\u0d34\u0d3f\u0d32\u0d4d \u0d2a\u0d3e\u0d30\u0d3e\u0d1f\u0d4d\u0d1f\u0d3f <br/> \u0d35\u0d3f\u0d21\u0d35\u0d28\u0d4d\u0d26 \u0d05\u0d28\u0d41\u0d2e\u0d28\u0d4d\u0d24\u0d28\u0d4d \u0d28\u0d3e\u0d28\u0d4d \u0d0e\u0d28\u0d4d\u0d31\u0d41 \u0d36\u0d40\u0d30\u0d3e\u0d1f\u0d4d\u0d1f\u0d3f \u0d35\u0d3f\u0d35\u0d30\u0d2e\u0d4d \u0d36\u0d4a\u0d32\u0d4d\u0d32 \u0d09\u0d2f\u0d3f\u0d30\u0d4d \u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3f\u0d30\u0d41\u0d15\u0d4d\u0d15\u0d3f\u0d30\u0d3e\u0d33\u0d4d \u0d36\u0d40\u0d2e\u0d3e\u0d1f\u0d4d\u0d1f\u0d3f <br/> \u0d26\u0d3f\u0d21\u0d2e\u0d3e \u0d32\u0d15\u0d4d\u0d37\u0d4d\u0d2e\u0d23\u0d28\u0d4d \u0d36\u0d46\u0d2f\u0d4d\u0d26 \u0d2a\u0d30\u0d4d\u0d23\u0d36\u0d3e\u0d32\u0d48 \u0d35\u0d40\u0d1f\u0d4d\u0d1f\u0d3f\u0d32\u0d4d \u0d26\u0d47\u0d35\u0d3e\u0d26\u0d3f \u0d26\u0d47\u0d35\u0d3e \u0d09\u0d28\u0d4d \u0d24\u0d3f\u0d30\u0d41\u0d35\u0d3e\u0d34\u0d3f\u0d24\u0d28\u0d3f\u0d15\u0d4d-\u0d15\u0d3e\u0d1f\u0d4d\u0d1f\u0d3f (\u0d15\u0d23\u0d4d\u0d21\u0d47\u0d28\u0d4d)"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0915\u0923\u094d\u0921\u0947\u0928\u094d \u0915\u0923\u094d\u0921\u0947\u0928\u094d \u0938\u0940\u0924\u0948\u092f\u0948\u0915\u094d-\