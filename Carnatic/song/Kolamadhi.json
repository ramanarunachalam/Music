u0c41\u0c28\u0c4d \u0c2e\u0c4b\u0c24\u0c41 \u0c24\u0c3f\u0c30\u0c48\u0c2e\u0c15\u0c30 <br/> \u0c35\u0c47\u0c32\u0c48 \u0c2f\u0c41\u0c32\u0c15\u0c48\u0c35\u0c32 \u0c2e\u0c3e\u0c15 \u0c35\u0c30\u0c41\u0c24\u0c41\u0c30\u0c15 \u0c2e\u0c2f\u0c3f\u0c32\u0c4d\u0c35\u0c40\u0c30\u0c3e"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 7", "T": "\u0c35\u0c40\u0c31\u0c41 \u0c15\u0c32\u0c3f\u0c1a\u0c48\u0c35\u0c30\u0c41 \u0c1a\u0c47\u0c35 \u0c15\u0c28\u0c24\u0c3f\u0c24\u0c2f <br/> \u0c2e\u0c47\u0c35\u0c41 \u0c2e\u0c41\u0c24\u0c32\u0c4d\u0c35\u0c35\u0c2f\u0c32\u0c4d \u0c35\u0c3e\u0c35\u0c3f \u0c2a\u0c41\u0c1f\u0c48\u0c2e\u0c30\u0c41\u0c35\u0c41 <br/> \u0c35\u0c40\u0c30\u0c48 \u0c35\u0c30\u0c41\u0c2a\u0c34\u0c28\u0c3f \u0c1e\u0c3e\u0c28 \u0c2e\u0c32\u0c48\u0c2f\u0c3f\u0c32\u0c4d\u0c35\u0c33\u0c30\u0c4d \u0c2a\u0c46\u0c30\u0c41\u0c2e\u0c3e\u0c33\u0c47"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0ccb\u0cb2 \u0cae\u0ca4\u0cbf\u0cb5\u0ca4\u0ca8\u0cae\u0ccd \u0cb5\u0cc7\u0cb0\u0ccd\u0cb5\u0cc1 \u0ca4\u0cb0\u0cbe\u0cb3\u0c95 <br/> \u0caa\u0cbe\u0cb0\u0cae\u0ccd \u0ca8\u0cc6\u0c95\u0cbf\u0cb3\u0cb5\u0cbf\u0cb3\u0cbf \u0cb5\u0cc7\u0cb2\u0ccd\u0c95\u0cb3\u0ccd \u0c9a\u0cc1\u0cb3\u0cb2\u0ca8\u0cc1\u0cb5\u0cb2\u0ccd <br/> \u0c95\u0ccb\u0cb5\u0cc8 \u0caf\u0cbf\u0ca4\u0cb3\u0ccd\u0cb5\u0cc6\u0cb3\u0cbf\u0cb1 \u0cb5\u0cbe\u0caf\u0ccd\u0cae\u0cc8 \u0caa\u0ca4\u0cb1\u0cbf\u0caf\u0cbf\u0cb3 \u0cae\u0cc1\u0c95\u0cc8\u0caf\u0cbe\u0ca8"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c95\u0ccb\u0c95 \u0ca8\u0c95\u0cb5\u0cc1\u0caa\u0caf \u0cae\u0cc7\u0cb0\u0cc1 \u0cae\u0cc1\u0cb2\u0cc8\u0caf\u0c9a\u0cc8\u0caf <br/> \u0ca8\u0cc2\u0cb2\u0cbf \u0ca8\u0cbf\u0c9f\u0cc8\u0ca4\u0cc1\u0cb5\u0cb3 \u0cb5\u0cc0\u0cb1\u0cc1 \u0caa\u0cb1\u0cb5\u0cc8\u0cb5\u0c95\u0cc8 <br/> \u0c95\u0cc2\u0cb1 \u0caf\u0cbf\u0ca8\u0cbf\u0caf\u0c95\u0cb3 \u0cae\u0ccb\u0cb2 \u0cae\u0cbf\u0c9f\u0cb5\u0cb3\u0cc8\u0c95\u0cb3\u0ccd \u0c95\u0cb0\u0cae\u0cc0\u0ca4\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c95\u0cbe\u0cb2\u0cbf \u0ca8\u0ca3\u0cbf\u0c95\u0ca8\u0c95 \u0ca8\u0cc2\u0caa\u0cc1 \u0cb0\u0cae\u0cc1\u0cae\u0cca\u0cb2\u0cbf\u0c95 <br/> \u0cb3\u0ccb\u0cb2 \u0cae\u0cbf\u0c9f\u0cbe\u0ca4\u0cbf\u0c95 \u0caa\u0ccb\u0c95 \u0cae\u0ca4\u0cc1\u0cae\u0cb0\u0cc1\u0cb5\u0cc1 <br/> \u0c95\u0cbe\u0cb2\u0cc8 \u0cb5\u0cc6\u0c95\u0cc1\u0c9a\u0cb0\u0c9a \u0cb2\u0cc0\u0cb2\u0cc8 \u0caf\u0cb3\u0cb5\u0cc1\u0c9a\u0cc6\u0caf\u0cc1 \u0cae\u0c9f\u0cae\u0cbe\u0ca8\u0cbe\u0cb0\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c95\u0cbe\u0ca4\u0cb2\u0ccd \u0caa\u0cc1\u0cb0\u0cbf\u0caf\u0cc1\u0cae\u0ca8\u0cc1 \u0caa\u0ccb\u0c95 \u0ca8\u0ca4\u0cbf\u0caf\u0cbf\u0ca8\u0cbf\u0c9f\u0cc8 <br/> \u0cb5\u0cc0\u0cb3\u0cc1 \u0c95\u0cbf\u0ca8\u0cc1\u0cae\u0c9f\u0cbf\u0cae\u0cc8 \u0cae\u0ccb\u0c9a \u0cae\u0cb1\u0cb5\u0cc1\u0ca8\u0ca4\u0cc1 <br/> \u0c95\u0cbe\u0cae\u0cb0\u0ccd \u0c95\u0cb3\u0cb2\u0cbf\u0ca3\u0cc8\u0c95 \u0cb3\u0cbe\u0ca8 \u0ca4\u0cca\u0cb0\u0cc1\u0c9a\u0cbf\u0cb1\u0cbf\u0ca4\u0cc1 \u0cae\u0cb1\u0cb5\u0cc7\u0ca8\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c9e\u0cbe\u0cb2 \u0cae\u0cc1\u0cb3\u0cc1\u0ca4\u0cc1\u0cae\u0cae \u0cb0\u0ccb\u0cb0\u0ccd\u0c95\u0cb3\u0ccd \u0caa\u0cc1\u0cb0\u0cbf\u0caf\u0cc1\u0cae\u0cbf\u0c95 <br/> \u0cb2\u0cbe\u0c95 \u0cb5\u0cb0\u0cc1\u0cae\u0cb5\u0cc1\u0ca3\u0cb0\u0ccd \u0c9a\u0cc7\u0cb0 \u0cb5\u0cc1\u0ca4\u0ca4\u0cbf\u0caf\u0cbf\u0c9f\u0cc8 <br/> \u0ca8\u0cbe\u0c9a \u0cae\u0cc1\u0cb1\u0cbe\u0cae\u0cb0\u0ccd\u0c9a\u0cc6\u0caf\u0ccd \u0cb5\u0cc0\u0cb0 \u0ca4\u0cb0\u0c95\u0cc1\u0cae\u0cb0 \u0cae\u0cc1\u0cb0\u0cc1\u0c95\u0ccb\u0ca8\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0ca8\u0cbe\u0c9f\u0cbf \u0caf\u0cca\u0cb0\u0cc1\u0c95\u0cc1\u0cb1\u0cae\u0cbf\u0ca8\u0ccd \u0cae\u0cc7\u0cb5\u0cc1 \u0ca4\u0cbf\u0ca8\u0cc8\u0c9a\u0cc6\u0caf\u0ccd\u0caa\u0cc1\u0ca8 <br/> \u0cae\u0cc0\u0ca4\u0cbf \u0cb2\u0cbf\u0caf\u0cb2\u0c95\u0cb2\u0ccd\u0c95\u0cb2\u0ccd \u0ca8\u0cc0\u0cb3 \u0cb2\u0cbf\u0c9f\u0cc8\u0ca8\u0cbf\u0