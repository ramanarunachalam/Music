0cbe\u0cb5\u0ca4\u0cbe\u0cb0\u0ccb \u0cb5\u0cc7\u0ca6\u0cb8\u0ccd\u0ca4\u0cc7\u0caf \u0ca6\u0cc1\u0cb7\u0ccd\u0ca4\u0cb9\u0cb0\u0ccb \u0cb5\u0cc7\u0ca6\u0cbe\u0ca6\u0cbf \u0cb0\u0c95\u0ccd\u0cb7\u0ca3\u0cb9\u0ccd \u0cb6\u0ccd\u0cb0\u0cc0 (\u0ca3\u0ca4)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c97\u0ccb\u0cb5\u0cbf\u0ca6\u0cae\u0ccd \u0ca8\u0cae\u0cbe\u0cae\u0ccd\u0caf\u0cb9\u0cae\u0ccd \u0c97\u0cc1\u0cb0\u0cc1\u0c97\u0cc1\u0cb9\u0ca8\u0cc1\u0ca4\u0c95\u0cc2\u0cb0\u0ccd\u0cae\u0cbe\u0cb5\u0ca4\u0cbe\u0cb0\u0cae\u0ccd \u0ca6\u0cc7\u0cb5\u0cb0\u0cbe\u0c9c\u0cbe\u0ca6\u0cbf \u0caa\u0cc2\u0c9c\u0cbf\u0ca4\u0cae\u0ccd \u0ca6\u0cbf\u0cb5\u0ccd\u0caf\u0cbe\u0cae\u0cc3\u0ca4\u0caa\u0ccd\u0cb0\u0ca6\u0cae\u0ccd (\u0c97\u0ccc\u0cb3)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0cb6\u0ccd\u0cb0\u0cc0\u0ca7\u0cb0\u0cc7\u0ca3 \u0cb0\u0c95\u0ccd\u0cb7\u0cbf\u0ca4\u0ccb\u0cb9\u0cae\u0ccd \u0cad\u0cc2\u0cae\u0cbf\u0caa\u0cbe\u0cb2 \u0cb8\u0cc2\u0c95\u0cb0\u0cc7\u0ca3 \u0cb9\u0cbf\u0ca4 \u0ca6\u0cc7\u0cb5\u0ccb\u0caa\u0c95\u0cbe\u0cb0\u0cc7\u0ca3 \u0cb9\u0caf\u0cbe\u0cb8\u0cc1\u0cb0\u0cb9\u0ca4\u0cbf \u0ca8\u0cbf\u0caa\u0cc1\u0ca3\u0cc7\u0ca8 (\u0cb6\u0ccd\u0cb0\u0cc0)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0ca8\u0cb0\u0cb8\u0cbf\u0cb9\u0ccd\u0cae\u0cbe\u0caf \u0ca8\u0cae\u0cb8\u0ccd\u0ca4\u0cc7 \u0caa\u0ccd\u0cb0\u0cb9\u0ccd\u0cb2\u0cbe\u0ca6 \u0caa\u0ccd\u0cb0\u0cbe\u0cb0\u0ccd\u0ca5\u0cbf\u0ca4\u0cbe\u0caf \u0cb9\u0cbf\u0cb0\u0ca3\u0ccd\u0caf \u0caa\u0ccd\u0cb0\u0cbe\u0ca3\u0cb9\u0cb0\u0cbe\u0caf \u0cb9\u0cb0\u0cbf\u0cb9\u0caf\u0cbe\u0ca6\u0cbf \u0cb5\u0ca8\u0ccd\u0ca6\u0cbf\u0ca4\u0cbe\u0caf (\u0c86\u0cb0\u0cad\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0cb5\u0cbe\u0cae\u0ca8\u0cbe\u0ca6\u0ca8\u0ccd\u0caf\u0cae\u0ccd \u0ca8 \u0c9c\u0cbe\u0ca8\u0cc7\u0cb9\u0cae\u0ccd \u0ca8\u0cb0\u0cb5\u0cb0 \u0cb0\u0cc2\u0caa\u0cbf\u0ca3\u0cb9\u0ccd \u0cb6\u0cc1\u0c95\u0ccd\u0cb0 \u0cac\u0cb2\u0cbf\u0cb0\u0cbe\u0c9c\u0cbe\u0ca6\u0cbf \u0cb6\u0cc