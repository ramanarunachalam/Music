u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b95\u0bc1\u0bb0\u0bc1 \u0b95\u0bc1\u0bb9 \u0b9a\u0bbe\u0bae\u0bb0 \u0baa\u0bb0\u0ba3\u0bae\u0bcd <br/> \u0b95\u0bc1\u0ba3 \u0ba4\u0bcb\u0bb7 \u0b9c\u0bbf\u0ba4\u0bbe\u0baa\u0bb0\u0ba3\u0bae\u0bcd <br/> \u0b95\u0bcd\u0bb0\u0bb9\u0ba3\u0bbe\u0ba4\u0bbf \u0b95\u0bbe\u0bb0\u0bcd\u0baf \u0b95\u0bbe\u0bb0\u0ba3\u0bae\u0bcd <br/> \u0b95\u0bcd\u0bb0\u0bb9\u0bbe\u0baa-\u0b9a\u0bb5\u0bcd\u0baf \u0b9a\u0ba9\u0bcd\u0b9a\u0bbe\u0bb0\u0bbf\u0ba3\u0bae\u0bcd"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2e\u0c39\u0c3e \u0c38\u0c41\u0c30\u0c02 \u0c15\u0c47\u0c24\u0c41\u0c2e\u0c39\u0c02 \u0c2d\u0c1c\u0c3e\u0c2e\u0c3f <br/> \u0c1b\u0c3e\u0c2f\u0c3e \u0c17\u0c4d\u0c30\u0c39 \u0c35\u0c30\u0c2e\u0c4d"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2e\u0c39\u0c3e \u0c35\u0c3f\u0c1a\u0c3f\u0c24\u0c4d\u0c30 \u0c2e\u0c15\u0c41\u0c1f \u0c27\u0c30\u0c02 <br/> \u0c2e\u0c02\u0c17\u0c33 \u0c35\u0c38\u0c4d\u0c24\u0c4d\u0c30\u0c3e\u0c26\u0c3f \u0c27\u0c30\u0c02"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c28\u0c30 \u0c2a\u0c40\u0c20 \u0c38\u0c4d\u0c25\u0c3f\u0c24\u0c02 \u0c38\u0c41\u0c16\u0c02 <br/> \u0c28\u0c35 \u0c17\u0c4d\u0c30\u0c39 \u0c2f\u0c41\u0c24\u0c02 \u0c38\u0c16\u0c2e\u0c4d"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c15\u0c47\u0c24\u0c41\u0c02-\u0c15\u0c43\u0c23\u0c4d\u0c35\u0c28\u0c4d-\u0c2e\u0c28\u0c4d\u0c24\u0c4d\u0c30\u0c3f\u0c23\u0c02 <br/> \u0c15\u0c4d\u0c30\u0c4b\u0c27 \u0c28\u0c3f\u0c27\u0c3f \u0c1c\u0c48\u0c2e\u0c3f\u0c28\u0c02 <br/> \u0c15\u0c41\u0c32\u0c41\u0c24\u0c4d\u0c25\u0c3e\u0c26\u0c3f \u0c2d\u0c15\u0c4d\u0c37\u0c23\u0c02 <br/> \u0c15\u0c4b\u0c23 \u0c27\u0c4d\u0c35\u0c1c \u0c2a\u0c24\u0c3e\u0c15\u0c3f\u0c28\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c17\u0c41\u0c30\u0c41 \u0c17\u0c41\u0c39 \u0c1a\u0c3e\u0c2e\u0c30 \u0c2d\u0c30\u0c23\u0c02 <br/> \u0c17\u0c41\u0c23 \u0c26\u0c4b\u0c37 \u0c1c\u0c3f\u0c24\u0c3e\u0c2d\u0c30\u0c23\u0c02 <br/> \u0c17\u0c4d\u0c30\u0c39\u0c23\u0c3e\u0c26\u0c3f \u0c15\u0c3e\u0c30\u0c4d\u0c2f \u0c15\u0c3e\u0c30\u0c23\u0c02 <br/> \u0c17\u0c4d\u0c30\u0c39\u0c3e\u0c2a-\u0c38\u0c35\u0c4d\u0c2f \u0c38\u0c28\u0c4d\u0c1a\u0c3e\u0c30\u0c3f\u0c23\u0c2e\u0c4d"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0cb9\u0cbe \u0cb8\u0cc1\u0cb0\u0c82 \u0c95\u0cc7\u0ca4\u0cc1\u0cae\u0cb9\u0c82 \u0cad\u0c9c\u0cbe\u0cae\u0cbf <br/> \u0c9b\u0cbe\u0caf\u0cbe \u0c97\u0ccd\u0cb0\u0cb9 \u0cb5\u0cb0\u0cae\u0ccd"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0cb9\u0cbe \u0cb5\u0cbf\u0c9a\u0cbf\u0ca4\u0ccd\u0cb0 \u0cae\u0c95\u0cc1\u0c9f \u0ca7\u0cb0\u0c82 <br/> \u0cae\u0c82\u0c97\u0cb3 \u0cb5\u0cb8\u0ccd\u0ca4\u0ccd\u0cb0\u0cbe\u0ca6\u0cbf \u0ca7\u0cb0\u0c82"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0ca8\u0cb0 \u0caa\u0cc0\u0ca0 \u0cb8\u0ccd\u0ca5\u0cbf\u0ca4\u0c82 \u0cb8\u0cc1\u0c96\u0c82 <br/> \u0ca8\u0cb5 \u0c97\u0ccd\u0cb0\u0cb9 \u0caf\u0cc1\u0ca4\u0c82 \u0cb8\u0c96\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0c95\u0cc7\u0ca4\u0cc1\u0c82-\u0c95\u0cc3\u0ca3\u0ccd\u0cb5\u0ca8\u0ccd-\u0cae\u0ca8\u0ccd\u0ca4\u0ccd\u0cb0\u0cbf\u0ca3\u0c82 <br/> \u0c95\u0ccd\u0cb0\u0ccb\u0ca7 \u0ca8\u0cbf\u0ca7\u0cbf \u0c9c\u0cc8\u0cae\u0cbf\u0ca8\u0c82 <br/> \u0c95\u0cc1\u0cb2\u0cc1\u0ca4\u0ccd\u0ca5\u0cbe\u0ca6\u0cbf \u0cad\u0c95\u0ccd\u0cb7\u0ca3\u0c82 <br/> \u0c95\u0ccb\u0ca3 \u0ca7\u0ccd\u0cb5\u0c9c \u0caa\u0ca4\u0cbe\u0c95\u0cbf\u0ca8\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c97\u0cc1\u0cb0\u0cc1 \u0c97\u0cc1\u0cb9 \u0c9a\u0cbe\u0cae\u0cb0 \u0cad\u0cb0\u0ca3\u0c82 <br/> \u0c97\u0cc1\u0ca3 \u0ca6\u0ccb\u0cb7 \u0c9c\u0cbf\u0ca4\u0cbe\u0cad\u0cb0\u0ca3\u0c82 <br/> \u0c97\u0ccd\u0cb0\u0cb9\u0ca3\u0cbe\u0ca6\u0cbf \u0c95\u0cbe\u0cb0\u0ccd\u0caf \u0c95\u0cbe\u0cb0\u0ca3\u0c82 <br/> \u0c97\u0ccd\u0cb0\u0cb9\u0cbe\u0caa-\u0cb8\u0cb5\u0ccd\u0caf \u0cb8\u0ca8\u0ccd\u0c9a\u0cbe\u0cb0\u0cbf\u0ca3\u0cae\u0ccd"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d39\u0d3e \u0d38\u0d41\u0d30\u0d02 \u0d15\u0d47\u0d24\u0d41\u0d2e\u0d39\u0d02 \u0d2d\u0d1c\u0d3e\u0d2e\u0d3f <br/> \u0d1b\u0d3e\u0d2f\u0d3e \u0d17\u0d4d\u0d30\u0d39 \u0d35\u0d30\u0d2e\u0d4d"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d39\u0d3e \u0d35\u0d3f\u0d1a\u0d3f\u0d24\u0d4d\u0d30 \u0d2e\u0d15\u0d41\u0d1f \u0d27\u0d30\u0d02 <br/> \u0d2e\u0d02\u0d17\u0d33 \u0d35\u0d38\u0d4d\u0d24\u0d4d\u0d30\u0d3e\u0d26\u0d3f \u0d27\u0d30\u0d02"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d28\u0d30 \u0d2a\u0d40\u0d20 \u0d38\u0d4d\u0d25\u0d3f\u0d24\u0d02 \u0d38\u0d41\u0d16\u0d02 <br/> \u0d28\u0d35 \u0d17\u0d4d\u0d30\u0d39 \u0d2f\u0d41\u0d24\u0d02 \u0d38\u0d16\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d15\u0d47\u0d24\u0d41\u0d02-\u0d15\u0d43\u0d23\u0d4d\u0d35\u0d28\u0d4d-\u0d2e\u0d28\u0d4d\u0d24\u0d4d\u0d30\u0d3f\u0d23\u0d02 <br/> \u0d15\u0d4d\u0d30\u0d4b\u0d27 \u0d28\u0d3f\u0d27\u0d3f \u0d1c\u0d48\u0d2e\u0d3f\u0d28\u0d02 <br/> \u0d15\u0d41\u0d32\u0d41\u0d24\u0d4d\u0d25\u0d3e\u0d26\u0d3f \u0d2d\u0d15\u0d4d\u0d37\u0d23\u0d02 <br/> \u0d15\u0d4b\u0d23 \u0d27\u0d4d\u0d35\u0d1c \u0d2a\u0d24\u0d3e\u0d15\u0d3f\u0d28\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d17\u0d41\u0d30\u0d41 \u0d17\u0d41\u0d39 \u0d1a\u0d3e\u0d2e\u0d30 \u0d2d\u0d30\u0d23\u0d02 <br/> \u0d17\u0d41\u0d23 \u0d26\u0d4b\u0d37 \u0d1c\u0d3f\u0d24\u0d3e\u0d2d\u0d30\u0d23\u0d02 <br/> \u0d17\u0d4d\u0d30\u0d39\u0d23\u0d3e\u0d26\u0d3f \u0d15\u0d3e\u0d30\u0d4d\u0d2f \u0d15\u0d3e\u0d30\u0d23\u0d02 <br/> \u0d17\u0d4d\u0d30\u0d39\u0d3e\u0d2a-\u0d38\u0d35\u0d4d\u0d2f \u0d38\u0d28\u0d4d\u0d1a\u0d3e\u0d30\u0d3f\u0d23\u0d2e\u0d4d"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092e\u0939\u093e \u0938\u0941\u0930\u0902 \u0915\u0947\u0924\u0941\u092e\u0939\u0902 \u092d\u091c\u093e\u092e\u093f <br/> \u091b\u093e\u092f\u093e \u0917\u094d\u0930\u0939 \u0935\u0930\u092e\u094d"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092e\u0939\u093e \u0935\u093f\u091a\u093f\u0924\u094d\u0930 \u092e\u0915\u0941\u091f \u0927\u0930\u0902 <br/> \u092e\u0902\u0917\u0932 \u0935\u0938\u094d\u0924\u094d\u0930\u093e\u0926\u093f \u0927\u0930\u0902"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0928\u0930 \u092a\u0940\u0920 \u0938\u094d\u0925\u093f\u0924\u0902 \u0938\u0941\u0916\u0902 <br/> \u0928\u0935 \u0917\u094d\u0930\u0939 \u092f\u0941\u0924\u0902 \u0938\u0916\u092e\u094d"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0915\u0947\u0924\u0941\u0902-\u0915\u0943\u0923\u094d\u0935\u0928\u094d-\u092e\u0928\u094d\u0924\u094d\u0930\u093f\u0923\u0902 <br/> \u0915\u094d\u0930\u094b\u0927 \u0928\u093f\u0927\u093f \u091c\u0948\u092e\u093f\u0928\u0902 <br/> \u0915\u0941\u0932\u0941\u0924\u094d\u0925\u093e\u0926\u093f \u092d\u0915\u094d\u0937\u0923\u0902 <br/> \u0915\u094b\u0923 \u0927\u094d\u0935\u091c \u092a\u0924\u093e\u0915\u093f\u0928\u092e\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0917\u0941\u0930\u0941 \u0917\u0941\u0939 \u091a\u093e\u092e\u0930 \u092d\u0930\u0923\u0902 <br/> \u0917\u0941\u0923 \u0926\u094b\u0937 \u091c\u093f\u0924\u093e\u092d\u0930\u0923\u0902 <br/> \u0917\u094d\u0930\u0939\u0923\u093e\u0926\u093f \u0915\u093e\u0930\u094d\u092f \u0915\u093e\u0930\u0923\u0902 <br/> \u0917\u094d\u0930\u0939\u093e\u092a-\u0938\u0935\u094d\u092f \u0938\u0928\u094d\u091a\u093e\u0930\u093f\u0923\u092e\u094d"}]}], "L": "l", "lyricsref": [{"links": [{"N": "guru-guha.blogspot.com", "L": "http://guru-guha.blogspot.com/2008/04/dikshitar-kriti-mahasuram-ketum-raga.html"}, {"N": "www.medieval.org", "L": "http://www.medieval.org/music/world/carnatic/lyrics/navagraha.html#ketu"}]}]}