u0bb0\u0bbf <br/> \u0b85\u0bae\u0bcd\u0baa\u0bbf\u0b95\u0bbe \u0baa\u0bcd\u0bb0\u0bc1\u0bb9\u0ba9\u0bcd-\u0ba8\u0bbe\u0baf\u0b95\u0bbf \u0b95\u0bcc\u0bb0\u0bc0 (\u0bae\u0bb9\u0bbf\u0bb2\u0bcb)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0b9a\u0bbe\u0bb0\u0b9a \u0ba4\u0bb3 \u0ba8\u0baf\u0ba9\u0bbe \u0bb5\u0ba4\u0ba9 <br/> \u0b9a\u0bb0\u0bcb\u0b9c \u0bb5\u0bbf\u0b9c\u0bbf\u0ba4 \u0b95\u0ba9 \u0baa\u0bc1\u0bb0\u0bcd(\u0b86)\u0bb0\u0bbf\u0bb5\u0bc8 \u0bb0\u0bbe\u0ba3\u0bbf <br/> \u0bb5\u0bb0\u0bbe\u0bb2\u0bcd(\u0b92)\u0b9a\u0b95\u0bbf\u0bae\u0bcd\u0baa\u0bb5\u0bc7 <br/> \u0baa\u0bbf\u0bb0\u0bbe\u0ba9 \u0baa\u0bcd\u0bb0\u0bcb\u0b9a\u0bbf\u0ba9 \u0baa\u0bb0\u0bbe \u0b9a\u0b95\u0bcd\u0ba4\u0bbf \u0b95\u0ba4\u0bbe (\u0bae\u0bb9\u0bbf\u0bb2\u0bcb)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b9a\u0bc1\u0bae\u0bc7\u0bb0\u0bc1 \u0bae\u0ba4\u0bcd\u0baf \u0bb2\u0ba4\u0bbe \u0bb2\u0bb2\u0bbf\u0ba4\u0bbe <br/> \u0bb0\u0bae\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1 \u0bb5\u0bbf\u0ba9\u0bc1 \u0ba8\u0bc7 \u0b95\u0bc1\u0bae\u0bbe\u0bb0\u0bc1\u0b9f\u0ba9\u0bcd(\u0b85)\u0ba8\u0bc1\u0b9a\u0bc1 <br/> \u0bae\u0bbe \u0ba4\u0bb2\u0bcd\u0bb2\u0bc0 \u0ba8\u0bc0\u0bb5\u0bc1 \u0bae\u0bbe\u0ba9\u0bb5\u0ba4\u0bc0 \u0ba8\u0bc0 <br/> \u0b9a\u0bae\u0bbe\u0ba9\u0bae\u0bcd(\u0b8e)\u0bb5\u0bb0\u0bc1 \u0b87\u0bb2\u0bb2\u0bcb \u0b9c\u0bc6\u0baa\u0bcd\u0baa\u0bcd(\u0b85\u0bb5\u0bcd)\u0b85\u0bae\u0bcd\u0bae\u0bbe (\u0bae\u0bb9\u0bbf\u0bb2\u0bcb)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0b9a\u0bbe\u0bae \u0b95\u0bbe\u0ba9 \u0ba8\u0bc1\u0ba4\u0bbe \u0b9a\u0bb0\u0b9a \u0baa\u0ba4\u0bbe <br/> \u0ba4\u0bbe\u0bae\u0b9a\u0bae\u0bc1 \u0b9c\u0bc7\u0b9a\u0bc7 \u0ba4\u0bbe\u0bb3\u0ba9\u0bc7 <br/> \u0bb6\u0bcd\u0baf\u0bbe\u0bae \u0b95\u0bcd\u0bb0\u0bc1\u0bb7\u0bcd\u0ba3 \u0b9a\u0bcb\u0ba4\u0bb0\u0bc0 \u0b9a\u0ba9\u0bcd\u0b95\u0bb0\u0bc0 <br/> \u0bb6\u0bcd\u0baf\u0bbe\u0bae\u0bb3\u0bcd(\u0b86)\u0bae\u0bcd\u0baa\u0bbe \u0ba8\u0bc0\u0bb5\u0bc7 \u0bae\u0bbe(\u0baf\u0bcd)\u0b85\u0bae\u0bcd\u0bae (\u0bae\u0bb9\u0bbf\u0bb2\u0bcb)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2e\u0c39\u0c3f\u0c32\u0c4b \u0c05\u0c2e\u0c4d\u0c2c\u0c3e \u0c28\u0c40\u0c26\u0c41 <br/> \u0c2e\u0c39\u0c3f\u0c2e\u0c4d(\u0c06)\u0c24\u0c3f\u0c36\u0c2f\u0c2e\u0c4d(\u0c0e)\u0c28\u0c4d\u0c28 \u0c24\u0c30\u0c2e\u0c3e"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c05\u0c39\u0c3f \u0c2d\u0c42\u0c37\u0c23\u0c41\u0c28\u0c3f \u0c30\u0c3e\u0c23\u0c40 \u0c36\u0c28\u0c4d\u0c15\u0c30\u0c3f <br/> \u0c05\u0c2e\u0c4d\u0c2c\u0c3f\u0c15\u0c3e \u0c2c\u0c43\u0c39\u0c28\u0c4d-\u0c28\u0c3e\u0c2f\u0c15\u0c3f \u0c17\u0c4c\u0c30\u0c40 (\u0c2e\u0c39\u0c3f\u0c32\u0c4b)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c38\u0c3e\u0c30\u0c38 \u0c26\u0c33 \u0c28\u0c2f\u0c28\u0c3e \u0c35\u0c26\u0c28 <br/> \u0c38\u0c30\u0c4b\u0c1c \u0c35\u0c3f\u0c1c\u0c3f\u0c24 \u0c18\u0c28 \u0c2a\u0c41\u0c30\u0c4d(\u0c06)\u0c30\u0c3f\u0c35\u0c48 \u0c30\u0c3e\u0c23\u0c3f <br/> \u0c35\u0c30\u0c3e\u0c32\u0c4d(\u0c12)\u0c38\u0c17\u0c3f\u0c2e\u0c4d\u0c2a\u0c35\u0c47 <br/> \u0c2c\u0c3f\u0c30\u0c3e\u0c28 \u0c2c\u0c4d\u0c30\u0c4b\u0c1a\u0c3f\u0c28 \u0c2a\u0c30\u0c3e \u0c36\u0c15\u0c4d\u0c24\u0c3f \u0c17\u0c26\u0c3e (\u0c2e\u0c39\u0c3f\u0c32\u0c4b)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c38\u0c41\u0c2e\u0c47\u0c30\u0c41 \u0c2e\u0c27\u0c4d\u0c2f \u0c32\u0c24\u0c3e \u0c32\u0c32\u0c3f\u0c24\u0c3e <br/> \u0c30\u0c2e\u0c3f\u0c28\u0c4d\u0c1a\u0c41 \u0c35\u0c3f\u0c28\u0c41 \u0c28\u0c47 \u0c15\u0c41\u0c2e\u0c3e\u0c30\u0c41\u0c21\u0c28\u0c4d(\u0c05)\u0c28\u0c41\u0c1a\u0c41 <br/> \u0c2e\u0c3e \u0c24\u0c32\u0c4d\u0c32\u0c40 \u0c28\u0c40\u0c35\u0c41 \u0c2e\u0c3e\u0c28\u0c35\u0c24\u0c40 \u0c28\u0c40 <br/> \u0c38\u0c2e\u0c3e\u0c28\u0c2e\u0c4d(\u0c0e)\u0c35\u0c30\u0c41 \u0c07\u0c32\u0c32\u0c4b \u0c1c\u0c46\u0c2a\u0c4d\u0c2a\u0c4d(\u0c05\u0c35\u0c4d)\u0c05\u0c2e\u0c4d\u0c2e\u0c3e (\u0c2e\u0c39\u0c3f\u0c32\u0c4b)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c38\u0c3e\u0c2e \u0c17\u0c3e\u0c28 \u0c28\u0c41\u0c24\u0c3e \u0c38\u0c30\u0c38 \u0c2a\u0c26\u0c3e <br/> \u0c24\u0c3e\u0c2e\u0c38\u0c2e\u0c41 \u0c1c\u0c47\u0c38\u0c47 \u0c24\u0c3e\u0c33\u0c28\u0c47 <br/> \u0c36\u0c4d\u0c2f\u0c3e\u0c2e \u0c15\u0c43\u0c37\u0c4d\u0c23 \u0c38\u0c4b\u0c26\u0c30\u0c40 \u0c36\u0c28\u0c4d\u0c15\u0c30\u0c40 <br/> \u0c36\u0c4d\u0c2f\u0c3e\u0c2e\u0c33\u0c4d(\u0c06)\u0c2e\u0c4d\u0c2c\u0c3e \u0c28\u0c40\u0c35\u0c47 \u0c2e\u0c3e(\u0c2f\u0c4d)\u0c05\u0c2e\u0c4d\u0c2e (\u0c2e\u0c39\u0c3f\u0c32\u0c4b)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0cb9\u0cbf\u0cb2\u0ccb \u0c85\u0cae\u0ccd\u0cac\u0cbe \u0ca8\u0cc0\u0ca6\u0cc1 <br/> \u0cae\u0cb9\u0cbf\u0cae\u0ccd(\u0c86)\u0ca4\u0cbf\u0cb6\u0caf\u0cae\u0ccd(\u0c8e)\u0ca8\u0ccd\u0ca8 \u0ca4\u0cb0\u0cae\u0cbe"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c85\u0cb9\u0cbf \u0cad\u0cc2\u0cb7\u0ca3\u0cc1\u0ca8\u0cbf \u0cb0\u0cbe\u0ca3\u0cc0 \u0cb6\u0ca8\u0ccd\u0c95\u0cb0\u0cbf <br/> \u0c85\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cbe \u0cac\u0cc3\u0cb9\u0ca8\u0ccd-\u0ca8\u0cbe\u0caf\u0c95\u0cbf \u0c97\u0ccc\u0cb0\u0cc0 (\u0cae\u0cb9\u0cbf\u0cb2\u0ccb)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cb8\u0cbe\u0cb0\u0cb8 \u0ca6\u0cb3 \u0ca8\u0caf\u0ca8\u0cbe \u0cb5\u0ca6\u0ca8 <br/> \u0cb8\u0cb0\u0ccb\u0c9c \u0cb5\u0cbf\u0c9c\u0cbf\u0ca4 \u0c98\u0ca8 \u0caa\u0cc1\u0cb0\u0ccd(\u0c86)\u0cb0\u0cbf\u0cb5\u0cc8 \u0cb0\u0cbe\u0ca3\u0cbf <br/> \u0cb5\u0cb0\u0cbe\u0cb2\u0ccd(\u0c92)\u0cb8\u0c97\u0cbf\u0cae\u0ccd\u0caa\u0cb5\u0cc7 <br/> \u0cac\u0cbf\u0cb0\u0cbe\u0ca8 \u0cac\u0ccd\u0cb0\u0ccb\u0c9a\u0cbf\u0ca8 \u0caa\u0cb0\u0cbe \u0cb6\u0c95\u0ccd\u0ca4\u0cbf \u0c97\u0ca6\u0cbe (\u0cae\u0cb9\u0cbf\u0cb2\u0ccb)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0cb8\u0cc1\u0cae\u0cc7\u0cb0\u0cc1 \u0cae\u0ca7\u0ccd\u0caf \u0cb2\u0ca4\u0cbe \u0cb2\u0cb2\u0cbf\u0ca4\u0cbe <br/> \u0cb0\u0cae\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1 \u0cb5\u0cbf\u0ca8\u0cc1 \u0ca8\u0cc7 \u0c95\u0