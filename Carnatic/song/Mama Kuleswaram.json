u0cae\u0cbf\u0ca4\u0ca8\u0ca4 \u0c9c\u0ca8 \u0ca6\u0cc1\u0cb0\u0cbf\u0ca4\u0cae\u0ccd \u0ca8\u0cc0\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cb0 \u0cb6\u0ccb\u0cad\u0cbf\u0ca4\u0cae\u0ccd \u0c85\u0cae\u0cbf\u0ca4 \u0cb8\u0cc1\u0c96 \u0cb6\u0ccd\u0cb0\u0cc0\u0caa\u0ccd\u0cb0\u0ca6\u0cae\u0ccd \u0c85\u0ca4\u0cc1\u0cb2\u0cbf\u0ca4 \u0ca6\u0cbf\u0cb5\u0ccd\u0caf \u0c97\u0cc1\u0ca3\u0cbe\u0ca8\u0ccd\u0cb5\u0cbf\u0ca4\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0c95\u0cae\u0cb2 \u0cb0\u0cc1\u0c9a\u0cbf\u0cb0 \u0c95\u0cb0\u0ca7\u0ccd\u0cb0\u0ccd\u0ca4 \u0c95\u0cb6\u0cbe\u0caf\u0cc1\u0ca7 \u0cb5\u0cbf\u0cad\u0cc2\u0cb7\u0cbf\u0ca4\u0cae\u0ccd \u0c85\u0cae\u0cb0 \u0cae\u0cc1\u0ca8\u0cc0\u0cb6\u0ccd\u0cb5\u0cb0 \u0cad\u0cc2\u0ca4 \u0cb5\u0c9f\u0cc1\u0c95 \u0ca8\u0cbe\u0caf\u0c95 \u0cb8\u0cc7\u0cb5\u0cbf\u0ca4\u0cae\u0ccd <br/> \u0c95\u0cae\u0ca8\u0cc0\u0caf \u0ca8\u0cb5 \u0caf\u0ccc\u0cb5\u0ca8 \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbe\u0cae\u0ccd\u0cac\u0cbe \u0cb8\u0cb9\u0cbf\u0ca4\u0cae\u0ccd \u0cb8\u0cc1\u0cae\u0cb6\u0cb0 \u0c97\u0cc1\u0cb9\u0cbe\u0c97\u0ccd\u0cb0\u0c9c\u0cae\u0ccd \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb0\u0cbe\u0c9c\u0cae\u0ccd \u0cb9\u0cb0\u0cbf\u0cb9\u0cb0 \u0cb8\u0cc1\u0ca4\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cb0\u0cbe\u0c95\u0cbe \u0cb6\u0cb6\u0cbf\u0ca8\u0cbf\u0cad \u0cb5\u0ca6\u0ca8\u0cae\u0ccd \u0cb0\u0cb5\u0cbf \u0cb8\u0ccb\u0cae\u0cbe\u0ca8\u0cb8 \u0ca8\u0caf\u0ca8\u0cae\u0ccd <br/> \u0cb0\u0caf\u0c97\u0ca4\u0cbf \u0ca4\u0cc1\u0cb0\u0c82\u0c97 \u0c97\u0cae\u0ca8\u0cae\u0ccd \u0cb0\u0c95\u0ccd\u0cb7\u0cbf\u0ca4 \u0cb8\u0cae\u0cb8\u0ccd\u0ca4 \u0cad\u0cc1\u0cb5\u0ca8\u0cae\u0ccd"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d2e \u0d15\u0d41\u0d32\u0d47\u0d36\u0d4d\u0d35\u0d30\u0d2e\u0d4d \u0d36\u0d3e\u0d38\u0d4d\u0d24\u0d3e\u0d30\u0d2e\u0d4d \u0d38\u0d28\u0d4d\u0d24\u0d24\u0d2e\u0d4d \u0d1a\u0d3f\u0d28\u0d4d\u0d24\u0d2f\u0d47\u0d39\u0d2e\u0d4d \u0d06\u0d28\u0d28\u0d4d\u0d26 \u0d24\u0d3e\u0d23\u0d4d\u0d21\u0d35 \u0d2a\u0d41\u0d30\u0d35\u0d3f\u0d39\u0d3e\u0d30\u0d2e\u0d4d"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d2e\u0d3f\u0d24\u0d28\u0d24 \u0d1c\u0d28 \u0d26\u0d41\u0d30\u0d3f\u0d24\u0d2e\u0d4d \u0d28\u0d40\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d30 \u0d36\u0d4b\u0d2d\u0d3f\u0d24\u0d2e\u0d4d \u0d05\u0d2e\u0d3f\u0d24 \u0d38\u0d41\u0d16 \u0d36\u0d4d\u0d30\u0d40\u0d2a\u0d4d\u0d30\u0d26\u0d2e\u0d4d \u0d05\u0d24\u0d41\u0d32\u0d3f\u0d24 \u0d26\u0d3f\u0d35\u0d4d\u0d2f \u0d17\u0d41\u0d23\u0d3e\u0d28\u0d4d\u0d35\u0d3f\u0d24\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d15\u0d2e\u0d32 \u0d30\u0d41\u0d1a\u0d3f\u0d30 \u0d15\u0d30\u0d27\u0d4d\u0d30\u0d4d\u0d24 \u0d15\u0d36\u0d3e\u0d2f\u0d41\u0d27 \u0d35\u0d3f\u0d2d\u0d42\u0d37\u0d3f\u0d24\u0d2e\u0d4d \u0d05\u0d2e\u0d30 \u0d2e\u0d41\u0d28\u0d40\u0d36\u0d4d\u0d35\u0d30 \u0d2d\u0d42\u0d24 \u0d35\u0d1f\u0d41\u0d15 \u0d28\u0d3e\u0d2f\u0d15 \u0d38\u0d47\u0d35\u0d3f\u0d24\u0d2e\u0d4d <br/> \u0d15\u0d2e\u0d28\u0d40\u0d2f \u0d28\u0d35 \u0d2f\u0d4c\u0d35\u0d28 \u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3e\u0d2e\u0d4d\u0d2c\u0d3e \u0d38\u0d39\u0d3f\u0d24\u0d2e\u0d4d \u0d38\u0d41\u0d2e\u0d36\u0d30 \u0d17\u0d41\u0d39\u0d3e\u0d17\u0d4d\u0d30\u0d1c\u0d2e\u0d4d \u0d36\u0d4d\u0d30\u0d40 \u0d30\u0d3e\u0d1c\u0d2e\u0d4d \u0d39\u0d30\u0d3f\u0d39\u0d30 \u0d38\u0d41\u0d24\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d30\u0d3e\u0d15\u0d3e \u0d36\u0d36\u0d3f\u0d28\u0d3f\u0d2d \u0d35\u0d26\u0d28\u0d2e\u0d4d \u0d30\u0d35\u0d3f \u0d38\u0d4b\u0d2e\u0d3e\u0d28\u0d38 \u0d28\u0d2f\u0d28\u0d2e\u0d4d <br/> \u0d30\u0d2f\u0d17\u0d24\u0d3f \u0d24\u0d41\u0d30\u0d02\u0d17 \u0d17\u0d2e\u0d28\u0d2e\u0d4d \u0d30\u0d15\u0d4d\u0d37\u0d3f\u0d24 \u0d38\u0d2e\u0d38\u0d4d\u0d24 \u0d2d\u0d41\u0d35\u0d28\u0d2e\u0d4d"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092e\u092e \u0915\u0941\u0932\u0947\u0936\u094d\u0935\u0930\u092e\u094d \u0936\u093e\u0938\u094d\u0924\u093e\u0930\u092e\u094d \u0938\u0928\u094d\u0924\u0924\u092e\u094d \u091a\u093f\u0928\u094d\u0924\u092f\u0947\u0939\u092e\u094d \u0906\u0928\u0928\u094d\u0926 \u0924\u093e\u0923\u094d\u0921\u0935 \u092a\u0941\u0930\u0935\u093f\u0939\u093e\u0930\u092e\u094d"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u092e\u093f\u0924\u0928\u0924 \u091c\u0928 \u0926\u0941\u0930\u093f\u0924\u092e\u094d \u0928\u0940\u0932\u093e\u092e\u094d\u092c\u0930 \u0936\u094b\u092d\u093f\u0924\u092e\u094d \u0905\u092e\u093f\u0924 \u0938\u0941\u0916 \u0936\u094d\u0930\u0940\u092a\u094d\u0930\u0926\u092e\u094d \u0905\u0924\u0941\u0932\u093f\u0924 \u0926\u093f\u0935\u094d\u092f \u0917\u0941\u0923\u093e\u0928\u094d\u0935\u093f\u0924\u092e\u094d"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0915\u092e\u0932 \u0930\u0941\u091a\u093f\u0930 \u0915\u0930\u0927\u094d\u0930\u094d\u0924 \u0915\u0936\u093e\u092f\u0941\u0927 \u0935\u093f\u092d\u0942\u0937\u093f\u0924\u092e\u094d \u0905\u092e\u0930 \u092e\u0941\u0928\u0940\u0936\u094d\u0935\u0930 \u092d\u0942\u0924 \u0935\u091f\u0941\u0915 \u0928\u093e\u092f\u0915 \u0938\u0947\u0935\u093f\u0924\u092e\u094d <br/> \u0915\u092e\u0928\u0940\u092f \u0928\u0935 \u092f\u094c\u0935\u0928 \u0938\u0941\u0928\u094d\u0926\u0930\u093e\u092e\u094d\u092c\u093e \u0938\u0939\u093f\u0924\u092e\u094d \u0938\u0941\u092e\u0936\u0930 \u0917\u0941\u0939\u093e\u0917\u094d\u0930\u091c\u092e\u094d \u0936\u094d\u0930\u0940 \u0930\u093e\u091c\u092e\u094d \u0939\u0930\u093f\u0939\u0930 \u0938\u0941\u0924\u092e\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0930\u093e\u0915\u093e \u0936\u0936\u093f\u0928\u093f\u092d \u0935\u0926\u0928\u092e\u094d \u0930\u0935\u093f \u0938\u094b\u092e\u093e\u0928\u0938 \u0928\u092f\u0928\u092e\u094d <br/> \u0930\u092f\u0917\u0924\u093f \u0924\u0941\u0930\u0902\u0917 \u0917\u092e\u0928\u092e\u094d \u0930\u0915\u094d\u0937\u093f\u0924 \u0938\u092e\u0938\u094d\u0924 \u092d\u0941\u0935\u0928\u092e\u094d"}]}], "L": "l", "lyricsref": [{"links": [{"N": "www.karnatik.com", "L": "http://www.karnatik.com/c3054.shtml"}]}]}