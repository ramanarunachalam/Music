e\u0bbe\u0bae\u0bcd-\u0b85\u0bb5 \u0b9a\u0ba4\u0ba4\u0bae\u0bcd \u0bb0\u0b95\u0bc1 \u0ba8\u0bbe\u0ba4"}, {"M": "\u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0bb6\u0bcd\u0bb0\u0bc0\u0bae\u0ba4\u0bcd-\u0b87\u0ba9\u0bbe\u0ba9\u0bcd\u0bb5\u0baf \u0b9a\u0bbe\u0b95\u0bb0 \u0b9a\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 <br/> \u0bb6\u0bcd\u0bb0\u0bbf\u0ba4 \u0b9c\u0ba9 \u0b9a\u0bc1\u0baa \u0baa\u0bb2\u0ba4 \u0b9a\u0bc1\u0b95\u0bc1\u0ba3 \u0b9a\u0bbe\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0baa\u0b95\u0bcd\u0ba4\u0bbf \u0bb0\u0bb9\u0bbf\u0ba4 \u0b9a\u0bbe\u0bb8\u0bcd\u0ba4\u0bcd\u0bb0-\u0bb5\u0bbf\u0ba4\u0bcd-\u0b85\u0ba4\u0bbf \u0ba4\u0bc2\u0bb0 <br/> \u0baa\u0b99\u0bcd\u0b95\u0b9c \u0ba4\u0bb3 \u0ba8\u0baf\u0ba9 \u0ba8\u0bcd\u0bb0\u0bc1\u0baa \u0b95\u0bc1\u0bae\u0bbe\u0bb0 <br/> \u0b9a\u0b95\u0bcd\u0ba4\u0bbf \u0ba4\u0ba9\u0baf \u0bb9\u0bcd\u0bb0\u0bc1\u0ba4\u0bcd-\u0b86\u0bb2\u0baf \u0bb0\u0b95\u0bc1 \u0bb5\u0bc0\u0bb0 <br/> \u0b9a\u0bbe\u0ba8\u0bcd\u0ba4 \u0ba8\u0bbf\u0bb0\u0bcd\u0bb5\u0bbf\u0b95\u0bbe\u0bb0 <br/> \u0baf\u0bc1\u0b95\u0bcd\u0ba4 \u0bb5\u0b9a\u0ba9 \u0b95\u0ba9\u0b95\u0bbe\u0b9a\u0bb2 \u0ba4\u0bc0\u0bb0 <br/> \u0b89\u0bb0\u0b95 \u0b9a\u0baf\u0ba9 \u0bae\u0bc1\u0ba9\u0bbf \u0b9c\u0ba9 \u0baa\u0bb0\u0bbf\u0bb5\u0bbe\u0bb0 <br/> \u0ba4\u0bcd\u0baf\u0b95\u0bcd\u0ba4 \u0b95\u0bbe\u0bae \u0bae\u0bcb\u0bb9 \u0bae\u0ba4 \u0b95\u0bae\u0bcd\u0baa\u0bc0\u0bb0 <br/> \u0ba4\u0bcd\u0baf\u0bbe\u0b95\u0bb0\u0bbe\u0b9c \u0bb0\u0bbf\u0baa\u0bc1 \u0b9c\u0bb2\u0ba4 \u0b9a\u0bae\u0bc0\u0bb0"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2e\u0c3e\u0c2e\u0c4d-\u0c05\u0c35 \u0c38\u0c24\u0c24\u0c02 \u0c30\u0c18\u0c41 \u0c28\u0c3e\u0c25"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c4d\u0c30\u0c40\u0c2e\u0c26\u0c4d-\u0c07\u0c28\u0c3e\u0c28\u0c4d\u0c35\u0c2f \u0c38\u0c3e\u0c17\u0c30 \u0c1a\u0c28\u0c4d\u0c26\u0c4d\u0c30 <br/> \u0c36\u0c4d\u0c30\u0c3f\u0c24 \u0c1c\u0c28 \u0c36\u0c41\u0c2d \u0c2b\u0c32\u0c26 \u0c38\u0c41\u0c17\u0c41\u0c23 \u0c38\u0c3e\u0c28\u0c4d\u0c26\u0c4d\u0c30"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c2d\u0c15\u0c4d\u0c24\u0c3f \u0c30\u0c39\u0c3f\u0c24 \u0c36\u0c3e\u0c38\u0c4d\u0c24\u0c4d\u0c30-\u0c35\u0c3f\u0c26\u0c4d-\u0c05\u0c24\u0c3f \u0c26\u0c42\u0c30 <br/> \u0c2a\u0c02\u0c15\u0c1c \u0c26\u0c33 \u0c28\u0c2f\u0c28 \u0c28\u0c43\u0c2a \u0c15\u0c41\u0c2e\u0c3e\u0c30 <br/> \u0c36\u0c15\u0c4d\u0c24\u0c3f \u0c24\u0c28\u0c2f \u0c39\u0c43\u0c26\u0c4d-\u0c06\u0c32\u0c2f \u0c30\u0c18\u0c41 \u0c35\u0c40\u0c30 <br/> \u0c36\u0c3e\u0c28\u0c4d\u0c24 \u0c28\u0c3f\u0c30\u0c4d\u0c35\u0c3f\u0c15\u0c3e\u0c30 <br/> \u0c2f\u0c41\u0c15\u0c4d\u0c24 \u0c35\u0c1a\u0c28 \u0c15\u0c28\u0c15\u0c3e\u0c1a\u0c32 \u0c27\u0c40\u0c30 <br/> \u0c09\u0c30\u0c17 \u0c36\u0c2f\u0c28 \u0c2e\u0c41\u0c28\u0c3f \u0c1c\u0c28 \u0c2a\u0c30\u0c3f\u0c35\u0c3e\u0c30 <br/> \u0c24\u0c4d\u0c2f\u0c15\u0c4d\u0c24 \u0c15\u0c3e\u0c2e \u0c2e\u0c4b\u0c39 \u0c2e\u0c26 \u0c17\u0c2e\u0c4d\u0c2d\u0c40\u0c30 <br/> \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c \u0c30\u0c3f\u0c2a\u0c41 \u0c1c\u0c32\u0c26 \u0c38\u0c2e\u0c40\u0c30"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0cbe\u0cae\u0ccd-\u0c85\u0cb5 \u0cb8\u0ca4\u0ca4\u0c82 \u0cb0\u0c98\u0cc1 \u0ca8\u0cbe\u0ca5"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0\u0cae\u0ca6\u0ccd-\u0c87\u0ca8\u0cbe\u0ca8\u0ccd\u0cb5\u0caf \u0cb8\u0cbe\u0c97\u0cb0 \u0c9a\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 <br/> \u0cb6\u0ccd\u0cb0\u0cbf\u0ca4 \u0c9c\u0ca8 \u0cb6\u0cc1\u0cad \u0cab\u0cb2\u0ca6 \u0cb8\u0cc1\u0c97\u0cc1\u0ca3 \u0cb8\u0cbe\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cad\u0c95\u0ccd\u0ca4\u0cbf \u0cb0\u0cb9\u0cbf\u0ca4 \u0cb6\u0cbe\u0cb8\u0ccd\u0ca4\u0ccd\u0cb0-\u0cb5\u0cbf\u0ca6\u0ccd-\u0c85\u0ca4\u0cbf \u0ca6\u0cc2\u0cb0 <br/> \u0caa\u0c82\u0c95\u0c9c \u0ca6\u0cb3 \u0ca8\u0caf\u0ca8 \u0ca8\u0cc3\u0caa \u0c95\u0cc1\u0cae\u0cbe\u0cb0 <br/> \u0cb6\u0c95\u0ccd\u0ca4\u0cbf \u0ca4\u0ca8\u0caf \u0cb9\u0cc3\u0ca6\u0ccd-\u0c86\u0cb2\u0caf \u0cb0\u0c98\u0cc1 \u0cb5\u0cc0\u0cb0 <br/> \u0cb6\u0cbe\u0ca8\u0ccd\u0ca4 \u0ca8\u0cbf\u0cb0\u0ccd\u0cb5\u0cbf\u0c95\u0cbe\u0cb0 <br/> \u0caf\u0cc1\u0c95\u0ccd\u0ca4 \u0cb5\u0c9a\u0ca8 \u0c95\u0ca8\u0c95\u0cbe\u0c9a\u0cb2 \u0ca7\u0cc0\u0cb0 <br/> \u0c89\u0cb0\u0c97 \u0cb6\u0caf\u0ca8 \u0cae\u0cc1\u0ca8\u0cbf \u0c9c\u0ca8 \u0caa\u0cb0\u0cbf\u0cb5\u0cbe\u0cb0 <br/> \u0ca4\u0ccd\u0caf\u0c95\u0ccd\u0ca4 \u0c95\u0cbe\u0cae \u0cae\u0ccb\u0cb9 \u0cae\u0ca6 \u0c97\u0cae\u0ccd\u0cad\u0cc0\u0cb0 <br/> \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c \u0cb0\u0cbf\u0caa\u0cc1 \u0c9c\u0cb2\u0ca6 \u0cb8\u0cae\u0cc0\u0cb0"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d3e\u0d2e\u0d4d-\u0d05\u0d35 \u0d38\u0d24\u0d24\u0d02 \u0d30\u0d18\u0d41 \u0d28\u0d3e\u0d25"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d4d\u0d30\u0d40\u0d2e\u0d26\u0d4d-\u0d07\u0d28\u0d3e\u0d28\u0d4d\u0d35\u0d2f \u0d38\u0d3e\u0d17\u0d30 \u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30 <br/> \u0d36\u0d4d\u0d30\u0d3f\u0d24 \u0d1c\u0d28 \u0d36\u0d41\u0d2d \u0d2b\u0d32\u0d26 \u0d38\u0d41\u0d17\u0d41\u0d23 \u0d38\u0d3e\u0d28\u0d4d\u0d26\u0d4d\u0d30"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d2d\u0d15\u0d4d\u0d24\u0d3f \u0d30\u0d39\u0d3f\u0d24 \u0d36\u0d3e\u0d38\u0d4d\u0d24\u0d4d\u0d30-\u0d35\u0d3f\u0d26\u0d4d-\u0d05\u0d24\u0d3f \u0d26\u0d42\u0d30 <br/> \u0d2a\u0d02\u0d15\u0d1c \u0d26\u0d33 \u0d28\u0d2f\u0d28 \u0d28\u0d43\u0d2a \u0d15\u0d41\u0d2e\u0d3e\u0d30 <br/> \u0d36\u0d15\u0d4d\u0d24\u0d3f \u0d24\u0d28\u0d2f \u0d39\u0d43\u0d26\u0d4d-\u0d06\u0d32\u0d2f \u0d30\u0d18\u0d41 \u0d35\u0d40\u0d30 <br/> \u0d36\u0d3e\u0d28\u0d4d\u0d24 \u0d28\u0d3f\u0d30\u0d4d\u0d35\u0d3f\u0d15\u0d3e\u0d30 <br/> \u0d2f\u0d41\u0d15\u0d4d\u0d24 \u0d35\u0d1a\u0d28 \u0d15\u0d28\u0d15\u0d3e\u0d1a\u0d32 \u0d27\u0d40\u0d30 <br/> \u0d09\u0d30\u0d17 \u0d36\u0d2f\u0d28 \u0d2e\u0d41\u0d28\u0d3f \u0d1c\u0d28 \u0d2a\u0d30\u0d3f\u0d35\u0d3e\u0d30 <br/> \u0d24\u0d4d\u0d2f\u0d15\u0d4d\u0d24 \u0d15\u0d3e\u0d2e \u0d2e\u0d4b\u0d39 \u0d2e\u0d26 \u0d17\u0d2e\u0d4d\u0d2d\u0d40\u0d30 <br/> \u0d24\u0d4d\u0d2f\u0d3e\u0d17\u0d30\u0d3e\u0d1c \u0d30\u0d3f\u0d2a\u0d41 \u0d1c\u0d32\u0d26 \u0d38\u0d2e\u0d40\u0d30"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092e\u093e\u092e\u094d-\u0905\u0935 \u0938\u0924\u0924\u0902 \u0930\u0918\u0941 \u0928\u093e\u0925"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u094d\u0930\u0940\u092e\u0926\u094d-\u0907\u0928\u093e\u0928\u094d\u0935\u092f \u0938\u093e\u0917\u0930 \u091a\u0928\u094d\u0926\u094d\u0930 <br/> \u0936\u094d\u0930\u093f\u0924 \u091c\u0928 \u0936\u0941\u092d \u092b\u0932\u0926 \u0938\u0941\u0917\u0941\u0923 \u0938\u093e\u0928\u094d\u0926\u094d\u0930"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u092d\u0915\u094d\u0924\u093f \u0930\u0939\u093f\u0924 \u0936\u093e\u0938\u094d\u0924\u094d\u0930-\u0935\u093f\u0926\u094d-\u0905\u0924\u093f \u0926\u0942\u0930 <br/> \u092a\u0902\u0915\u091c \u0926\u0932 \u0928\u092f\u0928 \u0928\u0943\u092a \u0915\u0941\u092e\u093e\u0930 <br/> \u0936\u0915\u094d\u0924\u093f \u0924\u0928\u092f \u0939\u0943\u0926\u094d-\u0906\u0932\u092f \u0930\u0918\u0941 \u0935\u0940\u0930 <br/> \u0936\u093e\u0928\u094d\u0924 \u0928\u093f\u0930\u094d\u0935\u093f\u0915\u093e\u0930 <br/> \u092f\u0941\u0915\u094d\u0924 \u0935\u091a\u0928 \u0915\u0928\u0915\u093e\u091a\u0932 \u0927\u0940\u0930 <br/> \u0909\u0930\u0917 \u0936\u092f\u0928 \u092e\u0941\u0928\u093f \u091c\u0928 \u092a\u0930\u093f\u0935\u093e\u0930 <br/> \u0924\u094d\u092f\u0915\u094d\u0924 \u0915\u093e\u092e \u092e\u094b\u0939 \u092e\u0926 \u0917\u092e\u094d\u092d\u0940\u0930 <br/> \u0924\u094d\u092f\u093e\u0917\u0930\u093e\u091c \u0930\u093f\u092a\u0941 \u091c\u0932\u0926 \u0938\u092e\u0940\u0930"}]}], "L": "l", "lyricsref": [{"links": [{"N": "thyagaraja-vaibhavam.blogspot.com", "L": "http://thyagaraja-vaibhavam.blogspot.com/2007/03/thyagaraja-kriti-maamava-satatam-raga.html"}, {"N": "www.sangeetasudha.org", "L": "http://www.sangeetasudha.org/thyagaraja/vol3/b1.html#MARK_MAMAVA"}, {"N": "www.karnatik.com", "L": "http://www.karnatik.com/c2540.shtml"}]}]}