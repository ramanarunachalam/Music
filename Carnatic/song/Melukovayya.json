u0c2f\u0c28 \u0c05\u0c28\u0c3f\u0c2e\u0c3f\u0c37 \u0c30\u0c2e\u0c23\u0c41\u0c32\u0c42\u0c21\u0c3f\u0c17\u0c2e\u0c41 \u0c38\u0c47\u0c2f <br/> \u0c05\u0c23\u0c41\u0c15\u0c41\u0c35\u0c17 \u0c28\u0c3f\u0c23\u0c4d\u0c21\u0c3e\u0c30\u0c41 \u0c2a\u0c4d\u0c30\u0c23\u0c41\u0c24\u0c3f \u0c1c\u0c47\u0c38\u0c46\u0c26\u0c30\u0c41 <br/> \u0c2e\u0c23\u0c3f-\u0c2e\u0c2f\u0c3e\u0c2d\u0c30\u0c23\u0c41\u0c32\u0c48 \u0c05\u0c23\u0c3f\u0c2e\u0c3e\u0c26\u0c41\u0c32\u0c3f\u0c21\u0c41 \u0c26\u0c40\u0c2a <br/> \u0c2e\u0c23\u0c41\u0c32\u0c41 \u0c24\u0c46\u0c32\u0c41\u0c2a\u0c3e\u0c2f\u0c46\u0c28\u0c41 \u0c24\u0c30\u0c23\u0c3f \u0c35\u0c02\u0c36 \u0c35\u0c30 \u0c24\u0c3f\u0c32\u0c15"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c30\u0c3e\u0c1c \u0c30\u0c3e\u0c1c\u0c47\u0c36\u0c4d\u0c35\u0c30 \u0c2d-\u0c30\u0c3e\u0c1c \u0c2e\u0c41\u0c16 \u0c38\u0c3e\u0c15\u0c47\u0c24 <br/> \u0c30\u0c3e\u0c1c \u0c38\u0c26\u0c4d-\u0c17\u0c41\u0c23 \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c \u0c28\u0c41\u0c24 \u0c1a\u0c30\u0c23 <br/> \u0c30\u0c3e\u0c1c\u0c28\u0c4d\u0c2f \u0c35\u0c3f\u0c2c\u0c41\u0c27 \u0c17\u0c23 \u0c30\u0c3e\u0c1c\u0c3e\u0c26\u0c41\u0c32\u0c46\u0c32\u0c4d\u0c32 \u0c28\u0c3f\u0c28\u0c41 <br/> \u0c2a\u0c42\u0c1c\u0c3f\u0c2e\u0c4d\u0c2a \u0c15\u0c3e\u0c1a\u0c3f\u0c28\u0c3e\u0c30\u0c40 \u0c1c\u0c17\u0c2e\u0c41 \u0c2a\u0c3e\u0c32\u0c3f\u0c2e\u0c4d\u0c2a"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0cc7\u0cb2\u0cc1\u0c95\u0ccb\u0cb5\u0caf\u0ccd\u0caf \u0cae\u0cae\u0ccd\u0cae\u0cc7\u0cb2\u0cc1\u0c95\u0ccb \u0cb0\u0cbe\u0cae <br/> \u0cae\u0cc7\u0cb2\u0cc8\u0ca8 \u0cb8\u0cc0\u0ca4\u0cbe \u0cb8\u0cae\u0cc7\u0ca4 \u0ca8\u0cbe \u0cad\u0cbe\u0c97\u0ccd\u0caf\u0cae\u0cbe"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca8\u0cbe\u0cb0\u0ca6\u0cbe\u0ca6\u0cc1\u0cb2\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc1 \u0c95\u0ccb\u0cb0\u0cbf \u0ca8\u0cc0 \u0cae\u0cb9\u0cbf\u0cae\u0cb2\u0ccd- <br/> \u0c85\u0cb5\u0ccd\u0cb5\u0cbe\u0cb0\u0cbf\u0c97\u0cbe \u0caa\u0cbe\u0ca1\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd\u0ca8\u0cbe\u0cb0\u0cbf\u0caa\u0cc1\u0ca1\u0cc1 \u0ca4\u0cc6\u0cb2\u0ccd\u0cb2- <br/> \u0cb5\u0cbe\u0cb0\u0c97\u0cbe \u0cb5\u0c9a\u0ccd\u0c9a\u0cbf\u0ca8\u0ca6\u0cbf \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb0\u0cbe\u0cae \u0ca8\u0cb5\u0ca8\u0cc0\u0ca4 <br/> \u0c95\u0ccd\u0cb7\u0cc0\u0cb0\u0cae\u0cc1\u0cb2\u0cc1 \u0cac\u0cbe\u0c97\u0cc1\u0c97\u0cbe\u0ca8\u0cbe\u0cb0\u0c97\u0cbf\u0cae\u0ccd\u0caa\u0ca8\u0cc1 \u0cb5\u0cc7\u0c97"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0cab\u0ca3\u0cbf \u0cb6\u0caf\u0ca8 \u0c85\u0ca8\u0cbf\u0cae\u0cbf\u0cb7 \u0cb0\u0cae\u0ca3\u0cc1\u0cb2\u0cc2\u0ca1\u0cbf\u0c97\u0cae\u0cc1 \u0cb8\u0cc7\u0caf <br/> \u0c85\u0ca3\u0cc1\u0c95\u0cc1\u0cb5\u0c97 \u0ca8\u0cbf\u0ca3\u0ccd\u0ca1\u0cbe\u0cb0\u0cc1 \u0caa\u0ccd\u0cb0\u0ca3\u0cc1\u0ca4\u0cbf \u0c9c\u0cc7\u0cb8\u0cc6\u0ca6\u0cb0\u0cc1 <br/> \u0cae\u0ca3\u0cbf-\u0cae\u0caf\u0cbe\u0cad\u0cb0\u0ca3\u0cc1\u0cb2\u0cc8 \u0c85\u0ca3\u0cbf\u0cae\u0cbe\u0ca6\u0cc1\u0cb2\u0cbf\u0ca1\u0cc1 \u0ca6\u0cc0\u0caa <br/> \u0cae\u0ca3\u0cc1\u0cb2\u0cc1 \u0ca4\u0cc6\u0cb2\u0cc1\u0caa\u0cbe\u0caf\u0cc6\u0ca8\u0cc1 \u0ca4\u0cb0\u0ca3\u0cbf \u0cb5\u0c82\u0cb6 \u0cb5\u0cb0 \u0ca4\u0cbf\u0cb2\u0c95"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb0\u0cbe\u0c9c \u0cb0\u0cbe\u0c9c\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0 \u0cad-\u0cb0\u0cbe\u0c9c \u0cae\u0cc1\u0c96 \u0cb8\u0cbe\u0c95\u0cc7\u0ca4 <br/> \u0cb0\u0cbe\u0c9c \u0cb8\u0ca6\u0ccd-\u0c97\u0cc1\u0ca3 \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c \u0ca8\u0cc1\u0ca4 \u0c9a\u0cb0\u0ca3 <br/> \u0cb0\u0cbe\u0c9c\u0ca8\u0ccd\u0caf \u0cb5\u0cbf\u0cac\u0cc1\u0ca7 \u0c97\u0ca3 \u0cb0\u0cbe\u0c9c\u0cbe\u0ca6\u0cc1\u0cb2\u0cc6\u0cb2\u0ccd\u0cb2 \u0ca8\u0cbf\u0ca8\u0cc1 <br/> \u0caa\u0cc2\u0c9c\u0cbf\u0cae\u0ccd\u0caa \u0c95\u0cbe\u0c9a\u0cbf\u0ca8\u0cbe\u0cb0\u0cc0 \u0c9c\u0c97\u0cae\u0cc1 \u0caa\u0cbe\u0cb2\u0cbf\u0cae\u0ccd\u0caa"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d47\u0d32\u0d41\u0d15\u0d4b\u0d35\u0d2f\u0d4d\u0d2f \u0d2e\u0d2e\u0d4d\u0d2e\u0d47\u0d32\u0d41\u0d15\u0d4b \u0d30\u0d3e\u0d2e <br/> \u0d2e\u0d47\u0d32\u0d48\u0d28 \u0d38\u0d40\u0d24\u0d3e \u0d38\u0d2e\u0d47\u0d24 \u0d28\u0d3e \u0d2d\u0d3e\u0d17\u0d4d\u0d2f\u0d2e\u0d3e"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d28\u0d3e\u0d30\u0d26\u0d3e\u0d26\u0d41\u0d32\u0d41 \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d41 \u0d15\u0d4b\u0d30\u0d3f \u0d28\u0d40 \u0d2e\u0d39\u0d3f\u0d2e\u0d32\u0d4d- <br/> \u0d05\u0d35\u0d4d\u0d35\u0d3e\u0d30\u0d3f\u0d17\u0d3e \u0d2a\u0d3e\u0d21\u0d41\u0d1a\u0d41\u0d28\u0d4d\u0d28\u0d3e\u0d30\u0d3f\u0d2a\u0d41\u0d21\u0d41 \u0d24\u0d46\u0d32\u0d4d\u0d32- <br/> \u0d35\u0d3e\u0d30\u0d17\u0d3e \u0d35\u0d1a\u0d4d\u0d1a\u0d3f\u0d28\u0d26\u0d3f \u0d36\u0d4d\u0d30\u0d40 \u0d30\u0d3e\u0d2e \u0d28\u0d35\u0d28\u0d40\u0d24 <br/> \u0d15\u0d4d\u0d37\u0d40\u0d30\u0d2e\u0d41\u0d32\u0d41 \u0d2c\u0d3e\u0d17\u0d41\u0d17\u0d3e\u0d28\u0d3e\u0d30\u0d17\u0d3f\u0d2e\u0d4d\u0d2a\u0d28\u0d41 \u0d35\u0d47\u0d17"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d2b\u0d23\u0d3f \u0d36\u0d2f\u0d28 \u0d05\u0d28\u0d3f\u0d2e\u0d3f\u0d37 \u0d30\u0d2e\u0d23\u0d41\u0d32\u0d42\u0d21\u0d3f\u0d17\u0d2e\u0d41 \u0d38\u0d47\u0d2f <br/> \u0d05\u0d23\u0d41\u0d15\u0d41\u0d35\u0d17 \u0d28\u0d3f\u0d23\u0d4d\u0d21\u0d3e\u0d30\u0d41 \u0d2a\u0d4d\u0d30\u0d23\u0d41\u0d24\u0d3f \u0d1c\u0d47\u0d38\u0d46\u0d26\u0d30\u0d41 <br/> \u0d2e\u0d23\u0d3f-\u0d2e\u0d2f\u0d3e\u0d2d\u0d30\u0d23\u0d41\u0d32\u0d48 \u0d05\u0d23\u0d3f\u0d2e\u0d3e\u0d26\u0d41\u0d32\u0d3f\u0d21\u0d41 \u0d26\u0d40\u0d2a <br/> \u0d2e\u0d23\u0d41\u0d32\u0d41 \u0d24\u0d46\u0d32\u0d41\u0d2a\u0d3e\u0d2f\u0d46\u0d28\u0d41 \u0d24\u0d30\u0d23\u0d3f \u0d35\u0d02\u0d36 \u0d35\u0d30 \u0d24\u0d3f\u0d32\u0d15"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d30\u0d3e\u0d1c \u0d30\u0d3e\u0d1c\u0d47\u0d36\u0d4d\u0d35\u0d30 \u0d2d-\u0d30\u0d3e\u0d1c \u0d2e\u0d41\u0d16 \u0d38\u0d3e\u0d15\u0d47\u0d24 <br/> \u0d30\u0d3e\u0d1c \u0d38\u0d26\u0d4d-\u0d17\u0d41\u0d23 \u0d24\u0d4d\u0d2f\u0d3e\u0d17\u0d30\u0d3e\u0d1c \u0d28\u0d41\u0d24 \u0d1a\u0d30\u0d23 <br/> \u0d30\u0d3e\u0d1c\u0d28\u0d4d\u0d2f \u0d35\u0d3f\u0d2c\u0d41\u0d27 \u0d17\u0d23 \u0d30\u0d3e\u0d1c\u0d3e\u0d26\u0d41\u0d32\u0d46\u0d32\u0d4d\u0d32 \u0d28\u0d3f\u0d28\u0d41 <br/> \u0d2a\u0d42\u0d1c\u0d3f\u0d2e\u0d4d\u0d2a \u0d15\u0d3e\u0d1a\u0d3f\u0d28\u0d3e\u0d30\u0d40 \u0d1c\u0d17\u0d2e\u0d41 \u0d2a\u0d3e\u0d32\u0d3f\u0d2e\u0d4d\u0d2a"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092e\u0947\u0932\u0941\u0915\u094b\u0935\u092f\u094d\u092f \u092e\u092e\u094d\u092e\u0947\u0932\u0941\u0915\u094b \u0930\u093e\u092e <br/> \u092e\u0947\u0932\u0948\u0928 \u0938\u0940\u0924\u093e \u0938\u092e\u0947\u0924 \u0928\u093e \u092d\u093e\u0917\u094d\u092f\u092e\u093e"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0928\u093e\u0930\u0926\u093e\u0926\u0941\u0932\u0941 \u0928\u093f\u0928\u094d\u0928\u0941 \u0915\u094b\u0930\u093f \u0928\u0940 \u092e\u0939\u093f\u092e\u0932\u094d- <br/> \u0905\u0935\u094d\u0935\u093e\u0930\u093f\u0917\u093e \u092a\u093e\u0921\u0941\u091a\u0941\u0928\u094d\u0928\u093e\u0930\u093f\u092a\u0941\u0921\u0941 \u0924\u0947\u0932\u094d\u0932- <br/> \u0935\u093e\u0930\u0917\u093e \u0935\u091a\u094d\u091a\u093f\u0928\u0926\u093f \u0936\u094d\u0930\u0940 \u0930\u093e\u092e \u0928\u0935\u0928\u0940\u0924 <br/> \u0915\u094d\u0937\u0940\u0930\u092e\u0941\u0932\u0941 \u092c\u093e\u0917\u0941\u0917\u093e\u0928\u093e\u0930\u0917\u093f\u092e\u094d\u092a\u0928\u0941 \u0935\u0947\u0917"}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u092b\u0923\u093f \u0936\u092f\u0928 \u0905\u0928\u093f\u092e\u093f\u0937 \u0930\u092e\u0923\u0941\u0932\u0942\u0921\u093f\u0917\u092e\u0941 \u0938\u0947\u092f <br/> \u0905\u0923\u0941\u0915\u0941\u0935\u0917 \u0928\u093f\u0923\u094d\u0921\u093e\u0930\u0941 \u092a\u094d\u0930\u0923\u0941\u0924\u093f \u091c\u0947\u0938\u0947\u0926\u0930\u0941 <br/> \u092e\u0923\u093f-\u092e\u092f\u093e\u092d\u0930\u0923\u0941\u0932\u0948 \u0905\u0923\u093f\u092e\u093e\u0926\u0941\u0932\u093f\u0921\u0941 \u0926\u0940\u092a <br/> \u092e\u0923\u0941\u0932\u0941 \u0924\u0947\u0932\u0941\u092a\u093e\u092f\u0947\u0928\u0941 \u0924\u0930\u0923\u093f \u0935\u0902\u0936 \u0935\u0930 \u0924\u093f\u0932\u0915"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u0930\u093e\u091c \u0930\u093e\u091c\u0947\u0936\u094d\u0935\u0930 \u092d-\u0930\u093e\u091c \u092e\u0941\u0916 \u0938\u093e\u0915\u0947\u0924 <br/> \u0930\u093e\u091c \u0938\u0926\u094d-\u0917\u0941\u0923 \u0924\u094d\u092f\u093e\u0917\u0930\u093e\u091c \u0928\u0941\u0924 \u091a\u0930\u0923 <br/> \u0930\u093e\u091c\u0928\u094d\u092f \u0935\u093f\u092c\u0941\u0927 \u0917\u0923 \u0930\u093e\u091c\u093e\u0926\u0941\u0932\u0947\u0932\u094d\u0932 \u0928\u093f\u0928\u0941 <br/> \u092a\u0942\u091c\u093f\u092e\u094d\u092a \u0915\u093e\u091a\u093f\u0928\u093e\u0930\u0940 \u091c\u0917\u092e\u0941 \u092a\u093e\u0932\u093f\u092e\u094d\u092a"}]}], "L": "l", "lyricsref": [{"links": [{"N": "thyagaraja-vaibhavam.blogspot.com", "L": "http://thyagaraja-vaibhavam.blogspot.com/2007/03/thyagaraja-kriti-melukovayya-raga.html"}, {"N": "www.sangeetasudha.org", "L": "http://www.sangeetasudha.org/thyagaraja/vol2/m.html#MARK_MELUKOVAYYA"}, {"N": "www.karnatik.com", "L": "http://www.karnatik.com/c1722.shtml"}]}]}