d\u0d28\u0d3f\u0d38\u0d48 \u0d2a\u0d3e\u0d21 \u0d2e\u0d32\u0d30\u0d4d <br/> \u0d2e\u0d23\u0d4d\u0d21\u0d3f\u0d2f \u0d2a\u0d3f\u0d28\u0d4d\u0d28\u0d30\u0d4d \u0d36\u0d21\u0d48\u0d2f\u0d26\u0d35\u0d3f\u0d34\u0d4d\u0d28\u0d4d\u0d26\u0d41 \u0d2e\u0d26\u0d41\u0d35\u0d41\u0d2e\u0d4d \u0d35\u0d34\u0d3f\u0d28\u0d4d\u0d26\u0d4b\u0d21 \u0d15\u0d30\u0d41\u0d2e\u0d4d <br/> \u0d15\u0d46\u0d23\u0d4d\u0d21\u0d48 \u0d35\u0d3f\u0d34\u0d3f\u0d17\u0d33\u0d4d \u0d07\u0d30\u0d23\u0d4d\u0d21\u0d41 \u0d2e\u0d28\u0d19\u0d4d\u0d17\u0d28\u0d4d \u0d15\u0d23\u0d48\u0d2f\u0d48 \u0d07\u0d17\u0d34\u0d4d\u0d28\u0d4d\u0d26\u0d3e\u0d21 \u0d28\u0d32\u0d4d\u0d32\u0d26\u0d23\u0d4d\u0d21\u0d2e\u0d3f\u0d34\u0d4d\u0d2a\u0d4d- <br/> \u0d2a\u0d3e\u0d21\u0d3f \u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3e\u0d21\u0d3f\u0d15\u0d4d-\u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3e\u0d21\u0d3f\u0d2f\u0d47 \u0d24\u0d2e\u0d4d\u0d24 \u0d24\u0d15\u0d41\u0d2e\u0d4d \u0d24\u0d15\u0d41 \u0d1d\u0d28\u0d4d\u0d24\u0d30\u0d3f \u0d15\u0d3f\u0d1f\u0d42 \u0d24\u0d3e\u0d2e\u0d4d \u0d15\u0d3f\u0d1f\u0d24\u0d4b\u0d2e\u0d4d \u0d0e\u0d28\u0d4d\u0d31\u0d24\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d36\u0d46\u0d28\u0d4d \u0d24\u0d3f\u0d30\u0d41 \u0d2e\u0d19\u0d4d\u0d17\u0d48 \u0d28\u0d41\u0d26\u0d32\u0d3f\u0d30\u0d4d-\u0d38\u0d3f\u0d28\u0d4d\u0d26\u0d42\u0d30\u0d24\u0d4d\u0d24\u0d3f\u0d32\u0d4d \u0d05\u0d26\u0d28\u0d4d\u0d26\u0d33\u0d26\u0d33\u0d46\u0d28\u0d4d\u0d28 \u0d15\u0d3e\u0d21\u0d3f\u0d32\u0d4d <br/> \u0d15\u0d4a\u0d28\u0d4d\u0d26\u0d33 \u0d35\u0d4b\u0d32\u0d48 \u0d1c\u0d3f\u0d2e\u0d3f\u0d15\u0d4d\u0d15\u0d3f \u0d2a\u0d4a\u0d30\u0d4d\u0d2a\u0d4d\u0d2a\u0d3e\u0d35\u0d3f\u0d32\u0d3f \u0d17\u0d4b\u0d36\u0d41\u0d2d \u0d2a\u0d33\u0d2a\u0d33\u0d46\u0d28\u0d4d\u0d28 \u0d35\u0d33\u0d48\u0d15\u0d4d- <br/> \u0d15\u0d28\u0d4d\u0d26\u0d30\u0d2e\u0d4d \u0d24\u0d28\u0d4d\u0d28\u0d3f\u0d32\u0d4d \u0d05\u0d23\u0d3f\u0d28\u0d4d\u0d26 \u0d28\u0d35\u0d30\u0d24\u0d4d\u0d28\u0d15\u0d4d-\u0d15\u0d3e\u0d30\u0d48 \u0d28\u0d3f\u0d18 \u0d28\u0d3f\u0d18\u0