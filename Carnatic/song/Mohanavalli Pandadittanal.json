0bcd\u0b9f\u0bc1 \u0bae\u0b95\u0bb0\u0ba8\u0bcd\u0ba4\u0bae\u0bcd \u0b89\u0ba3\u0bcd\u0b9f\u0bbf\u0ba9\u0bcd\u0ba9\u0bbf\u0b9a\u0bc8 \u0baa\u0bbe\u0b9f \u0bae\u0bb2\u0bb0\u0bcd <br/> \u0bae\u0ba3\u0bcd\u0b9f\u0bbf\u0baf \u0baa\u0bbf\u0ba9\u0bcd\u0ba9\u0bb0\u0bcd \u0b9a\u0b9f\u0bc8\u0baf\u0ba4\u0bb5\u0bbf\u0bb4\u0bcd\u0ba8\u0bcd\u0ba4\u0bc1 \u0bae\u0ba4\u0bc1\u0bb5\u0bc1\u0bae\u0bcd \u0bb5\u0bb4\u0bbf\u0ba8\u0bcd\u0ba4\u0bcb\u0b9f \u0b95\u0bb0\u0bc1\u0bae\u0bcd <br/> \u0b95\u0bc6\u0ba3\u0bcd\u0b9f\u0bc8 \u0bb5\u0bbf\u0bb4\u0bbf\u0b95\u0bb3\u0bcd \u0b87\u0bb0\u0ba3\u0bcd\u0b9f\u0bc1 \u0bae\u0ba9\u0b99\u0bcd\u0b95\u0ba9\u0bcd \u0b95\u0ba3\u0bc8\u0baf\u0bc8 \u0b87\u0b95\u0bb4\u0bcd\u0ba8\u0bcd\u0ba4\u0bbe\u0b9f \u0ba8\u0bb2\u0bcd\u0bb2\u0ba4\u0ba3\u0bcd\u0b9f\u0bae\u0bbf\u0bb4\u0bcd\u0baa\u0bcd- <br/> \u0baa\u0bbe\u0b9f\u0bbf \u0b95\u0bca\u0ba3\u0bcd\u0b9f\u0bbe\u0b9f\u0bbf\u0b95\u0bcd-\u0b95\u0bca\u0ba3\u0bcd\u0b9f\u0bbe\u0b9f\u0bbf\u0baf\u0bc7 \u0ba4\u0bae\u0bcd\u0ba4 \u0ba4\u0b95\u0bc1\u0bae\u0bcd \u0ba4\u0b95\u0bc1 \u0b9c\u0ba8\u0bcd\u0ba4\u0bb0\u0bbf \u0b95\u0bbf\u0b9f\u0bc2 \u0ba4\u0bbe\u0bae\u0bcd \u0b95\u0bbf\u0b9f\u0ba4\u0bcb\u0bae\u0bcd \u0b8e\u0ba9\u0bcd\u0bb1\u0ba4\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b9a\u0bc6\u0ba9\u0bcd \u0ba4\u0bbf\u0bb0\u0bc1 \u0bae\u0b99\u0bcd\u0b95\u0bc8 \u0ba8\u0bc1\u0ba4\u0bb2\u0bbf\u0bb0\u0bcd-\u0b9a\u0bbf\u0ba8\u0bcd\u0ba4\u0bc2\u0bb0\u0ba4\u0bcd\u0ba4\u0bbf\u0bb2\u0bcd \u0b85\u0ba4\u0ba8\u0bcd\u0ba4\u0bb3\u0ba4\u0bb3\u0bc6\u0ba9\u0bcd\u0ba9 \u0b95\u0bbe\u0b9f\u0bbf\u0bb2\u0bcd <br/> \u0b95\u0bca\u0ba8\u0bcd\u0ba4\u0bb3 \u0bb5\u0bcb\u0bb2\u0bc8 \u0b9c\u0bbf\u0bae\u0bbf\u0b95\u0bcd\u0b95\u0bbf \u0baa\u0bca\u0bb0\u0bcd\u0baa\u0bcd\u0baa\u0bbe\u0bb5\u0bbf\u0bb2\u0bbf \u0b95\u0bcb\u0b9a\u0bc1\u0baa \u0baa\u0bb3\u0baa\u0bb3\u0bc6\u0ba9\u0bcd\u0ba9 \u0bb5\u0bb3\u0bc8\u0b95\u0bcd- <br/> \u0b95\u0ba8\u0bcd\u0ba4\u0bb0\u0bae\u0bcd \u0ba4\u0ba9\u0bcd\u0ba9\u0bbf\u0bb2\u0bcd \u0b85\u0ba3\u0bbf\u0ba8\u0bcd\u0ba4 \u0ba8\u0bb5\u0bb0\u0ba4\u0bcd\u0ba9\u0b95\u0bcd-\u0b95\u0bbe\u0bb0\u0bc8 \u0ba8\u0bbf\u0b95 \u0ba8\u0bbf\u0b95\u0bc6\u0ba9\u0bcd\u0ba9 \u0baa\u0bc2\u0bb0\u0bcd\u0ba3\u0b9a\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0\u0ba9\u0bc6\u0ba9\u0bc1\u0bae\u0bcd <br/> \u0bb5\u0ba4\u0ba9\u0ba4\u0bcd\u0ba4\u0bbf\u0bb2\u0bcd \u0b92\u0bb3\u0bbf \u0bae\u0bbf\u0ba9\u0bcd\u0ba9\u0ba4\u0bcd-\u0ba4\u0b95\u0bcd\u0b95 \u0ba4\u0b95 \u0ba4\u0b9c\u0bcd\u0b9c\u0ba3\u0bc1 \u0ba4\u0b95 \u0ba4\u0bb0\u0bbf\u0b95\u0bbf\u0b9f \u0ba4\u0b9c\u0ba3\u0bc2 \u0ba4\u0b95\u0ba4\u0bcb\u0bae\u0bcd \u0b8e\u0ba9\u0bcd\u0bb1\u0ba4\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0b95\u0bc1\u0bae\u0bcd\u0b95\u0bc1\u0bae \u0b95\u0ba8\u0bcd\u0ba4\u0bae\u0bcd \u0b85\u0ba3\u0bc0\u0ba8\u0bcd\u0ba4 \u0bb8\u0bcd\u0ba4\u0ba9\u0bae\u0bcd\u0b95\u0bb3\u0bcd \u0b95\u0bc1\u0bb2\u0bc1\u0b99\u0bcd\u0b95\u0bbf \u0b87\u0bb2\u0b99\u0bcd\u0b95\u0bbf\u0b9f\u0bb5\u0bc7 \u0ba4\u0bbf\u0bb5\u0bcd\u0baf <br/> \u0bae\u0b99\u0bcd\u0b95\u0bb3\u0bae\u0bcd \u0b9a\u0bc7\u0bb0\u0bcd \u0bae\u0bc1\u0ba4\u0bcd\u0ba4\u0bc1 \u0bae\u0bbe\u0bb2\u0bc8\u0baf\u0bca\u0b9f\u0bc1 \u0bb0\u0ba4\u0bcd\u0ba9 \u0bae\u0bbe\u0bb2\u0bc8 \u0b85\u0b9a\u0bc8\u0ba8\u0bcd\u0ba4\u0bbf\u0b9f\u0bb5\u0bc7 \u0bb5\u0bb0\u0bcd\u0ba3 <br/> \u0b9a\u0bc6\u0ba9\u0bcd \u0b95\u0bc8\u0b95\u0bcd\u0b95\u0bcd-\u0b85\u0b9f\u0bc8\u0baf\u0b99\u0bcd\u0b95\u0bb3\u0bcd \u0b95\u0b99\u0bcd\u0b95\u0ba3\u0bb5\u0bcb\u0b9a\u0bc8 \u0b9c\u0baf\u0bae\u0bcd \u0b9c\u0baf\u0bae\u0bc6\u0ba9\u0bcd\u0bb1\u0bbf\u0b9f\u0bb5\u0bc7 \u0b89\u0baf\u0bbf\u0bb0\u0bcd \u0ba4\u0b99\u0bcd\u0b95\u0b95\u0bcd-\u0b95\u0bca\u0b9f\u0bbf <br/> \u0b87\u0b9f\u0bc8\u0baf\u0bae\u0bcd \u0b95\u0bc8 \u0b95\u0b9f\u0b99\u0bcd\u0b95\u0bbf\u0b9f\u0ba4\u0bcd-\u0ba4\u0bae\u0bcd\u0b95\u0ba4 \u0ba4\u0ba4\u0bbf\u0b99\u0bcd\u0b95\u0bbf\u0ba3 \u0ba4\u0bc0\u0b99\u0bcd\u0b95\u0bbf\u0ba3 \u0ba4\u0b95 \u0ba4\u0ba4\u0bbf\u0b99\u0bcd\u0b95\u0bbf\u0ba3\u0ba4\u0bcb\u0bae\u0bcd \u0b8e\u0ba9\u0bcd\u0bb1\u0ba4\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0b85\u0ba9\u0bcd\u0ba9\u0ba4\u0bcd\u0ba4\u0bc8 \u0b9a\u0bc2\u0bb4\u0bcd\u0ba8\u0bcd\u0ba4 \u0bae\u0baf\u0bbf\u0bb0\u0bcd \u0b95\u0bc1\u0bb2\u0bae\u0bcd \u0baa\u0bcb\u0bb2\u0bcd \u0b9a\u0b95\u0bbf\u0bae\u0bbe\u0bb0\u0bcd\u0b95\u0bb3\u0bcd \u0b89\u0bb0\u0bc1\u0b95\u0bbf\u0bb0\u0bc1\u0ba8\u0bcd\u0ba4\u0bc1 \u0baa\u0bbe\u0b9f <br/> \u0bae\u0bbf\u0ba9\u0bcd\u0ba9\u0bca\u0ba4\u0bcd\u0ba4 \u0bae\u0bbe\u0b95\u0bb2\u0bc8 \u0ba4\u0bc1\u0ba9\u0bcd\u0ba9\u0bbf\u0b9f\u0bae\u0bbf\u0b95 \u0bb5\u0bbf\u0baf\u0ba8\u0bcd\u0ba4\u0bc1 \u0ba8\u0b9f\u0bae\u0bcd \u0baa\u0bc1\u0bb0\u0bbf\u0ba8\u0bcd\u0ba4\u0bc1 \u0ba4\u0bbe\u0bb3\u0bbf\u0bb2\u0bcd <br/> \u0ba4\u0bc1\u0ba9\u0bcd\u0ba9\u0bbf\u0baf \u0ba4\u0ba3\u0bcd\u0b9f\u0bc8 \u0b9a\u0ba4\u0b99\u0bcd\u0b95\u0bc8 \u0b95\u0bb2\u0bbf\u0bb0\u0bcd \u0b95\u0bb2\u0bc0\u0bb0\u0bc6\u0ba9\u0bcd\u0ba9 \u0bae\u0ba9\u0bae\u0bcd \u0baa\u0bc1\u0bb0\u0bbf\u0ba8\u0bcd\u0ba4\u0bc1 \u0ba4\u0bc7\u0bb5 \u0b95\u0bbf\u0ba9\u0bcd\u0ba9\u0bb0 <br/> \u0bb0\u0bae\u0bcd\u0baa\u0bc8\u0baf\u0bb0\u0bcd \u0b95\u0bbf\u0bae\u0bcd\u0baa\u0bc1\u0bb0\u0bc1\u0bb7\u0bbe\u0ba4\u0bbf\u0baf\u0bb0\u0bcd \u0b95\u0ba8\u0bcd\u0ba4\u0bb0\u0bcd\u0bb5\u0bbe\u0ba4\u0bbf\u0baf\u0bb0\u0bcd \u0b95\u0bbe\u0ba9\u0bae\u0bcd \u0b87\u0b9a\u0bc8\u0ba8\u0bcd\u0ba4\u0bc1 \u0baa\u0bc1\u0bb0\u0bbf\u0ba8\u0bcd\u0ba4\u0bbf\u0b9f\u0bb5\u0bc7 \u0b85\u0ba4\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0bae\u0ba4\u0bcd\u0ba4\u0bb3 \u0ba4\u0bbe\u0bb3\u0b99\u0bcd\u0b95\u0bb3\u0bcd \u0ba4\u0bc1\u0ba4\u0bcd\u0ba4\u0bbf\u0baf\u0bca\u0b9f\u0bc1\u0bae\u0bcd \u0ba8\u0baf\u0bae\u0bbe\u0ba9 \u0bb6\u0bcd\u0bb0\u0bc1\u0ba4\u0bbf \u0b95\u0bc2\u0b9f\u0bbf \u0ba4\u0bbf\u0bb5\u0bcd\u0baf <br/> \u0bb8\u0bcd\u0ba4\u0bb5\u0b95\u0bc1\u0ba3 \u0bb9\u0bb0\u0bbf \u0baa\u0b95\u0bcd\u0ba4 \u0b9c\u0ba9\u0bbe\u0ba4\u0bbf\u0baf\u0bb0\u0bcd \u0ba4\u0bae\u0bcd\u0bae\u0bc8\u0baa\u0bcd-\u0baa\u0bc1\u0b95\u0bb4\u0bcd\u0ba8\u0bcd\u0ba4\u0bbe\u0b9f\u0bbf \u0b85\u0ba4\u0bbf <br/> \u0baa\u0b95\u0bcd\u0ba4\u0bbf\u0baf\u0bc1\u0b