caa\u0ccd\u0caa\u0ccb \u0cae\u0cc2\u0cb5\u0cc1\u0cb2\u0c97\u0cc1\u0cae\u0ccd \u0cb5\u0cbe\u0cb3 \u0c97\u0cbe\u0ca8 <br/> \u0cae\u0cb3\u0cc8 \u0caa\u0cca\u0cb3\u0cbf\u0ca8\u0ccd\u0ca6\u0ccb\u0ca8\u0ccd \u0caa\u0cc1\u0ca8\u0ccd\u0ca8\u0c95\u0cc8 \u0cb8\u0cbf\u0ca6\u0cb1\u0cbe\u0cae\u0cc7 \u0caa\u0cc1\u0cb3\u0ccd\u0cb3\u0cbf\u0ca8\u0ccd \u0cb5\u0cbe\u0caf\u0ccd \u0caa\u0cbf\u0cb3\u0ca8\u0ccd\u0ca6\u0ccb\u0ca8\u0ccd \u0c9a\u0cbf\u0ca8\u0ccd\u0ca8\u0caa\u0ccd-"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0caa\u0cc1\u0cb2\u0ccd\u0cb2\u0cbe\u0c99\u0ccd\u0c95\u0cc1\u0cb3\u0cb2\u0cc8\u0c95\u0ccd- \u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cc1 \u0c8e\u0cb2\u0ccd\u0cb2\u0cbe \u0c89\u0cb2\u0c97\u0cbe\u0cb3 \u0cb5\u0cb2\u0ccd\u0cb2\u0cbe\u0ca8\u0ccd-\u0c87\u0cb5\u0ca8\u0cc8\u0caa\u0ccd-\u0caa\u0cbe\u0ca1 \u0cb5\u0cbe\u0caf\u0cbe\u0cb0 \u0ca4\u0cc7\u0ca8\u0cc2\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca8\u0cbf\u0ca8\u0cc8\u0ca8\u0ccd\u0ca6\u0cbe\u0cb2\u0cc1\u0cae\u0ccd \u0c92\u0cb0\u0cc1 \u0cb8\u0cc1\u0c96\u0cae\u0cc7 \u0ca8\u0cbf\u0ca8\u0cc8\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cbf\u0ca8\u0cc8\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cbf\u0ca8\u0cc8\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cbf\u0ca8\u0cc8\u0ca8\u0ccd\u0ca6\u0cc1 <br/> \u0cae\u0ca8\u0cae\u0ccd \u0c95\u0cb8\u0cbf\u0ca8\u0ccd\u0ca6\u0cc1 \u0c95\u0ca3\u0ccd\u0ca3\u0cc0\u0cb0\u0cc1\u0c97\u0cc1\u0c97 \u0ca8\u0ca8\u0cc8\u0ca8\u0ccd\u0ca6\u0cbe\u0cb2\u0cc1\u0cae\u0ccd \u0c92\u0cb0\u0cc1 \u0cb8\u0cc1\u0c96\u0cae\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c9e\u0cbe\u0ca8\u0cae\u0cc6\u0ca8\u0cae\u0ccd \u0c86\u0caf\u0cb0\u0ccd \u0cb5\u0cbe\u0cb3\u0ccd \u0cae\u0ca8\u0cc8\u0caf\u0cbf\u0cb2\u0ccd \u0cb5\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca4\u0cbe\u0ca8\u0cc1\u0cae\u0cc1\u0cb1\u0cb5\u0cbe\u0c97\u0cc1\u0cae\u0ccd \u0ca4\u0ca8\u0ccd\u0cae\u0cc8\u0caf\u0ca6\u0cc1 \u0cb8\u0cca\u0cb2\u0ccd\u0cb2\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c86\u0ca1\u0cc1\u0cb5\u0cbe\u0cb0\u0ccd \u0c86\u0ca1\u0cbe\u0ca6\u0cb5\u0cb0\u0ccd \u0caa\u0cc1\u0c97\u0cb3\u0ccd \u0cb8\u0cca\u0cb2\u0ccd\u0cb2\u0cbf \u0caa\u0cbe\u0ca1\u0cc1\u0cb5\u0cbe\u0cb0\u0ccd \u0caa\u0cbe\u0ca1\u0cbe\u0ca6\u0cb5\u0cb0\u0ccd \u0cae\u0cb1\u0cc8 \u0cb5\u0cb3\u0cbf \u0caa\u0cc1\u0c98 <br/> \u0ca4\u0cc7\u0ca1\u0cc1\u0cb5\u0cbe\u0cb0\u0ccd \u0ca4\u0cc7\u0ca1\u0cbe\u0ca6\u0cb5\u0cb0\u0ccd \u0caf\u0cbe\u0cb5\u0cb0\u0cc1\u0cb3\u0cae\u0cc1\u0cae\u0ccd \u0c85\u0cb1\u0cbf\u0ca8\u0ccd\u0ca6\u0cc1 \u0c85\u0cb0\u0cc1\u0cb3\u0ccd \u0cae\u0cb3\u0cc8 \u0caa\u0cca\u0cb3\u0cbf\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0ca6\u0cc8"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c85\u0c98\u0cae\u0cc1\u0cb1 \u0cb5\u0cc6\u0cb3\u0ccd\u0cb3\u0cbe\u0ca8\u0cc8 \u0c8f\u0cb1\u0cbf \u0cae\u0ca6\u0cae\u0ccd \u0c95\u0cc1\u0cb1\u0cc8\u0caf \u0c85\u0ca1\u0cbf\u0caf\u0cb5\u0cb0\u0ccd \u0c95\u0cca\u0ca1\u0cc1 \u0caa\u0cc2\u0cb8\u0ca8\u0cc8 \u0c85\u0cb3\u0cbf\u0caf\u0cb5\u0cc1\u0cae\u0ccd \u0ca4\u0cb0\u0cc1\u0cae\u0cc6\u0ca8 <br/> \u0cb5\u0ca8\u0ccd\u0ca6 \u0cae\u0cbe\u0cae\u0cb3\u0cc8\u0ca4\u0ccd-\u0ca4\u0ca1\u0cc1\u0c95\u0ccd\u0c95 \u0cae\u0cb2\u0cc8 \u0cb8\u0cc1\u0cae\u0ca8\u0ccd\u0ca6\u0cc1 \u0c85\u0cb0\u0cc1\u0cb3\u0ccd \u0cae\u0cb3\u0cc8 \u0caa\u0cca\u0cb3\u0cbf\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0ca6\u0cc8"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0cb8\u0c99\u0ccd\u0c97\u0ccb\u0ca1\u0cc1 \u0cb8\u0cbe\u0cb0\u0ccd\u0c99\u0ccd\u0c97\u0cae\u0cc1\u0cae\u0ccd \u0c86\u0cb3\u0cbf\u0caf\u0cc1\u0cae\u0ccd \u0c95\u0cc1\u0ca1\u0cc8 \u0c95\u0cae\u0ca3\u0ccd\u0ca1\u0cb2\u0cae\u0cc1\u0cae\u0ccd \u0c95\u0ccb\u0ca1\u0cb0\u0cbf \u0c95\u0ccb\u0ca6\u0ca3\u0ccd\u0ca1\u0cae\u0cc1\u0cae\u0ccd <br/> \u0c87\u0c99\u0ccd\u0c97\u0cbf\u0cb5\u0cc8 \u0c86\u0c98\u0cbe\u0ca6\u0cc6\u0ca8 \u0cb5\u0ca8\u0cae\u0cb2\u0cb0\u0ccd \u0cae\u0cbe\u0cb2\u0cc8\u0caf\u0cc1\u0cae\u0ccd \u0caa\u0cca\u0ca8\u0ccd \u0ca4\u0cc1\u0c98\u0cbf\u0cb2\u0cc1\u0cae\u0ccd \u0cae\u0cbe\u0cae\u0caf\u0cbf\u0cb2\u0ccd \u0c87\u0cb1\u0c98\u0cc1\u0cae\u0ccd <br/> \u0ca4\u0c99\u0ccd\u0c97\u0cbf\u0caf \u0cae\u0cb0\u0c95\u0ca4 \u0cae\u0cc7\u0ca8\u0cbf\u0caf\u0cc1\u0cae\u0ccd \u0c86\u0cb5\u0cb0\u0cbf\u0c95\u0ccd-\u0c95\u0cca\u0cae\u0ccd\u0cac\u0ccb\u0ca1\u0cc1 \u0c95\u0cc1\u0cb3\u0cb2\u0cc1\u0cae\u0ccd \u0c95\u0cc1\u0c9e\u0ccd\u0c9a\u0cbf\u0ca4\u0caa\u0ccd-\u0caa\u0ca6\u0cae\u0cc1\u0cae\u0ccd <br/> \u0c8e\u0c99\u0ccd\u0c97\u0cc6\u0c99\u0ccd\u0c97\u0cc1 \u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cbf\u0ca8\u0cc1\u0cae\u0ccd \u0caa\u0cca\u0c99\u0ccd\u0c97\u0cc1 \u0c95\u0cc1\u0cb1\u0cc1 \u0ca8\u0c98\u0cc8\u0caf\u0cc1\u0cae\u0ccd \u0cae\u0c99\u0ccd\u0c97\u0cb3 \u0cae\u0cc1\u0c96\u0cae\u0cca\u0ca1\u0cc1 \u0cae\u0cbe\u0ca8\u0ccd\u0cae\u0ca4 \u0ca4\u0cbf\u0cb2\u0c95\u0cae\u0cc1\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0c85\u0cb0\u0cc8\u0c95\u0ccd\u0c95\u0cb8\u0cc8\u0ca8\u0ccd\u0ca6 \u0c9c\u0ccd\u0c9e\u0cbe\u0ca3\u0cca\u0ca1\u0cc1 \u0caa\u0cc1\u0cb2\u0cbf \u0ca8\u0c96\u0cae\u0ccd \u0c85\u0cb8\u0cc8\u0ca8\u0ccd\u0ca6\u0cbe\u0ca1 \u0c95\u0cbf\u0c99\u0ccd\u0c95\u0cbf\u0ca3\u0cbf \u0cb8\u0ca6\u0c99\u0ccd\u0c97\u0cc8\u0caf\u0cca\u0cb2\u0cbf <br/> \u0c95\u0cb1\u0cb5\u0cc8\u0c97\u0cb3\u0ccd \u0c85\u0ca3\u0cbf\u0caa\u0ca3\u0cbf \u0ca8\u0cbe\u0ca6\u0cae\u0ccb\u0ca1\u0cc1 \u0c95\u0cbe\u0ca6\u0cb2\u0ccd \u0cae\u0ca1\u0cb5\u0cbe\u0cb0\u0ccd\u0ca4\u0cb0\u0cc1 \u0c95\u0cb0\u0cb5\u0ca8\u0cc8\u0caf\u0cca\u0cb2\u0cbf\u0c97\u0cb3\u0cc1\u0cae\u0ccd <br/> \u0c95\u0ca8\u0ccd\u0cb1\u0cc1 \u0c95\u0cc1\u0cb3\u0cae\u0ccd \u0caa\u0cca\u0cb2\u0cbf\u0caf\u0cc1\u0cae\u0ccd \u0cae\u0cb2\u0cb0\u0cbf\u0ca8\u0ccd \u0cae\u0ca6\u0cc1 \u0cb5\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cc1\u0c98\u0cb0\u0ccd\u0ca8\u0ccd\u0ca6\u0cbf\u0ca1\u0cc1 \u0cb5\u0ca3\u0ccd\u0ca1\u0cca\u0cb2\u0cbf\u0caf\u0cc1\u0cae\u0ccd \u0c95\u0cb2\u0cae\u0ccd <br/> \u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cc1 \u0c87\u0cb8\u0cc8\u0caf\u0cc1\u0cae\u0ccd \u0cb8\u0cbf\u0cb1\u0cc1\u0cb5\u0cb0\u0ccd\u0c97\u0cb3\u0ccd \u0caa\u0cb2\u0cb0\u0cc1\u0ca1\u0ca8\u0ccd \u0cb5\u0ca8\u0cae\u0ccd \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cc1 \u0c95\u0cc1\u0cb3\u0cb2\u0cc2\u0ca6\u0cc1\u0cae\u0ccd \u0caa\u0ca3\u0ccd\u0ca3\u0cbf\u0cb8\u0cc8\u0caf\u0cc1\u0cae\u0ccd \u0c87\u0c99\u0ccd\u0c97\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 7", "T": "\u0cae\u0cb1\u0cc8 \u0cae\u0cc1\u0ca6\u0cb2\u0cbf\u0ca1\u0cc8 \u0c85\u0ca8\u0ccd\u0ca6\u0cae\u0cbe\u0c98\u0cbf\u0caf \u0caa\u0cca\u0cb0\u0cc1\u0cb3\u0cc1\u0cae\u0ccd \u0cae\u0ca3\u0ccd \u0cb5\u0cbf\u0ca3\u0ccd\u0ca3\u0cca\u0ca1\u0cc1 \u0caa\u0cb0\u0ca8\u0ccd\u0ca6\u0ca6\u0cca\u0cb0\u0cc1 \u0cb5\u0cc6\u0cb3\u0cbf\u0caf\u0cc1\u0cae\u0ccd <br/> \u0c85\u0cb1\u0cae\u0cc1\u0cae\u0ccd \u0c85\u0cb1\u0cae\u0ccd \u0ca4\u0cae\u0c95\u0ccd\u0c95\u0ccb\u0cb0\u0ccd \u0c89\u0cb0\u0cc1\u0cb5\u0cae\u0cc1\u0cae\u0ccd \u0c85\u0cb1\u0cbf\u0cb5\u0cc1\u0cae\u0ccd \u0c86\u0cb5\u0ca8\u0cb5\u0cc1\u0cae\u0ccd \u0ca8\u0cbe\u0ca8\u0cc7 \u0c8e\u0ca8 <br/> \u0caa\u0cbf\u0cb1\u0cc8 \u0cb5\u0cb3\u0cbf \u0cb5\u0ca8\u0ccd\u0ca6\u0cbe\u0cb0\u0ccd\u0c95\u0ccd-\u0c95\u0cca\u0ca1\u0cc1 \u0cb8\u0cae\u0cb0\u0cbf\u0ca1\u0cc8\u0caf\u0cbf\u0ca8\u0cbf\u0cb2\u0ccd \u0caa\u0cc6\u0cb0\u0cc1\u0cae\u0ccd \u0caa\u0cb5\u0cb3 \u0cae\u0cb2\u0cb0\u0ccd\u0c95\u0ccd-\u0c95\u0cca\u0ca1\u0cbf \u0ca8\u0cbf\u0cb3\u0cb2\u0cc1\u0cae\u0ccd <br/> \u0ca4\u0cbf\u0cb1\u0ca8\u0ccd\u0ca6\u0cb0\u0cc1\u0cb3\u0cc1\u0cae\u0ccd \u0cb5\u0cbe\u0caf\u0cbf\u0ca6\u0cc1\u0cb5\u0cc7 \u0cae\u0ca8\u0cc8\u0ca6\u0cca\u0cb0\u0cc1\u0cae\u0ccd \u0ca4\u0cc7\u0ca1\u0cbf \u0cb5\u0cc6\u0ca3\u0ccd\u0ca3\u0cc8\u0caf\u0cc1\u0ca3\u0ccd\u0ca1 \u0cb5\u0cbe\u0caf\u0ccd \u0c87\u0ca6\u0cc1\u0cb5\u0cc7 \u0c8e\u0ca8"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 8", "T": "\u0c8e\u0ca8\u0cc8\u0caf\u0cbe\u0cb3\u0cc1\u0cae\u0ccd \u0c88\u0cb0\u0cc7\u0cb3\u0cc1 \u0c89\u0cb2\u0c97\u0cbe\u0cb3\u0cc1\u0cae\u0ccd \u0c85\u0ca1\u0cbf\u0caf\u0cb5\u0cb0\u0ccd \u0cae\u0ca8\u0cae\u0cbe\u0cb3\u0cc1\u0cae\u0ccd \u0caf\u0ca6\u0cc1\u0c95\u0cc1\u0cb2\u0cae\u0ccd \u0ca4\u0ca8\u0cc8\u0caf\u0cbe\u0cb3\u0cc1\u0cae\u0ccd \u0c95\u0ca3\u0ccd\u0ca3\u0ca8\u0cc8 <br/> \u0cae\u0cc1\u0ca8\u0cc8\u0ca8\u0ccd\u0ca6\u0ccb\u0ca1\u0cc1\u0cae\u0ccd \u0caf\u0cae\u0cc1\u0ca8\u0cc8\u0ca4\u0ccd \u0ca4\u0cc1\u0cb1\u0cc8\u0cb5\u0ca8\u0cc8 \u0c8e\u0cb5\u0ccd\u0cb5\u0cc1\u0caf\u0cbf\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd \u0c87\u0cb1\u0cc8\u0cb5\u0ca8\u0cc8 \u0c8e\u0ca8\u0ccd \u0cae\u0ca8 \u0ca8\u0cbf\u0cb1\u0cc8\u0cb5\u0ccb\u0ca8\u0cc8\u0caa\u0ccd \u0caa\u0cbe\u0ca1\u0caa\u0ccd \u0caa\u0cbe\u0ca1"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d41\u0d28\u0d4d \u0d38\u0d46\u0d2f\u0d4d\u0d26 \u0d24\u0d35\u0d2a\u0d4d-\u0d2a\u0d2f\u0d28\u0d47 \u0d0e\u0d19\u0d4d\u0d17\u0d33\u0d4d \u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3f \u0d24\u0d30\u0d41\u0d2e\u0d4d \u0d2e\u0d3e\u0d27\u0d35\u0d28\u0d48 \u0d2d\u0d15\u0d4d\u0d24\u0d3f \u0d38\u0d46\u0d2f\u0d4d\u0d2f \u0d15\u0d3f\u0d21\u0d48\u0d24\u0d4d\u0d24\u0d26\u0d41"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d41\u0d28\u0d4d\u0d28\u0d4a\u0d30\u0d41 \u0d15\u0d3e\u0d32\u0d21\u0d3f\u0d2f\u0d3f\u0d32\u0d4d \u0d2e\u0d42\u0d35\u0d41\u0d32\u0d17\u0d33\u0d28\u0d4d\u0d26\u0d4b\u0d28\u0d4d \u0d07\u0d2a\u0d4d\u0d2a\u0d4b \u0d2e\u0d42\u0d35\u0d41\u0d32\u0d17\u0d41\u0d2e\u0d4d \u0d35\u0d3e\u0d34 \u0d17\u0d3e\u0d28 <br/> \u0d2e\u0d34\u0d48 \u0d2a\u0d4a\u0d34\u0d3f\u0d28\u0d4d\u0d26\u0d4b\u0d28\u0d4d \u0d2a\u0d41\u0d28\u0d4d\u0d28\u0d15\u0d48 \u0d38\u0d3f\u0d26\u0d31\u0d3e\u0d2e\u0d47 \u0d2a\u0d41\u0d33\u0d4d\u0d33\u0d3f\u0d28\u0d4d \u0d35\u0d3e\u0d2f\u0d4d \u0d2a\u0d3f\u0d33\u0d28\u0d4d\u0d26\u0d4b\u0d28\u0d4d \u0d1a\u0d3f\u0d28\u0d4d\u0d28\u0d2a\u0d4d-"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d2a\u0d41\u0d32\u0d4d\u0d32\u0d3e\u0d19\u0d4d\u0d15\u0d41\u0d34\u0d32\u0d48\u0d15\u0d4d- \u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d41 \u0d0e\u0d32\u0d4d\u0d32\u0d3e \u0d09\u0d32\u0d17\u0d3e\u0d33 \u0d35\u0d32\u0d4d\u0d32\u0d3e\u