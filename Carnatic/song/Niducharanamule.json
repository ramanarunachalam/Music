u0c24\u0c41\u0c21\u0c28\u0c3f <br/> \u0c35\u0c46\u0c32\u0c2f\u0c41 \u0c36\u0c4d\u0c30\u0c40 \u0c38\u0c30\u0c4d\u0c35\u0c47\u0c36 \u0c30\u0c3e\u0c2e"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c2a\u0c3e\u0c2a \u0c15\u0c30\u0c4d\u0c2e\u0c2e\u0c41 \u0c28\u0c3f\u0c23\u0c4d\u0c21\u0c41\u0c30\u0c3e\u0c2f\u0c3f\u0c28\u0c4d\u0c26\u0c41 <br/> \u0c2c\u0c3e\u0c27 \u0c2a\u0c21 \u0c28\u0c47 \u0c1c\u0c3e\u0c32\u0c30\u0c3e <br/> \u0c0f \u0c35\u0c17\u0c17\u0c3e\u0c28\u0c48\u0c28\u0c28\u0c41 <br/> \u0c28\u0c28\u0c4d\u0c28\u0c47\u0c32\u0c41\u0c15\u0c4b \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c \u0c28\u0c41\u0c24"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0ca8\u0cc0\u0ca6\u0cc1 \u0c9a\u0cb0\u0ca3\u0cae\u0cc1\u0cb2\u0cc7 \u0c97\u0ca4\u0cbf\u0caf\u0ca8\u0cbf <br/> \u0ca8\u0cc6\u0cb0\u0ca4 \u0ca8\u0cae\u0ccd\u0cae\u0cbf\u0ca8 \u0cb5\u0cbe\u0ca1\u0ca8\u0cc1\u0cb0\u0cbe \u0cb0\u0cbe\u0cae"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb5\u0cc7\u0ca6 \u0cb5\u0cc7\u0ca6\u0cbe\u0ca8\u0ccd\u0ca4 \u0cb5\u0cbf\u0ca6\u0cbf\u0ca4\u0cc1\u0ca1\u0ca8\u0cbf <br/> \u0cb5\u0cc6\u0cb2\u0caf\u0cc1 \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb8\u0cb0\u0ccd\u0cb5\u0cc7\u0cb6 \u0cb0\u0cbe\u0cae"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0caa\u0cbe\u0caa \u0c95\u0cb0\u0ccd\u0cae\u0cae\u0cc1 \u0ca8\u0cbf\u0ca3\u0ccd\u0ca1\u0cc1\u0cb0\u0cbe\u0caf\u0cbf\u0ca8\u0ccd\u0ca6\u0cc1 <br/> \u0cac\u0cbe\u0ca7 \u0caa\u0ca1 \u0ca8\u0cc7 \u0c9c\u0cbe\u0cb2\u0cb0\u0cbe <br/> \u0c8f \u0cb5\u0c97\u0c97\u0cbe\u0ca8\u0cc8\u0ca8\u0ca8\u0cc1 <br/> \u0ca8\u0ca8\u0ccd\u0ca8\u0cc7\u0cb2\u0cc1\u0c95\u0ccb \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c \u0ca8\u0cc1\u0ca4"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d28\u0d40\u0d26\u0d41 \u0d1a\u0d30\u0d23\u0d2e\u0d41\u0d32\u0d47 \u0d17\u0d24\u0d3f\u0d2f\u0d28\u0d3f <br/> \u0d28\u0d46\u0d30\u0d24 \u0d28\u0d2e\u0d4d\u0d2e\u0d3f\u0d28 \u0d35\u0d3e\u0d21\u0d28\u0d41\u0d30\u0d3e \u0d30\u0d3e\u0d2e"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d35\u0d47\u0d26 \u0d35\u0d47\u0d26\u0d3e\u0d28\u0d4d\u0d24 \u0d35\u0d3f\u0d26\u0d3f\u0d24\u0d41\u0d21\u0d28\u0d3f <br/> \u0d35\u0d46\u0d32\u0d2f\u0d41 \u0d36\u0d4d\u0d30\u0d40 \u0d38\u0d30\u0d4d\u0d35\u0d47\u0d36 \u0d30\u0d3e\u0d2e"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d2a\u0d3e\u0d2a \u0d15\u0d30\u0d4d\u0d2e\u0d2e\u0d41 \u0d28\u0d3f\u0d23\u0d4d\u0d21\u0d41\u0d30\u0d3e\u0d2f\u0d3f\u0d28\u0d4d\u0d26\u0d41 <br/> \u0d2c\u0d3e\u0d27 \u0d2a\u0d21 \u0d28\u0d47 \u0d1c\u0d3e\u0d32\u0d30\u0d3e <br/> \u0d0f \u0d35\u0d17\u0d17\u0d3e\u0d28\u0d48\u0d28\u0d28\u0d41 <br/> \u0d28\u0d28\u0d4d\u0d28\u0d47\u0d32\u0d41\u0d15\u0d4b \u0d24\u0d4d\u0d2f\u0d3e\u0d17\u0d30\u0d3e\u0d1c \u0d28\u0d41\u0d24"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0928\u0940\u0926\u0941 \u091a\u0930\u0923\u092e\u0941\u0932\u0947 \u0917\u0924\u093f\u092f\u0928\u093f <br/> \u0928\u0947\u0930\u0924 \u0928\u092e\u094d\u092e\u093f\u0928 \u0935\u093e\u0921\u0928\u0941\u0930\u093e \u0930\u093e\u092e"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0935\u0947\u0926 \u0935\u0947\u0926\u093e\u0928\u094d\u0924 \u0935\u093f\u0926\u093f\u0924\u0941\u0921\u0928\u093f <br/> \u0935\u0947\u0932\u092f\u0941 \u0936\u094d\u0930\u0940 \u0938\u0930\u094d\u0935\u0947\u0936 \u0930\u093e\u092e"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u092a\u093e\u092a \u0915\u0930\u094d\u092e\u092e\u0941 \u0928\u093f\u0923\u094d\u0921\u0941\u0930\u093e\u092f\u093f\u0928\u094d\u0926\u0941 <br/> \u092c\u093e\u0927 \u092a\u0921 \u0928\u0947 \u091c\u093e\u0932\u0930\u093e <br/> \u090f \u0935\u0917\u0917\u093e\u0928\u0948\u0928\u0928\u0941 <br/> \u0928\u0928\u094d\u0928\u0947\u0932\u0941\u0915\u094b \u0924\u094d\u092f\u093e\u0917\u0930\u093e\u091c \u0928\u0941\u0924"}]}], "L": "l", "lyricsref": [{"links": [{"N": "thyagaraja-vaibhavam.blogspot.com", "L": "http: