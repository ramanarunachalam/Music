u0c9c\u0cbf\u0cb2\u0ccd\u0cb2 \u0caa\u0cb2\u0ccd\u0c95\u0cc7 \u0cae\u0ca6\u0ca8 \u0c9c\u0ca8\u0c95\u0cc1\u0ca1\u0cc1 (\u0caa\u0cbe\u0cb9\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0cb5\u0cb2\u0ccd\u0cb5\u0cb2\u0cc1 \u0ca6\u0cbf\u0ca6\u0ccd\u0ca6\u0cbf \u0cb8\u0ccc\u0cae\u0cbf\u0ca4\u0ccd\u0cb0\u0cbf \u0cb5\u0cb2\u0c9a\u0cbf \u0ca8\u0cbf\u0cb2\u0ccd\u0cb5\u0c97\u0cbe <br/> \u0c95\u0cb2\u0cc1\u0cb5 \u0cb0\u0cc7\u0c95\u0cc1\u0cb2\u0ca8\u0cc1 \u0c95\u0cc7\u0cb0\u0cc1 \u0c95\u0ca8\u0cc1\u0cb2 \u0c9c\u0cc2\u0c9a\u0cc6\u0ca8\u0cc1 (\u0caa\u0cbe\u0cb9\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cad\u0cb0\u0ca4\u0cc1\u0ca1\u0ccd(\u0c86) \u0cb5\u0cc7\u0cb3 \u0c95\u0cb0\u0c97\u0cbf \u0c95\u0cb0\u0c97\u0cbf \u0ca8\u0cbf\u0cb2\u0ccd\u0cb5\u0c97\u0cbe <br/> \u0c95\u0cb0\u0cae\u0cc1 \u0caa\u0c9f\u0ccd\u0c9f\u0cbf \u0c95\u0ccc\u0c97\u0cbf\u0cb2\u0cbf\u0ca8\u0ccd\u0c9a\u0cc7 \u0cb5\u0cb0\u0ca6\u0cc1\u0ca1\u0ccd(\u0c85)\u0caa\u0ccd\u0caa\u0cc1\u0ca1\u0cc1 (\u0caa\u0cbe\u0cb9\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c9a\u0ca3\u0ccd\u0ca1 \u0cb6\u0ca4\u0ccd\u0cb0\u0cc1\u0c98\u0ccd\u0ca8\u0cc1\u0ca1\u0ccd(\u0c85)\u0caa\u0ccd\u0caa\u0cc1\u0ca1\u0ccd(\u0c85)\u0c96\u0ca3\u0ccd\u0ca1 \u0cb5\u0cc3\u0ca4\u0ccd\u0ca4\u0cbf\u0ca4\u0ccb\u0ca8\u0ccd- <br/> (\u0c89)\u0ca3\u0ccd\u0ca1 \u0cb8\u0ca8\u0ccd\u0ca4\u0cb8\u0cbf\u0cb2\u0ccd\u0cb2\u0cc7 \u0c95\u0ccb\u0ca6\u0ca3\u0ccd\u0ca1 \u0cb0\u0cbe\u0cae\u0cc1\u0ca1\u0cc1 (\u0caa\u0cbe\u0cb9\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0cae\u0ca8\u0cb8\u0cc1 \u0ca4\u0cc6\u0cb2\u0cbf\u0cb8\u0cbf \u0c95\u0cb2\u0cb8\u0cbf \u0cb9\u0ca8\u0cc1\u0cae\u0ca8\u0ccd\u0ca4\u0cc1\u0ca1\u0ccd(\u0c89)\u0ca3\u0ccd\u0ca1\u0c97\u0cbe <br/> \u0c9a\u0ca8\u0cc1\u0cb5\u0cc1 \u0cae\u0cbe\u0c9f\u0cb2\u0ccd(\u0c86)\u0ca1\u0cc1\u0c9a\u0ccd(\u0c89)\u0ca3\u0ccd\u0ca1\u0cc7 \u0cb8\u0cbe\u0cb0\u0ccd\u0cb5\u0cad\u0ccc\u0cae\u0cc1\u0ca1\u0cc1 (\u0caa\u0cbe\u0cb9\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0cb5\u0cc0\u0cb0\u0cbf \u0c95\u0cb0\u0cc1\u0ca3 \u0c95\u0cb2\u0cbf\u0c97\u0cbf(\u0caf\u0ccd)\u0c8e\u0caa\u0cc1\u0ca1\u0cc1 \u0cb5\u0cc6\u0cb2\u0cb8\u0cbf(\u0caf\u0ccd)\u0c89\u0ca8\u0ccd\u0ca6\u0cc1\u0ca8\u0ccb <br/> \u0cb8\u0cbe\u0cb0\u0cae\u0cc8\u0ca8 \u0cad\u0c95\u0ccd\u0ca4\u0cbf\u0c9a\u0cc7 \u0cb8\u0ca8\u0ccd-\u0ca8\u0cc1\u0ca4\u0cbf\u0ca8\u0ccd\u0ca4\u0cc1\u0ca8\u0ccb (\u0caa\u0cbe\u0cb9\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 7", "T": "\u0ca7\u0cb0\u0ccd\u0cae\u0ccd(\u0c86)\u0cb0\u0ccd\u0ca5 \u0c95\u0cbe\u0cae \u0cae\u0ccb\u0c95\u0ccd\u0cb7 \u0ca6\u0cbe\u0ca8\u0cae\u0ccd(\u0c8f)\u0cb2\u0ca8\u0cc7 <br/> \u0cae\u0cb0\u0ccd\u0cae\u0cae\u0ccd(\u0c8e)\u0cb0\u0cc1\u0c97 \u0cb2\u0cc7\u0ca8\u0cbf \u0c87\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0cb6\u0cb0\u0ccd\u0cae\u0cae\u0ccd(\u0c8f)\u0cb2\u0ca8\u0cc7 (\u0caa\u0cbe\u0cb9\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 8", "T": "\u0cac\u0cbe\u0c97 \u0c95\u0cb0\u0cc1\u0ca3 \u0c9c\u0cc7\u0cb8\u0cbf(\u0caf\u0ccd)\u0c8e\u0caa\u0cc1\u0ca1\u0cc1 \u0cad\u0cb5\u0ccd\u0caf\u0cae\u0ccd(\u0c92)\u0cb8\u0c97\u0cc1\u0ca8\u0ccb <br/> \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c\u0cc1 \u0c9a\u0cc7\u0caf\u0cbf \u0caa\u0c9f\u0ccd\u0c9f\u0cbf \u0ca6\u0caf\u0ca8\u0cc1 \u0cac\u0ccd\u0cb0\u0ccb\u0c9a\u0cc1\u0ca8\u0ccb (\u0caa\u0cbe\u0cb9\u0cbf)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2a\u0d3e\u0d39\u0d3f \u0d30\u0d3e\u0d2e \u0d30\u0d3e\u0d2e(\u0d2f\u0d4d)\u0d05\u0d28\u0d41\u0d1a\u0d41 \u0d2d\u0d1c\u0d28 \u0d38\u0d47\u0d2f\u0d35\u0d47"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d15\u0d28\u0d3f\u0d15\u0d30\u0d2e\u0d4d\u0d2c\u0d41 \u0d15\u0d32\u0d4d\u0d17\u0d3f \u0d38\u0d40\u0d24\u0d3e \u0d15\u0d3e\u0d28\u0d4d\u0d24\u0d41\u0d28\u0d3f \u0d15\u0d28\u0d17\u0d3e <br/> \u0d2e\u0d28\u0d38\u0d41 \u0d30\u0d28\u0d4d\u0d1c\u0d3f\u0d32\u0d4d\u0d32 \u0d2a\u0d32\u0d4d\u0d15\u0d47 \u0d2e\u0d26\u0d28 \u0d1c\u0d28\u0d15\u0d41\u0d21\u0d41 (\u0d2a\u0d3e\u0d39\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d35\u0d32\u0d4d\u0d35\u0d32\u0d41 \u0d26\u0d3f\u0d26\u0d4d\u0d26\u0d3f \u0d38\u0d4c\u0d2e\u0d3f\u0d24\u0d4d\u0d30\u0d3f \u0d35\u0d32\u0d1a\u0d3f \u0d28\u0d3f\u0d32\u0d4d\u0d35\u0d17\u0d3e <br/> \u0d15\u0d32\u0d41\u0d35 \u0d30\u0d47\u0d15\u0d41\u0d32\u0d28\u0d41 \u0d15\u0d47\u0d30\u0d41 \u0d15\u0d28\u0d41\u0d32 \u0d1c\u0d42\u0d1a\u0d46\u0d28\u0d41 (\u0d2a\u0d3e\u0d39\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d2d\u0d30\u0d24\u0d41\u0d21\u0d4d(\u0d06) \u0d35\u0d47\u0d33 \u0d15\u0d30\u0d17\u0d3f \u0d15\u0d30\u0d17\u0d3f \u0d28\u0d3f\u0d32\u0d4d\u0d35\u0d17\u0d3e <br/> \u0d15\u0d30\u0d2e\u0d41 \u0d2a\u0d1f\u0d4d\u0d1f\u0d3f \u0d15\u0d4c\u0d17\u0d3f\u0d32\u0d3f\u0d28\u0d4d\u0d1a\u0d47 \u0d35\u0d30\u0d26\u0d41\u0d21\u0d4d(\u0d05)\u0d2a\u0d4d\u0d2a\u0d41\u0d21\u0d41 (\u0d2a\u0d3e\u0d39\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d1a\u0d23\u0d4d\u0d21 \u0d36\u0d24\u0d4d\u0d30\u0d41\u0d18\u0d4d\u0d28\u0d41\u0d21\u0d4d(\u0d05)\u0d2a\u0d4d\u0d2a\u0d41\u0d21\u0d4d(\u0d05)\u0d16\u0d23\u0d4d\u0d21 \u0d35\u0d43\u0d24\u0d4d\u0d24\u0d3f\u0d24\u0d4b\u0d28\u0d4d- <br/> (\u0d09)\u0d23\u0d4d\u0d21 \u0d38\u0d28\u0d4d\u0d24\u0d38\u0d3f\u0d32\u0d4d\u0d32\u0d47 \u0d15\u0d4b\u0d26\u0d23\u0d4d\u0d21 \u0d30\u0d3e\u0d2e\u0d41\u0d21\u0d41 (\u0d2a\u0d3e\u0d39\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 5", "T": "\u0d2e\u0d28\u0d38\u0d41 \u0d24\u0d46\u0d32\u0d3f\u0d38\u0d3f \u0d15\u0d32\u0d38\u0d3f \u0d39\u0d28\u0d41\u0d2e\u0d28\u0d4d\u0d24\u0d41\u0d21\u0d4d(\u0d09)\u0d23\u0d4d\u0d21\u0d17\u0d3e <br/> \u0d1a\u0d28\u0d41\u0d35\u0d41 \u0d2e\u0d3e\u0d1f\u0d32\u0d4d(\u0d06)\u0d21\u0d41\u0d1a\u0d4d(\u0d09)\u0d23\u0d4d\u0d21\u0d47 \u0d38\u0d3e\u0d30\u0d4d\u0d35\u0d2d\u0d4c\u0d2e\u0d41\u0d21\u0d41 (\u0d2a\u0d3e\u0d39\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 6", "T": "\u0d35\u0d40\u0d30\u0d3f \u0d15\u0d30\u0d41\u0d23 \u0d15\u0d32\u0d3f\u0d17\u0d3f(\u0d2f\u0d4d)\u0d0e\u0d2a\u0d41\u0d21\u0d41 \u0d35\u0d46\u0d32\u0d38\u0d3f(\u0d2f\u0d4d