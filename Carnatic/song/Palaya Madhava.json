d3e\u0d35\u0d30\u0d40\u0d36 \u0d35\u0d26\u0d28\u0d3e\u0d2e\u0d32\u0d3e\u0d28\u0d41\u0d2a\u0d2e"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d28\u0d40\u0d32 \u0d1c\u0d32\u0d26\u0d38\u0d2e \u0d38\u0d2e \u0d32\u0d4b\u0d15\u0d3e\u0d26\u0d3f \u0d17\u0d23\u0d39\u0d30\u0d23 \u0d28\u0d3f\u0d2a\u0d41\u0d23 <br/> \u0d15\u0d2e\u0d28\u0d40\u0d2f \u0d1a\u0d30\u0d3f\u0d24 \u0d2e\u0d4d\u0d30\u0d4d\u0d26\u0d41 \u0d35\u0d1a\u0d28 \u0d1a\u0d48\u0d32\u0d27\u0d3e\u0d30\u0d23 \u0d39\u0d4d\u0d30\u0d4d\u0d24\u0d2e\u0d26 \u0d26\u0d47\u0d35 \u0d2a\u0d30\u0d3f\u0d35\u0d4d\u0d30\u0d4d\u0d22 \u0d38\u0d41\u0d30\u0d41\u0d1a\u0d3f\u0d30 <br/> \u0d38\u0d3e\u0d30\u0d38\u0d3e\u0d2f\u0d24 \u0d35\u0d3f\u0d32\u0d4b\u0d1a\u0d28 \u0d2e\u0d02\u0d1c\u0d41\u0d33 \u0d38\u0d3e\u0d2e\u0d17\u0d3e\u0d28 \u0d30\u0d38\u0d3f\u0d15\u0d3e\u0d24\u0d3f \u0d2e\u0d28\u0d4b\u0d39\u0d30"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d2d\u0d4b\u0d17\u0d3f \u0d36\u0d2f\u0d28 \u0d28\u0d24\u0d1c\u0d28 \u0d36\u0d4b\u0d15\u0d4c\u0d16 \u0d35\u0d3f\u0d36\u0d2e \u0d17\u0d41\u0d23 \u0d28\u0d3f\u0d35\u0d38\u0d28 <br/> \u0d2c\u0d4b\u0d27\u0d2e\u0d2f \u0d2d\u0d41\u0d35\u0d28\u0d2e\u0d39\u0d3f\u0d24 \u0d28\u0d3e\u0d17\u0d35\u0d30 \u0d38\u0d2e \u0d17\u0d2e\u0d28 \u0d28\u0d3e\u0d30\u0d26 \u0d2e\u0d41\u0d16 \u0d38\u0d47\u0d35\u0d3f\u0d24 <br/> \u0d38\u0d3e\u0d17\u0d30\u0d3e\u0d32\u0d2f \u0d17\u0d2d\u0d40\u0d30 \u0d28\u0d28\u0d4d\u0d26 \u0d38\u0d41\u0d24 \u0d38\u0d3e\u0d2e\u0d1c\u0d47\u0d28\u0d4d\u0d26\u0d4d\u0d30 \u0d35\u0d30\u0d26\u0d3e\u0d30\u0d4d\u0d24\u0d4d\u0d24 \u0d38\u0d41\u0d30\u0d26\u0d4d\u0d30\u0d41\u0d2e"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092a\u093e\u0932\u092f \u092e\u093e\u0927\u0935 \u092e\u093e\u092e\u094d \u0905\u0928\u093f\u0936\u092e\u094d \u092e\u0941\u0926\u093e \u0938\u0926\u093e \u0928\u093f\u0926\u093e\u0928\u0938\u092e\u094d"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092b\u093e\u0932\u0924\u0932 \u092a\u0930\u093f\u0932\u0938\u093f\u0924 \u092e\u0902\u091c\u0941\u0932 \u0939\u0930\u093f\u0923 \u092e\u0926 \u0924\u093f\u0932\u0915! <br/> \u092a\u0928\u094d\u0928\u0917\u093e\u0930\u093f \u0935\u0930 \u0935\u093e\u0939\u0928 \u092e\u094b\u0939\u0928 \u092a\u0926\u094d\u092e\u0928\u093e\u092d \u092d\u0941\u0935\u0928\u0924\u094d\u0930\u094d\u092f \u0928\u093e\u092f\u0915"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0936\u094d\u0930\u0940\u0915\u0930 \u092f\u0941\u0917 \u0938\u0930\u0938\u093f\u091c \u0938\u093e\u092e\u094b\u0926\u092a\u0930\u093f \u092e\u094d\u0930\u094d\u0926\u093f\u0924 \u091a\u0930\u0923\u0924\u0932 <br/> \u091a\u0947\u0926\u093f\u092e\u0939\u093f\u092a\u0924\u0928\u094d-\u0909\u0926\u0932\u0928 \u092a\u093e\u0915 \u0930\u093f\u092a\u0941 \u0935\u093f\u0928\u0941\u0924 \u0917\u0941\u0923 \u0926\u0940\u0928 \u0936\u0930\u0923 \u092f\u0926\u0941\u0935\u0930 <br/> \u092d\u093e\u0928\u0941 \u092d\u093e\u0938\u0941\u0930 \u0915\u093f\u0930\u0940\u091f \u0936\u094b\u092d\u093f\u0924 \u0935\u093f\u092d\u093e\u0935\u0930\u0940\u0936 \u0935\u0926\u0928\u093e\u092e\u0932\u093e\u0928\u0941\u092a\u092e"}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u0928\u0940\u0932 \u091c\u0932\u0926\u0938\u092e \u0938\u092e \u0932\u094b\u0915\u093e\u0926\u093f \u0917\u0923\u0939\u0930\u0923 \u0928\u093f\u092a\u0941\u0923 <br/> \u0915\u092e\u0928\u0940\u092f \u091a\u0930\u093f\u0924 \u092e\u094d\u0930\u094d\u0926\u0941 \u0935\u091a\u0928 \u091a\u0948\u0932\u0927\u093e\u0930\u0923 \u0939\u094d\u0930\u094d\u0924\u092e\u0926 \u0926\u0947\u0935 \u092a\u0930\u093f\u0935\u094d\u0930\u094d\u0922 \u0938\u0941\u0930\u0941\u091a\u093f\u0930 <br/> \u0938\u093e\u0930\u0938\u093e\u092f\u0924 \u0935\u093f\u0932\u094b\u091a\u0928 \u092e\u0902\u091c\u0941\u0932 \u0938\u093e\u092e\u0917\u093e\u0928 \u0930\u0938\u093f\u0915\u093e\u0924\u093f \u092e\u0928\u094b\u0939\u0930"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u092d\u094b\u0917\u093f \u0936\u092f\u0928 \u0928\u0924\u091c\u0928 \u0936\u094b\u0915\u094c\u0916 \u0935\u093f\u0936\u092e \u0917\u0941\u0923 \u0928\u093f\u0935\u0938\u0928 <br/> \u092c\u094b\u0927\u092e\u092f \u092d\u0941\u0935\u0928\u092e\u0939\u093f\u0924 \u0928\u093e\u0917\u0935\u0930 \u0938\u092e \u0917\u092e\u0928 \u0928\u093e\u0930\u0926 \u092e\u0941\u0916 \u0938\u0947\u0935\u093f\u0924 <br/> \u0938\u093e\u0917\u0930\u093e\u0932\u092f \u0917\u092d\u0940\u0930 \u0928\u0928\u094d\u0926 \u0938\u0941\u0924 \u0938\u093e\u092e\u091c\u0947\u0928\u094d\u0926\u094d\u0930 \u0935\u0930\u0926\u093e\u0930\u094d\u0924\u094d\u0924 \u0938\u0941\u0930\u0926\u094d\u0930\u0941\u092e"}]}], "L": "l", "lyricsref": [{"links": [{"N": "swathithirunalfestival.org", "L": "http://swathithirunalfestival.org/compositions/palaya-madhava"}]}]}