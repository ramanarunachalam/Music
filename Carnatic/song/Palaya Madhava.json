u0bbf\u0bb2\u0b95! <br/> \u0baa\u0ba9\u0bcd\u0ba9\u0b95\u0bbe\u0bb0\u0bbf \u0bb5\u0bb0 \u0bb5\u0bbe\u0bb9\u0ba9 \u0bae\u0bcb\u0bb9\u0ba9 \u0baa\u0ba4\u0bcd\u0bae\u0ba9\u0bbe\u0baa \u0baa\u0bc1\u0bb5\u0ba9\u0ba4\u0bcd\u0bb0\u0bcd\u0baf \u0ba8\u0bbe\u0baf\u0b95"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0bb6\u0bcd\u0bb0\u0bc0\u0b95\u0bb0 \u0baf\u0bc1\u0b95 \u0b9a\u0bb0\u0b9a\u0bbf\u0b9c \u0b9a\u0bbe\u0bae\u0bcb\u0ba4\u0baa\u0bb0\u0bbf \u0bae\u0bcd\u0bb0\u0bcd\u0ba4\u0bbf\u0ba4 \u0b9a\u0bb0\u0ba3\u0ba4\u0bb2 <br/> \u0b9a\u0bc7\u0ba4\u0bbf\u0bae\u0bb9\u0bbf\u0baa\u0ba4\u0ba9\u0bcd-\u0b89\u0ba4\u0bb3\u0ba9 \u0baa\u0bbe\u0b95 \u0bb0\u0bbf\u0baa\u0bc1 \u0bb5\u0bbf\u0ba9\u0bc1\u0ba4 \u0b95\u0bc1\u0ba3 \u0ba4\u0bc0\u0ba9 \u0b9a\u0bb0\u0ba3 \u0baf\u0ba4\u0bc1\u0bb5\u0bb0 <br/> \u0baa\u0bbe\u0ba9\u0bc1 \u0baa\u0bbe\u0b9a\u0bc1\u0bb0 \u0b95\u0bbf\u0bb0\u0bc0\u0b9f \u0b9a\u0bcb\u0baa\u0bbf\u0ba4 \u0bb5\u0bbf\u0baa\u0bbe\u0bb5\u0bb0\u0bc0\u0b9a \u0bb5\u0ba4\u0ba9\u0bbe\u0bae\u0bb2\u0bbe\u0ba9\u0bc1\u0baa\u0bae"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0ba8\u0bc0\u0bb2 \u0b9c\u0bb2\u0ba4\u0b9a\u0bae \u0b9a\u0bae \u0bb2\u0bcb\u0b95\u0bbe\u0ba4\u0bbf \u0b95\u0ba3\u0bb9\u0bb0\u0ba3 \u0ba8\u0bbf\u0baa\u0bc1\u0ba3 <br/> \u0b95\u0bae\u0ba9\u0bc0\u0baf \u0b9a\u0bb0\u0bbf\u0ba4 \u0bae\u0bcd\u0bb0\u0bcd\u0ba4\u0bc1 \u0bb5\u0b9a\u0ba9 \u0b9a\u0bc8\u0bb2\u0ba4\u0bbe\u0bb0\u0ba3 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0bae\u0ba4 \u0ba4\u0bc7\u0bb5 \u0baa\u0bb0\u0bbf\u0bb5\u0bcd\u0bb0\u0bcd\u0b9f \u0b9a\u0bc1\u0bb0\u0bc1\u0b9a\u0bbf\u0bb0 <br/> \u0b9a\u0bbe\u0bb0\u0b9a\u0bbe\u0baf\u0ba4 \u0bb5\u0bbf\u0bb2\u0bcb\u0b9a\u0ba9 \u0bae\u0b9e\u0bcd\u0b9c\u0bc1\u0bb3 \u0b9a\u0bbe\u0bae\u0b95\u0bbe\u0ba9 \u0bb0\u0b9a\u0bbf\u0b95\u0bbe\u0ba4\u0bbf \u0bae\u0ba9\u0bcb\u0bb9\u0bb0"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0baa\u0bcb\u0b95\u0bbf \u0b9a\u0baf\u0ba9 \u0ba8\u0ba4\u0b9c\u0ba9 \u0b9a\u0bcb\u0b95\u0bcc\u0b95 \u0bb5\u0bbf\u0b9a\u0bae \u0b95\u0bc1\u0ba3 \u0ba8\u0bbf\u0bb5\u0b9a\u0ba9 <br/> \u0baa\u0bcb\u0ba4\u0bae\u0baf \u0baa\u0bc1\u0bb5\u0ba9\u0bae\u0bb9\u0bbf\u0ba4 \u0ba8\u0bbe\u0b95\u0bb5\u0bb0 \u0b9a\u0bae \u0b95\u0bae\u0ba9 \u0ba8\u0bbe\u0bb0\u0ba4 \u0bae\u0bc1\u0b95 \u0b9a\u0bc7\u0bb5\u0bbf\u0ba4 <br/> \u0b9a\u0bbe\u0b95\u0bb0\u0bbe\u0bb2\u0baf \u0b95\u0baa\u0bc0\u0bb0 \u0ba8\u0ba8\u0bcd\u0ba4 \u0b9a\u0bc1\u0ba4 \u0b9a\u0bbe\u0bae\u0b9c\u0bc7\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 \u0bb5\u0bb0\u0ba4\u0bbe\u0bb0\u0bcd\u0ba4\u0bcd\u0ba4 \u0b9a\u0bc1\u0bb0\u0ba4\u0bcd\u0bb0\u0bc1\u0bae"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2a\u0c3e\u0c32\u0c2f \u0c2e\u0c3e\u0c27\u0c35 \u0c2e\u0c3e\u0c2e\u0c4d \u0c05\u0c28\u0c3f\u0c36\u0c2e\u0c4d \u0c2e\u0c41\u0c26\u0c3e \u0c38\u0c26\u0c3e \u0c28\u0c3f\u0c26\u0c3e\u0c28\u0c38\u0c2e\u0c4d"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2b\u0c3e\u0c32\u0c24\u0c32 \u0c2a\u0c30\u0c3f\u0c32\u0c38\u0c3f\u0c24 \u0c2e\u0c02\u0c1c\u0c41\u0c33 \u0c39\u0c30\u0c3f\u0c23 \u0c2e\u0c26 \u0c24\u0c3f\u0c32\u0c15! <br/> \u0c2a\u0c28\u0c4d\u0c28\u0c17\u0c3e\u0c30\u0c3f \u0c35\u0c30 \u0c35\u0c3e\u0c39\u0c28 \u0c2e\u0c4b\u0c39\u0c28 \u0c2a\u0c26\u0c4d\u0c2e\u0c28\u0c3e\u0c2d \u0c2d\u0c41\u0c35\u0c28\u0c24\u0c4d\u0c30\u0c4d\u0c2f \u0c28\u0c3e\u0c2f\u0c15"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c36\u0c4d\u0c30\u0c40\u0c15\u0c30 \u0c2f\u0c41\u0c17 \u0c38\u0c30\u0c38\u0c3f\u0c1c \u0c38\u0c3e\u0c2e\u0c4b\u0c26\u0c2a\u0c30\u0c3f \u0c2e\u0c4d\u0c30\u0c4d\u0c26\u0c3f\u0c24 \u0c1a\u0c30\u0c23\u0c24\u0c32 <br/> \u0c1a\u0c47\u0c26\u0c3f\u0c2e\u0c39\u0c3f\u0c2a\u0c24\u0c28\u0c4d-\u0c09\u0c26\u0c33\u0c28 \u0c2a\u0c3e\u0c15 \u0c30\u0c3f\u0c2a\u0c41 \u0c35\u0c3f\u0c28\u0c41\u0c24 \u0c17\u0c41\u0c23 \u0c26\u0c40\u0c28 \u0c36\u0c30\u0c23 \u0c2f\u0c26\u0c41\u0c35\u0c30 <br/> \u0c2d\u0c3e\u0c28\u0c41 \u0c2d\u0c3e\u0c38\u0c41\u0c30 \u0c15\u0c3f\u0c30\u0c40\u0c1f \u0c36\u0c4b\u0c2d\u0c3f\u0c24 \u0c35\u0c3f\u0c2d\u0c3e\u0c35\u0c30\u0c40\u0c36 \u0c35\u0c26\u0c28\u0c3e\u0c2e\u0c32\u0c3e\u0c28\u0c41\u0c2a\u0c2e"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c28\u0c40\u0c32 \u0c1c\u0c32\u0c26\u0c38\u0c2e \u0c38\u0c2e \u0c32\u0c4b\u0c15\u0c3e\u0c26\u0c3f \u0c17\u0c23\u0c39\u0c30\u0c23 \u0c28\u0c3f\u0c2a\u0c41\u0c23 <br/> \u0c15\u0c2e\u0c28\u0c40\u0c2f \u0c1a\u0c30\u0c3f\u0c24 \u0c2e\u0c4d\u0c30\u0c4d\u0c26\u0c41 \u0c35\u0c1a\u0c28 \u0c1a\u0c48\u0c32\u0c27\u0c3e\u0c30\u0c23 \u0c39\u0c4d\u0c30\u0c4d\u0c24\u0c2e\u0c26 \u0c26\u0c47\u0c35 \u0c2a\u0c30\u0c3f\u0c35\u0c4d\u0c30\u0c4d\u0c22 \u0c38\u0c41\u0c30\u0c41\u0c1a\u0c3f\u0c30 <br/> \u0c38\u0c3e\u0c30\u0c38\u0c3e\u0c2f\u0c24 \u0c35\u0c3f\u0c32\u0c4b\u0c1a\u0c28 \u0c2e\u0c02\u0c1c\u0c41\u0c33 \u0c38\u0c3e\u0c2e\u0c17\u0c3e\u0c28 \u0c30\u0c38\u0c3f\u0c15\u0c3e\u0c24\u0c3f \u0c2e\u0c28\u0c4b\u0c39\u0c30"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c2d\u0c4b\u0c17\u0c3f \u0c36\u0c2f\u0c28 \u0c28\u0c24\u0c1c\u0c28 \u0c36\u0c4b\u0c15\u0c4c\u0c16 \u0c35\u0c3f\u0c36\u0c2e \u0c17\u0c41\u0c23 \u0c28\u0c3f\u0c35\u0c38\u0c28 <br/> \u0c2c\u0c4b\u0c27\u0c2e\u0c2f \u0c2d\u0c41\u0c35\u0c28\u0c2e\u0c39\u0c3f\u0c24 \u0c28\u0c3e\u0c17\u0c35\u0c30 \u0c38\u0c2e \u0c17\u0c2e\u0c28 \u0c28\u0c3e\u0c30\u0c26 \u0c2e\u0c41\u0c16 \u0c38\u0c47\u0c35\u0c3f\u0c24 <br/> \u0c38\u0c3e\u0c17\u0c30\u0c3e\u0c32\u0c2f \u0c17\u0c2d\u0c40\u0c30 \u0c28\u0c28\u0c4d\u0c26 \u0c38\u0c41\u0c24 \u0c38\u0c3e\u0c2e\u0c1c\u0c47\u0c28\u0c4d\u0c26\u0c4d\u0c30 \u0c35\u0c30\u0c26\u0c3e\u0c30\u0c4d\u0c24\u0c4d\u0c24 \u0c38\u0c41\u0c30\u0c26\u0c4d\u0c30\u0c41\u0c2e"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0caa\u0cbe\u0cb2\u0caf \u0cae\u0cbe\u0ca7\u0cb5 \u0cae\u0cbe\u0cae\u0ccd \u0c85\u0ca8\u0cbf\u0cb6\u0cae\u0ccd \u0cae\u0cc1\u0ca6\u0cbe \u0cb8\u0ca6\u0cbe \u0ca8\u0cbf\u0ca6\u0cbe\u0ca8\u0cb8\u0cae\u0ccd"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cab\u0cbe\u0cb2\u0ca4\u0cb2 \u0caa\u0cb0\u0cbf\u0cb2\u0cb8\u0cbf\u0ca4 \u0cae\u0c82\u0c9c\u0cc1\u0cb3 \u0cb9\u0cb0\u0cbf\u0ca3 \u0cae\u0ca6 \u0ca4\u0cbf\u0cb2\u0c95! <br/> \u0caa\u0ca8\u0ccd\u0ca8\u0c97\u0cbe\u0cb0\u0cbf \u0cb5\u0cb0 \u0cb5\u0cbe\u0cb9\u0ca8 \u0cae\u0ccb\u0cb9\u0ca8 \u0caa\u0ca6\u0ccd\u0cae\u0ca8\u0cbe\u0cad \u0cad\u0cc1\u0cb5\u0ca8\u0ca4\u0ccd\u0cb0\u0ccd\u0caf \u0ca8\u0cbe\u0caf\u0c95"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cb6\u0ccd\u0cb0\u0cc0\u0c95\u0cb0 \u0caf\u0cc1\u0c97 \u0cb8\u0cb0\u0cb8\u0cbf\u0c9c \u0cb8\u0cbe\u0cae\u0ccb\u0ca6\u0caa\u0cb0\u0cbf \u0cae\u0ccd\u0cb0\u0ccd\u0ca6\u0cbf\u0ca4 \u0c9a\u0cb0\u0ca3\u0ca4\u0cb2 <br/> \u0c9a\u0cc7\u0ca6\u0cbf\u0cae\u0cb9\u0cbf\u0caa\u0ca4\u0ca8\u0ccd-\u0c89\u0ca6\u0cb3\u0ca8 \u0caa\u0cbe\u0c95 \u0cb0\u0cbf\u0caa\u0cc1 \u0cb5\u0cbf\u0ca8\u0cc1\u0ca4 \u0c97\u0cc1\u0ca3 \u0ca6\u0cc0\u0ca8 \u0cb6\u0cb0\u0ca3 \u0caf\u0ca6\u0cc1\u0cb5\u0cb0 <br/> \u0cad\u0cbe\u0ca8\u0cc1 \u0cad\u0cbe\u0cb8\u0cc1\u0cb0 \u0c95\u0cbf\u0cb0\u0cc0\u0c9f \u0cb6\u0ccb\u0cad\u0cbf\u0ca4 \u0cb5\u0cbf\u0cad\u0cbe\u0cb5\u0cb0\u0cc0\u0cb6 \u0cb5\u0ca6\u0ca8\u0cbe\u0cae\u0cb2\u0cbe\u0ca8\u0cc1\u0caa\u0cae"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0ca8\u0cc0\u0cb2 \u0c9c\u0cb2\u0ca6\u0cb8\u0cae \u0cb8\u0cae \u0cb2\u0ccb\u0c95\u0cbe\u0ca6\u0cbf \u0c97\u0ca3\u0cb9\u0cb0\u0ca3 \u0ca8\u0cbf\u0caa\u0cc1\u0ca3 <br/> \u0c95\u0cae\u0ca8\u0cc0\u0caf \u0c9a\u0cb0\u0cbf\u0ca4 \u0cae\u0ccd\u0cb0\u0ccd\u0ca6\u0cc1 \u0cb5\u0c9a\u0ca8 \u0c9a\u0cc8\u0cb2\u0ca7\u0cbe\u0cb0\u0ca3 \u0cb9\u0ccd\u0cb0\u0ccd\u0ca4\u0cae\u0ca6 \u0ca6\u0cc7\u0cb5 \u0caa\u0cb0\u0cbf\u0cb5\u0ccd\u0cb0\u0ccd\u0ca2 \u0cb8\u0cc1\u0cb0\u0cc1\u0c9a\u0cbf\u0cb0 <br/> \u0cb8\u0cbe\u0cb0\u0cb8\u0cbe\u0caf\u0ca4 \u0cb5\u0cbf\u0cb2\u0ccb\u0c9a\u0ca8 \u0cae\u0c82\u0c9c\u0cc1\u0cb3 \u0cb8\u0cbe\u0cae\u0c97\u0cbe\u0ca8 \u0cb0\u0cb8\u0cbf\u0c95\u0cbe\u0ca4\u0cbf \u0cae\u0ca8\u0ccb\u0cb9\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cad\u0ccb\u0c97\u0cbf \u0cb6\u0caf\u0ca8 \u0ca8\u0ca4\u0c9c\u0ca8 \u0cb6\u0ccb\u0c95\u0ccc\u0c96 \u0cb5\u0cbf\u0cb6\u0c