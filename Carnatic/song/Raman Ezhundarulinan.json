u0cae\u0cc1\u0ca8\u0ccd - \u0cb8\u0cc0\u0ca4\u0cbe <br/> \u0cb0\u0cbe\u0cae\u0ca8\u0ccd \u0c8e\u0cb3\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cc1\u0cb3\u0cbf\u0ca8\u0cbe\u0ca8\u0ccd"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c86\u0cae\u0cc6\u0ca8\u0cc1\u0cae\u0ccd \u0ca6\u0cc7\u0cb5\u0cb0\u0ccd\u0c97\u0cb3\u0cc1\u0c95\u0ccd \u0c95\u0cae\u0cc1\u0ca6\u0cae\u0ccd\u0ca6\u0ca8\u0ccd\u0ca6\u0cbe\u0cb1\u0ccd\u0caa\u0ccb\u0cb2\u0ccd <br/> \u0cad\u0cc2\u0cae\u0cbf\u0caf\u0cbf\u0cb2\u0ccd \u0c89\u0cb3\u0ccd\u0cb3\u0ccb\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0caa\u0ccb\u0ca8\u0caa\u0cca\u0cb0\u0cc1\u0cb3\u0ccd \u0cb5\u0ca8\u0ccd\u0ca6\u0cbe\u0cb1\u0ccd\u0caa\u0ccb\u0cb2"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca4\u0cc2\u0caf\u0cb5\u0cb0\u0ccd\u0c95\u0ccd \u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0cae\u0cc1\u0ca4\u0ccd\u0ca4\u0cbf\u0ca4\u0ccd \u0ca4\u0cc1\u0cb1\u0cc8\u0c97\u0cb3\u0ccd \u0cb8\u0cbf\u0ca4\u0ccd\u0ca4\u0cbf\u0ca4\u0ccd\u0ca4\u0cbe\u0cb1\u0ccd \u0caa\u0ccb\u0cb2\u0ccd <br/> \u0c93\u0caf\u0cc1\u0cae\u0ccd \u0c89\u0ca1\u0cb2\u0ccb\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0c89\u0caf\u0cbf\u0cb0\u0ccd\u0caa\u0cc1\u0c97\u0cc1\u0ca4\u0ccd\u0ca4\u0cc1\u0c9f\u0ccd\u0cb1\u0cbe\u0cb1\u0ccd \u0caa\u0ccb\u0cb2\u0ccd <br/> \u0ca8\u0cc7\u0caf \u0cae\u0cbe\u0ca6\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc6\u0cb2\u0cbe\u0cae\u0ccd \u0c95\u0ca3\u0ccd \u0ca8\u0cbf\u0cb3\u0cb2\u0ccd\u0cb5\u0ca8\u0ccd\u0ca6\u0cc1\u0ca6\u0cbf\u0ca4\u0ccd\u0ca4\u0cbe\u0cb1\u0ccd \u0caa\u0ccb\u0cb2\u0ccd <br/> \u0ca4\u0cbe\u0caf\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0c85\u0caa\u0ccd\u0caa\u0ccb\u0ca6\u0cc1 \u0ca4\u0cbe\u0ca8\u0ccd \u0caa\u0cbf\u0cb3\u0ccd\u0cb3\u0cc8\u0caa\u0ccd\u0caa\u0cc6\u0c9f\u0ccd\u0cb1\u0cbe\u0cb1\u0ccd\u0caa\u0ccb\u0cb2"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c89\u0ca4\u0ccd\u0ca4\u0cae\u0cb0\u0ccd\u0c97\u0cb3\u0ccd \u0ca8\u0cbe\u0cb5\u0cc1\u0c95\u0ccd\u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0c8a\u0cb1\u0cc1\u0cae\u0ccd\u0ca4\u0cc7\u0ca8\u0ccd \u0cae\u0caf\u0cae\u0cbe\u0c97 <br/> \u0cae\u0c9f\u0ccd\u0cb1\u0cb5\u0cb0\u0ccd\u0c95\u0ca3\u0ccd \u0cb5\u0ca3\u0ccd\u0ca1\u0cc1\u0c95\u0ccd\u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0cae\u0cb2\u0cb0\u0ccd\u0ca8\u0ccd\u0ca6\u0caa\u0ca3\u0ccd \u0c95\u0caf\u0cae\u0cbe\u0c97 <br/> \u0c85\u0c9c\u0ca6\u0cb8\u0cc6\u0ca8\u0ccd\u0ca6\u0cbe \u0cae\u0cb0\u0cc8\u0c95\u0ccd\u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0c85\u0cb0\u0cc1\u0ca8\u0ccd\u0c9e\u0ccd\u0c9a\u0ca8\u0ccd\u0ca6\u0cbf\u0cb0\u0ccb \u0ca4\u0caf\u0cae\u0cbe\u0c97 <br/> \u0caa\u0cc1\u0ca4\u0ccd\u0ca4\u0cbf\u0cb0\u0cb0\u0ccd\u0c95\u0ccd\u0c95\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0ca4\u0cbe\u0caf\u0cc8\u0caa\u0ccd \u0caa\u0ccb\u0cb2\u0cc7 \u0cb5\u0cb8\u0cbf\u0caf\u0cae\u0cbe\u0c97"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb8\u0cb0\u0cbf\u0caf\u0cc1\u0cae\u0ccd \u0cae\u0cb0\u0cb5\u0cc1\u0cb0\u0cbf\u0caf\u0cc1\u0cae\u0ccd \u0cb8\u0ca1\u0cc8\u0caf\u0cc1\u0cae\u0ccd \u0ca8\u0ca1\u0cc8\u0caf\u0cc1\u0cae\u0ccd \u0cb8\u0ccb\u0cb0 <br/> \u0cb5\u0cb0\u0cc1\u0c97\u0cbf\u0cb1 \u0ca4\u0cae\u0ccd\u0cac\u0cbf\u0cb5\u0ca8\u0ccd\u0ca6 \u0cb5\u0cb0\u0cb5\u0cc1\u0c95\u0ca3\u0ccd\u0ca1\u0cc1 \u0c95\u0ca3\u0ccd\u0ca3\u0cbe\u0cb0 <br/> \u0c89\u0cb0\u0cc1\u0c97\u0cbf \u0c89\u0cb0\u0cc1\u0c97\u0cbf\u0caf\u0cb5\u0ca8\u0cc1\u0ca1\u0cc8\u0caf \u0c95\u0cc1\u0cb1\u0cc8\u0c97\u0cb3\u0ccd \u0ca4\u0cc0\u0cb0 <br/> \u0ca4\u0cbf\u0cb0\u0cc1\u0cb5\u0cc1\u0cae\u0ccd \u0ca4\u0cae\u0ccd\u0cac\u0cbf\u0caf\u0cc1\u0cae\u0ccd \u0c95\u0cb5\u0cbf\u0c9a\u0ccd \u0c9a\u0cc7\u0ca8\u0cc8\u0caf\u0cc1\u0cae\u0ccd \u0ca4\u0cbe\u0ca8\u0cc1\u0cae\u0ccd \u0cb8\u0cc7\u0cb0"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d30\u0d3e\u0d2e\u0d28\u0d4d \u0d0e\u0d34\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d41\u0d33\u0d3f\u0d28\u0d3e\u0d28\u0d4d \u0d2d\u0d30\u0d26\u0d28\u0d4d\u0d2e\u0d41\u0d28\u0d4d - \u0d38\u0d40\u0d24\u0d3e <br/> \u0d30\u0d3e\u0d2e\u0d28\u0d4d \u0d0e\u0d34\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d41\u0d33\u0d3f\u0d28\u0d3e\u0d28\u0d4d"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d06\u0d2e\u0d46\u0d28\u0d41\u0d2e\u0d4d \u0d26\u0d47\u0d35\u0d30\u0d4d\u0d17\u0d33\u0d41\u0d15\u0d4d \u0d15\u0d2e\u0d41\u0d26\u0d2e\u0d4d\u0d26\u0d28\u0d4d\u0d26\u0d3e\u0d31\u0d4d\u0d2a\u0d4b\u0d32\u0d4d <br/> \u0d2d\u0d42\u0d2e\u0d3f\u0d2f\u0d3f\u0d32\u0d4d \u0d09\u0d33\u0d4d\u0d33\u0d4b\u0d30\u0d4d\u0d15\u0d4d\u0d15\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d2a\u0d4b\u0d28\u0d2a\u0d4a\u0d30\u0d41\u0d33\u0d4d \u0d35\u0d28\u0d4d\u0d26\u0d3e\u0d31\u0d4d\u0d2a\u0d4b\u0d32"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d24\u0d42\u0d2f\u0d35\u0d30\u0d4d\u0d15\u0d4d \u0d15\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d2e\u0d41\u0d24\u0d4d\u0d24\u0d3f\u0d24\u0d4d \u0d24\u0d41\u0d31\u0d48\u0d17\u0d33\u0d4d \u0d38\u0d3f\u0d24\u0d4d\u0d24\u0d3f\u0d24\u0d4d\u0d24\u0d3e\u0d31\u0d4d \u0d2a\u0d4b\u0d32\u0d4d <br/> \u0d13\u0d2f\u0d41\u0d2e\u0d4d \u0d09\u0d21\u0d32\u0d4b\u0d30\u0d4d\u0d15\u0d4d\u0d15\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d09\u0d2f\u0d3f\u0d30\u0d4d\u0d2a\u0d41\u0d17\u0d41\u0d24\u0d4d\u0d24\u0d41\u0d1f\u0d4d\u0d31\u0d3e\u0d31\u0d4d \u0d2a\u0d4b\u0d32\u0d4d <br/> \u0d28\u0d47\u0d2f \u0d2e\u0d3e\u0d26\u0d30\u0d4d\u0d15\u0d4d\u0d15\u0d46\u0d32\u0d3e\u0d2e\u0d4d \u0d15\u0d23\u0d4d \u0d28\u0d3f\u0d34\u0d32\u0d4d\u0d35\u0d28\u0d4d\u0d26\u0d41\u0d26\u0d3f\u0d24\u0d4d\u0d24\u0d3e\u0d31\u0d4d \u0d2a\u0d4b\u0d32\u0d4d <br/> \u0d24\u0d3e\u0d2f\u0d30\u0d4d\u0d15\u0d4d\u0d15\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d05\u0d2a\u0d4d\u0d2a\u0d4b\u0d26\u0d41 \u0d24\u0d3e\u0d28\u0d4d \u0d2a\u0d3f\u0d33\u0d4d\u0d33\u0d48\u0d2a\u0d4d\u0d2a\u0d46\u0d1f\u0d4d\u0d31\u0d3e\u0d31\u0d4d\u0d2a\u0d4b\u0d32"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d09\u0d24\u0d4d\u0d24\u0d2e\u0d30\u0d4d\u0d17\u0d33\u0d4d \u0d28\u0d3e\u0d35\u0d41\u0d15\u0d4d\u0d15\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d0a\u0d31\u0d41\u0d2e\u0d4d\u0d24\u0d47\u0d28\u0d4d \u0d2e\u0d2f\u0d2e\u0d3e\u0d17 <br/> \u0d2e\u0d1f\u0d4d\u0d31\u0d35\u0d30\u0d4d\u0d15\u0d23\u0d4d \u0d35\u0d23\u0d4d\u0d21\u0d41\u0d15\u0d4d\u0d15\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d2e\u0d32\u0d30\u0d4d\u0d28\u0d4d\u0d26\u0d2a\u0d23\u0d4d \u0d15\u0d2f\u0d2e\u0d3e\u0d17 <br/> \u0d05\u0d1c\u0d26\u0d38\u0d46\u0d28\u0d4d\u0d26\u0d3e \u0d2e\u0d30\u0d48\u0d15\u0d4d\u0d15\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d05\u0d30\u0d41\u0d28\u0d4d\u0d1e\u0d4d\u0d1a\u0d28\u0d4d\u0d26\u0d3f\u0d30\u0d4b \u0d24\u0d2f\u0d2e\u0d3e\u0d17 <br/> \u0d2a\u0d41\u0d24\u0d4d\u0d24\u0d3f\u0d30\u0d30\u0d4d\u0d15\u0d4d\u0d15\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d24\u0d3e\u0d2f\u0d48\u0d2a\u0d4d \u0d2a\u0d4b\u0d32\u0d47 \u0d35\u0d38\u0d3f\u0d2f\u0d2e\u0d3e\u0d17"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d38\u0d30\u0d3f\u0d2f\u0d41\u0d2e\u0d4d \u0d2e\u0d30\u0d35\u0d41\u0d30\u0d3f\u0d2f\u0d41\u0d2e\u0d4d \u0d38\u0d21\u0d48\u0d2f\u0d41\u0d2e\u0d4d \u0d28\u0d21\u0d48\u0d2f\u0d41\u0d2e\u0d4d \u0d38\u0d4b\u0d30 <br/> \u0d35\u0d30\u0d41\u0d17\u0d3f\u0d31 \u0d24\u0d2e\u0d4d\u0d2c\u0d3f\u0d35\u0d28\u0d4d\u0d26 \u0d35\u0d30\u0d35\u0d41\u0d15\u0d23\u0d4d\u0d21\u0d41 \u0d15\u0d23\u0d4d\u0d23\u0d3e\u0d30 <br/> \u0d09\u0d30\u0d41\u0d17\u0d3f \u0d09\u0d30\u0d41\u0d17\u0d3f\u0d2f\u0d35\u0d28\u0d41\u0d21\u0d48\u0d2f \u0d15\u0d41\u0d31\u0d48\u0d17\u0d33\u0d4d \u0d24\u0d40\u0d30 <br/> \u0d24\u0d3f\u0d30\u0d41\u0d35\u0d41\u0d2e\u0d4d \u0d24\u0d2e\u0d4d\u0d2c\u0d3f\u0d2f\u0d41\u0d2e\u0d4d \u0d15\u0d35\u0d3f\u0d1a\u0d4d \u0d1a\u0d47\u0d28\u0d48\u0d2f\u0d41\u0d2e\u0d4d \u0d24\u0d3e\u0d28\u0d41\u0d2e\u0d4d \u0d38\u0d47\u0d30"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0930\u093e\u092e\u0928\u094d \u090f\u0932\u0941\u0928\u094d\u0926\u0930\u0941\u0932\u093f\u0928\u093e\u0928\u094d \u092d\u0930\u0926\u0928\u094d\u092e\u0941\u0928