u0b9c\u0ba9\u0b99\u0bcd\u0b95\u0bb3\u0bc8 \u0ba4\u0bca\u0b9f\u0bb0\u0bcd\u0ba8\u0bcd\u0ba4\u0bc7\u0ba9\u0bcd \u0baa\u0bb4\u0bbf \u0b8f\u0bb0\u0bcd\u0b95\u0bcd\u0b95\u0bbf\u0bb0\u0bbe\u0baf\u0bcd <br/> \u0ba4\u0bc2\u0b95\u0bcd\u0b95\u0bbf \u0b8f\u0bb0 \u0bb5\u0bbf\u0b9f\u0bcd\u0b9f\u0bc7\u0ba3\u0bbf\u0baf\u0bc8 \u0bb5\u0bbe\u0b99\u0bcd\u0b95\u0bc1\u0bae\u0bcd \u0ba4\u0bc2\u0bb0\u0bcd\u0ba4\u0bcd\u0ba4\u0bb0\u0bcd \u0bb5\u0bbe\u0bb0\u0bcd\u0ba4\u0bcd\u0ba4\u0bc8\u0baf\u0bc8\u0b95\u0bcd-\u0b95\u0bc7\u0ba4\u0bcd\u0b95\u0bbf\u0bb0\u0bbe\u0baf\u0bcd <br/> \u0ba4\u0b9f\u0bc8 \u0baa\u0b9f\u0bbe\u0bae\u0bb2\u0bcd \u0b8e\u0ba9\u0bcd \u0b85\u0baf\u0bcd\u0baf\u0b99\u0bcd\u0b95\u0bbe\u0bb2\u0bcd \u0bae\u0bc0\u0ba4\u0bc7 \u0b9a\u0bb0\u0ba3\u0bcd \u0b8e\u0ba9\u0bcd\u0bb1\u0bbe\u0bb2\u0bcd \u0baa\u0bbf\u0bb4\u0bc8\u0baa\u0bcd\u0baa\u0bbe\u0baf\u0bc7 \u0b87\u0baa\u0bcd\u0baa\u0bcb\u0ba4\u0bc7 <br/> \u0b85\u0b9f\u0b95\u0bcd\u0b95 \u0b89\u0ba9\u0bcd \u0ba4\u0bb0\u0bae\u0bcd \u0b85\u0bb2\u0bcd\u0bb2\u0b9f\u0bbe \u0b9a\u0bc0\u0ba4\u0bc8 \u0b85\u0bb5\u0ba9\u0bcd \u0b87\u0bb5\u0ba9\u0bcd \u0bb5\u0bbf\u0b9f \u0bb5\u0ba8\u0bcd\u0ba4\u0bbe\u0ba9\u0bcd \u0b8e\u0ba9\u0bcd\u0ba9\u0bbe\u0ba4\u0bc7"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b95\u0bbe\u0bb2\u0bae\u0bc1\u0bae\u0bcd \u0baa\u0bb2\u0bae\u0bc1\u0bae\u0bcd \u0ba4\u0bc6\u0bb0\u0bbf\u0baf\u0bbe\u0bae\u0bb2\u0bcd \u0b95\u0bc1\u0ba4\u0bbf\u0b95\u0bcd\u0b95\u0bbf\u0bb0\u0bbe\u0baf\u0bcd \u0b8e\u0ba9\u0bcd\u0ba9 \u0bae\u0bbe\u0baf\u0bae\u0bcb <br/> \u0b95\u0bbe\u0bae\u0ba4\u0bcd\u0ba4\u0bbe\u0bb2\u0bc7 \u0ba4\u0bb0\u0bcd\u0bae \u0baa\u0bc1\u0ba4\u0bcd\u0ba4\u0bbf\u0baf\u0bc8 \u0b95\u0b9f\u0b95\u0bcd\u0b95\u0bbf\u0bb0\u0bbe\u0baf\u0bcd \u0b87\u0ba4\u0bc1 \u0b89\u0baa\u0bbe\u0baf\u0bae\u0bcb <br/> \u0b9a\u0bbe\u0bb2\u0bae\u0bcb \u0b95\u0bc7\u0b9f\u0bc1 \u0b95\u0bbe\u0bb2\u0bae\u0bcb \u0b85\u0b9f\u0bbe \u0b89\u0ba9\u0b95\u0bcd\u0b95\u0bc1\u0bae\u0bcd \u0ba4\u0bc6\u0baf\u0bcd\u0bb5 \u0b9a\u0bb9\u0bbe\u0baf\u0bae\u0bcb <br/> \u0ba4\u0b99\u0bcd\u0b95\u0bc8 \u0bae\u0bc2\u0b95\u0bcd\u0b95\u0bb0\u0bc1\u0baa\u0bcd\u0baa\u0bcd-\u0b89\u0ba3\u0bcd\u0b9f\u0ba4\u0bb2\u0bcd\u0bb2\u0bb5\u0bcb \u0b9a\u0ba3\u0bcd\u0b9f\u0bbe\u0bb3\u0bbe \u0b87\u0ba4\u0bc1 \u0ba8\u0bcd\u0baf\u0bbe\u0baf\u0bae\u0bcb <br/> \u0bb5\u0bbe\u0bb2\u0bbf\u0baf\u0bc1\u0bae\u0bcd \u0baa\u0bcb\u0ba9\u0bbe\u0ba9\u0bcd \u0b89\u0ba9\u0bcd\u0ba9\u0bc8 \u0b9a\u0bbf\u0bb0\u0bc8 \u0bb5\u0bc8\u0ba4\u0bcd\u0ba4 \u0bb5\u0bbe\u0bb2\u0bc1\u0bae\u0bcd \u0baa\u0bcb\u0baf\u0bcd \u0bb5\u0bbf\u0b9f\u0bcd\u0b9f\u0ba4\u0bc1 \u0b85\u0b9e\u0bcd\u0b9a\u0bbe\u0ba4\u0bc7 \u0bae\u0bc6\u0ba4\u0bcd\u0ba4 <br/> \u0b9a\u0bc0\u0bb2 \u0bb0\u0bbe\u0b95\u0bb5\u0ba9\u0bcd \u0b85\u0bae\u0bcd\u0baa\u0bbe\u0bb2\u0bcd \u0b85\u0bb5\u0ba9\u0bcd \u0b9a\u0bc6\u0ba4\u0bcd\u0ba4 \u0b9a\u0bc7\u0ba4\u0bbf\u0baf\u0bc8 \u0ba8\u0bc0\u0baf\u0bb0\u0bc1\u0baf\u0bbe\u0baf\u0bcb\u0b9f\u0bbe \u0baa\u0bbf\u0ba4\u0bcd\u0ba4"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0b95\u0bc0\u0ba4\u0bae\u0bcd \u0ba4\u0bc6\u0bb0\u0bbf\u0ba8\u0bcd\u0ba4\u0bc1\u0bae\u0bcd \u0bb5\u0bc7\u0ba4\u0bae\u0bcd \u0ba4\u0bc6\u0bb0\u0bbf\u0ba8\u0bcd\u0ba4\u0bc1\u0bae\u0bcd \u0baa\u0bbe\u0ba4\u0b95\u0b99\u0bcd\u0b95\u0bb3\u0bc8 \u0b9a\u0bc2\u0bb4\u0bcd\u0b95\u0bbf\u0bb0\u0bbe\u0baf\u0bcd <br/> \u0b95\u0bc6\u0b9f\u0bc1\u0bae\u0ba4\u0bbf \u0b95\u0ba3\u0bcd\u0ba3\u0bc1\u0b95\u0bcd\u0b95\u0bc1\u0ba4\u0bcd-\u0ba4\u0bcb\u0ba9\u0bcd\u0bb1\u0bbe\u0bae\u0bb2\u0bcd \u0baa\u0b9f\u0bc1 \u0ba8\u0bb0\u0b95\u0ba4\u0bcd\u0ba4\u0bbf\u0bb2\u0bcd \u0bae\u0bc2\u0bb4\u0bcd\u0b95\u0bbf\u0bb0\u0bbe\u0baf\u0bcd <br/> \u0bae\u0bbe\u0ba4\u0bb0\u0bcd \u0bae\u0bcb\u0bb9\u0ba4\u0bcd\u0ba4\u0bbf\u0bb2\u0bc7 \u0ba4\u0bc1\u0bb5\u0ba3\u0bcd\u0b9f\u0bc1 \u0ba4\u0bbe\u0bb4\u0bbe\u0ba4\u0bb5\u0bb0\u0bc8\u0ba4\u0bcd-\u0ba4\u0bbe\u0bb4\u0bcd\u0b95\u0bbf\u0bb0\u0bbe\u0baf\u0bcd <br/> \u0bae\u0baf\u0b95\u0bcd\u0b95\u0bae\u0bcb \u0b95\u0bc8\u0b95\u0bcd\u0b95\u0bc1\u0bb3\u0bcd \u0bb5\u0bbf\u0bb3\u0b95\u0bcd\u0b95\u0bc8\u0baa\u0bcd-\u0baa\u0bbf\u0b9f\u0bbf\u