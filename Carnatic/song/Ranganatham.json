u0ca8\u0ccd\u0ca6\u0ccd\u0caf\u0cae\u0cbe\u0ca8 \u0cb6\u0ccb\u0cad\u0ca8\u0cc0\u0caf\u0cae\u0ccd \u0c85\u0ca4\u0cbf \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0 \u0cae\u0cc1\u0cb0\u0cbe\u0cb0\u0cb5\u0cbf\u0ca8\u0ccd\u0ca6"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0c82\u0c97\u0cb3\u0c95\u0cb0 \u0ca8\u0cbf\u0cb7\u0ccd\u0c95\u0cb3\u0c82\u0c95 \u0ca6\u0c95\u0ccd\u0cb7\u0cc0\u0ca3 \u0cae\u0ca8\u0ccd\u0ca6\u0cbe\u0c95\u0cbf\u0ca8\u0cbf \u0c95\u0cbe\u0cb5\u0cc7\u0cb0\u0cbf \u0cae\u0ca7\u0ccd\u0caf\u0cb8\u0ccd\u0ca4\u0cae\u0ccd \u0cb6\u0c82\u0c96 \u0c9a\u0c95\u0ccd\u0cb0 \u0c97\u0ca6\u0cbe <br/> \u0caa\u0ca6\u0ccd\u0cae\u0ca7\u0cb0 \u0cb9\u0cb8\u0ccd\u0ca4\u0cae\u0ccd \u0cb8\u0cb0\u0cb8\u0cbf\u0c9c \u0cb5\u0cbf\u0c95\u0cb8\u0cbf\u0ca4 \u0ca4\u0cb3\u0cb5\u0cb0 \u0ca8\u0cc7\u0ca4\u0ccd\u0cb0\u0cae\u0ccd \u0c9c\u0cb2\u0ca7 \u0c95\u0cb0 \u0cae\u0cb0\u0c95\u0ca4 \u0cb5\u0cb0\u0ccd\u0ca3 \u0c97\u0cbe\u0ca4\u0ccd\u0cb0\u0cae\u0ccd <br/> \u0c9c\u0ca8\u0ca8 \u0cae\u0cb0\u0ca3 \u0cad\u0caf \u0cb6\u0cae\u0ca8 \u0caa\u0cb5\u0cbf\u0ca4\u0ccd\u0cb0\u0cae\u0ccd \u0c9c\u0cb2\u0c9c \u0cb8\u0cae\u0ccd\u0cad\u0cb5 \u0cb8\u0ca8\u0ccd\u0ca8\u0cc1\u0ca4 \u0c9a\u0cb0\u0cbf\u0ca4\u0ccd\u0cb0\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cb2\u0cbe\u0cb5\u0ca3\u0ccd\u0caf \u0caa\u0ca6 \u0cb8\u0cb0\u0ccb\u0cb0\u0cc1\u0cb9\u0cae\u0ccd \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb0\u0cbe\u0c9c\u0caf\u0ccb\u0c97 \u0ca6\u0cb0\u0ccd\u0cb6\u0ca8 \u0cb8\u0ca8\u0ccd\u0ca4\u0ccb\u0cb9\u0cae\u0ccd \u0cad\u0cbe\u0cb5 <br/> \u0cae\u0ca7\u0cc1\u0cb0 \u0cb8\u0cb0\u0cb8\u0cae\u0ccd \u0caa\u0ccd\u0cb0\u0cb5\u0cbe\u0cb9\u0cae\u0ccd \u0cad\u0c95\u0ccd\u0ca4\u0ccb\u0ca4\u0ccd\u0cb8\u0cb5 \u0caa\u0cb0\u0cae\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6 \u0ca6\u0cc7\u0cb9\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cb8\u0cc7\u0cb5\u0cbf\u0ca4 \u0ca8\u0cbf\u0c9c \u0c9c\u0ca8 \u0cb5\u0cb0\u0cb5\u0cbf \u0c98\u0ccb\u0cb7\u0cae\u0ccd \u0cb6\u0ccd\u0cb0\u0cc0\u0c95\u0cb0 \u0cb0\u0c9a\u0cbf\u0ca4 \u0cb0\u0cc1\u0c9a\u0cbf\u0cb0 \u0cb5\u0cbf\u0cad\u0cc2\u0cb7\u0ca3\u0cae\u0ccd <br/> \u0cb8\u0ccc\u0cb5\u0cbf\u0ca7 \u0cb5\u0cbf\u0cb9\u0c82\u0c97 \u0cad\u0ccb\u0c97\u0cbf \u0cad\u0cbe\u0cb7\u0ca3\u0cae\u0ccd \u0cb8\u0ca8\u0ccd\u0ca8\u0cc1\u0ca4 \u0cb2\u0c82\u0c95\u0cbe\u0ca7\u0cbf\u0caa \u0cb5\u0cbf\u0cad\u0cc0\u0cb7\u0ca3\u0cae\u0ccd <br/> \u0cb8\u0ccd\u0cb5\u0cb0\u0c9c\u0ca4\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c9d\u0cca\u0cae\u0ccd\u0ca4 \u0ca7\u0cbf\u0cb0\u0cbf \u0ca4\u0ca3\u0c95 \u0ca3\u0cae\u0ccd\u0ca4 \u0ca7\u0cbf\u0cb0\u0cbf \u0ca4\u0ca3\u0c95 \u0c9d\u0cca\u0cae\u0ccd\u0ca4\u0c95 \u0ca7\u0ccd\u0cb0\u0cc1\u0c95 \u0ca4\u0ca3\u0cbe\u0ca4 \u0ca4\u0ca6\u0cbf\u0c82\u0c97\u0cbf\u0ca3\u0ca4\u0ccb\u0cae\u0ccd <br/> \u0ca4\u0cc1\u0c82\u0c97\u0cae\u0ca3\u0cbf \u0c95\u0ca8\u0c95 \u0cae\u0c82\u0c9c\u0cc1 \u0c95\u0c9f\u0c95 \u0c95\u0cb0 \u0c95\u0c82\u0c95\u0ca3 \u0cb5\u0cbf\u0cb2\u0cb8\u0cbf\u0ca4\u0cbe\u0c82\u0c97 \u0cb6\u0cc1\u0cad\u0c95\u0cb0 \u0cb6\u0ccd\u0cb0\u0cc0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c9d\u0ca3 \u0c9d\u0ca3\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf \u0ca4\u0ca4\u0c95\u0cc1\u0c9d\u0cae\u0ccd \u0ca4\u0c9d\u0cae\u0ccd \u0c95\u0cc1\u0ca8\u0ccd\u0ca4\u0ca4 \u0c95\u0cc1\u0c95\u0cc1\u0cae\u0ccd\u0ca4\u0ca4 \u0ca7\u0cc0\u0cae\u0ccd\u0ca4 \u0c97\u0cbf\u0ca3\u0ca4\u0ccb\u0cae\u0ccd \u0c95\u0cbf\u0c9f <br/> \u0cae\u0cb9\u0ca6\u0cb9\u0c82\u0c95\u0cbe\u0cb0 \u0cae\u0cb9\u0ca6 \u0caa\u0c82\u0c9a \u0cad\u0cc2\u0ca4\u0cc8\u0cb0\u0ca4\u0cbf \u0c95\u0ccd\u0cb0\u0ccd\u0ca4 \u0cad\u0cc1\u0cb5\u0ca8\u0cc8\u0c95 \u0c9a\u0cb0\u0ca3\u0cbe\u0cb2\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0ca7\u0cbf\u0cb0\u0ca8 \u0ca7\u0cbf\u0cb0\u0ca8 \u0ca4\u0c95\u0ccd\u0cb0\u0ccd\u0ca4 \u0ca4\u0cca\u0cae\u0ccd \u0ca4\u0c95\u0cbf\u0c9f\u0ca4\u0cca\u0cae\u0ccd \u0cb8\u0cb0\u0cbf \u0ca4\u0c95\u0cc1\u0cae\u0ccd \u0ca4\u0c95\u0ca7\u0cc0..\u0cae\u0cbf \u0ca4\u0c95 <br/> \u0c97\u0cc1\u0ca3 \u0caa\u0cbe\u0cb5\u0ca8\u0cae\u0cbe\u0ca4\u0ccd\u0cae\u0c95\u0cae\u0ccd \u0c85\u0ca4\u0cc0\u0ca4\u0cae\u0ccd \u0caa\u0cb0\u0cae \u0cb0\u0c82\u0c9c\u0ca8\u0cae\u0ccd \u0c86\u0c9a\u0cc7\u0cb7\u0cbf\u0ca4 \u0cb5\u0cc8\u0cad\u0cb5 <br/> \u0ca4\u0cb2\u0cbe\u0ca3\u0ccd...\u0c97\u0cc1 \u0ca4\u0ca3\u0cae\u0ccd \u0cae\u0cbf\u0ca4\u0ca4\u0c95 \u0c9d\u0cc1\u0ca3\u0cc1\u0ca4\u0c95\u0ca3\u0cae\u0ccd \u0cae\u0cbf\u0ca4 \u0ca4\u0ca4\u0c95\u0cbf\u0c9f \u0c95\u0cc1\u0ca4\u0ca3\u0cae\u0ccd \u0cae\u0cbf\u0ca4 \u0ca4\u0c95\u0c9d\u0ca3\u0cc1 <br/> \u0cb0\u0ccb...\u0ca8\u0ca4 \u0cae\u0cbe\u0ca7\u0cc1\u0cb0\u0cae\u0ccd \u0c85\u0ca4\u0cbf\u0cb6\u0caf\u0cae\u0ccd \u0c85\u0ca4\u0cbf \u0cb6\u0ccb\u0cad\u0cbf\u0ca4 \u0cb8\u0cc1\u0cb5\u0ca6\u0ca8\u0cae\u0ccd \u0c85\u0ca4\u0cbf \u0cb6\u0cc0\u0ca4\u0cb3 \u0caa\u0ca6\u0caf\u0cc1\u0c97"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0ca4\u0cbe\u0cae\u0ccd \u0ca7\u0cc0\u0cae\u0ccd\u0ca4 \u0ca4\u0c9c\u0ccd\u0c9d\u0ca3\u0cc1 \u0ca7\u0cbf\u0cae\u0cbf\u0ca4 \u0ca7\u0cbf\u0cae\u0cbf\u0ca4 \u0ca7\u0c95\u0ccd\u0cb0\u0ccd\u0ca4 \u0c9d\u0cae\u0ccd \u0c9d\u0cae\u0ccd \u0ca4\u0c95\u0c9d\u0cae\u0ccd \u0ca4\u0ca4\u0ca3\u0cbe <br/> \u0c86\u0ca8\u0ca8\u0ccd...\u0ca6\u0cae\u0ccd \u0cb5\u0ccd\u0caf\u0caf\u0cae\u0ccd \u0c85\u0ca6\u0ccd\u0cb5\u0cbf\u0ca4\u0cc0\u0caf\u0cae\u0ccd \u0c85\u0c96\u0cbf\u0cb2\u0ca8\u0cc8\u0c95\u0cae\u0cc8\u0c95 \u0cae\u0ca8\u0cae\u0cc8\u0c95 \u0cad\u0cb0\u0cae\u0ccd <br/> \u0ca4\u0c95\u0c9d\u0ca8\u0ccd\u0ca4 \u0ca7\u0cbf\u0cae\u0cbf \u0ca7\u0cbf\u0cae\u0cbf \u0ca4\u0c9d\u0ca3\u0cc1 \u0ca4\u0ca4\u0cb0\u0cbf \u0ca4\u0ca6\u0cc0\u0c82\u0c97\u0cbf\u0ca3\u0ca4\u0ccb\u0cae\u0ccd \u0c95\u0cbf\u0c9f\u0ca4\u0c95 \u0ca4\u0c95\u0cbf\u0c9f \u0ca4\u0c95\u0cbf\u0c9f <br/> \u0cae\u0ca7\u0cc1\u0c95\u0cc8\u0ca4\u0cad \u0cae\u0cb0\u0ca8\u0cb0 \u0cb5\u0ca6\u0ca8\u0cae\u0ccd \u0cb6\u0cb6\u0cbe\u0c82\u0c95 \u0cb5\u0ca6\u0ca8\u0cae\u0ccd \u0c9c\u0cbf\u0ca4\u0cae\u0ca6\u0ca8\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0cad\u0c95\u0ccd\u0ca4\u0cbf \u0cb8\u0cbe\u0cb0 \u0c95\u0cc1\u0cb2\u0cb6\u0cc7\u0c96\u0cb0 \u0caf\u0ca4\u0cc0\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0caa\u0c9f\u0ccd\u0c9f \u0ca8\u0cbe\u0ca5 \u0cad\u0cc2\u0ca4 \u0cae\u0cc1\u0ca8\u0cbf \u0cb5\u0cbe\u0cb9\u0ca8 <br/> \u0cad\u0c95\u0ccd\u0ca4 \u0caa\u0cbe\u0ca6 \u0ca7\u0cc2\u0cb3\u0cbf \u0cae\u0ca7\u0cc1\u0cb0 \u0c95\u0cb5\u0cbf\u0cb8\u0cb0 \u0caa\u0cb0\u0c95\u0cbe\u0cb2 \u0cae\u0cb9\u0ca4\u0cbe\u0cb9\u0ccd\u0cb5\u0caf \u0caa\u0ccd\u0cb0\u0cae\u0cc1\u0c96\u0cc8\u0cb0\u0ccd- <br/> \u0c85\u0cb5\u0cbf\u0cb0\u0cb2 \u0cb8\u0cae\u0ccd\u0cb8\u0ccd\u0ca5\u0cbe\u0caf \u0cae\u0cbe\u0ca8\u0cae\u0ccd \u0ca4\u0ca4\u0ccd \u0ca4\u0ccd\u0cb0\u0cae\u0cbf\u0cb3\u0c97\u0cbe\u0ca8 \u0cb0\u0cb8\u0cbe\u0cb2\u0cb8 \u0c95\u0cbf\u0c82\u0c9a\u0cbf\u0ca4\u0cc1\u0ca8\u0ccd <br/> \u0cae\u0cc0\u0cb2\u0cbf\u0ca4 \u0ca8\u0caf\u0ca8\u0cae\u0ccd \u0caa\u0cbe\u0cb2\u0cbf\u0ca4 \u0cad\u0cc1\u0cb5\u0ca8\u0cae\u0ccd \u0c96\u0cc7\u0cb2\u0cbf\u0ca4 \u0cb0\u0c82\u0c97 \u0caa\u0cc1\u0cb0\u0cbe\u0ca8\u0ccd\u0ca6\u0cb0 \u0cad\u0cb5\u0ca8\u0cae\u0ccd"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d30\u0d02\u0d17\u0d28\u0d3e\u0d25\u0d2e\u0d4d \u0d05\u0d28\u0d3f\u0d36\u0d2e\u0d4d \u0d35\u0d28\u0d4d\u0d26\u0d47 \u0d36\u0d4d\u0d30\u0d40 \u0d28\u0d3e\u0d30\u0d26\u0d3e\u0d26\u0d3f \u0d35\u0d28\u0d4d\u0d26\u0d4d\u0d2f\u0d2e\u0d3e\u0d28 \u0d36\u0d4b\u0d2d\u0d28\u0d40\u0d2f\u0d2e\u0d4d \u0d05\u0d24\u0d3f \u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30 \u0d2e\u0d41\u0d30\u0d3e\u0d30\u0d35\u0d3f\u0d28\u0d4d\u0d26"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d02\u0d17\u0d33\u0d15\u0d30 \u0d28\u0d3f\u0d37\u0d4d\u0d15\u0d33\u0d02\u0d15 \u0d26\u0d15\u0d4d\u0d37\u0d40\u0d23 \u0d2e\u0d28\u0d4d\u0d26\u0d3e\u0d15\u0d3f\u0d28\u0d3f \u0d15\u0d3e\u0d35\u0d47\u0d30\u0d3f \u0d2e\u0d27\u0d4d\u0d2f\u0d38\u0d4d\u0d24\u0d2e\u0d4d \u0d36\u0d02\u0d16 \u0d1a\u0d15\u0d4d\u0d30 \u0d17\u0d26\u0d3e <br/> \u0d2a\u0d26\u0d4d\u0d2e\u0d27\u0d30 \u0d39\u0d38\u0d4d\u0d24\u0d2e\u0d4d \u0d38\u0d30\u0d38\u0d3f\u0d1c \u0d35\u0d3f\u0d15\u0d38\u0d3f\u0d24 \u0d24\u0d33\u0d35\u0d30 \u0d28\u0d47\u0d24\u0d4d\u0d30\u0d2e\u0d4d \u0d1c\u0d32\u0d27 \u0d15\u0d30 \u0d2e\u0d30\u0d15\u0d24 \u0d35\u0d30\u0d4d\u0d23 \u0d17\u0d3e\u0d24\u0d4d\u0d30\u0d2e\u0d4d <br/> \u0d1c\u0d28\u0d28 \u0d2e\u0d30\u0d23 \u0d2d\u0d2f \u0d36\u0d2e\u0d28 \u0d2a\u0d35\u0d3f\u0d24\u0d4d\u0d30\u0d2e\u0d4d \u0d1c\u0d32\u0d1c \u0d38\u0d2e\u0d4d\u0d2d\u0d35 \u0d38\u0d28\u0d4d\u0d28\u0d41\u0d24 \u0d1a\u0d30\u0d3f\u0d24\u0d4d\u0d30\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d32\u0d3e\u0d35\u0d23\u0d4d\u0d2f \u0d2a\u0d26 \u0d38\u0d30\u0d4b\u0d30\u0d41\u0d39\u0d2e\u0d4d \u0d36\u0d4d\u0d30\u0d40 \u0d30\u0d3e\u0d1c\u0d2f\u0d4b\u0d17 \u0d26\u0d30\u0d4d\u0d36\u0d28 \u0d38\u0d28\u0d4d\u0d24\u0d4b\u0d39\u0d2e\u0d4d \u0d2d\u0d3e\u0d35 <br/> \u0d2e\u0d27\u0d41\u0d30 \u0d38\u0d30\u0d38\u0d2e\u0d4d \u0d2a\u0d4d\u0d30\u0d35\u0d3e\u0d39\u0d2e\u0d4d \u0d2d\u0d15\u0d4d\u0d24\u0d4b\u0d24\u0d4d\u0d38\u0d35 \u0d2a\u0d30\u0d2e\u0d3e\u0d28\u0d28\u0d4d\u0d26 \u0d26\u0d47\u0d39\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d38\u0d47\u0d35\u0d3f\u0d24 \u0d28\u0d3f\u0d1c \u0d1c\u0d28 \u0d35\u0d30\u0d35\u0d3f \u0d18\u0d4b\u0d37\u0d2e\u0d4d \u0d36\u0d4d\u0d30\u0d40\u0d15\u0d30 \u0d30\u0d1a\u0d3f\u0d24 \u0d30\u0d41\u0d1a\u0d3f\u0d30 \u0d35\u0d3f\u0d2d\u0d42\u0d37\u0d23\u0d2e\u0d4d <br/> \u0d38\u0d4c\u0d35\u0d3f\u0d27 \u0d35\u0d3f\u0d39\u0d02\u0d17 \u0d2d\u0d4b\u0d17\u0d3f \u0d2d\u0d3e\u0d37\u0d23\u0d2e\u0d4d \u0d38\u0d28\u0d4d\u0d28\u0d41\u0d24 \u0d32\u0d02\u0d15\u0d3e\u0d27\u0d3f\u0d2a \u0d35\u0d3f\u0d2d\u0d40\u0d37\u0d23\u0d2e\u0d4d <br/> \u0d38\u0d4d\u0d35\u0d30\u0d1c\u0d24\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d1d\u0d4a\u0d2e\u0d4d\u0d24 \u0d27\u0d3f\u0d30\u0d3f \u0d24\u0d23\u0d15 \u0d23\u0d2e\u0d4d\u0d24 \u0d27\u0d3f\u0d30\u0d3f \u0d24\u0d23\u0d15 \u0d1d\u0d4a\u0d2e\u0d4d\u0d24\u0d15 \u0d27\u0d4d\u0d30\u0d41\u0d15 \u0d24\u0d23\u0d3e\u0d24 \u0d24\u0d26\u0d3f\u0d02\u0d17\u0d3f\u0d23\u0d24\u0d4b\u0d2e\u0d4d <br/> \u0d24\u0d41\u0d02\u0d17\u0d2e\u0d23\u0d3f \u0d15\u0d28\u0d15 \u0d2e\u0d02\u0d1c\u0d41 \u0d15\u0d1f\u0d15 \u0d15\u0d30 \u0d15\u0d02\u0d15\u0d23 \u0d35\u0d3f\u0d32\u0d38\u0d3f\u0d24\u0d3e\u0d02\u0d17 \u0d36\u0d41\u0d2d\u0d15\u0d30 \u0d36\u0d4d\u0d30\u0d40"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d1d\u0d23 \u0d1d\u0d23\u0d28\u0d4d\u0d26\u0d30\u0d3f \u0d24\u0d24\u0d15\u0d41\u0d1d\u0d2e\u0d4d \u0d24\u0d1d\u0d2e\u0d4d \u0d15\u0d41\u0d28\u0d4d\u0d24\u0d24 \u0d15\u0d41\u0d15\u0d41\u0d2e\u0d4d\u0d24\u0d24 \u0d27\u0d40\u0d2e\u0d4d\u0d24 \u0d17\u0d3f\u0d23\u0d24\u0d4b\u0d2e\u0d4d \u0d15\u0d3f\u0d1f <br/> \u0d2e\u0d39\u0d26\u0d39\u0d02\u0d15\u0d3e\u0d30 \u0d2e\u0d39\u0d26 \u0d2a\u0d02\u0d1a \u0d2d\u0d42\u0d24\u0d48\u0d30\u0d24\u0d3f \u0d15\u0d4d\u0d30\u0d4d\u0d24 \u0d2d\u0d41\u0d35\u0d28\u0d48\u0d15 \u0d1a\u0d30\u0d23\u0d3e\u0d32\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d27\u0d3f\u0d30\u0d28 \u0d27\u0d3f\u0d30\u0d28 \u0d24\u0d15\u0d4d\u0d30\u0d4d\u0d24 \u0d24\u0d4a\u0d2e\u0d4d \u0d24\u0d15\u0d3f\u0d1f\u0d24\u0d4a\u0d2e\u0d4d \u0d38\u0d30\u0d3f \u0d24\u0d15\u0d41\u0d2e\u0d4d \u0d24\u0d15\u0d27\u0d40..\u0d2e\u0d3f \u0d24\u0d15 <br/> \u0d17\u0d41\u0d23 \u0d2a\u0d3e\u0d35\u0d28\u0d2e\u0d3e\u0d24\u0d4d\u0d2e\u0d15\u0d2e\u0d4d \u0d05\u0d24\u0d40\u0d24\u0d2e\u0d4d \u0d2a\u0d30\u0d2e \u0d30\u0d02\u0d1c\u0d28\u0d2e\u0d4d \u0d06\u0d1a\u0d47\u0d37\u0d3f\u0d24 \u0d35\u0d48\u0d2d\u0d35 <br/> \u0d24\u0d32\u0d3e\u0d23\u0d4d...\u0d17\u0d41 \u0d24\u0d23\u0d2e\u0d4d \u0d2e\u0d3f\u0d24\u0d24\u0d15 \u0d1d\u0d41\u0d23\u0d41\u0d24\u0d15\u0d23\u0d2e\u0d4d \u0d2e\u0d3f\u0d24 \u0d24\u0d24\u0d15\u0d3f\u0d1f \u0d15\u0d41\u0d24\u0d23\u0d2e\u0d4d \u0d2e\u0d3f\u0d24 \u0d24\u0d15\u0d1d\u0d23\u0d41 <br/> \u0d30\u0d4b...\u0d28\u0d24 \u0d2e\u0d3e\u0d27\u0d41\u0d30\u0d2e\u0d4d \u0d05\u0d24\u0d3f\u0d36\u0d2f\u0d2e\u0