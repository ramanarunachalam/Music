\u0bbf\u0ba4 \u0ba4\u0bb3\u0bb5\u0bb0 \u0ba8\u0bc7\u0ba4\u0bcd\u0bb0\u0bae\u0bcd \u0b9c\u0bb2\u0ba4 \u0b95\u0bb0 \u0bae\u0bb0\u0b95\u0ba4 \u0bb5\u0bb0\u0bcd\u0ba3 \u0b95\u0bbe\u0ba4\u0bcd\u0bb0\u0bae\u0bcd <br/> \u0b9c\u0ba9\u0ba9 \u0bae\u0bb0\u0ba3 \u0baa\u0baf \u0b9a\u0bae\u0ba9 \u0baa\u0bb5\u0bbf\u0ba4\u0bcd\u0bb0\u0bae\u0bcd \u0b9c\u0bb2\u0b9c \u0b9a\u0bae\u0bcd\u0baa\u0bb5 \u0b9a\u0ba9\u0bcd\u0ba9\u0bc1\u0ba4 \u0b9a\u0bb0\u0bbf\u0ba4\u0bcd\u0bb0\u0bae\u0bcd"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0bb2\u0bbe\u0bb5\u0ba3\u0bcd\u0baf \u0baa\u0ba4 \u0b9a\u0bb0\u0bcb\u0bb0\u0bc1\u0bb9\u0bae\u0bcd \u0bb6\u0bcd\u0bb0\u0bc0 \u0bb0\u0bbe\u0b9c\u0baf\u0bcb\u0b95 \u0ba4\u0bb0\u0bcd\u0b9a\u0ba9 \u0b9a\u0ba8\u0bcd\u0ba4\u0bcb\u0bb9\u0bae\u0bcd \u0baa\u0bbe\u0bb5 <br/> \u0bae\u0ba4\u0bc1\u0bb0 \u0b9a\u0bb0\u0b9a\u0bae\u0bcd \u0baa\u0bcd\u0bb0\u0bb5\u0bbe\u0bb9\u0bae\u0bcd \u0baa\u0b95\u0bcd\u0ba4\u0bcb\u0ba4\u0bcd\u0b9a\u0bb5 \u0baa\u0bb0\u0bae\u0bbe\u0ba9\u0ba8\u0bcd\u0ba4 \u0ba4\u0bc7\u0bb9\u0bae\u0bcd"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b9a\u0bc7\u0bb5\u0bbf\u0ba4 \u0ba8\u0bbf\u0b9c \u0b9c\u0ba9 \u0bb5\u0bb0\u0bb5\u0bbf \u0b95\u0bcb\u0bb7\u0bae\u0bcd \u0bb6\u0bcd\u0bb0\u0bc0\u0b95\u0bb0 \u0bb0\u0b9a\u0bbf\u0ba4 \u0bb0\u0bc1\u0b9a\u0bbf\u0bb0 \u0bb5\u0bbf\u0baa\u0bc2\u0bb7\u0ba3\u0bae\u0bcd <br/> \u0b9a\u0bcc\u0bb5\u0bbf\u0ba4 \u0bb5\u0bbf\u0bb9\u0b99\u0bcd\u0b95 \u0baa\u0bcb\u0b95\u0bbf \u0baa\u0bbe\u0bb7\u0ba3\u0bae\u0bcd \u0b9a\u0ba9\u0bcd\u0ba9\u0bc1\u0ba4 \u0bb2\u0b99\u0bcd\u0b95\u0bbe\u0ba4\u0bbf\u0baa \u0bb5\u0bbf\u0baa\u0bc0\u0bb7\u0ba3\u0bae\u0bcd <br/> \u0bb8\u0bcd\u0bb5\u0bb0\u0b9c\u0ba4\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0b9c\u0bca\u0bae\u0bcd\u0ba4 \u0ba4\u0bbf\u0bb0\u0bbf \u0ba4\u0ba3\u0b95 \u0ba3\u0bae\u0bcd\u0ba4 \u0ba4\u0bbf\u0bb0\u0bbf \u0ba4\u0ba3\u0b95 \u0b9c\u0bca\u0bae\u0bcd\u0ba4\u0b95 \u0ba4\u0bcd\u0bb0\u0bc1\u0b95 \u0ba4\u0ba3\u0bbe\u0ba4 \u0ba4\u0ba4\u0bbf\u0b99\u0bcd\u0b95\u0bbf\u0ba3\u0ba4\u0bcb\u0bae\u0bcd <br/> \u0ba4\u0bc1\u0b99\u0bcd\u0b95\u0bae\u0ba3\u0bbf \u0b95\u0ba9\u0b95 \u0bae\u0b9e\u0bcd\u0b9c\u0bc1 \u0b95\u0b9f\u0b95 \u0b95\u0bb0 \u0b95\u0b99\u0bcd\u0b95\u0ba3 \u0bb5\u0bbf\u0bb2\u0b9a\u0bbf\u0ba4\u0bbe\u0b99\u0bcd\u0b95 \u0b9a\u0bc1\u0baa\u0b95\u0bb0 \u0bb6\u0bcd\u0bb0\u0bc0"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b9c\u0ba3 \u0b9c\u0ba3\u0ba8\u0bcd\u0ba4\u0bb0\u0bbf \u0ba4\u0ba4\u0b95\u0bc1\u0b9c\u0bae\u0bcd \u0ba4\u0b9c\u0bae\u0bcd \u0b95\u0bc1\u0ba8\u0bcd\u0ba4\u0ba4 \u0b95\u0bc1\u0b95\u0bc1\u0bae\u0bcd\u0ba4\u0ba4 \u0ba4\u0bc0\u0bae\u0bcd\u0ba4 \u0b95\u0bbf\u0ba3\u0ba4\u0bcb\u0bae\u0bcd \u0b95\u0bbf\u0b9f <br/> \u0bae\u0bb9\u0ba4\u0bb9\u0b99\u0bcd\u0b95\u0bbe\u0bb0 \u0bae\u0bb9\u0ba4 \u0baa\u0b9e\u0bcd\u0b9a \u0baa\u0bc2\u0ba4\u0bc8\u0bb0\u0ba4\u0bbf \u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0baa\u0bc1\u0bb5\u0ba9\u0bc8\u0b95 \u0b9a\u0bb0\u0ba3\u0bbe\u0bb2\u0baf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0ba4\u0bbf\u0bb0\u0ba9 \u0ba4\u0bbf\u0bb0\u0ba9 \u0ba4\u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0ba4\u0bca\u0bae\u0bcd \u0ba4\u0b95\u0bbf\u0b9f\u0ba4\u0bca\u0bae\u0bcd \u0b9a\u0bb0\u0bbf \u0ba4\u0b95\u0bc1\u0bae\u0bcd \u0ba4\u0b95\u0ba4\u0bc0..\u0bae\u0bbf \u0ba4\u0b95 <br/> \u0b95\u0bc1\u0ba3 \u0baa\u0bbe\u0bb5\u0ba9\u0bae\u0bbe\u0ba4\u0bcd\u0bae\u0b95\u0bae\u0bcd \u0b85\u0ba4\u0bc0\u0ba4\u0bae\u0bcd \u0baa\u0bb0\u0bae \u0bb0\u0b9e\u0bcd\u0b9c\u0ba9\u0bae\u0bcd \u0b86\u0b9a\u0bc7\u0bb7\u0bbf\u0ba4 \u0bb5\u0bc8\u0baa\u0bb5 <br/> \u0ba4\u0bb2\u0bbe\u0ba3\u0bcd...\u0b95\u0bc1 \u0ba4\u0ba3\u0bae\u0bcd \u0bae\u0bbf\u0ba4\u0ba4\u0b95 \u0b9c\u0bc1\u0ba3\u0bc1\u0ba4\u0b95\u0ba3\u0bae\u0bcd \u0bae\u0bbf\u0ba4 \u0ba4\u0ba4\u0b95\u0bbf\u0b9f \u0b95\u0bc1\u0ba4\u0ba3\u0bae\u0bcd \u0bae\u0bbf\u0ba4 \u0ba4\u0b95\u0b9c\u0ba3\u0bc1 <br/> \u0bb0\u0bcb...\u0ba8\u0ba4 \u0bae\u0bbe\u0ba4\u0bc1\u0bb0\u0bae\u0bcd \u0b85\u0ba4\u0bbf\u0b9a\u0baf\u0bae\u0bcd \u0b85\u0ba4\u0bbf \u0b9a\u0bcb\u0baa\u0bbf\u0ba4 \u0b9a\u0bc1\u0bb5\u0ba4\u0ba9\u0bae\u0bcd \u0b85\u0ba4\u0bbf \u0b9a\u0bc0\u0ba4\u0bb3 \u0baa\u0ba4\u0baf\u0bc1\u0b95"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0ba4\u0bbe\u0bae\u0bcd \u0ba4\u0bc0\u0bae\u0bcd\u0ba4 \u0ba4\u0b9c\u0bcd\u0b9c\u0ba3\u0bc1 \u0ba4\u0bbf\u0bae\u0bbf\u0ba4 \u0ba4\u0bbf\u0bae\u0bbf\u0ba4 \u0ba4\u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0b9c\u0bae\u0bcd \u0b9c\u0bae\u0bcd \u0ba4\u0b95\u0b9c\u0bae\u0bcd \u0ba4\u0ba4\u0ba3\u0bbe <br/> \u0b86\u0ba9\u0ba9\u0bcd...\u0ba4\u0bae\u0bcd \u0bb5\u0bcd\u0baf\u0baf\u0bae\u0bcd \u0b85\u0ba4\u0bcd\u0bb5\u0bbf\u0ba4\u0bc0\u0baf\u0bae\u0bcd \u0b85\u0b95\u0bbf\u0bb2\u0ba9\u0bc8\u0b95\u0bae\u0bc8\u0b95 \u0bae\u0ba9\u0bae\u0bc8\u0b95 \u0baa\u0bb0\u0bae\u0bcd <br/> \u0ba4\u0b95\u0b9c\u0ba8\u0bcd\u0ba4 \u0ba4\u0bbf\u0bae\u0bbf \u0ba4\u0bbf\u0bae\u0bbf \u0ba4\u0b9c\u0ba3\u0bc1 \u0ba4\u0ba4\u0bb0\u0bbf \u0ba4\u0ba4\u0bc0\u0b99\u0bcd\u0b95\u0bbf\u0ba3\u0ba4\u0bcb\u0bae\u0bcd \u0b95\u0bbf\u0b9f\u0ba4\u0b95 \u0ba4\u0b95\u0bbf\u0b9f \u0ba4\u0b95\u0bbf\u0b9f <br/> \u0bae\u0ba4\u0bc1\u0b95\u0bc8\u0ba4\u0baa \u0bae\u0bb0\u0ba9\u0bb0 \u0bb5\u0ba4\u0ba9\u0bae\u0bcd \u0b9a\u0b9a\u0bbe\u0b99\u0bcd\u0b95 \u0bb5\u0ba4\u0ba9\u0bae\u0bcd \u0b9c\u0bbf\u0ba4\u0bae\u0ba4\u0ba9\u0bae\u0bcd"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0baa\u0b95\u0bcd\u0ba4\u0bbf \u0b9a\u0bbe\u0bb0 \u0b95\u0bc1\u0bb2\u0b9a\u0bc7\u0b95\u0bb0 \u0baf\u0ba4\u0bc0\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 \u0baa\u0b9f\u0bcd\u0b9f \u0ba8\u0bbe\u0ba4 \u0baa\u0bc2\u0ba4 \u0bae\u0bc1\u0ba9\u0bbf \u0bb5\u0bbe\u0bb9\u0ba9 <br/> \u0baa\u0b95\u0bcd\u0ba4 \u0baa\u0bbe\u0ba4 \u0ba4\u0bc2\u0bb3\u0bbf \u0bae\u0ba4\u0bc1\u0bb0 \u0b95\u0bb5\u0bbf\u0b9a\u0bb0 \u0baa\u0bb0\u0b95\u0bbe\u0bb2 \u0bae\u0bb9\u0ba4\u0bbe\u0bb9\u0bcd\u0bb5\u0baf \u0baa\u0bcd\u0bb0\u0bae\u0bc1\u0b95\u0bc8\u0bb0\u0bcd- <br/> \u0b85\u0bb5\u0bbf\u0bb0\u0bb2 \u0b9a\u0bae\u0bcd\u0bb8\u0bcd\u0ba4\u0bbe\u0baf \u0bae\u0bbe\u0ba9\u0bae\u0bcd \u0ba4\u0ba4\u0bcd \u0ba4\u0bcd\u0bb0\u0bae\u0bb