bc7 \u0ba4\u0bcd\u0bb5\u0bbe\u0bae\u0bc7\u0bb5 \u0b9a\u0bb0\u0ba3\u0bae\u0bcd \u0baa\u0bcd\u0bb0\u0baa\u0ba4\u0bcd\u0baf\u0bc7 (\u0b9a\u0b95\u0bb2)"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b9a\u0bb0\u0bcd\u0bb5 \u0bb0\u0bcb\u0b95 \u0bb9\u0bb0 \u0b9a\u0b95\u0bcd\u0bb0\u0bae\u0baf\u0bc7 \u0b9a\u0bb0\u0bcd\u0bb5\u0bbe\u0ba9\u0ba8\u0bcd\u0ba4\u0bae\u0baf\u0bc7 \u0bae\u0b99\u0bcd\u0b95\u0bb3\u0bae\u0baf\u0bc7 (\u0b9a\u0b95\u0bb2)"}, {"M": "\u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0b85 \u0b95 \u0b9a \u0b9f \u0ba4 \u0baa \u0baf\u0bb0 \u0bb2\u0bb5 \u0b9a\u0bbe\u0ba4\u0bbf \u0b95\u0bcd\u0bb7\u0bbe\u0ba8\u0bcd\u0ba4 \u0b85\u0b95\u0bcd\u0bb7\u0bb0\u0bae\u0baf\u0bbf \u0bb5\u0bbe\u0b99\u0bcd\u0b95\u0bcd\u0bae\u0baf\u0bbf \u0b9a\u0bbf\u0ba8\u0bcd\u0ba4\u0baf\u0bbe\u0bae\u0bbf <br/> \u0b9a\u0bc1\u0b95 \u0ba8\u0bbe\u0bb0\u0ba4 \u0b95\u0bc1\u0bae\u0bcd\u0baa\u0b9c \u0bae\u0bc1\u0ba9\u0bbf\u0bb5\u0bb0 \u0bb8\u0bcd\u0ba4\u0bc1\u0ba4\u0bbf \u0b95\u0bbe\u0baf\u0b95 \u0b9c\u0ba9 \u0b9a\u0ba9\u0bcd\u0ba9\u0bc1\u0ba4\u0bc7 (\u0b9a\u0b95\u0bb2)"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0ba8\u0bbe\u0b95\u0ba9\u0bbe\u0baf\u0b95 \u0b9a\u0ba4\u0ba4\u0bcd-\u0ba4\u0b9a\u0bbe\u0bb0 \u0baa\u0ba3 \u0bb2\u0bcb\u0b95 \u0bb5\u0bb9\u0bbf\u0ba4 \u0ba4\u0bb0 \u0b95\u0bb0 \u0bb5\u0bb2\u0baf\u0bc7 <br/> \u0bb2\u0bcb\u0b95 \u0bb2\u0bcb\u0b95 \u0b9a\u0bae\u0bcd\u0bae\u0bcb\u0bb9\u0bbf\u0ba4 \u0bb9\u0bbf\u0ba4\u0b95\u0bb0 \u0baa\u0bc1\u0ba4\u0bcd\u0ba4\u0bbf \u0b9a\u0bbf\u0ba4\u0bcd\u0ba4\u0bbf \u0ba8\u0ba4 \u0baa\u0bc1\u0bb0\u0ba9\u0bbf\u0bb2\u0baf\u0bc7 (\u0b9a\u0b95\u0bb2)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0baa\u0bb5\u0bb0\u0bcb\u0b95 \u0bb9\u0bb0 \u0bb5\u0bc8\u0baa\u0bb5\u0bc7 \u0baa\u0bb0\u0bae \u0b95\u0bb2\u0bcd\u0baf\u0bbe\u0ba3\u0b95\u0bc1\u0ba3 \u0ba8\u0bbf\u0b95\u0bb0\u0bc7 <br/> \u0ba8\u0bb5 \u0bb0\u0b9a\u0bbe\u0bb2\u0b99\u0bcd\u0b95\u0bbe\u0bb0 \u0b95\u0bbe\u0bb5\u0bcd\u0baf \u0ba8\u0bbe\u0b9f\u0b95 \u0bb5\u0bb0\u0bcd\u0ba3\u0bbf\u0ba4\u0bc7 \u0b9a\u0bc1\u0baa\u0b95\u0bb0\u0bc7 <br/> \u0b95\u0bc1\u0bb5\u0bb2\u0baf\u0ba4\u0bb3 \u0ba8\u0bb5 \u0ba8\u0bc0\u0bb2 \u0b9a\u0bb0\u0bc0\u0bb0 \u0b95\u0bcb\u0bb5\u0bbf\u0ba8\u0bcd\u0ba4 \u0b9a\u0bcb\u0ba4\u0bb0\u0bc7 \u0bb6\u0bcd\u0bb0\u0bc0\u0b95\u0bb0\u0bc7 <br/> \u0b9a\u0bbf\u0bb5 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0baf \u0b95\u0bae\u0bb2 \u0ba8\u0bbf\u0bb2\u0baf\u0bc7 \u0ba4\u0bcd\u0bb0\u0bbf\u0baa\u0bc1\u0bb0\u0bbe\u0b9a\u0bbf\u0ba4\u0bcd\u0ba4\u0bc0\u0bb6\u0bcd\u0bb5\u0bb0\u0bbf \u0ba8\u0ba4 \u0bb6\u0bcd\u0bb0\u0bc0 \u0ba8\u0b95\u0bb0\u0bc7"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b85\u0bb5\u0ba9\u0ba4 \u0bb0\u0bb9\u0bb8\u0bcd\u0baf \u0baf\u0bcb\u0b95\u0bbf\u0ba9\u0bbf \u0b95\u0bc2\u0bb2\u0bc7 \u0b9a\u0ba4 \u0ba4\u0bbf\u0ba9\u0b95\u0bb0 \u0b9a\u0bae\u0ba4\u0bcd\u0baf\u0ba4\u0bbf \u0bae\u0bc1\u0b95\u0b9c\u0bbe\u0bb2\u0bc7 <br/> \u0baa\u0bc1\u0bb5\u0ba9 \u0baa\u0bcd\u0bb0\u0b9a\u0bbf\u0ba4\u0bcd\u0ba4 \u0bb9\u0bcd\u0bb0\u0bc0\u0b99\u0bcd\u0b95\u0bbe\u0bb0 \u0b95\u0bbe\u0bae\u0bc7\u0bb6\u0bcd\u0bb5\u0bb0 \u0baa\u0bc0\u0b9c \u0bae\u0ba8\u0bcd\u0ba4\u0bb0 \u0bb2\u0bcb\u0bb2\u0bc7 (\u0b9a\u0b95\u0bb2)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c15\u0c32 \u0c32\u0c4b\u0c15 \u0c28\u0c3e\u0c2f\u0c3f\u0c15\u0c47 \u0c24\u0c4d\u0c35\u0c3e\u0c2e\u0c47\u0c35 \u0c36\u0c30\u0c23\u0c2e\u0c4d \u0c2a\u0c4d\u0c30\u0c2a\u0c26\u0c4d\u0c2f\u0c47 (\u0c38\u0c15\u0c32)"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c38\u0c30\u0c4d\u0c35 \u0c30\u0c4b\u0c17 \u0c39\u0c30 \u0c1a\u0c15\u0c4d\u0c30\u0c2e\u0c2f\u0c47 \u0c38\u0c30\u0c4d\u0c35\u0c3e\u0c28\u0c28\u0c4d\u0c26\u0c2e\u0c2f\u0c47 \u0c2e\u0c19\u0c4d\u0c17\u0c33\u0c2e\u0c2f\u0c47 (\u0c38\u0c15\u0c32)"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c05 \u0c15 \u0c1a \u0c1f \u0c24 \u0c2a \u0c2f\u0c30 \u0c32\u0c35 \u0c38\u0c3e\u0c26\u0c3f \u0c15\u0c4d\u0c37\u0c3e\u0c28\u0c4d\u0c24 \u0c05\u0c15\u0c4d\u0c37\u0c30\u0c2e\u0c2f\u0c3f \u0c35\u0c3e\u0c19\u0c4d\u0c17\u0c4d\u0c2e\u0c2f\u0c3f \u0c1a\u0c3f\u0c28\u0c4d\u0c24\u0c2f\u0c3e\u0c2e\u0c3f <br/> \u0c36\u0c41\u0c15 \u0c28\u0c3e\u0c30\u0c26 \u0c15\u0c41\u0c2e\u0c4d\u0c2d\u0c1c \u0c2e\u0c41\u0c28\u0c3f\u0c35\u0c30 \u0c38\u0c4d\u0c24\u0c41\u0c24\u0c3f \u0c17\u0c3e\u0c2f\u0c15 \u0c1c\u0c28 \u0c38\u0c28\u0c4d\u0c28\u0c41\u0c24\u0c47 (\u0c38\u0c15\u0c32)"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c28\u0c3e\u0c17\u0c28\u0c3e\u0c2f\u0c15 \u0c38\u0c26\u0c24\u0c4d-\u0c26\u0c36\u0c3e\u0c30 \u0c2b\u0c23 \u0c32\u0c4b\u0c15 \u0c35\u0c39\u0c3f\u0c24 \u0c27\u0c30 \u0c15\u0c30 \u0c35\u0c32\u0c2f\u0c47 <br/> \u0c32\u0c4b\u0c15 \u0c32\u0c4b\u0c15 \u0c38\u0c2e\u0c4d\u0c2e\u0c4b\u0c39\u0c3f\u0c24 \u0c39\u0c3f\u0c24\u0c15\u0c30 \u0c2c\u0c41\u0c26\u0c4d\u0c27\u0c3f \u0c38\u0c3f\u0c26\u0c4d\u0c27\u0c3f \u0c28\u0c24 \u0c2a\u0c41\u0c30\u0c28\u0c3f\u0c32\u0c2f\u0c47 (\u0c38\u0c15\u0c32)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c2d\u0c35\u0c30\u0c4b\u0c17 \u0c39\u0c30 \u0c35\u0c48\u0c2d\u0c35\u0c47 \u0c2a\u0c30\u0c2e \u0c15\u0c32\u0c4d\u0c2f\u0c3e\u0c23\u0c17\u0c41\u0c23 \u0c28\u0c3f\u0c15\u0c30\u0c47 <br/> \u0c28\u0c35 \u0c30\u0c38\u0c3e\u0c32\u0c19\u0c4d\u0c15\u0c3e\u0c30 \u0c15\u0c3e\u0c35\u0c4d\u0c2f \u0c28\u0c3e\u0c1f\u0c15 \u0c35\u0c30\u0c4d\u0c23\u0c3f\u0c24\u0c47 \u0c36\u0c41\u0c2d\u0c15\u0c30\u0c47 <br/> \u0c15\u0c41\u0c35\u0c32\u0c2f\u0c26\u0c33 \u0c28\u0c35 \u0c28\u0c40\u0c32 \u0c36\u0c30\u0c40\u0c30 \u0c17\u0c4b\u0c35\u0c3f\u0c28\u0c4d\u0c26 \u0c38\u0c4b\u0c26\u0c30\u0c47 \u0c36\u0c4d\u0c30\u0c40\u0c15\u0c30\u0c47 <br/> \u0c36\u0c3f\u0c35 \u0c39\u0c4d\u0c30\u0c4d\u0c26\u0c2f \u0c15\u0c2e\u0c32 \u0c28\u0c3f\u0c32\u0c2f\u0c47 \u0c24\u0c4d\u0c30\u0c3f\u0c2a\u0c41\u0c30\u0c3e\u0c38\u0c3f\u0c26\u0c4d\u0c27\u0c40\u0c36\u0c4d\u0c35\u0c30\u0c3f \u0c28\u0c24 \u0c36\u0c4d\u0c30\u0c40 \u0c28\u0c17\u0c30\u0c47"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c05\u0c35\u0c28\u0c24 \u0c30\u0c39\u0c38\u0c4d\u0c2f \u0c2f\u0c4b\u0c17\u0c3f\u0c28\u0c3f \u0c15\u0c42\u0c32\u0c47 \u0c36\u0c24 \u0c26\u0c3f\u0c28\u0c15\u0c30 \u0c38\u0c2e\u0c26\u0c4d\u0c2f\u0c24\u0c3f \u0c2e\u0c41\u0c16\u0c1c\u0c3e\u0c32\u0c47 <br/> \u0c2d\u0c41\u0c35\u0c28 \u0c2a\u0c4d\u0c30\u0c38\u0c3f\u0c26\u0c4d\u0c27 \u0c39\u0c4d\u0c30\u0c40\u0c19\u0c4d\u0c15\u0c3e\u0c30 \u0c15\u0c3e\u0c2e\u0c47\u0c36\u0c4d\u0c35\u0c30 \u0c2d\u0c40\u0c1c \u0c2e\u0c28\u0c4d\u0c26\u0c30 \u0c32\u0c4b\u0c32\u0c47 (\u0c38\u0c15\u0c32)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0c95\u0cb2 \u0cb2\u0ccb\u0c95 \u0ca8\u0cbe\u0caf\u0cbf\u0c95\u0cc7 \u0ca4\u0ccd\u0cb5\u0cbe\u0cae\u0cc7\u0cb5 \u0cb6\u0cb0\u0ca3\u0cae\u0ccd \u0caa\u0ccd\u0cb0\u0caa\u0ca6\u0ccd\u0caf\u0cc7 (\u0cb8\u0c95\u0cb2)"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cb8\u0cb0\u0ccd\u0cb5 \u0cb0\u0ccb\u0c97 \u0cb9\u0cb0 \u0c9a\u0c95\u0ccd\u0cb0\u0cae\u0caf\u0cc7 \u0cb8\u0cb0\u0ccd\u0cb5\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6\u0cae\u0caf\u0cc7 \u0cae\u0c99\u0ccd\u0c97\u0cb3\u0cae\u0caf\u0cc7 (\u0cb8\u0c95\u0cb2)"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c85 \u0c95 \u0c9a \u0c9f \u0ca4 \u0caa \u0caf\u0cb0 \u0cb2\u0cb5 \u0cb8\u0cbe\u0ca6\u0cbf \u0c95\u0ccd\u0cb7\u0cbe\u0ca8\u0ccd\u0ca4 \u0c85\u0c95\u0ccd\u0cb7\u0cb0\u0cae\u0caf\u0cbf \u0cb5\u0cbe\u0c99\u0ccd\u0c97\u0ccd\u0cae\u0caf\u0cbf \u0c9a\u0cbf\u0ca8\u0ccd\u0ca4\u0caf\u0cbe\u0cae\u0cbf <br/> \u0cb6\u0cc1\u0c95 \u0ca8\u0cbe\u0cb0\u0ca6 \u0c95\u0cc1\u0cae\u0ccd\u0cad\u0c9c \u0cae\u0cc1\u0ca8\u0cbf\u0cb5\u0cb0 \u0cb8\u0ccd\u0ca4\u0cc1\u0ca4\u0cbf \u0c97\u0cbe\u0caf\u0c95 \u0c9c\u0ca8 \u0cb8\u0ca8\u0ccd\u0ca8\u0cc1\u0ca4\u0cc7 (\u0cb8\u0c95\u0cb2)"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0ca8\u0cbe\u0c97\u0ca8\u0cbe\u0caf\u0c95 \u0cb8\u0ca6\u0ca4\u0ccd-\u0ca6\u0cb6\u0cbe\u0cb0 \u0cab\u0ca3 \u0cb2\u0ccb\u0c95 \u0cb5\u0cb9\u0cbf\u0ca4 \u0ca7\u0cb0 \u0c95\u0cb0 \u0cb5\u0cb2\u0caf\u0cc7 <br/> \u0cb2\u0ccb\u0c95 \u0cb2\u0ccb\u0c95 \u0cb8\u0cae\u0ccd\u0cae\u0ccb\u0cb9\u0cbf\u0ca4 \u0cb9\u0cbf\u0ca4\u0c95\u0cb0 \u0cac\u0cc1\u0ca6\u0ccd\u0ca7\u0cbf \u0cb8\u0cbf\u0ca6\u0ccd\u0ca7\u0cbf \u0ca8\u0ca4 \u0caa\u0cc1\u0cb0\u0ca8\u0cbf\u0cb2\u0caf\u0cc7 (\u0cb8\u0c95\u0cb2)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cad\u0cb5\u0cb0\u0ccb\u0c97 \u0cb9\u0cb0 \u0cb5\u0cc8\u0cad\u0cb5\u0cc7 \u0caa\u0cb0\u0cae \u0c95\u0cb2\u0ccd\u0caf\u0cbe\u0ca3\u0c97\u0cc1\u0ca3 \u0ca8\u0cbf\u0c95\u0cb0\u0cc7 <br/> \u0ca8\u0cb5 \u0cb0\u0cb8\u0cbe\u0cb2\u0c99\u0ccd\u0c95\u0cbe\u0cb0 \u0c95\u0cbe\u0cb5\u0ccd\u0caf \u0ca8\u0cbe\u0c9f\u0c95 \u0cb5\u0cb0\u0ccd\u0ca3\u0cbf\u0ca4\u0cc7 \u0cb6\u0cc1\u0cad\u0c95\u0cb0\u0cc7 <br/> \u0c95\u0cc1\u0cb5\u0cb2\u0caf\u0ca6\u0cb3 \u0ca8\u0cb5 \u0ca8\u0cc0\u0cb2 \u0cb6\u0cb0\u0cc0\u0cb0 \u0c97\u0ccb\u0cb5\u0cbf\u0ca8\u0ccd\u0ca6 \u0cb8\u0ccb\u0ca6\u0cb0\u0cc7 \u0cb6\u0ccd\u0cb0\u0cc0\u0c95\u0cb0\u0cc7 <br/> \u0cb6\u0cbf\u0cb5 \u0cb9\u0ccd\u0cb0\u0ccd\u0ca6\u0caf \u0c95\u0cae\u0cb2 \u0ca8\u0cbf\u0cb2\u0caf\u0cc7 \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0\u0cbe\u0cb8\u0cbf\u0ca6\u0ccd\u0ca7\u0cc0\u0cb6\u0ccd\u0cb5\u0cb0\u0cbf \u0ca8\u0ca4 \u0cb6\u0ccd\u0cb0\u0cc0 \u0ca8\u0c97\u0cb0\u0cc7"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c85\u0cb5\u0ca8\u0ca4 \u0cb0\u0cb9\u0cb8\u0ccd\u0caf \u0caf\u0ccb\u0c97\u0cbf\u0ca8\u0cbf \u0c95\u0cc2\u0cb2\u0cc7 \u0cb6\u0ca4 \u0ca6\u0cbf\u0ca8\u0c95\u0cb0 \u0cb8\u0cae\u0ca6\u0ccd\u0caf\u0ca4\u0cbf \u0cae\u0cc1\u0c96\u0c9c\u0cbe\u0cb2\u0cc7 <br/> \u0cad\u0cc1\u0cb5\u0ca8 \u0caa\u0ccd\u0cb0\u0cb8\u0cbf\u0ca6\u0ccd\u0ca7 \u0cb9\u0ccd\u0cb0\u0cc0\u0c99\u0ccd\u0c95\u0cbe\u0cb0 \u0c95\u0cbe\u0cae\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0 \u0cad\u0cc0\u0c9c \u0cae\u0ca8\u0ccd\u0ca6\u0cb0 \u0cb2\u0ccb\u0cb2\u0cc7 (\u0cb8\u0c95\u0cb2)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d15\u0d32 \u0d32\u0d4b\u0d15 \u0d28\u0d3e\u0d2f\u0d3f\u0d15\u0d47 \u0d24\u0d4d\u0d35\u0d3e\u0d2e\u0d47\u0d35 \u0d36\u0d30\u0d23\u0d2e\u0d4d \u0d2a\u0d4d\u0d30\u0d2a\u0d26\u0d4d\u0d2f\u0d47 (\u0d38\u0d15\u0d32)"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d38\u0d30\u0d4d\u0d35 \u0d30\u0d4b\u0d17 \u0d39\u0d30 \u0d1a\u0d15\u0d4d\u0d30\u0d2e\u0d2f\u0d47 \u0d38\u0d30\u0d4d\u0d35\u0d3e\u0d28\u0d28\u0d4d\u0d26\u0d2e\u0d2f\u0d47 \u0d2e\u0d19\u0d4d\u0d17\u0d33\u0d2e\u0d2f\u0d47 (\u0d38\u0d15\u0d32)"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d05 \u0d15 \u0d1a \u0d1f \u0d24 \u0d2a \u0d2f\u0d30 \u0d32\u0d35 \u0d38\u0d3e\u0d26\u0d3f \u0d15\u0d4d\u0d37\u0d3e\u0d28\u0d4d\u0d24 \u0d05\u0d15\u0d4d\u0d37\u0d30\u0d2e\u0d2f\u0d3f \u0d35\u0d3e\u0d19\u0d4d\u0d17\u0d4d\u0d2e\u0d2f\u0d3f \u0d1a\u0d3f\u0d28\u0d4d\u0d24\u0d2f\u0d3e\u0d2e\u0d3f <br/> \u0d36\u0d41\u0d15 \u0d28\u0d3e\u0d30\u0d26 \u0d15\u0d41\u0d2e\u0d4d\u0d2d\u0d1c \u0d2e\u0d41\u0d28\u0d3f\u0d35\u0d30 \u0d38\u0d4d\u0d24\u0d41\u0d24\u0d3f \u0d17\u0d3e\u0d2f\u0d15 \u0d1c\u0d28 \u0d38\u0d28\u0d4d\u0d28\u0d41\u0d24\u0d47 (\u0d38\u0d15\u0d32)"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d28\u0d3e\u0d17\u0d28\u0d3e\u0d2f\u0d15 \u0d38\u0d26\u0d24\u0d4d-\u0d26\u0d36\u0d3e\u0d30 \u0d2b\u0d23 \u0d32\u0d4b\u0d15 \u0d35\u0d39\u0d3f\u0d24 \u0d27\u0d30 \u0d15\u0d30 \u0d35\u0d32\u0d2f\u0d47 <br/> \u0d32\u0d4b\u0d15 \u0d32\u0d4b\u0d15 \u0d38\u0d2e\u0d4d\u0d2e\u0d4b\u0d39\u0d3f\u0d24 \u0d39\u0d3f\u0d24\u0d15\u0d30 \u0d2c\u0d41\u0d26\u0d4d\u0d27\u0d3f \u0d38\u0d3f\u0d26\u0d4d\u0d27\u0d3f \u0d28\u0d24 \u0d2a\u0d41\u0d30\u0d28\u0d3f\u0d32\u0d2f\u0d47 (\u0d38\u0d15\u0d32)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d2d\u0d35\u0d30\u0d4b\u0d17 \u0d39\u0d30 \u0d35\u0d48\u0d2d\u0d35\u0d47 \u0d2a\u0d30\u0d2e \u0d15\u0d32\u0d4d\u0d2f\u0d3e\u0d23\u0d17\u0d41\u0d23 \u0d28\u0d3f\u0d15\u0d30\u0d47 <br/> \u0d28\u0d35 \u0d30\u0d38\u0d3e\u0d32\u0d19\u0d4d\u0d15\u0d3e\u0d30 \u0d15\u0d3e\u0d35\u0d4d\u0d2f \u0d28\u0d3e\u0d1f\u0d15 \u0d35\u0d30\u0d4d\u0d23\u0d3f\u0d24\u0d47 \u0d36\u0d41\u0d2d\u0d15\u0d30\u0d47 <br/> \u0d15\u0d41\u0d35\u0d32\u0d2f\u0d26\u0d33 \u0d28\u0d35 \u0d28\u0d40\u0d32 \u0d36\u0d30\u0d40\u0d30 \u0d17\u0d4b\u0d35\u0d3f\u0d28\u0d4d\u0d26 \u0d38\u0d4b\u0d26\u0d30\u0d47 \u0d36\u0d4d\u0d30\u0d40\u0d15\u0d30\u0d47 <br/> \u0d36\u0d3f\u0d35 \u0d39\u0d4d\u0d30\u0d4d\u0d26\u0d2f \u0d15\u0d2e\u0d32 \u0d28\u0d3f\u0d32\u0d2f\u0d47 \u0d24\u0d4d\u0d30\u0d3f\u0d2a\u0d41\u0d30\u0d3e\u0d38\u0d3f\u0d26\u0d4d\u0d27\u0d40\u0d36\u0d4d\u0d35\u0d30\u0d3f \u0d28\u0d24 \u0d36\u0d4d\u0d30\u0d40 \u0d28\u0d17\u0d30\u0d47"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d05\u0d35\u0d28\u0d24 \u0d30\u0d39\u0d38\u0d4d\u0d2f \u0d2f\u0d4b\u0d17\u0d3f\u0d28\u0d3f \u0d15\u0d42\u0d32\u0d47 \u0d36\u0d24 \u0d26\u0d3f\u0d28\u0d15\u0d30 \u0d38\u0d2e\u0d26\u0d4d\u0d2f\u0d24\u0d3f \u0d2e\u0d41\u0d16\u0d1c\u0d3e\u0d32\u0d47 <br/> \u0d2d\u0d41\u0d35\u0d28 \u0d2a\u0d4d\u0d30\u0d38\u0d3f\u0d26\u0d4d\u0d27 \u0d39\u0d4d\u0d30\u0d40\u0d19\u0d4d\u0d15\u0d3e\u0d30 \u0d15\u0d3e\u0d2e\u0d47\u0d36\u0d4d\u0d35\u0d30 \u0d2d\u0d40\u0d1c \u0d2e\u0d28\u0d4d\u0d26\u0d30 \u0d32\u0d4b\u0d32\u0d47 (\u0d38\u0d15\u0d32)"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0938\u0915\u0932 \u0932\u094b\u0915 \u0928\u093e\u092f\u093f\u0915\u0947 \u0924\u094d\u0935\u093e\u092e\u0947\u0935 \u0936\u0930\u0923\u092e\u094d \u092a\u094d\u0930\u092a\u0926\u094d\u092f\u0947 (\u0938\u0915\u0932)"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0938\u0930\u094d\u0935 \u0930\u094b\u0917 \u0939\u0930 \u091a\u0915\u094d\u0930\u092e\u092f\u0947 \u0938\u0930\u094d\u0935\u093e\u0928\u0928\u094d\u0926\u092e\u092f\u0947 \u092e\u0919\u094d\u0917\u0932\u092e\u092f\u0947 (\u0938\u0915\u0932)"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0905 \u0915 \u091a \u091f \u0924 \u092a \u092f\u0930 \u0932\u0935 \u0938\u093e\u0926\u093f \u0915\u094d\u0937\u093e\u0928\u094d\u0924 \u0905\u0915\u094d\u0937\u0930\u092e\u092f\u093f \u0935\u093e\u0919\u094d\u0917\u094d\u092e\u092f\u093f \u091a\u093f\u0928\u094d\u0924\u092f\u093e\u092e\u093f <br/> \u0936\u0941\u0915 \u0928\u093e\u0930\u0926 \u0915\u0941\u092e\u094d\u092d\u091c \u092e\u0941\u0928\u093f\u0935\u0930 \u0938\u094d\u0924\u0941\u0924\u093f \u0917\u093e\u092f\u0915 \u091c\u0928 \u0938\u0928\u094d\u0928\u0941\u0924\u0947 (\u0938\u0915\u0932)"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0928\u093e\u0917\u0928\u093e\u092f\u0915 \u0938\u0926\u0924\u094d-\u0926\u0936\u093e\u0930 \u092b\u0923 \u0932\u094b\u0915 \u0935\u0939\u093f\u0924 \u0927\u0930 \u0915\u0930 \u0935\u0932\u092f\u0947 <br/> \u0932\u094b\u0915 \u0932\u094b\u0915 \u0938\u092e\u094d\u092e\u094b\u0939\u093f\u0924 \u0939\u093f\u0924\u0915\u0930 \u092c\u0941\u0926\u094d\u0927\u093f \u0938\u093f\u0926\u094d\u0927\u093f \u0928\u0924 \u092a\u0941\u0930\u0928\u093f\u0932\u092f\u0947 (\u0938\u0915\u0932)"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u092d\u0935\u0930\u094b\u0917 \u0939\u0930 \u0935\u0948\u092d\u0935\u0947 \u092a\u0930\u092e \u0915\u0932\u094d\u092f\u093e\u0923\u0917\u0941\u0923 \u0928\u093f\u0915\u0930\u0947 <br/> \u0928\u0935 \u0930\u0938\u093e\u0932\u0919\u094d\u0915\u093e\u0930 \u0915\u093e\u0935\u094d\u092f \u0928\u093e\u091f\u0915 \u0935\u0930\u094d\u0923\u093f\u0924\u0947 \u0936\u0941\u092d\u0915\u0930\u0947 <br/> \u0915\u0941\u0935\u0932\u092f\u0926\u0932 \u0928\u0935 \u0928\u0940\u0932 \u0936\u0930\u0940\u0930 \u0917\u094b\u0935\u093f\u0928\u094d\u0926 \u0938\u094b\u0926\u0930\u0947 \u0936\u094d\u0930\u0940\u0915\u0930\u0947 <br/> \u0936\u093f\u0935 \u0939\u094d\u0930\u094d\u0926\u092f \u0915\u092e\u0932 \u0928\u093f\u0932\u092f\u0947 \u0924\u094d\u0930\u093f\u092a\u0941\u0930\u093e\u0938\u093f\u0926\u094d\u0927\u0940\u0936\u094d\u0935\u0930\u093f \u0928\u0924 \u0936\u094d\u0930\u0940 \u0928\u0917\u0930\u0947"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0905\u0935\u0928\u0924 \u0930\u0939\u0938\u094d\u092f \u092f\u094b\u0917\u093f\u0928\u093f \u0915\u0942\u0932\u0947 \u0936\u0924 \u0926\u093f\u0928\u0915\u0930 \u0938\u092e\u0926\u094d\u092f\u0924\u093f \u092e\u0941\u0916\u091c\u093e\u0932\u0947 <br/> \u092d\u0941\u0935\u0928 \u092a\u094d\u0930\u0938\u093f\u0926\u094d\u0927 \u0939\u094d\u0930\u0940\u0919\u094d\u0915\u093e\u0930 \u0915\u093e\u092e\u0947\u0936\u094d\u0935\u0930 \u092d\u0940\u091c \u092e\u0928\u094d\u0926\u0930 \u0932\u094b\u0932\u0947 (\u0938\u0915\u0932)"}]}], "lyricsref": [{"links": [{"N": "www.oocities.org", "L": "http://www.oocities.org/vc_sekaran/Oothukkadu_compositions.html#sakala"}]}], "videos": [{"folder": [{"A": [3094, 3095], "songs": [{"C": [5025, 5026], "D": "8:51", "I": "6akKlCDZ7Kg", "R": [1009, 1009], "S": [10332, 12259], "T": [114, 115], "V": "1,400"}, {"C": [5025, 5026], "D": "6:00", "I": "HRMAGdMR_R4", "R": [1009, 1009], "S": [10332, 12259], "T": [114, 115], "V": "478"}, {"C": [5025, 5026], "D": "8:51", "I": "_nFs2R7Pn50", "R": [1009, 1009], "S": [10332, 12259], "T": [114, 115], "V": "7"}]}, {"A": [3291, 3294], "songs": [{"C": [5025, 5026], "D": "11:28", "I":