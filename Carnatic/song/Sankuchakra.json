u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0b9a\u0ba3\u0bcd\u0b95\u0b9a\u0b95\u0bcd\u0bb0 \u0b95\u0ba4\u0bbe\u0baa\u0bbe\u0ba3\u0bbf\u0bae\u0bcd \u0b85\u0bb9\u0bae\u0bcd \u0bb5\u0ba8\u0bcd\u0ba4\u0bc7 <br/> \u0bb6\u0bcd\u0bb0\u0bc0 \u0b9a\u0bbe\u0bb0\u0bcd\u0b99\u0bcd\u0b95 \u0ba8\u0ba8\u0bcd\u0ba4\u0b95 \u0b95\u0bcc\u0bb8\u0bcd\u0ba4\u0bc1\u0baa \u0ba4\u0bbe\u0bb0\u0bbf\u0ba3\u0bae\u0bcd \u0bb5\u0ba8\u0bcd\u0ba4\u0bc7"}, {"M": "\u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0baa\u0b99\u0bcd\u0b95\u0bc7\u0bb0\u0bc1\u0bb9 \u0b9a\u0bb0\u0ba3\u0bae\u0bcd \u0baa\u0ba4\u0bcd\u0bae\u0bbe\u0bb5\u0ba4\u0bc0 \u0bb0\u0bae\u0ba3\u0bae\u0bcd <br/> \u0b9a\u0b99\u0bcd\u0b95\u0b9f\u0bbe\u0baa \u0bb9\u0bb0\u0ba3\u0bae\u0bcd \u0b9a\u0ba4\u0bbe\u0baa\u0b95\u0bcd\u0ba4 \u0baa\u0bb0\u0ba3\u0bae\u0bcd"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b95\u0bc1\u0b99\u0bcd\u0b95\u0bc1\u0bae\u0baa\u0b99\u0bcd\u0b95\u0bbf\u0bb2\u0b95\u0bbe\u0ba4\u0bcd\u0bb0\u0bae\u0bcd \u0baa\u0b99\u0bcd\u0b95\u0b9c\u0bb5\u0bbf\u0b9a\u0bbe\u0bb2 \u0ba8\u0bc7\u0ba4\u0bcd\u0bb0\u0bae\u0bcd <br/> \u0b9a\u0b99\u0bcd\u0b95\u0bb0 \u0b9a\u0ba9\u0bcd\u0ba9\u0bc1\u0ba4\u0bbf \u0baa\u0bbe\u0ba4\u0bcd\u0bb0\u0bae\u0bcd \u0bb5\u0bc7\u0b99\u0bcd\u0b95\u0b9f\u0bb5\u0bb0\u0ba4\u0b95\u0bcd\u0bb7\u0bc7\u0ba4\u0bcd\u0bb0\u0bae\u0bcd"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0b95\u0b9c\u0bc7\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 \u0b9a\u0bae\u0bcd\u0bb0\u0b95\u0bcd\u0bb7\u0ba3\u0bae\u0bcd <br/> \u0b95\u0bb0\u0bc1\u0b9f\u0bb5\u0bbe\u0bb9 \u0bb5\u0bbf\u0b9a\u0b95\u0bcd\u0bb7\u0ba3\u0bae\u0bcd <br/> \u0b85\u0b9c\u0bc7\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0\u0bbe\u0ba4\u0bbf \u0bb5\u0bbf\u0bb2\u0b95\u0bcd\u0bb7\u0ba3\u0bae\u0bcd <br/> \u0b85\u0bae\u0bcd\u0bb0\u0bc1\u0ba4\u0b9a\u0bbe\u0bb0 \u0baa\u0b95\u0bcd\u0bb7\u0ba3\u0bae\u0bcd <br/> \u0b85\u0b9c\u0bbe\u0bae\u0bbf\u0bb3 \u0baa\u0bcd\u0bb0\u0baa\u0bcd\u0bb0\u0bc1\u0ba4\u0bcd\u0baf\u0bcd \u0b85\u0ba9\u0bc7\u0b95\u0ba4\u0bbe\u0b9a\u0bbe\u0bb3\u0bbf \u0b9a\u0ba8\u0bcd\u0ba4\u0bb0\u0ba3\u0bae\u0bcd <br/> \u0baa\u0b9c\u0bc7 \u0baa\u0bc2\u0bb0\u0bcd\u0ba3 \u0b9a\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0\u0bbf\u0b95\u0bbe\u0b99\u0bcd\u0b95 <br/> \u0b95\u0bc1\u0bb0\u0bc1 \u0b95\u0bc1\u0bb9\u0bbe\u0ba8\u0bcd\u0ba4\u0bb9\u0bcd\u0b95\u0bb0\u0ba3\u0bae\u0bcd"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b95\u0bc1\u0b9c\u0bbe\u0ba4\u0bbf \u0b95\u0bcd\u0bb0\u0bb9 \u0bb5\u0bbf\u0bb9\u0bbf\u0ba4\u0bae\u0bcd \u0bb0\u0b9c\u0bbe \u0ba8\u0ba4\u0bc0 \u0ba4\u0b9f \u0bb8\u0bcd\u0ba4\u0bbf\u0ba4\u0bae\u0bcd <br/> \u0b9a\u0b9c\u0bbe\u0ba4\u0bc0\u0baf\u0bbe\u0ba4\u0bbf \u0bb0\u0bb9\u0bbf\u0ba4\u0bae\u0bcd \u0ba8\u0bbf\u0b9c\u0bbe\u0ba9\u0ba8\u0bcd\u0ba4 \u0baa\u0bcb\u0ba4\u0bb9\u0bbf\u0ba4\u0bae\u0bcd"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c23\u0c4d\u0c16\u0c1a\u0c15\u0c4d\u0c30 \u0c17\u0c26\u0c3e\u0c2a\u0c3e\u0c23\u0c3f\u0c2e\u0c4d \u0c05\u0c39\u0c2e\u0c4d \u0c35\u0c28\u0c4d\u0c26\u0c47 <br/> \u0c36\u0c4d\u0c30\u0c40 \u0c36\u0c3e\u0c30\u0c4d\u0c02\u0c17 \u0c28\u0c28\u0c4d\u0c26\u0c15 \u0c15\u0c4c\u0c38\u0c4d\u0c24\u0c41\u0c2d \u0c27\u0c3e\u0c30\u0c3f\u0c23\u0c2e\u0c4d \u0c35\u0c28\u0c4d\u0c26\u0c47"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2a\u0c02\u0c15\u0c47\u0c30\u0c41\u0c39 \u0c1a\u0c30\u0c23\u0c2e\u0c4d \u0c2a\u0c26\u0c4d\u0c2e\u0c3e\u0c35\u0c24\u0c40 \u0c30\u0c2e\u0c23\u0c2e\u0c4d <br/> \u0c38\u0c02\u0c15\u0c1f\u0c3e\u0c2a \u0c39\u0c30\u0c23\u0c2e\u0c4d \u0c38\u0c26\u0c3e\u0c2d\u0c15\u0c4d\u0c24 \u0c2d\u0c30\u0c23\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c15\u0c41\u0c02\u0c15\u0c41\u0c2e\u0c2a\u0c02\u0c15\u0c3f\u0c32\u0c17\u0c3e\u0c24\u0c4d\u0c30\u0c2e\u0c4d \u0c2a\u0c02\u0c15\u0c1c\u0c35\u0c3f\u0c36\u0c3e\u0c32 \u0c28\u0c47\u0c24\u0c4d\u0c30\u0c2e\u0c4d <br/> \u0c36\u0c02\u0c15\u0c30 \u0c38\u0c28\u0c4d\u0c28\u0c41\u0c24\u0c3f \u0c2a\u0c3e\u0c24\u0c4d\u0c30\u0c2e\u0c4d \u0c35\u0c47\u0c02\u0c15\u0c1f\u0c35\u0c30\u0c26\u0c15\u0c4d\u0c37\u0c47\u0c24\u0c4d\u0c30\u0c2e\u0c4d"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c17\u0c1c\u0c47\u0c28\u0c4d\u0c26\u0c4d\u0c30 \u0c38\u0c2e\u0c4d\u0c30\u0c15\u0c4d\u0c37\u0c23\u0c2e\u0c4d <br/> \u0c17\u0c30\u0c41\u0c21\u0c35\u0c3e\u0c39 \u0c35\u0c3f\u0c1a\u0c15\u0c4d\u0c37\u0c23\u0c2e\u0c4d <br/> \u0c05\u0c1c\u0c47\u0c28\u0c4d\u0c26\u0c4d\u0c30\u0c3e\u0c26\u0c3f \u0c35\u0c3f\u0c32\u0c15\u0c4d\u0c37\u0c23\u0c2e\u0c4d <br/> \u0c05\u0c2e\u0c43\u0c24\u0c38\u0c3e\u0c30 \u0c2d\u0c15\u0c4d\u0c37\u0c23\u0c2e\u0c4d <br/> \u0c05\u0c1c\u0c3e\u0c2e\u0c3f\u0c33 \u0c2a\u0c4d\u0c30\u0c2d\u0c43\u0c24\u0c4d\u0c2f\u0c4d \u0c05\u0c28\u0c47\u0c15\u0c26\u0c3e\u0c38\u0c3e\u0c33\u0c3f \u0c38\u0c28\u0c4d\u0c24\u0c30\u0c23\u0c2e\u0c4d <br/> \u0c2d\u0c1c\u0c47 \u0c2a\u0c42\u0c30\u0c4d\u0c23 \u0c1a\u0c28\u0c4d\u0c26\u0c4d\u0c30\u0c3f\u0c15\u0c3e\u0c02\u0c17 <br/> \u0c17\u0c41\u0c30\u0c41 \u0c17\u0c41\u0c39\u0c3e\u0c28\u0c4d\u0c24\u0c39\u0c4d\u0c15\u0c30\u0c23\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c15\u0c41\u0c1c\u0c3e\u0c26\u0c3f \u0c17\u0c4d\u0c30\u0c39 \u0c35\u0c3f\u0c39\u0c3f\u0c24\u0c2e\u0c4d \u0c30\u0c1c\u0c3e \u0c28\u0c26\u0c40 \u0c24\u0c1f \u0c38\u0c4d\u0c25\u0c3f\u0c24\u0c2e\u0c4d <br/> \u0c38\u0c1c\u0c3e\u0c24\u0c40\u0c2f\u0c3e\u0c26\u0c3f \u0c30\u0c39\u0c3f\u0c24\u0c2e\u0c4d \u0c28\u0c3f\u0c1c\u0c3e\u0c28\u0c28\u0c4d\u0c26 \u0c2c\u0c4b\u0c27\u0c39\u0c3f\u0c24\u0c2e\u0c4d"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ca3\u0ccd\u0c96\u0c9a\u0c95\u0ccd\u0cb0 \u0c97\u0ca6\u0cbe\u0caa\u0cbe\u0ca3\u0cbf\u0cae\u0ccd \u0c85\u0cb9\u0cae\u0ccd \u0cb5\u0ca8\u0ccd\u0ca6\u0cc7 <br/> \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb6\u0cbe\u0cb0\u0ccd\u0c82\u0c97 \u0ca8\u0ca8\u0ccd\u0ca6\u0c95 \u0c95\u0ccc\u0cb8\u0ccd\u0ca4\u0cc1\u0cad \u0ca7\u0cbe\u0cb0\u0cbf\u0ca3\u0cae\u0ccd \u0cb5\u0ca8\u0ccd\u0ca6\u0cc7"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0caa\u0c82\u0c95\u0cc7\u0cb0\u0cc1\u0cb9 \u0c9a\u0cb0\u0ca3\u0cae\u0ccd \u0caa\u0ca6\u0ccd\u0cae\u0cbe\u0cb5\u0ca4\u0cc0 \u0cb0\u0cae\u0ca3\u0cae\u0ccd <br/> \u0cb8\u0c82\u0c95\u0c9f\u0cbe\u0caa \u0cb9\u0cb0\u0ca3\u0cae\u0ccd \u0cb8\u0ca6\u0cbe\u0cad\u0c95\u0ccd\u0ca4 \u0cad\u0cb0\u0ca3\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c95\u0cc1\u0c82\u0c95\u0cc1\u0cae\u0caa\u0c82\u0c95\u0cbf\u0cb2\u0c97\u0cbe\u0ca4\u0ccd\u0cb0\u0cae\u0ccd \u0caa\u0c82\u0c95\u0c9c\u0cb5\u0cbf\u0cb6\u0cbe\u0cb2 \u0ca8\u0cc7\u0ca4\u0ccd\u0cb0\u0cae\u0ccd <br/> \u0cb6\u0c82\u0c95\u0cb0 \u0cb8\u0ca8\u0ccd\u0ca8\u0cc1\u0ca4\u0cbf \u0caa\u0cbe\u0ca4\u0ccd\u0cb0\u0cae\u0ccd \u0cb5\u0cc7\u0c82\u0c95\u0c9f\u0cb5\u0cb0\u0ca6\u0c95\u0ccd\u0cb7\u0cc7\u0ca4\u0ccd\u0cb0\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0c97\u0c9c\u0cc7\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0cb8\u0cae\u0ccd\u0cb0\u0c95\u0ccd\u0cb7\u0ca3\u0cae\u0ccd <br/> \u0c97\u0cb0\u0cc1\u0ca1\u0cb5\u0cbe\u0cb9 \u0cb5\u0cbf\u0c9a\u0c95\u0ccd\u0cb7\u0ca3\u0cae\u0ccd <br/> \u0c85\u0c9c\u0cc7\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0\u0cbe\u0ca6\u0cbf \u0cb5\u0cbf\u0cb2\u0c95\u0ccd\u0cb7\u0ca3\u0cae\u0ccd <br/> \u0c85\u0cae\u0cc3\u0ca4\u0cb8\u0cbe\u0cb0 \u0cad\u0c95\u0ccd\u0cb7\u0ca3\u0cae\u0ccd <br/> \u0c85\u0c9c\u0cbe\u0cae\u0cbf\u0cb3 \u0caa\u0ccd\u0cb0\u0cad\u0cc3\u0ca4\u0ccd\u0caf\u0ccd \u0c85\u0ca8\u0cc7\u0c95\u0ca6\u0cbe\u0cb8\u0cbe\u0cb3\u0cbf \u0cb8\u0ca8\u0ccd\u0ca4\u0cb0\u0ca3\u0cae\u0ccd <br/> \u0cad\u0c9c\u0cc7 \u0caa\u0cc2\u0cb0\u0ccd\u0ca3 \u0c9a\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0\u0cbf\u0c95\u0cbe\u0c82\u0c97 <br/> \u0c97\u0cc1\u0cb0\u0cc1 \u0c97\u0cc1\u0cb9\u0cbe\u0ca8\u0ccd\u0ca4\u0cb9\u0ccd\u0c95\u0cb0\u0ca3\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c95\u0cc1\u0c9c\u0cbe\u0ca6\u0cbf \u0c97\u0ccd\u0cb0\u0cb9 \u0cb5\u0cbf\u0cb9\u0cbf\u0ca4\u0cae\u0ccd \u0cb0\u0c9c\u0cbe \u0ca8\u0ca6\u0cc0 \u0ca4\u0c9f \u0cb8\u0ccd\u0ca5\u0cbf\u0ca4\u0cae\u0ccd <br/> \u0cb8\u0c9c\u0cbe\u0ca4\u0cc0\u0caf\u0cbe\u0ca6\u0cbf \u0cb0\u0cb9\u0cbf\u0ca4\u0cae\u0ccd \u0ca8\u0cbf\u0c9c\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6 \u0cac\u0ccb\u0ca7\u0cb9\u0cbf\u0ca4\u0cae\u0ccd"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d23\u0d4d\u0d16\u0d1a\u0d15\u0d4d\u0d30 \u0d17\u0d26\u0d3e\u0d2a\u0d3e\u0d23\u0d3f\u0d2e\u0d4d \u0d05\u0d39\u0d2e\u0d4d \u0d35\u0d28\u0d4d\u0d26\u0d47 <br/> \u0d36\u0d4d\u0d30\u0d40 \u0d36\u0d3e\u0d30\u0d4d\u0d02\u0d17 \u0d28\u0d28\u0d4d\u0d26\u0d15 \u0d15\u0d4c\u0d38\u0d4d\u0d24\u0d41\u0d2d \u0d27\u0d3e\u0d30\u0d3f\u0d23\u0d2e\u0d4d \u0d35\u0d28\u0d4d\u0d26\u0d47"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2a\u0d02\u0d15\u0d47\u0d30\u0d41\u0d39 \u0d1a\u0d30\u0d23\u0d2e\u0d4d \u0d2a\u0d26\u0d4d\u0d2e\u0d3e\u0d35\u0d24\u0d40 \u0d30\u0d2e\u0d23\u0d2e\u0d4d <br/> \u0d38\u0d02\u0d15\u0d1f\u0d3e\u0d2a \u0d39\u0d30\u0d23\u0d2e\u0d4d \u0d38\u0d26\u0d3e\u0d2d\u0d15\u0d4d\u0d24 \u0d2d\u0d30\u0d23\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d15\u0d41\u0d02\u0d15\u0d41\u0d2e\u0d2a\u0d02\u0d15\u0d3f\u0d32\u0d17\u0d3e\u0d24\u0d4d\u0d30\u0d2e\u0d4d \u0d2a\u0d02\u0d15\u0d1c\u0d35\u0d3f\u0d36\u0d3e\u0d32 \u0d28\u0d47\u0d24\u0d4d\u0d30\u0d2e\u0d4d <br/> \u0d36\u0d02\u0d15\u0d30 \u0d38\u0d28\u0d4d\u0d28\u0d41\u0d24\u0d3f \u0d2a\u0d3e\u0d24\u0d4d\u0d30\u0d2e\u0d4d \u0d35\u0d47\u0d02\u0d15\u0d1f\u0d35\u0d30\u0d26\u0d15\u0d4d\u0d37\u0d47\u0d24\u0d4d\u0d30\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d17\u0d1c\u0d47\u0d28\u0d4d\u0d26\u0d4d\u0d30 \u0d38\u0d2e\u0d4d\u0d30\u0d15\u0d4d\u0d37\u0d23\u0d2e\u0d4d <br/> \u0d17\u0d30\u0d41\u0d21\u0d35\u0d3e\u0d39 \u0d35\u0d3f\u0d1a\u0d15\u0d4d\u0d37\u0d23\u0d2e\u0d4d <br/> \u0d05\u0d1c\u0d47\u0d28\u0d4d\u0d26\u0d4d\u0d30\u0d3e\u0d26\u0d3f \u0d35\u0d3f\u0d32\u0d15\u0d4d\u0d37\u0d23\u0d2e\u0d4d <br/> \u0d05\u0d2e\u0d43\u0d24\u0d38\u0d3e\u0d30 \u0d2d\u0d15\u0d4d\u0d37\u0d23\u0d2e\u0d4d <br/> \u0d05\u0d1c\u0d3e\u0d2e\u0d3f\u0d33 \u0d2a\u0d4d\u0d30\u0d2d\u0d43\u0d24\u0d4d\u0d2f\u0d4d \u0d05\u0d28\u0d47\u0d15\u0d26\u0d3e\u0d38\u0d3e\u0d33\u0d3f \u0d38\u0d28\u0d4d\u0d24\u0d30\u0d23\u0d2e\u0d4d <br/> \u0d2d\u0d1c\u0d47 \u0d2a\u0d42\u0d30\u0d4d\u0d23 \u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30\u0d3f\u0d15\u0d3e\u0d02\u0d17 <br/> \u0d17\u0d41\u0d30\u0d41 \u0d17\u0d41\u0d39\u0d3e\u0d28\u0d4d\u0d24\u0d39\u0d4d\u0d15\u0d30\u0d23\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d15\u0d41\u0d1c\u0d3e\u0d26\u0d3f \u0d17\u0d4d\u0d30\u0d39 \u0d35\u0d3f\u0d39\u0d3f\u0d24\u0d2e\u0d4d \u0d30\u0d1c\u0d3e \u0d28\u0d26\u0d40 \u0d24\u0d1f \u0d38\u0d4d\u0d25\u0d3f\u0d24\u0d2e\u0d4d <br/> \u0d38\u0d1c\u0d3e\u0d24\u0d40\u0d2f\u0d3e\u0d26\u0d3f \u0d30\u0d39\u0d3f\u0d24\u0d2e\u0d4d \u0d28\u0d3f\u0d1c\u0d3e\u0d28\u0d28\u0d4d\u0d26 \u0d2c\u0d4b\u0d27\u0d39\u0d3f\u0d24\u0d2e\u0d4d"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u0923\u094d\u0916\u091a\u0915\u094d\u0930 \u0917\u0926\u093e\u092a\u093e\u0923\u093f\u092e\u094d \u0905\u0939\u092e\u094d \u0935\u0928\u094d\u0926\u0947 <br/> \u0936\u094d\u0930\u0940 \u0936\u093e\u0930\u094d\u0902\u0917 \u0928\u0928\u094d\u0926\u0915 \u0915\u094c\u0938\u094d\u0924\u0941\u092d \u0927\u093e\u0930\u093f\u0923\u092e\u094d \u0935\u0928\u094d\u0926\u0947"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092a\u0902\u0915\u0947\u0930\u0941\u0939 \u091a\u0930\u0923\u092e\u094d \u092a\u0926\u094d\u092e\u093e\u0935\u0924\u0940 \u0930\u092e\u0923\u092e\u094d <br/> \u0938\u0902\u0915\u091f\u093e\u092a \u0939\u0930\u0923\u092e\u094d \u0938\u0926\u093e\u092d\u0915\u094d\u0924 \u092d\u0930\u0923\u092e\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0915\u0941\u0902\u0915\u0941\u092e\u092a\u0902\u0915\u093f\u0932\u0917\u093e\u0924\u094d\u0930\u092e\u094d \u092a\u0902\u0915\u091c\u0935\u093f\u0936\u093e\u0932 \u0928\u0947\u0924\u094d\u0930\u092e\u094d <br/> \u0936\u0902\u0915\u0930 \u0938\u0928\u094d\u0928\u0941\u0924\u093f \u092a\u093e\u0924\u094d\u0930\u092e\u094d \u0935\u0947\u0902\u0915\u091f\u0935\u0930\u0926\u0915\u094d\u0937\u0947\u0924\u094d\u0930\u092e\u094d"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0917\u091c\u0947\u0928\u094d\u0926\u094d\u0930 \u0938\u092e\u094d\u0930\u0915\u094d\u0937\u0923\u092e\u094d <br/> \u0917\u0930\u0941\u0921\u0935\u093e\u0939 \u0935\u093f\u091a\u0915\u094d\u0937\u0923\u092e\u094d <br/> \u0905\u091c\u0947\u0928\u094d\u0926\u094d\u0930\u093e\u0926\u093f \u0935\u093f\u0932\u0915\u094d\u0937\u0923\u092e\u094d <br/> \u0905\u092e\u0943\u0924\u0938\u093e\u0930 \u092d\u0915\u094d\u0937\u0923\u092e\u094d <br/> \u0905\u091c\u093e\u092e\u093f\u0932 \u092a\u094d\u0930\u092d\u0943\u0924\u094d\u092f\u094d \u0905\u0928\u0947\u0915\u0926\u093e\u0938\u093e\u0932\u093f \u0938\u0928\u094d\u0924\u0930\u0923\u092e\u094d <br/> \u092d\u091c\u0947 \u092a\u0942\u0930\u094d\u0923 \u091a\u0928\u094d\u0926\u094d\u0930\u093f\u0915\u093e\u0902\u0917 <br/> \u0917\u0941\u0930\u0941 \u0917\u0941\u0939\u093e\u0928\u094d\u0924\u0939\u094d\u0915\u0930\u0923\u092e\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0915\u0941\u091c\u093e\u0926\u093f \u0917\u094d\u0930\u0939 \u0935\u093f\u0939\u093f\u0924\u092e\u094d \u0930\u091c\u093e \u0928\u0926\u0940 \u0924\u091f \u0938\u094d\u0925\u093f\u0924\u092e\u094d <br/> \u0938\u091c\u093e\u0924\u0940\u092f\u093e\u0926\u093f \u0930\u0939\u093f\u0924\u092e\u094d \u0928\u093f\u091c\u093e\u0928\u0928\u094d\u0926 \u092c\u094b\u0927\u0939\u093f\u0924\u092e\u094d"}]}], "L": "l", "lyricsref": [{"links": [{"N": "www.medieval.org", "L": "http://www.medieval.org/music/world/carnatic/lyrics/TKG/shankhacakra_gadhapanim.html"}, {"N": "guru-guha.blogspot.com", "L": "http://guru-guha.blogspot.com/2008/06/dikshitar-kriti-shankha-chakra-gada.html"}]}]}