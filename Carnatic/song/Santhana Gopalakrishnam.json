d\u0b9c\u0bc1\u0ba9 \u0baa\u0bcd\u0bb0\u0bc7\u0bae\u0bbe\u0bb8\u0bcd\u0baa\u0ba4\u0bae\u0bcd \u0b86\u0bb6\u0bcd\u0bb0\u0bbf\u0ba4 \u0b9c\u0ba9 \u0baa\u0bb2\u0ba4\u0bae\u0bcd"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0ba8\u0bb0\u0bcd\u0ba4\u0bcd\u0ba4\u0ba9 \u0bae\u0bc1\u0bb0\u0bb3\u0bc0\u0ba4\u0bb0\u0bae\u0bcd \u0ba8\u0ba4 \u0b9a\u0bc1\u0b95 \u0b9a\u0ba9\u0b95 \u0ba8\u0bbe\u0bb0\u0ba4\u0bae\u0bcd <br/> \u0ba8\u0bc0\u0bb0\u0b9c\u0bbe\u0b9a\u0ba9\u0bbe\u0ba4\u0bbf \u0ba8\u0bc1\u0ba4\u0bae\u0bcd \u0ba8\u0bc0\u0bb2 \u0bae\u0bc7\u0b95 \u0b9c\u0bbf\u0ba4 \u0b95\u0bbe\u0ba4\u0bcd\u0bb0\u0bae\u0bcd"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c28\u0c4d\u0c24\u0c3e\u0c28 \u0c17\u0c4b\u0c2a\u0c3e\u0c32\u0c15\u0c43\u0c37\u0c4d\u0c23\u0c2e\u0c4d \u0c09\u0c2a\u0c3e\u0c38\u0c4d\u0c2e\u0c39\u0c47 \u0c36\u0c4d\u0c30\u0c40"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c28\u0c4d\u0c24\u0c3e\u0c28 \u0c38\u0c4c\u0c2d\u0c3e\u0c17\u0c4d\u0c2f \u0c35\u0c3f\u0c24\u0c30\u0c23 \u0c28\u0c3f\u0c2a\u0c41\u0c23 \u0c24\u0c30\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c38\u0c28\u0c4d\u0c24\u0c24\u0c2e\u0c4d \u0c38\u0c26\u0c4d\u0c17\u0c41\u0c30\u0c41\u0c17\u0c41\u0c39 \u0c38\u0c28\u0c4d\u0c28\u0c41\u0c24\u0c2e\u0c4d \u0c38\u0c3e\u0c30\u0c38\u0c15\u0c30\u0c2e\u0c4d"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c36\u0c4d\u0c30\u0c40 \u0c30\u0c41\u0c15\u0c4d\u0c2e\u0c3f\u0c23\u0c40 \u0c38\u0c24\u0c4d\u0c2f \u0c2d\u0c3e\u0c2e\u0c3e \u0c38\u0c2e\u0c47\u0c24\u0c2e\u0c4d <br/> \u0c05\u0c30\u0c4d\u0c1c\u0c41\u0c28 \u0c2a\u0c4d\u0c30\u0c47\u0c2e\u0c3e\u0c38\u0c4d\u0c2a\u0c26\u0c2e\u0c4d \u0c06\u0c36\u0c4d\u0c30\u0c3f\u0c24 \u0c1c\u0c28 \u0c2b\u0c32\u0c26\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f