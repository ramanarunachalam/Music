u0bc6\u0ba9\u0ba4\u0bca\u0bb0\u0bc1 \u0ba4\u0bc1\u0ba3\u0bc8\u0baf\u0bc1\u0ba9\u0bcd \u0baa\u0ba4\u0bae\u0bc7 \u0ba4\u0bbf\u0bb0\u0bc1\u0bae\u0bbe\u0bb2\u0bcd \u0bae\u0bb0\u0bc1\u0b95\u0ba9\u0bc7 \u0b9a\u0bbf\u0bb5 \u0b95\u0bc1\u0bb0\u0bc1\u0baa\u0bb0"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0ba4\u0bbe\u0baa\u0bae\u0bcd \u0b85\u0b95\u0bb2 \u0ba8\u0bc0 \u0b9a\u0bb0\u0bbf\u0baf\u0bbe\u0baf\u0bcd \u0bae\u0bbe\u0bb0\u0bcd\u0b95\u0bae\u0ba4\u0bb3\u0bc0"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0ba4\u0ba9\u0bbf\u0b9a\u0bae\u0baf \u0ba8\u0bc0 \u0b8e\u0ba9\u0b95\u0bcd\u0b95\u0bb0\u0bc1\u0bb3\u0bcd \u0baa\u0bc1\u0bb0\u0bbf \u0b95\u0bbe\u0bb0\u0bcd\u0ba4\u0bbf\u0b95\u0bc7\u0baf \u0bae\u0bb9\u0ba9\u0bc0\u0baf \u0bb5\u0ba9\u0bbf\u0ba4\u0bbe \u0b9a\u0bb9\u0bbe\u0baf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0baa\u0bbe\u0bb0\u0bcd\u0bb5\u0ba4\u0bbf \u0ba4\u0ba9\u0baf\u0bbe \u0baa\u0bbe\u0ba4\u0bae\u0bcd \u0ba8\u0bc0 \u0ba4\u0bbe \u0baa\u0bbe\u0bb0\u0bbf\u0ba9\u0bbf\u0bb2\u0bc7 \u0ba4\u0bc1\u0ba3\u0bc8 \u0baf\u0bbe\u0bb0\u0bcd \u0b8e\u0ba9\u0b95\u0bcd\u0b95\u0bbf\u0ba9\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0bae\u0bbe\u0bae\u0ba9\u0bcd \u0bae\u0bbe\u0ba4\u0bb5 \u0bae\u0bb0\u0bc1\u0b95\u0bbe \u0b86\u0bb0\u0bc1\u0bae\u0bc1\u0b95\u0bbe \u0bae\u0bc1\u0bb0\u0bc1\u0b95\u0bbe \u0b95\u0bc1\u0bb9\u0bbe \u0b9a\u0bc1\u0b95\u0bae\u0bcd \u0baa\u0bc6\u0bb0\u0bc1\u0b95 <br/> \u0bae\u0ba9\u0bae\u0b95\u0bbf\u0bb4\u0bcd\u0ba8\u0bcd\u0ba4\u0bca\u0bb0\u0bc1 \u0bb5\u0bb0\u0bae\u0bcd \u0ba4\u0bbe \u0ba8\u0bc0 \u0b9a\u0bb0\u0bbf \u0b9a\u0bae\u0bbe\u0ba9\u0bae\u0bb0\u0bbf\u0baf\u0bc7\u0ba9\u0bcd \u0b85\u0b9c\u0bbf\u0ba4 \u0bae\u0bbe\u0bb9\u0bbe \u0bae\u0ba4\u0ba9"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0ba4\u0bbe\u0bae\u0ba4 \u0bae\u0bbf\u0b95\u0bae\u0bbe\u0ba4\u0bc6\u0ba9\u0b95\u0bcd\u0b95\u0bc1 \u0b9a\u0bc1\u0b95\u0bae\u0bbe \u0ba4\u0ba9\u0bbf \u0bb5\u0bbf\u0bb0\u0bb9\u0bae\u0bbe \u0bae\u0bbf\u0ba4\u0bc1 \u0b9a\u0bae\u0bcd\u0bae\u0ba4\u0bae\u0bbe \u0b87\u0ba4\u0bc1\u0bb5\u0bc1\u0bae\u0bcd <br/> \u0ba4\u0b95\u0bc1\u0bae\u0bbe \u0ba4\u0bb0\u0ba3\u0bbf \u0ba4\u0ba9\u0bbf\u0bb2\u0bcd \u0ba8\u0bbe\u0ba9\u0bcd \u0b89\u0ba9\u0bc8 \u0b85\u0b9f\u0bc8\u0baf \u0bae\u0bbe \u0b95\u0bb0\u0bc1\u0ba3\u0bc8 \u0ba8\u0bbf\u0ba4 \u0ba8\u0bbf\u0ba4 \u0bae\u0bbe \u0b95\u0ba9\u0bae\u0bc6\u0ba9\u0bcd\u0bb1\u0ba3\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 6", "T": "\u0baa\u0bbe\u0ba4\u0b95\u0bae\u0bbe\u0bae\u0bcd \u0b89\u0ba9\u0bc8 \u0bae\u0bb1\u0ba8\u0bcd\u0ba4\u0bbe\u0bb2\u0bc6\u0ba9\u0bcd\u0ba9 \u0b9a\u0bbe\u0bae\u0bbf \u0ba8\u0bc0 \u0b89\u0bb0\u0ba4\u0bcd\u0ba4\u0ba4\u0bc8 \u0ba8\u0bbe\u0ba9\u0bcb \u0b9a\u0ba4\u0bbe <br/> \u0ba8\u0bc0\u0ba4\u0bae\u0bbe\u0b95 \u0bae\u0ba9\u0ba4\u0bbe\u0bb0\u0bb5\u0bc7 \u0b87\u0bb0\u0bc1\u0ba4\u0bcd\u0ba4\u0bbf \u0b9a\u0ba9\u0bcd\u0ba9\u0bbf\u0ba4\u0bbf \u0ba4\u0ba9\u0bbf\u0bb2\u0bcd \u0bae\u0bc1\u0bb0\u0bc1\u0b95\u0bbe \u0b85\u0ba4\u0bbf <br/> \u0bb5\u0bbe\u0b95\u0bae\u0bbe\u0b95 \u0bae\u0ba9\u0bae\u0b95\u0bbf\u0bb4\u0bcd\u0ba8\u0bcd\u0ba4\u0bcd-\u0b89\u0ba9\u0ba4\u0bb0\u0bc1\u0ba3\u0bcd-\u0bae\u0bbe\u0bb0\u0bcd\u0b95\u0bae\u0bcd \u0b85\u0ba4\u0bbe\u0b95\u0bbf\u0ba9\u0bc7 \u0ba8\u0bbf\u0ba9\u0bcd \u0ba4\u0bb0\u0bbf\u0b9a\u0ba9\u0bae\u0bcd \u0ba4\u0bbe <br/> \u0bb5\u0bb0\u0ba4\u0bbe \u0ba4\u0ba9\u0bbf\u0ba4\u0bcd\u0ba4\u0bb0\u0bc1\u0bb3\u0bc1\u0bae\u0bcd \u0b9a\u0bbe\u0bae\u0bbf \u0bb5\u0bbe\u0ba4\u0baa\u0bc1\u0bb0\u0bbf \u0b95\u0bbe\u0b99\u0bcd\u0b95\u0bc7\u0baf \u0bb5\u0bbf\u0bb6\u0bcd\u0bb5\u0ba9\u0bbe\u0ba4 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0baf \u0bb5\u0bb0"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c3e\u0c30\u0c38\u0c26\u0c33 \u0c28\u0c2f\u0c28\u0c47 \u0c2e\u0c41\u0c30\u0c41\u0c17\u0c3e \u0c17\u0c41\u0c39\u0c3e \u0c26\u0c2f\u0c3e\u0c35\u0c39\u0c28\u0c47 \u0c37\u0c3e\u0c23\u0c4d\u0c2e\u0c41\u0c16\u0c28\u0c47"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2a\u0c3e\u0c30\u0c15\u0c2e\u0c4d \u0c07\u0c26\u0c