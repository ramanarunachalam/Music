ba4\u0bb9\u0bc0\u0ba9 \u0bae\u0bcc\u0ba9\u0bbf \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0baf\u0bc8\u0b95\u0baa\u0ba4\u0bc7 \u0b9a\u0bc1\u0bb0 <br/> \u0baa\u0bbe\u0bae\u0bbf\u0ba9\u0bc0 \u0ba8\u0bbf\u0bb5\u0bb9 \u0b95\u0bc0\u0ba4 \u0b95\u0bc1\u0ba3\u0bc7 \u0b9a\u0b9a\u0bbf\u0ba4\u0bbe\u0bae \u0bae\u0ba8\u0bcd\u0ba4\u0bb9\u0b9a\u0bbf\u0ba4\u0bc7 \u0b9c\u0baf \u0baa\u0b95\u0bb5\u0ba4\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0bae\u0b99\u0bcd\u0b95\u0bb3\u0bbe\u0bb0\u0bb5\u0b9a\u0bbe\u0bb2\u0bbf \u0bb5\u0bc0\u0ba3\u0bbe\u0b9a\u0b99\u0bcd\u0b95\u0bbf\u0b95\u0bb0 \u0bae\u0bcd\u0bb0\u0bcd\u0ba4\u0bc1\u0bb3\u0bbe\u0b99\u0bcd\u0b95\u0bc1\u0bb3\u0bbf \u0ba4\u0bb3\u0b9c\u0bbf\u0ba4 <br/> \u0ba4\u0bc1\u0b99\u0bcd\u0b95 \u0b95\u0bb2\u0bcd\u0baa\u0b95 \u0b95\u0bbf\u0b9a\u0bb2\u0baf \u0ba8\u0bbf\u0bb5\u0bb9\u0bc7 \u0ba4\u0bc1\u0bb0\u0bbf\u0ba4 \u0b9a\u0bc7\u0bb5\u0b95 \u0ba4\u0bbe\u0baa \u0baa\u0bb0\u0bc7 <br/> \u0baa\u0bcd\u0bb0\u0bcd\u0b99\u0bcd\u0b95 \u0b9a\u0b9e\u0bcd\u0b9a\u0baf \u0bb5\u0bb2\u0baf\u0bbf\u0ba4 \u0b9a\u0bc1 \u0bae \u0b9a\u0bc1\u0bb0 \u0baa\u0bc0\u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0ba4\u0bae\u0bcd\u0bae\u0bbf\u0bb2\u0bcd\u0bb2\u0bb0 \u0b9a\u0bbf\u0ba4\u0bae\u0ba4 <br/> \u0baa\u0b99\u0bcd\u0b95 \u0b9a\u0b9c\u0bb2\u0bbe\u0bae\u0bcd\u0baa\u0bcb\u0ba4 \u0b95\u0bc1\u0bb2\u0bc7 \u0ba8\u0bb5 \u0baa\u0b99\u0bcd\u0b95\u0b9c\u0bcb\u0baa\u0bae \u0ba8\u0baf\u0baf \u0baf\u0bc1\u0b95\u0bc7 \u0bb2\u0b9a\u0ba4\u0b99\u0bcd\u0b95 <br/> \u0bb0\u0bbe\u0b95 \u0b95\u0ba9 \u0b9a\u0bbe\u0bb0 \u0bb9\u0bb0\u0bbf\u0ba3 \u0bae\u0ba4 \u0bb9\u0bbe\u0bb0 \u0baa\u0bbe\u0b9a\u0bbf \u0b95\u0bc1\u0b9a\u0b95\u0bc1\u0bae\u0bcd\u0baa\u0baf\u0bc1\u0b95\u0bc7 \u0bb9\u0bb0\u0bbf <br/> \u0baa\u0bc1\u0b99\u0bcd\u0b95\u0bb5\u0bcb\u0baa\u0bae\u0bbf\u0ba4 \u0bae\u0ba4\u0bcd\u0baf\u0ba4\u0bb2\u0bc7 \u0baa\u0bbe\u0baa\u0bcb\u0ba4 \u0bb0\u0bc2\u0baa\u0bbf\u0ba3\u0bbf \u0baa\u0bb0\u0bc7 \u0b9c\u0b95\u0ba4\u0bc0\u0bb6\u0bcd\u0bb5\u0bb0\u0bbf"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0baa\u0bbe\u0bb2\u0b9a\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 \u0bb5\u0ba4\u0bae\u0bcd\u0b9a\u0bbf\u0ba9\u0bbf \u0baa\u0bb0\u0bbf\u0b9c\u0ba9 \u0baa\u0bbe\u0bb2\u0ba9\u0bc8\u0b95\u0baa\u0bb0\u0bc7 \u0b95\u0ba9 \u0baa\u0bbe\u0ba4\u0b95 <br/> \u0b9a\u0bc8\u0bb2 \u0baa\u0bbf\u0ba4\u0bc1\u0bb0\u0bbe\u0baf\u0bbf\u0ba4 \u0b9a\u0bb0\u0bbf\u0ba4\u0bc7 \u0b95\u0bb0\u0bc1\u0ba3\u0bbe\u0bb2\u0bb5\u0bbe\u0bb2\u0bbe\u0baf\u0bbf\u0ba4 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0baf\u0bc7 <br/> \u0bb2\u0bcb\u0bb2 \u0bb0\u0bc1\u0b9a\u0bbf\u0bb0\u0bbe\u0bb3\u0b95 \u0baa\u0bb0\u0bbf\u0bb5\u0bcd\u0bb0\u0bcd\u0ba4 \u0b9a\u0bc1-\u0bb5\u0bbf\u0b9a\u0bbe\u0bb2 \u0ba8\u0bbf\u0b9f\u0bbf\u0bb2\u0bbe\u0baf\u0bbf\u0ba4 \u0bb5\u0ba4\u0ba9\u0bc7 <br/> \u0ba8\u0bbe\u0bb2\u0bae\u0bbf\u0bb9 \u0baa\u0ba3\u0bbf \u0ba8\u0bbe\u0baf\u0b95\u0bcb\u0baa\u0bbf \u0baa\u0ba4\u0bbe\u0bb2\u0baa\u0bbf\u0ba4\u0bc1\u0bae\u0baf\u0bbf \u0bae\u0bb9\u0bbf\u0bae\u0bbe\u0ba9\u0bae\u0bcd \u0ba4\u0bc7 <br/> \u0ba4\u0bc1\u0bb2\u0ba4\u0bbe\u0bae \u0ba8\u0baf\u0bae\u0bae\u0bcb\u0ba4\u0bcd\u0baf\u0ba4 \u0bb5\u0bbf\u0ba4\u0bcd\u0baf\u0bbe \u0b9c\u0bbe\u0bb2\u0bae\u0bbe\u0b9a\u0bc1\u0ba9\u0ba9\u0bc1 \u0baa\u0bb0\u0bbe\u0ba4\u0bbf \u0b9a\u0b95\u0bb2\u0bae\u0bcd <br/> \u0baa\u0bbe\u0bb2 \u0bb2\u0bcb\u0b9a\u0ba9 \u0bae\u0bc1\u0b95\u0bbe\u0b95\u0bbf\u0bb2 \u0bb5\u0ba8\u0bcd\u0ba4\u0bbf\u0ba4 \u0baa\u0ba4\u0bcd\u0bae\u0ba9\u0bbe\u0baa \u0baa\u0ba4\u0b9a\u0bc7\u0bb5\u0b95 \u0ba8\u0bbf\u0bb0\u0ba4\u0bc7"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c30\u0c4b\u0c30\u0c41\u0c39\u0c3e\u0c38\u0c28 \u0c1c\u0c3e\u0c2f\u0c47 \u0c2d\u0c35\u0c24\u0c3f \u0c38\u0c3e\u0c2e\u0c4b\u0c26\u0c2e\u0c4d \u0c05\u0c2e\u0c4d\u0c2c \u0c28\u0c2e\u0c3e\u0c2e\u0c3f"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2a\u0c41\u0c30\u0c28\u0c4d\u0c26\u0c30\u0c3e\u0c26\u0c3f \u0c38\u0c41\u0c30\u0c4b\u0c24\u0c4d\u0c24\u0c2e \u0c38\u0c41\u0c30\u0c41\u0c1a\u0c3f\u0c30 \u0c15\u0c3f\u0c30\u0c40\u0c1f \u0c2e\u0c23\u0c3f \u0c15\u0c3f