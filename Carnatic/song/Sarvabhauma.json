u0c35)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0cbe\u0cb0\u0ccd\u0cb5\u0cad\u0ccc\u0cae \u0cb8\u0cbe\u0c95\u0cc7\u0ca4 \u0cb0\u0cbe\u0cae <br/> \u0cae\u0ca8\u0cb8\u0cbe\u0cb0 \u0caa\u0cb2\u0ccd\u0c95 \u0cb0\u0cbe\u0ca6\u0cbe \u0ca6\u0cc7\u0cb5\u0ca4\u0cbe (\u0cb8\u0cbe\u0cb0\u0ccd\u0cb5)"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0caa\u0cbe\u0cb0\u0ccd\u0cb5\u0ca4\u0cc0 \u0cb0\u0cae\u0ca3\u0ccd(\u0c86)\u0cb0\u0ccd\u0c9a\u0cbf\u0ca4 \u0caa\u0cbe\u0ca6 \u0cb0\u0cae\u0cbe <br/> \u0caa\u0ca4\u0cbf \u0cb5\u0ca8\u0ccd\u0ca6\u0ccd\u0caf \u0caa\u0cb0\u0cbe\u0ca4\u0ccd\u0caa\u0cb0 \u0ca6\u0cc0\u0ca8 \u0cac\u0ca8\u0ccd\u0ca7\u0ccb (\u0cb8\u0cbe\u0cb0\u0ccd\u0cb5)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cae\u0cc1\u0ca6\u0ccd\u0ca6\u0cc1 \u0cae\u0cc1\u0ca6\u0ccd\u0ca6\u0cc1\u0c97 \u0cae\u0cbe\u0c9f\u0cb2\u0ccd(\u0c86)\u0ca1\u0cbf\u0ca8\u0ca6\u0cbf <br/> \u0cae\u0cc1\u0ca8\u0ccd\u0ca6\u0cc1 \u0cb0\u0cbe\u0c95 \u0cb8\u0ca6\u0cbe \u0cb5\u0cc6\u0ca4 \u0c9c\u0cc6\u0ca8\u0ccd\u0ca6\u0cc1\u0c9a\u0cc1\u0ca8\u0cc1 <br/> \u0c95\u0ca6\u0ccd\u0ca6\u0cc1 \u0c95\u0ca6\u0ccd\u0ca6\u0ccd(\u0c85)\u0ca8\u0cc1\u0c9a\u0cc1 \u0c9a\u0cbf\u0cb0 \u0c95\u0cbe\u0cb2\u0cae\u0cc1\u0ca8\u0cc1 <br/> \u0c95\u0cb0\u0c97\u0cc1\u0c9a\u0ccd(\u0c89)\u0ca3\u0ccd\u0ca1 \u0cb5\u0cb2\u0cc6\u0ca8\u0cbe \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c \u0ca8\u0cc1\u0ca4 (\u0cb8\u0cbe\u0cb0\u0ccd\u0cb5)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d3e\u0d30\u0d4d\u0d35\u0d2d\u0d4c\u0d2e \u0d38\u0d3e\u0d15\u0d47\u0d24 \u0d30\u0d3e\u0d2e <br/> \u0d2e\u0d28\u0d38\u0d3e\u0d30 \u0d2a\u0d32\u0d4d\u0d15 \u0d30\u0d3e\u0d26\u0d3e \u0d26\u0d47\u0d35\u0d24\u0d3e (\u0d38\u0d3e\u0d30\u0d4d\u0d35)"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2a\u0d3e\u0d30\u0d4d\u0d35\u0d24\u0d40 \u0d30\u0d2e\u0d23\u0d4d(\u0d06)\u0d30\u0d4d\u0d1a\u0d3f\u0d24 \u0d2a\u0d3e\u0d26 