f\u0bbe\u0ba9\u0bae\u0bcd- <br/> \u0b85\u0ba9\u0bc1 \u0baf\u0bbe\u0b95\u0bae\u0bc1 \u0bb5\u0bc7\u0bb3 \u0bae\u0ba9\u0b9a\u0bc1\u0ba9 \u0baa\u0bc1\u0b9f\u0bcd\u0b9f\u0bbf\u0ba9 <br/> \u0b95\u0ba9 \u0b9f\u0bae\u0bcd\u0baa\u0bc1\u0ba9\u0bbf \u0ba4\u0bcb\u0b9f\u0ba9\u0bc1 \u0bae\u0bbe\u0bb0\u0bc0\u0b9a\u0bc1\u0ba9\u0bbf <br/> \u0baa\u0ba9\u0bbf \u0b9a\u0bc6\u0bb0\u0b9a\u0bbf\u0ba9\u0baf\u0bbe \u0ba4\u0bcd\u0baf\u0bbe\u0b95\u0bb0\u0bbe\u0b9c\u0bbe\u0bb0\u0bcd\u0b9a\u0bbf\u0ba4"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c36\u0c3f \u0c35\u0c26\u0c28 \u0c2d\u0c15\u0c4d\u0c24 \u0c1c\u0c28\u0c3e\u0c35\u0c28 <br/> \u0c36\u0c02\u0c15\u0c30 \u0c28\u0c47 \u0c24\u0c3e\u0c33 \u0c17\u0c32\u0c28\u0c3e"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2a\u0c38\u0c3f-\u0c24\u0c28\u0c2e\u0c28\u0c4d\u0c26\u0c47 \u0c2e\u0c41\u0c28\u0c3f \u0c2f\u0c3e\u0c17\u0c2e\u0c41\u0c28 \u0c28\u0c40 <br/> \u0c2c\u0c3e\u0c39\u0c41 \u0c2a\u0c30\u0c3e\u0c15\u0c4d\u0c30\u0c2e\u0c2e\u0c41\u0c28\u0c46\u0c30\u0c41\u0c17\u0c28\u0c3e \u0c30\u0c3e\u0c15\u0c3e"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c26\u0c3f\u0c28 \u0c26\u0c3f\u0c28\u0c2e\u0c4c\u0c2a\u0c3e\u0c38\u0c28 \u0c1c\u0c2a \u0c24\u0c2a \u0c27\u0c4d\u0c2f\u0c3e\u0c28\u0c2e\u0c4d- <br/> \u0c05\u0c28\u0c41 \u0c2f\u0c3e\u0c17\u0c2e\u0c41 \u0c35\u0c47\u0c33 \u0c2e\u0c28\u0c38\u0c41\u0c28 \u0c2a\u0c41\u0c1f\u0c4d\u0c1f\u0c3f\u0c28 <br/> \u0c18\u0c28 \u0c21\u0c2e\u0c4d\u0c2c\u0c41\u0c28\u0c3f \u0c24\u0c4b\u0c21\u0c28\u0c41 \u0c2e\u0c3e\u0c30\u0c40\u0c1a\u0c41\u0c28\u0c3f <br/> \u0c2a\u0c28\u0c3f \u0c1a\u0c46\u0c30\u0c1a\u0c3f\u0c28\u0c2f\u0c3e \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c\u0c3e\u0c30\u0c4d\u0c1a\u0c3f\u0c24"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0cb6\u0cbf \u0cb5\u0ca6\u0ca8 \u0cad\u0c95\u0ccd\u0ca4 \u0c9c\u0ca8\u0cbe\u0cb5\u0ca8 <br/> \u0cb6\u0c82\u0c95\u0cb0 \u0ca8\u0cc7 \u0ca4\u0cbe\u0cb3 \u0c97\u0cb2\u0ca8\u0cbe"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0caa\u0cb8\u0cbf-\u0ca4\u0ca8\u0cae\u0ca8\u0ccd\u0ca6\u0cc7 \u0cae\u0cc1\u0ca8\u0cbf \u0caf\u0cbe\u0c97\u0cae\u0cc1\u0ca8 \u0ca8\u0cc0 <br/> \u0cac\u0cbe\u0cb9\u0cc1 \u0caa\u0cb0\u0cbe\u0c95\u0ccd\u0cb0\u0cae\u0cae\u0cc1\u0ca8\u0cc6\u0cb0\u0cc1\u0c97\u0ca8\u0cbe \u0cb0\u0cbe\u0c95\u0cbe"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0ca6\u0cbf\u0ca8 \u0ca6\u0cbf\u0ca8\u0cae\u0ccc\u0caa\u0cbe\u0cb8\u0ca8 \u0c9c\u0caa \u0ca4\u0caa \u0ca7\u0ccd\u0caf\u0cbe\u0ca8\u0cae\u0ccd- <br/> \u0c85\u0ca8\u0cc1 \u0caf\u0cbe\u0c97\u0cae\u0cc1 \u0cb5\u0cc7\u0cb3 \u0cae\u0ca8\u0cb8\u0cc1\u0ca8 \u0caa\u0cc1\u0c9f\u0ccd\u0c9f\u0cbf\u0ca8 <br/> \u0c98\u0ca8 \u0ca1\u0cae\u0ccd\u0cac\u0cc1\u0ca8\u0cbf \u0ca4\u0ccb\u0ca1\u0ca8\u0cc1 \u0cae\u0cbe\u0cb0\u0cc0\u0c9a\u0cc1\u0ca8\u0cbf <br/> \u0caa\u0ca8\u0cbf \u0c9a\u0cc6\u0cb0\u0c9a\u0cbf\u0ca8\u0caf\u0cbe \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c\u0cbe\u0cb0\u0ccd\u0c9a\u0cbf\u0ca4"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d36\u0d3f \u0d35\u0d26\u0d28 \u0d2d\u0d15\u0d4d\u0d24 \u0d1c\u0d28\u0d3e\u0d35\u0d28 <br/> \u0d36\u0d02\u0d15\u0d30 \u0d28\u0d47 \u0d24\u0d3e\u0d33 \u0d17\u0d32\u0d28\u0d3e"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2a\u0d38\u0d3f-\u0d24\u0d28\u0d2e\u0d28\u0d4d\u0d26\u0d47 \u0d2e\u0d41\u0d28\u0d3f \u0d2f\u0d3e\u0d17\u0d2e\u0d41\u0d28 \u0d28\u0d40 <br/> \u0d2c\u0d3e\u0d39\u0d41 \u0d2a\u0d30\u0d3e\u0d15\u0d4d\u0d30\u0d2e\u0d2e\u0d41\u0d28\u0d46\u0d30\u0d41\u0d17\u0d28\u0d3e \u0d30\u0d3e\u0d15\u0d3e"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d26\u0d3f\u0d28 \u0d26\u0d3f\u0d28\u0d2e\u0d4c\u0d2a\u0d3e\u0d38\u0d28 \u0d1c\u0d2a \u0d24\u0d2a \u0d27\u0d4d\u0d2f\u0d3e\u0d28\u0d2e\u0d4d- <br/> \u0d05\u0d28\u0d41 \u0d2f\u0d3e\u0d17\u0d2e\u0d41 \u0d35\u0d47\u0d33 \u0d2e\u0d28\u0d38\u0d41\u0d28 \u0d2a\u0d41\u0d1f\u0d4d\u0d1f\u0d3f\u0d28 <br/> \u0d18\u0d28 \u0d21\u0d2e\u0d4d\u0d2c\u0d41\u0d28\u0d3f \u0d24\u0d4b\u0d21\u0d28\u0d41 \u0d2e\u0d3e\u0d30\u0d40\u0d1a\u0d41\u0d28\u0d3f <br/> \u0d2a\u0d28\u0d3f \u0d1a\u0d46\u0d30\u0d1a\u0d3f\u0d28\u0d2f\u0d3e \u0d24\u0d4d\u0d2f\u0d3e\u0d17\u0d30\u0d3e\u0d1c\u0d3e\u0d30\u0d4d\u0d1a\u0d3f\u0d24"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u0936\u093f \u0935\u0926\u0928 \u092d\u0915\u094d\u0924 \u091c\u0928\u093e\u0935\u0928 <br/> \u0936\u0902\u0915\u0930 \u0928\u0947 \u0924\u093e\u0932 \u0917\u0932\u0928\u093e"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092a\u0938\u093f-\u0924\u0928\u092e\u0928\u094d\u0926\u0947 \u092e\u0941\u0928\u093f \u092f\u093e\u0917\u092e\u0941\u0928 \u0928\u0940 <br/> \u092c\u093e\u0939\u0941 \u092a\u0930\u093e\u0915\u094d\u0930\u092e\u092e\u0941\u0928\u0947\u0930\u0941\u0917\u0928\u093e \u0930\u093e\u0915\u093e"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0926\u093f\u0928 \u0926\u093f\u0928\u092e\u094c\u092a\u093e\u0938\u0928 \u091c\u092a \u0924\u092a \u0927\u094d\u092f\u093e\u0928\u092e\u094d- <br/> \u0905\u0928\u0941 \u092f\u093e\u0917\u092e\u0941 \u0935\u0947\u0932 \u092e\u0928\u0938\u0941\u0928 \u092a\u0941\u091f\u094d\u091f\u093f\u0928 <br/> \u0918\u0928 \u0921\u092e\u094d\u092c\u0941\u0928\u093f \u0924\u094b\u0921\u0928\u0941 \u092e\u093e\u0930\u0940\u091a\u0941\u0928\u093f <br/> \u092a\u0928\u093f \u091a\u0947\u0930\u091a\u093f\u0928\u092f\u093e \u0924\u094d\u092f\u093e\u0917\u0930\u093e\u091c\u093e\u0930\u094d\u091a\u093f\u0924"}]}], "L": "l", "lyricsref": [{"links": [{"N": "thyagaraja-vaibhavam.blogspot.com", "L": "http://thyagaraja-vaibhavam.blogspot.com/2008/08/thyagaraja-kriti-sasi-vadana-raga.html"}, {"N": "www.sangeetasudha.org", "L": "http://www.sangeetasudha.org/thyagaraja/vol3/a3.html#MARK_SASIVADANA"}, {"N": "www.karnatik.com", "L": "http://www.karnatik.com/c2828.shtml"}]}]}