0c4d ! \u0c1a\u0c4a\u0c32\u0c4d ! \u0c1a\u0c4a\u0c32\u0c4d ! \u0c15\u0c3f\u0c33\u0c3f\u0c2f\u0c47 !"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c35\u0c46\u0c33\u0c4d\u0c33\u0c3f \u0c28\u0c3f\u0c32\u0c35\u0c3f\u0c32\u0c4d \u0c35\u0c46\u0c31\u0c3f \u0c15\u0c4a\u0c33\u0c4d\u0c33\u0c41\u0c2e\u0c4d <br/> \u0c2e\u0c4b\u0c15\u0c28 \u0c15\u0c40\u0c24\u0c2e\u0c4d <br/> \u0c05\u0c33\u0c4d\u0c33\u0c3f\u0c1a\u0c4d \u0c1a\u0c4a\u0c30\u0c3f\u0c28\u0c4d\u0c24\u0c47 \u0c0e\u0c28\u0c48 <br/> \u0c2e\u0c48\u0c2f\u0c32\u0c4d \u0c1a\u0c46\u0c2f\u0c4d\u0c24\u0c3e\u0c28\u0c4d ! <br/> \u0c24\u0c41\u0c33\u0c4d\u0c33\u0c41\u0c2e\u0c4d \u0c09\u0c35\u0c15\u0c48 \u0c2a\u0c4a\u0c02\u0c15 <br/> \u0c09\u0c33\u0c4d\u0c33\u0c2e\u0c4d \u0c2e\u0c15\u0c3f\u0c34\u0c4d\u0c28\u0c4d\u0c24\u0c3f\u0c30\u0c41\u0c28\u0c4d\u0c24\u0c47\u0c28\u0c4d, <br/> \u0c24\u0c41\u0c28\u0c4d\u0c2a\u0c2e\u0c4d \u0c24\u0c4a\u0c1f\u0c30\u0c4d\u0c28\u0c4d\u0c24\u0c24\u0c46\u0c28\u0c4d\u0c28? <br/> \u0c1a\u0c4a\u0c32\u0c4d ! \u0c1a\u0c4a\u0c32\u0c4d ! \u0c1a\u0c4a\u0c32\u0c4d ! \u0c15\u0c3f\u0c33\u0c3f\u0c2f\u0c47 !"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 4", "T": "\u0c24\u0c1e\u0c4d\u0c1a\u0c2e\u0c46\u0c28\u0c4d\u0c24 \u0c28\u0c41\u0c28\u0c41\u0c15\u0c4d\u0c15\u0c35\u0c28\u0c4d <br/> \u0c24\u0c3f\u0c30\u0c41\u0c35\u0c1f\u0c3f\u0c2f\u0c28\u0c4d\u0c31\u0c3f \u0c35\u0c47\u0c31\u0c47 <br/> \u0c24\u0c30\u0c23\u0c3f\u0c2f\u0c3f\u0c32\u0c4d \u0c07\u0c32\u0c4d\u0c32\u0c48\u0c2f\u0c46\u0c28\u0c4d\u0c31\u0c41 <br/> \u0c1a\u0c4a\u0c32\u0c4d ! \u0c1a\u0c4a\u0c32\u0c4d ! \u0c1a\u0c4a\u0c32\u0c4d ! \u0c15\u0c3f\u0c33\u0c3f\u0c2f\u0c47 ! <br/> \u0c05\u0c1e\u0c4d\u0c1a\u0c32\u0c46\u0c28\u0c4d\u0c31\u0c30\u0c41\u0c33\u0c4d \u0c1a\u0c46\u0c2f\u0c4d\u0c2f\u0c3e\u0c28\u0c4b <br/> \u0c05\u0c28\u0c4d\u0c2a\u0c3f\u0c28\u0c41\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d \u0c2a\u0c1e\u0c4d\u0c1a\u0c2e\u0c41\u0c23\u0c4d\u0c1f\u0c4b <br/> \u0c05\u0c31\u0c3f\u0c2f\u0c3e\u0c2a\u0c4d \u0c2a\u0c47\u0c24\u0c48 \u0c28\u0c3e\u0c28\u0c46\u0c28\u0c4d\u0c31\u0c41 <br/> \u0c1a\u0c4a\u0c32\u0c4d ! \u0c1a\u0c4a\u0c32\u0c4d ! \u0c1a\u0c4a\u0c32\u0c4d ! \u0c15\u0c3f\u0c33\u0c3f\u0c2f\u0c47 !"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0cc6\u0c82\u0c95\u0ca8\u0cbf \u0cb5\u0cbe\u0caf\u0cbf\u0cb2\u0ccd \u0c92\u0cb0\u0cc1 <br/> \u0cb5\u0cc7\u0caf\u0ccd\u0c82\u0c95\u0cc1\u0cb3\u0cb2\u0ccd \u0c95\u0cca\u0ca3\u0ccd\u0c9f\u0cbf\u0c9a\u0cc8\u0ca4\u0ccd\u0ca4\u0cc1 <br/> \u0c87\u0ca8\u0ccd\u0ca8\u0cae\u0cc1\u0ca4\u0cae\u0ccd \u0caa\u0cca\u0cb3\u0cbf\u0cb5\u0ccb\u0ca8\u0ccd <br/> \u0caf\u0cbe\u0cb0\u0ccd? \u0caf\u0cbe\u0cb0\u0ccd? \u0c9a\u0cca\u0cb2\u0ccd \u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 ! <br/> \u0c9a\u0cc6\u0ca8\u0ccd\u0ca4\u0cbe\u0cae\u0cb0\u0cc8 \u0cae\u0cc1\u0c95\u0ca4\u0ccd\u0ca4\u0cbf\u0cb2\u0ccd <br/> \u0cae\u0ca8\u0ccd\u0ca4\u0c95\u0cbe\u0c9a\u0cae\u0ccd \u0caa\u0cc1\u0cb0\u0cbf\u0ca8\u0ccd\u0ca4\u0cc1 <br/> \u0c9a\u0cbf\u0ca8\u0ccd\u0ca4\u0cc8 \u0ca4\u0cbf\u0cb0\u0cc8\u0caf\u0cbe\u0caf\u0ccd\u0c95\u0ccd \u0c95\u0cca\u0cb3\u0ccd\u0cb5\u0ccb\u0ca8\u0ccd <br/> \u0caf\u0cbe\u0cb0\u0ccd? \u0caf\u0cbe\u0cb0\u0ccd? \u0c9a\u0cca\u0cb2\u0ccd \u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 !"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c95\u0ca3\u0ccd\u0ca3\u0ca8\u0ccd \u0c8e\u0ca8\u0ccd\u0cb1\u0cbf\u0c82\u0c95\u0cc7 \u0caf\u0cca\u0cb0\u0cc1 <br/> \u0c95\u0cb3\u0ccd\u0cb5\u0ca8\u0ccd \u0c89\u0cb3\u0ca8\u0ccd \u0c8e\u0ca8\u0ccd\u0cb1\u0cc1 <br/> \u0c95\u0ca8\u0ccd\u0ca8\u0cbf\u0caf\u0cb0\u0ccd \u0c9a\u0cca\u0ca8\u0ccd\u0ca8\u0ca4\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd <br/> \u0cae\u0cc6\u0caf\u0ccd\u0ca4\u0cbe\u0ca8\u0ccb? \u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 ! <br/> \u0cb5\u0cc6\u0ca3\u0ccd\u0ca3\u0cc6\u0caf\u0ccd \u0ca4\u0cbf\u0cb0\u0cc1\u0c9f\u0cc1\u0cae\u0ccd \u0caa\u0cbf\u0cb3\u0ccd\u0cb3\u0cc8 <br/> \u0c8e\u0ca8\u0ccd\u0ca8\u0cc1\u0cb3\u0c99\u0ccd \u0c95\u0cb5\u0cb0\u0ccd\u0ca8\u0ccd\u0ca4 \u0ca4\u0cc6\u0ca8\u0ccd\u0ca8? <br/> \u0c95\u0ca3\u0ccd\u0ca3\u0cbe\u0cb2\u0ccd \u0cae\u0cca\u0cb3\u0cbf\u0ca8\u0ccd\u0ca4\u0ca4\u0cc6\u0ca8\u0ccd\u0ca8? <br/> \u0c9a\u0cca\u0cb2\u0ccd\u0cb5\u0cbe\u0caf\u0ccd ! \u0caa\u0cc8\u0c82\u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 !"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0ca4\u0cc6\u0ca8\u0ccd\u0cb1\u0cb2\u0ccd \u0ca4\u0cb5\u0cb3\u0cc1\u0ca8\u0ccd \u0ca4\u0cc6\u0caf\u0ccd\u0cb5 <br/> \u0caf\u0cae\u0cc1\u0ca8\u0cbe \u0ca8\u0ca4\u0cbf\u0c95\u0ccd \u0c95\u0cb0\u0cc8\u0caf\u0cbf\u0cb2\u0ccd <br/> \u0ca4\u0cc7\u0ca8\u0ccd \u0cb5\u0ca3\u0ccd\u0c9f\u0cc1 \u0caa\u0cbe\u0c9f\u0cc1\u0cae\u0ccd \u0caa\u0cc1\u0ca8\u0ccd\u0ca8\u0cc8 <br/> \u0c9a\u0ccb\u0cb2\u0cc8 \u0ca4\u0ca8\u0ccd\u0ca8\u0cbf\u0cb2\u0ccd <br/> \u0c85\u0ca8\u0ccd\u0cb1\u0cbf\u0cb2\u0cc8\u0caa\u0ccd \u0caa\u0ccb\u0cb2\u0cc7 \u0c85\u0cb5\u0ca8\u0ccd <br/> \u0c9a\u0ca8\u0ccd\u0ca4\u0cbf\u0cb0 \u0cb5\u0ca4\u0ca8\u0cae\u0ccd \u0ca8\u0ccb\u0c95\u0ccd\u0c95\u0cbf <br/> \u0ca8\u0c82\u0c95\u0cc8 \u0c87\u0cb0\u0cc1\u0ca8\u0ccd\u0ca4\u0cbe\u0cb3\u0cc6\u0ca8\u0ccd\u0cb1\u0cc1 <br/> \u0c9a\u0cca\u0cb2\u0ccd ! \u0c9a\u0cca\u0cb2\u0ccd ! \u0c9a\u0cca\u0cb2\u0ccd ! \u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 !"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb5\u0cc6\u0cb3\u0ccd\u0cb3\u0cbf \u0ca8\u0cbf\u0cb2\u0cb5\u0cbf\u0cb2\u0ccd \u0cb5\u0cc6\u0cb1\u0cbf \u0c95\u0cca\u0cb3\u0ccd\u0cb3\u0cc1\u0cae\u0ccd <br/> \u0cae\u0ccb\u0c95\u0ca8 \u0c95\u0cc0\u0ca4\u0cae\u0ccd <br/> \u0c85\u0cb3\u0ccd\u0cb3\u0cbf\u0c9a\u0ccd \u0c9a\u0cca\u0cb0\u0cbf\u0ca8\u0ccd\u0ca4\u0cc7 \u0c8e\u0ca8\u0cc8 <br/> \u0cae\u0cc8\u0caf\u0cb2\u0ccd \u0c9a\u0cc6\u0caf\u0ccd\u0ca4\u0cbe\u0ca8\u0ccd ! <br/> \u0ca4\u0cc1\u0cb3\u0ccd\u0cb3\u0cc1\u0cae\u0ccd \u0c89\u0cb5\u0c95\u0cc8 \u0caa\u0cca\u0c82\u0c95 <br/> \u0c89\u0cb3\u0ccd\u0cb3\u0cae\u0ccd \u0cae\u0c95\u0cbf\u0cb3\u0ccd\u0ca8\u0ccd\u0ca4\u0cbf\u0cb0\u0cc1\u0ca8\u0ccd\u0ca4\u0cc7\u0ca8\u0ccd, <br/> \u0ca4\u0cc1\u0ca8\u0ccd\u0caa\u0cae\u0ccd \u0ca4\u0cca\u0c9f\u0cb0\u0ccd\u0ca8\u0ccd\u0ca4\u0ca4\u0cc6\u0ca8\u0ccd\u0ca8? <br/> \u0c9a\u0cca\u0cb2\u0ccd ! \u0c9a\u0cca\u0cb2\u0ccd ! \u0c9a\u0cca\u0cb2\u0ccd ! \u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 !"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0ca4\u0c9e\u0ccd\u0c9a\u0cae\u0cc6\u0ca8\u0ccd\u0ca4 \u0ca8\u0cc1\u0ca8\u0cc1\u0c95\u0ccd\u0c95\u0cb5\u0ca8\u0ccd <br/> \u0ca4\u0cbf\u0cb0\u0cc1\u0cb5\u0c9f\u0cbf\u0caf\u0ca8\u0ccd\u0cb1\u0cbf \u0cb5\u0cc7\u0cb1\u0cc7 <br/> \u0ca4\u0cb0\u0ca3\u0cbf\u0caf\u0cbf\u0cb2\u0ccd \u0c87\u0cb2\u0ccd\u0cb2\u0cc8\u0caf\u0cc6\u0ca8\u0ccd\u0cb1\u0cc1 <br/> \u0c9a\u0cca\u0cb2\u0ccd ! \u0c9a\u0cca\u0cb2\u0ccd ! \u0c9a\u0cca\u0cb2\u0ccd ! \u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 ! <br/> \u0c85\u0c9e\u0ccd\u0c9a\u0cb2\u0cc6\u0ca8\u0ccd\u0cb1\u0cb0\u0cc1\u0cb3\u0ccd \u0c9a\u0cc6\u0caf\u0ccd\u0caf\u0cbe\u0ca8\u0ccb <br/> \u0c85\u0ca8\u0ccd\u0caa\u0cbf\u0ca8\u0cc1\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd \u0caa\u0c9e\u0ccd\u0c9a\u0cae\u0cc1\u0ca3\u0ccd\u0c9f\u0ccb <br/> \u0c85\u0cb1\u0cbf\u0caf\u0cbe\u0caa\u0ccd \u0caa\u0cc7\u0ca4\u0cc8 \u0ca8\u0cbe\u0ca8\u0cc6\u0ca8\u0ccd\u0cb1\u0cc1 <br/> \u0c9a\u0cca\u0cb2\u0ccd ! \u0c9a\u0cca\u0cb2\u0ccd ! \u0c9a\u0cca\u0cb2\u0ccd ! \u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 !"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d46\u0d02\u0d15\u0d28\u0d3f \u0d35\u0d3e\u0d2f\u0d3f\u0d32\u0d4d \u0d12\u0d30\u0d41 <br/> \u0d35\u0d47\u0d2f\u0d4d\u0d02\u0d15\u0d41\u0d34\u0d32\u0d4d \u0d15\u0d4a\u0d23\u0d4d\u0d1f\u0d3f\u0d1a\u0d48\u0d24\u0d4d\u0d24\u0d41 <br/> \u0d07\u0d28\u0d4d\u0d28\u0d2e\u0d41\u0d24\u0d2e\u0d4d \u0d2a\u0d4a\u0d34\u0d3f\u0d35\u0d4b\u0d28\u0d4d <br/> \u0d2f\u0d3e\u0d30\u0d4d? \u0d2f\u0d3e\u0d30\u0d4d? \u0d1a\u0d4a\u0d32\u0d4d \u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 ! <br/> \u0d1a\u0d46\u0d28\u0d4d\u0d24\u0d3e\u0d2e\u0d30\u0d48 \u0d2e\u0d41\u0d15\u0d24\u0d4d\u0d24\u0d3f\u0d32\u0d4d <br/> \u0d2e\u0d28\u0d4d\u0d24\u0d15\u0d3e\u0d1a\u0d2e\u0d4d \u0d2a\u0d41\u0d30\u0d3f\u0d28\u0d4d\u0d24\u0d41 <br/> \u0d1a\u0d3f\u0d28\u0d4d\u0d24\u0d48 \u0d24\u0d3f\u0d30\u0d48\u0d2f\u0d3e\u0d2f\u0d4d\u0d15\u0d4d \u0d15\u0d4a\u0d33\u0d4d\u0d35\u0d4b\u0d28\u0d4d <br/> \u0d2f\u0d3e\u0d30\u0d4d? \u0d2f\u0d3e\u0d30\u0d4d? \u0d1a\u0d4a\u0d32\u0d4d \u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 !"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d15\u0d23\u0d4d\u0d23\u0d28\u0d4d \u0d0e\u0d28\u0d4d\u0d31\u0d3f\u0d02\u0d15\u0d47 \u0d2f\u0d4a\u0d30\u0d41 <br/> \u0d15\u0d33\u0d4d\u0d35\u0d28\u0d4d \u0d09\u0d33\u0d28\u0d4d \u0d0e\u0d28\u0d4d\u0d31\u0d41 <br/> \u0d15\u0d28\u0d4d\u0d28\u0d3f\u0d2f\u0d30\u0d4d \u0d1a\u0d4a\u0d28\u0d4d\u0d28\u0d24\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d <br/> \u0d2e\u0d46\u0d2f\u0d4d\u0d24\u0d3e\u0d28\u0d4b? \u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 ! <br/> \u0d35\u0d46\u0d23\u0d4d\u0d23\u0d46\u0d2f\u0d4d \u0d24\u0d3f\u0d30\u0d41\u0d1f\u0d41\u0d2e\u0d4d \u0d2a\u0d3f\u0d33\u0d4d\u0d33\u0d48 <br/> \u0d0e\u0d28\u0d4d\u0d28\u0d41\u0d33\u0d19\u0d4d \u0d15\u0d35\u0d30\u0d4d\u0d28\u0d4d\u0d24 \u0d24\u0d46\u0d28\u0d4d\u0d28? <br/> \u0d15\u0d23\u0d4d\u0d23\u0d3e\u0d32\u0d4d \u0d2e\u0d4a\u0d34\u0d3f\u0d28\u0d4d\u0d24\u0d24\u0d46\u0d28\u0d4d\u0d28? <br/> \u0d1a\u0d4a\u0d32\u0d4d\u0d35\u0d3e\u0d2f\u0d4d ! \u0d2a\u0d48\u0d02\u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 !"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d24\u0d46\u0d28\u0d4d\u0d31\u0d32\u0d4d \u0d24\u0d35\u0d34\u0d41\u0d28\u0d4d \u0d24\u0d46\u0d2f\u0d4d\u0d35 <br/> \u0d2f\u0d2e\u0d41\u0d28\u0d3e \u0d28\u0d24\u0d3f\u0d15\u0d4d \u0d15\u0d30\u0d48\u0d2f\u0d3f\u0d32\u0d4d <br/> \u0d24\u0d47\u0d28\u0d4d \u0d35\u0d23\u0d4d\u0d1f\u0d41 \u0d2a\u0d3e\u0d1f\u0d41\u0d2e\u0d4d \u0d2a\u0d41\u0d28\u0d4d\u0d28\u0d48 <br/> \u0d1a\u0d4b\u0d32\u0d48 \u0d24\u0d28\u0d4d\u0d28\u0d3f\u0d32\u0d4d <br/> \u0d05\u0d28\u0d4d\u0d31\u0d3f\u0d32\u0d48\u0d2a\u0d4d \u0d2a\u0d4b\u0d32\u0d47 \u0d05\u0d35\u0d28\u0d4d <br/> \u0d1a\u0d28\u0d4d\u0d24\u0d3f\u0d30 \u0d35\u0d24\u0d28\u0d2e\u0d4d \u0d28\u0d4b\u0d15\u0d4d\u0d15\u0d3f <br/> \u0d28\u0d02\u0d15\u0d48 \u0d07\u0d30\u0d41\u0d28\u0d4d\u0d24\u0d3e\u0d33\u0d46\u0d28\u0d4d\u0d31\u0d41 <br/> \u0d1a\u0d4a\u0d32\u0d4d ! \u0d1a\u0d4a\u0d32\u0d4d ! \u0d1a\u0d4a\u0d32\u0d4d ! \u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 !"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d35\u0d46\u0d33\u0d4d\u0d33\u0d3f \u0d28\u0d3f\u0d32\u0d35\u0d3f\u0d32\u0d4d \u0d35\u0d46\u0d31\u0d3f \u0d15\u0d4a\u0d33\u0d4d\u0d33\u0d41\u0d2e\u0d4d <br/> \u0d2e\u0d4b\u0d15\u0d28 \u0d15\u0d40\u0d24\u0d2e\u0d4d <br/> \u0d05\u0d33\u0d4d\u0d33\u0d3f\u0d1a\u0d4d \u0d1a\u0d4a\u0d30\u0d3f\u0d28\u0d4d\u0d24\u0d47 \u0d0e\u0d28\u0d48 <br/> \u0d2e\u0d48\u0d2f\u0d32\u0d4d \u0d1a\u0d46\u0d2f\u0d4d\u0d24\u0d3e\u0d28\u0d4d ! <br/> \u0d24\u0d41\u0d33\u0d4d\u0d33\u0d41\u0d2e\u0d4d \u0d09\u0d35\u0d15\u0d48 \u0d2a\u0d4a\u0d02\u0d15 <br/> \u0d09\u0d33\u0d4d\u0d33\u0d2e\u0d4d \u0d2e\u0d15\u0d3f\u0d34\u0d4d\u0d28\u0d4d\u0d24\u0d3f\u0d30\u0d41\u0d28\u0d4d\u0d24\u0d47\u0d28\u0d4d, <br/> \u0d24\u0d41\u0d28\u0d4d\u0d2a\u0d2e\u0d4d \u0d24\u0d4a\u0d1f\u0d30\u0d4d\u0d28\u0d4d\u0d24\u0d24\u0d46\u0d28\u0d4d\u0d28? <br/> \u0d1a\u0d4a\u0d32\u0d4d ! \u0d1a\u0d4a\u0d32\u0d4d ! \u0d1a\u0d4a\u0d32\u0d4d ! \u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 !"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d24\u0d1e\u0d4d\u0d1a\u0d2e\u0d46\u0d28\u0d4d\u0d24 \u0d28\u0d41\u0d28\u0d41\u0d15\u0d4d\u0d15\u0d35\u0d28\u0d4d <br/> \u0d24\u0d3f\u0d30\u0d41\u0d35\u0d1f\u0d3f\u0d2f\u0d28\u0d4d\u0d31\u0d3f \u0d35\u0d47\u0d31\u0d47 <br/> \u0d24\u0d30\u0d23\u0d3f\u0d2f\u0d3f\u0d32\u0d4d \u0d07\u0d32\u0d4d\u0d32\u0d48\u0d2f\u0d46\u0d28\u0d4d\u0d31\u0d41 <br/> \u0d1a\u0d4a\u0d32\u0d4d ! \u0d1a\u0d4a\u0d32\u0d4d ! \u0d1a\u0d4a\u0d32\u0d4d ! \u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 ! <br/> \u0d05\u0d1e\u0d4d\u0d1a\u0d32\u0d46\u0d28\u0d4d\u0d31\u0d30\u0d41\u0d33\u0d4d \u0d1a\u0d46\u0d2f\u0d4d\u0d2f\u0d3e\u0d28\u0d4b <br/> \u0d05\u0d28\u0d4d\u0d2a\u0d3f\u0d28\u0d41\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d \u0d2a\u0d1e\u0d4d\u0d1a\u0d2e\u0d41\u0d23\u0d4d\u0d1f\u0d4b <br/> \u0d05\u0d31\u0d3f\u0d2f\u0d3e\u0d2a\u0d4d \u0d2a\u0d47\u0d24\u0d48 \u0d28\u0d3e\u0d28\u0d46\u0d28\u0d4d\u0d31\u0d41 <br/> \u0d1a\u0d4a\u0d32\u0d4d ! \u0d1a\u0d4a\u0d32\u0d4d ! \u0d1a\u0d4a\u0d32\u0d4d ! \u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 !"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0938\u0947\u0902\u0915\u0928\u093f \u0935\u093e\u092f\u093f\u0932\u094d \u0913\u0930\u0941 <br/> \u0935\u0947\u092f\u094d\u0902\u0915\u0941\u0932\u0932\u094d \u0915\u094b\u0923\u094d\u091f\u093f\u091a\u0948\u0924\u094d\u0924\u0941 <br/> \u0907\u0928\u094d\u0928\u092e\u0941\u0924\u092e\u094d \u092a\u094b\u0932\u093f\u0935\u094b\u0928\u094d <br/> \u092f\u093e\u0930\u094d? \u092f\u093e\u0930\u094d? \u091a\u094b\u0932\u094d \u0915\u093f\u0932\u093f\u092f\u0947 ! <br/> \u091a\u0947\u0928\u094d\u0924\u093e\u092e\u0930\u0948 \u092e\u0941\u0915\u0924\u094d\u0924\u093f\u0932\u094d <br/> \u092e\u0928\u094d\u0924\u0915\u093e\u091a\u092e\u094d \u092a\u0941\u0930\u093f\u0928\u094d\u0924\u0941 <br/> \u091a\u093f\u0928\u094d\u0924\u0948 \u0924\u093f\u0930\u0948\u092f\u093e\u092f\u094d\u0915\u094d \u0915\u094b\u0932\u094d\u0935\u094b\u0928\u094d <br/> \u092f\u093e\u0930\u094d? \u092f\u093e\u0930\u094d? \u091a\u094b\u0932\u094d \u0915\u093f\u0932\u093f\u092f\u0947 !"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0915\u0923\u094d\u0923\u0928\u094d \u090f\u0928\u094d\u0930\u093f\u0902\u0915\u0947 \u092f\u094b\u0930\u0941 <br/> \u0915\u0932\u094d\u0935\u0928\u094d \u0909\u0932\u0928\u094d \u090f\u0928\u094d\u0930\u0941 <br/> \u0915\u0928\u094d\u0928\u093f\u092f\u0930\u094d \u091a\u094b\u0928\u094d\u0928\u0924\u0947\u0932\u094d\u0932\u093e\u092e\u094d <br/> \u092e\u0947\u092f\u094d\u0924\u093e\u0928\u094b? \u0915\u093f\u0932\u093f\u092f\u0947 ! <br/> \u0935\u0947\u0923\u094d\u0923\u0947\u092f\u094d \u0924\u093f\u0930\u0941\u091f\u0941\u092e\u094d \u092a\u093f\u0932\u094d\u0932\u0948 <br/> \u090f\u0928\u094d\u0928\u0941\u0932\u0919\u094d \u0915\u0935\u0930\u094d\u0928\u094d\u0924 \u0924\u0947\u0928\u094d\u0928? <br/> \u0915\u0923\u094d\u0923