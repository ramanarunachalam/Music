d4d\u0d26\u0d2f\u0d47\u0d36\u0d4d\u0d35\u0d48 \u0d17\u0d4c\u0d30\u0d3f \u0d2e\u0d41\u0d15\u0d4d\u0d28\u0d41\u0d17\u0d4d\u0d30\u0d39 \u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3f \u0d2a\u0d4d\u0d30\u0d26\u0d3e\u0d2f\u0d3f\u0d28\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30\u0d36\u0d47\u0d16\u0d30\u0d3e\u0d30\u0d4d\u0d27\u0d3e\u0d19\u0d4d\u0d17 \u0d36\u0d30\u0d40\u0d30\u0d3f\u0d23\u0d3f \u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30 \u0d15\u0d32\u0d3e\u0d27\u0d30\u0d47 \u0d38\u0d30\u0d4d\u0d35 \u0d1c\u0d40\u0d35\u0d47\u0d36\u0d4d\u0d35\u0d30\u0d3f <br/> \u0d2e\u0d28\u0d4d\u0d24\u0d4d\u0d30 \u0d2f\u0d4b\u0d17 \u0d2a\u0d40\u0d20 \u0d2e\u0d27\u0d4d\u0d2f \u0d28\u0d3f\u0d35\u0d3e\u0d38\u0d3f\u0d28\u0d3f \u0d2e\u0d3e\u0d27\u0d35 \u0d38\u0d4b\u0d26\u0d30\u0d3f \u0d2e\u0d3e\u0d2e\u0d35 \u0d36\u0d41\u0d2d\u0d15\u0d30\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d2d\u0d35\u0d2d\u0d2f \u0d39\u0d3e\u0d30\u0d3f\u0d23\u0d3f \u0d2a\u0d3e\u0d2a \u0d38\u0d2e\u0d4d\u0d39\u0d3e\u0d30\u0d3f\u0d23\u0d3f \u0d36\u0d3f\u0d35 \u0d38\u0d24\u0d3f \u0d1a\u0d3f\u0d28\u0d4d\u0d24\u0d3e\u0d2e\u0d23\u0d3f \u0d26\u0d4d\u0d35\u0d40\u0d2a \u0d35\u0d3e\u0d38\u0d3f\u0d28\u0d3f <br/> \u0d28\u0d35\u0d30\u0d38 \u0d2a\u0d42\u0d30\u0d3f\u0d24 \u0d28\u0d3e\u0d26 \u0d38\u0d4d\u0d35\u0d30\u0d42\u0d2a\u0d3f\u0d23\u0d3f \u0d28\u0d35 \u0d06\u0d35\u0d30\u0d23 \u0d2a\u0d42\u0d1c\u0d3e (\u0d06)\u0d28\u0d28\u0d4d\u0d26 \u0d2e\u0d28\u0d4b\u0d28\u0d4d\u0d2e\u0d23\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d2e\u0d4d\u0d30\u0d4d\u0d26\u0d41\u0d32 \u0d15\u0d30 \u0d15\u0d2e\u0d32 \u0d35\u0d40\u0d23\u0d3e \u0d35\u0d3e\u0d26\u0d4d\u0d2f\u0d47 \u0d36\u0d4d\u0d30\u0d41\u0d24\u0d3f\u0d28\u0d41\u0d24 \u0d35\u0d47\u0d26 \u0d35\u0d47\u0d26\u0d3e\u0d28\u0d4d\u0d24 \u0d35\u0d47\u0d26\u0d4d\u0d2f\u0d47 <br/> \u0d2a\u0d4d\u0d30\u0d4d\u0d25\u0d4d\u0d35\u0d40 \u0d15\u0d4d\u0d37\u0d47\u0d24\u0d4d\u0d30 \u0d15\u0d3e\u0d1e\u0d4d\u0d1a\u0d3f \u0d0f\u0d15\u0d3e\u0d27\u0d3f\u0d2a\u0d