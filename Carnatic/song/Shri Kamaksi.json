u0c95\u0cbe\u0cb5\u0cb5\u0cc7 \u0ca8\u0ca8\u0cc1 \u0c95\u0cb0\u0cc1\u0ca3\u0cbe \u0c95\u0c9f\u0cbe\u0c95\u0ccd\u0cb7\u0cbf <br/> \u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cbe\u0ca8\u0ccd\u0ca4\u0cbf\u0cae\u0ca4\u0cc0 \u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cbe\u0ca8\u0ccd\u0c9a\u0cc0 \u0caa\u0cc1\u0cb0 \u0cb5\u0cbe\u0cb8\u0cbf\u0ca8\u0cbf"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c8f\u0c95\u0cbe\u0cae\u0ccd\u0cb0\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0\u0cc0 \u0ca8\u0cc0\u0c95\u0cc1 \u0c8f\u0cb2\u0cbe\u0c97\u0cc1 \u0ca6\u0caf \u0cb5\u0c9a\u0ccd\u0c9a\u0cc1\u0ca8\u0ccb <br/> \u0cb2\u0ccb\u0c95\u0cc1\u0cb2\u0cc1 \u0c95\u0ccb\u0cb0\u0cbf\u0ca8 \u0ca6\u0cc8\u0cb5\u0cae\u0cc1 \u0ca8\u0cc0\u0cb5\u0cc7 \u0c97\u0cbe\u0ca6\u0cbe <br/> \u0c8f\u0c95 \u0cad\u0cbe\u0cb5\u0cc1\u0ca1\u0cc8\u0ca8 \u0ca8\u0ca8\u0ccd\u0ca8\u0cca\u0c95\u0ca8\u0cbf \u0cac\u0ccd\u0cb0\u0ccb\u0cb5 \u0cac\u0cb0\u0cc1\u0cb5\u0cbe"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c95\u0ccb\u0cb0\u0cbf \u0cb5\u0c9a\u0ccd\u0c9a\u0cbf\u0ca8 \u0cad\u0c95\u0ccd\u0ca4 \u0c9c\u0ca8\u0cc1\u0cb2\u0c95\u0cc1 <br/> \u0c95\u0ccb\u0cae\u0cb3\u0cbe\u0c82\u0c97\u0cc0 \u0ca8\u0cc0\u0cb5\u0cc7 \u0cb8\u0cbe\u0cae\u0ccd\u0cb0\u0cbe\u0c9c\u0ccd\u0caf\u0cae\u0cc1 <br/> \u0c95\u0cbe\u0cae\u0cbe\u0c95\u0ccd\u0cb7\u0cae\u0ccd\u0cae\u0cbe \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc7 \u0cb5\u0cc7\u0ca1\u0cbf\u0ca8 \u0cac\u0cbf\u0ca1\u0ccd\u0ca1\u0ca8\u0cc1 <br/> \u0c95\u0cbe\u0caa\u0cbe\u0ca1\u0cb5\u0cae\u0ccd\u0cae\u0cbe \u0c95\u0cb0\u0cc1\u0ca3 \u0c9c\u0cc2\u0ca1\u0cb5\u0cae\u0ccd\u0cae\u0cbe <br/> \u0cb8\u0cbe\u0cb0\u0cb8 \u0ca6\u0cb3 \u0ca8\u0cc7\u0ca4\u0ccd\u0cb0\u0cc0 \u0c95\u0cbe\u0cae \u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0cc0 <br/> \u0cb8\u0ccb\u0cae \u0cb6\u0cc7\u0c96\u0cb0\u0cc1\u0ca8\u0cbf \u0cb0\u0cbe\u0ca3\u0cc0 \u0caa\u0cc1\u0cb0\u0cbe\u0ca3\u0cc0 <br/> \u0cb6\u0ccd\u0caf\u0cbe\u0cae\u0cb3\u0cbe\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cc7 \u0c95\u0cbe\u0cb3\u0cbf\u0c95\u0cc7 \u0c95\u0cb2\u0cc7 <br/> \u0cb8\u0cbe\u0cae \u0c97\u0cbe\u0ca8 \u0cae\u0ccb\u0ca6\u0cbf\u0ca8\u0cc0 \u0c9c\u0ca8\u0ca8\u0cc0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0ca8\u0cc0\u0cb0\u0c9c \u0cb2\u0ccb\u0c9a\u0ca8\u0cbe \u0cb8\u0ccd\u0ca5\u0cbf\u0cb0\u0cae\u0ca8\u0cbf \u0cad\u0c95\u0ccd\u0ca4\u0cbf\u0ca4\u0ccb <br/> \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc7 \u0cb6\u0cb0\u0ca3\u0ca3\u0ccd\u0c9f\u0cbf\u0ca8 \u0ca6\u0cbe\u0cb8\u0cc1\u0ca1\u0cc1 \u0ca8\u0cc7\u0ca8\u0cc1 <br/> \u0ca8\u0cc0 \u0cb8\u0ca8\u0ccd\u0ca8\u0cbf\u0ca7\u0cbf\u0ca8\u0cbf \u0c9c\u0cc7\u0cb0\u0cbf\u0ca8 \u0ca8\u0cbe\u0caa\u0cc8 <br/> \u0ca8\u0cbf\u0cb0\u0cc0\u0c95\u0ccd\u0cb7\u0ca3\u0cae\u0cc1 \u0c9a\u0cc7\u0caf \u0ca4\u0c97\u0cc1\u0ca8\u0cbe <br/> \u0ca8\u0cc0 \u0ca8\u0cbe\u0cae \u0ca7\u0ccd\u0caf\u0cbe\u0ca8\u0cae\u0cc7 \u0ca8\u0cbf\u0caf\u0ca4\u0cbf \u0cb5\u0cc7\u0cb0\u0cc7 <br/> \u0c9c\u0caa \u0ca4\u0caa\u0cae\u0cc1\u0cb2\u0cc6\u0cb0\u0cc1\u0c97\u0ca8\u0cc7 \u0cae\u0cbe\u0caf\u0cae\u0ccd\u0cae\u0cbe <br/> \u0ca8\u0cc0 \u0cb8\u0cbe\u0c9f\u0cc6\u0cb5\u0cb0\u0cc1 \u0cb6\u0ccd\u0caf\u0cbe\u0cae\u0cb3\u0cc7 \u0cb6\u0cbf\u0cb5\u0cc7 <br/> \u0cb6\u0ccd\u0caf\u0cbe\u0cae \u0c95\u0cc3\u0cb7\u0ccd\u0ca3 \u0caa\u0cbe\u0cb2\u0cbf\u0ca4 \u0c9c\u0ca8\u0ca8\u0cc0"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0ca8\u0cbe \u0cae\u0ca8\u0cb5\u0cbf \u0cb5\u0cbf\u0ca8\u0cc1\u0cae\u0cbf\u0c95 \u0c97\u0cbf\u0cb0\u0cbf <br/> \u0ca4\u0ca8\u0caf\u0cbe \u0cae\u0cc1\u0ca6\u0cae\u0cc1\u0ca4\u0ccb \u0cb5\u0c9a\u0ccd\u0c9a\u0cbf \u0c95\u0ccb\u0cb0\u0cbf\u0ca4\u0cbf <br/> \u0ca8\u0cbe \u0cb5\u0cc6\u0ca4\u0cb2\u0ca8\u0cc1 \u0ca6\u0cc0\u0cb0\u0ccd\u0c9a\u0cb5\u0cc7 \u0cae\u0cbe\u0c95\u0cad\u0caf <br/> \u0ca6\u0cbe\u0ca8\u0cae\u0cc0\u0caf\u0cb5\u0cc7 \u0ca4\u0cbe\u0cae\u0cb8\u0cae\u0cc1 \u0cb8\u0cc7\u0caf\u0c95\u0ca8\u0cc7"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d3e\u0d2e\u0d3e\u0d15\u0d4d\u0d37\u0d40 \u0d15\u0d3e\u0d35\u0d35\u0d47 \u0d28\u0d28\u0d41 \u0d15\u0d30\u0d41\u0d23\u0d3e \u0d15\u0d1f\u0d3e\u0d15\u0d4d\u0d37\u0d3f <br/> \u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d3e\u0d28\u0d4d\u0d24\u0d3f\u0d2e\u0d24\u0d40 \u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d3e\u0d28\u0d4d\u0d1a\u0d40 \u0d2a\u0d41\u0d30 \u0d35\u0d3e\u0d38\u0d3f\u0d28\u0d3f"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d0f\u0d15\u0d3e\u0d2e\u0d4d\u0d30\u0d47\u0d36\u0d4d\u0d35\u0d30\u0d40 \u0d28\u0d40\u0d15\u0d41 \u0d0f\u0d32\u0d3e\u0d17\u0d41 \u0d26\u0d2f \u0d35\u0d1a\u0d4d\u0d1a\u0d41\u0d28\u0d4b <br/> \u0d32\u0d4b\u0d15\u0d41\u0d32\u0d41 \u0d15\u0d4b\u0d30\u0d3f\u0d28 \u0d26\u0d48\u0d35\u0d2e\u0d41 \u0d28\u0d40\u0d35\u0d47 \u0d17\u0d3e\u0d26\u0d3e <br/> \u0d0f\u0d15 \u0d2d\u0d3e\u0d35\u0d41\u0d21\u0d48\u0d28 \u0d28\u0d28\u0d4d\u0d28\u0d4a\u0d15\u0d28\u0d3f \u0d2c\u0d4d\u0d30\u0d4b\u0d35 \u0d2c\u0d30\u0d41\u0d35\u0d3e"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d15\u0d4b\u0d30\u0d3f \u0d35\u0d1a\u0d4d\u0d1a\u0d3f\u0d28 \u0d2d\u0d15\u0d4d\u0d24 \u0d1c\u0d28\u0d41\u0d32\u0d15\u0d41 <br/> \u0d15\u0d4b\u0d2e\u0d33\u0d3e\u0d02\u0d17\u0d40 \u0d28\u0d40\u0d35\u0d47 \u0d38\u0d3e\u0d2e\u0d4d\u0d30\u0d3e\u0d1c\u0d4d\u0d2f\u0d2e\u0d41 <br/> \u0d15\u0d3e\u0d2e\u0d3e\u0d15\u0d4d\u0d37\u0d2e\u0d4d\u0d2e\u0d3e \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d47 \u0d35\u0d47\u0d21\u0d3f\u0d28 \u0d2c\u0d3f\u0d21\u0d4d\u0d21\u0d28\u0d41 <br/> \u0d15\u0d3e\u0d2a\u0d3e\u0d21\u0d35\u0d2e\u0d4d\u0d2e\u0d3e \u0d15\u0d30\u0d41\u0d23 \u0d1c\u0d42\u0d21\u0d35\u0d2e\u0d4d\u0d2e\u0d3e <br/> \u0d38\u0d3e\u0d30\u0d38 \u0d26\u0d33 \u0d28\u0d47\u0d24\u0d4d\u0d30\u0d40 \u0d15\u0d3e\u0d2e \u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d40 <br/> \u0d38\u0d4b\u0d2e \u0d36\u0d47\u0d16\u0d30\u0d41\u0d28\u0d3f \u0d30\u0d3e\u0d23\u0d40 \u0d2a\u0d41\u0d30\u0d3e\u0d23\u0d40 <br/> \u0d36\u0d4d\u0d2f\u0d3e\u0d2e\u0d33\u0d3e\u0d2e\u0d4d\u0d2c\u0d3f\u0d15\u0d47 \u0d15\u0d3e\u0d33\u0d3f\u0d15\u0d47 \u0d15\u0d32\u0d47 <br/> \u0d38\u0d3e\u0d2e \u0d17\u0d3e\u0d28 \u0d2e\u0d4b\u0d26\u0d3f\u0d28\u0d40 \u0d1c\u0d28\u0d28\u0d40"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d28\u0d40\u0d30\u0d1c \u0d32\u0d4b\u0d1a\u0d28\u0d3e \u0d38\u0d4d\u0d25\u0d3f\u0d30\u0d2e\u0d28\u0d3f \u0d2d\u0d15\u0d4d\u0d24\u0d3f\u0d24\u0d4b <br/> \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d47 \u0d36\u0d30\u0d23\u0d23\u0d4d\u0d1f\u0d3f\u0d28 \u0d26\u0d3e\u0d38\u0d41\u0d21\u0d41 \u0d28\u0d47\u0d28\u0d41 <br/> \u0d28\u0d40 \u0d38\u0d28\u0d4d\u0d28\u0d3f\u0d27\u0d3f\u0d28\u0d3f \u0d1c\u0d47\u0d30\u0d3f\u0d28 \u0d28\u0d3e\u0d2a\u0d48 <br/> \u0d28\u0d3f\u0d30\u0d40\u0d15\u0d4d\u0d37\u0d23\u0d2e\u0d41 \u0d1a\u0d47\u0d2f \u0d24\u0d17\u0d41\u0d28\u0d3e <br/> \u0d28\u0d40 \u0d28\u0d3e\u0d2e \u0d27\u0d4d\u0d2f\u0d3e\u0d28\u0d2e\u0d47 \u0d28\u0d3f\u0d2f\u0d24\u0d3f \u0d35\u0d47\u0d30\u0d47 <br/> \u0d1c\u0d2a \u0d24\u0d2a\u0d2e\u0d41\u0d32\u0d46\u0d30\u0d41\u0d17\u0d28\u0d47 \u0d2e\u0d3e\u0d2f\u0d2e\u0d4d\u0d2e\u0d3e <br/> \u0d28\u0d40 \u0d38\u0d3e\u0d1f\u0d46\u0d35\u0d30\u0d41 \u0d36\u0d4d\u0d2f\u0d3e\u0d2e\u0d33\u0d47 \u0d36\u0d3f\u0d35\u0d47 <br/> \u0d36\u0d4d\u0d2f\u0d3e\u0d2e \u0d15\u0d43\u0d37\u0d4d\u0d23 \u0d2a\u0d3e\u0d32\u0d3f\u0d24 \u0d1c\u0d28\u0d28\u0d40"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d28\u0d3e \u0d2e\u0d28\u0d35\u0d3f \u0d35\u0d3f\u0d28\u0d41\u0d2e\u0d3f\u0d15 \u0d17\u0d3f\u0d30\u0d3f <br/> \u0d24\u0d28\u0d2f\u0d3e \u0d2e\u0d41\u0d26\u0d2e\u0d41\u0d24\u0d4b \u0d35\u0d1a\u0d4d\u0d1a\u0d3f \u0d15\u0d4b\u0d30\u0d3f\u0d24\u0d3f <br/> \u0d28\u0d3e \u0d35\u0d46\u0d24\u0d32\u0d28\u0d41 \u0d26\u0d40\u0d30\u0d4d\u0d1a\u0d35\u0d47 \u0d2e\u0d3e\u0d15\u0d2d\u0d2f <br/> \u0d26\u0d3e\u0d28\u0d2e\u0d40\u0d2f\u0d35\u0d47 \u0d24\u0d3e\u0d2e\u0d38\u0d2e\u0d41 \u0d38\u0d47\u0d2f\u0d15\u0d28\u0d47"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u094d\u0930\u0940 \u0915\u093e\u092e\u093e\u0915\u094d\u0937\u0940 \u0915\u093e\u0935\u0935\u0947 \u0928\u0928\u0941 \u0915\u0930\u0941\u0923\u093e \u0