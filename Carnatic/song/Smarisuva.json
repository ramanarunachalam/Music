0c3f\u0c28\u0c4d\u0c28\u0c28\u0c41"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0ccd\u0cae\u0cb0\u0cbf\u0cb8\u0cc1\u0cb5 \u0c9c\u0ca8\u0c95\u0cc6\u0cb2\u0ccd\u0cb2 \u0cad\u0cb5\u0cad\u0caf \u0caa\u0cb0\u0cbf\u0ca4\u0cbe\u0caa\u0c97\u0cb3\u0cbf\u0cb2\u0ccd\u0cb2"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0cb0\u0ca3\u0cbe\u0c97\u0ca4 \u0c9c\u0ca8 \u0cb5\u0ca4\u0ccd\u0cb8\u0cb2\u0ca8\u0cc6\u0ca8\u0cbf\u0cb8\u0cbf\u0ca6 \u0c95\u0cb0\u0cbf\u0c97\u0cbf\u0cb0\u0cbf\u0ca6\u0cc1\u0cb0\u0ccd\u0c97\u0ca6 \u0ca8\u0cb0\u0cb9\u0cb0\u0cbf \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0ca8\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0caa\u0cc2\u0cb0\u0ccd\u0cb5 \u0cb8\u0cc1\u0c95\u0ccd\u0cb0\u0ccd\u0ca4\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6 \u0cb8\u0cc1\u0c9c\u0ca8\u0c95\u0cc6 \u0ca4\u0ccb\u0cb0\u0ccd\u0cb5\u0cc6 \u0ca8\u0cc0 \u0cae\u0cc1\u0ca6\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6\u0cb8\u0cb0\u0ccd\u0cb5 <br/> \u0c95\u0cbe\u0cb2\u0ca6\u0cca\u0cb3\u0cc1 \u0cae\u0cb9\u0ca6\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6\u0ca6\u0cbf \u0ca6\u0cc1\u0cb0\u0ccd\u0cb5\u0cbe\u0cb8\u0cb0\u0cc1 \u0caa\u0cc2\u0c9c\u0cbf\u0caa \u0cae\u0cc2\u0cb0\u0cc1\u0ca4\u0cbf \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c95\u0cbe\u0cb6\u0cbf \u0ca4\u0cc0\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6 \u0c9a\u0c95\u0ccd\u0cb0 \u0caa\u0cb0\u0cbe\u0cb6\u0cb0 \u0ca4\u0cc0\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6 \u0cb2\u0cc7\u0cb8\u0cbe\u0ca6 <br/> \u0cae\u0cbe\u0cb0\u0cc1\u0ca4 \u0caa\u0cbe\u0ca6\u0ca4\u0cc0\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4 \u0ca8\u0cb0 \u0c95\u0cc7\u0cb8\u0cb0\u0cbf \u0ca4\u0cc0\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0ca6\u0cbf\u0ca8\u0ccd\u0ca6\u0cca\u0caa\u0ccd\u0caa\u0cc1\u0cb5 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0ca8\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0ca8\u0cc0\u0ca8\u0cbf\u0cb9 \u0c95\u0ccd\u0cb7\u0cc7\u0ca4\u0ccd\u0cb0\u0ca6\u0cca\u0cb3\u0cc1 \u0cb8\u0cc7\u0cb0\u0cbf\u0cb9 \u0cb5\u0cbe\u0ca8\u0cb0 \u0caa\u0c95\u0ccd\u0cb7\u0cbf\u0c97\u0ccd\u0cb3\u0cc1 \u0cae\u0ca8\u0cb5\u0cbe\u0ca6\u0cbf <br/> \u0cac\u0cb9\u0cc1\u0caa\u0ccd\u0cb0\u0cbe\u0ca3\u0cbf \u0cb8\u0cae\u0cc2\u0cb9\u0c97\u0cc6 \u0c8f\u0ca8\u0cc1 \u0caa\u0cc1\u0ca3\u0ccd\u0caf \u0cac\u0cb2\u0cb5\u0cbf\u0cb0\u0cc1\u0cb5\u0cc1\u0ca6\u0cca \u0ca4\u0cbf\u0cb2\u0cc0\u0caf\u0ca6\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c85\u0ca4\u0ccd\u0ca4\u0cbf\u0caf \u0cae\u0cb0\u0ca6\u0cb2\u0ccd\u0cb2\u0cbf \u0c95\u0cbe\u0c87\u0c97\u0cb3\u0cc1 \u0cb9\u0ca4\u0ccd\u0ca4\u0cbf\u0cb0\u0cc1\u0cb5\u0ca8\u0ccd\u0ca6\u0ca6\u0cb2\u0cbf \u0cae\u0cca\u0ca4\u0ccd\u0ca4\u0cae\u0cca\u0ca4\u0ccd\u0ca4\u0cb5\u0cbe\u0ca6 <br/> \u0cac\u0ccd\u0cb0\u0cb9\u0ccd\u0cae\u0cbe\u0ca3\u0ccd\u0ca1 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cca\u0cb3\u0cc1 \u0cb9\u0ca4\u0ccd\u0ca4\u0cbf\u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cbf\u0caa\u0ccd\u0caa\u0cc1\u0ca6\u0ca8\u0ca8\u0ccd\u0ca4 \u0c95\u0ccb\u0c9f\u0cbf\u0caf\u0cc6\u0ca8\u0ccd\u0ca6\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0c85\u0ca1\u0cb5\u0cbf\u0caf \u0cae\u0ccd\u0cb0\u0ccd\u0c97\u0c97\u0cb3\u0cbf\u0c97\u0cc6 \u0cac\u0cb9\u0cc1 \u0cac\u0c97\u0cc6 \u0c97\u0cbf\u0ca1\u0cb2\u0ca6\u0cc6 \u0cae\u0cb0\u0c97\u0cb3\u0cbf\u0c97\u0cc6 \u0c92\u0ca1\u0cc6\u0caf <br/> \u0ca8\u0cc0\u0ca8\u0cb2\u0ccd\u0cb2\u0ca6\u0cc6 \u0c85\u0ca8\u0ccd\u0ca8\u0ccb\u0ca6\u0c95\u0c97\u0cb3 \u0c95\u0cca\u0ca1\u0cc1\u0cb5\u0cb0\u0cc6 \u0cae\u0ca8\u0cc1\u0c9c\u0cb0\u0cc1 \u0ca8\u0cc0 \u0c97\u0ca4\u0cbf \u0cb8\u0cb2\u0cb9\u0cc6\u0ca8\u0ccd\u0ca6\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0c95\u0ccd\u0cb7\u0ca4\u0ccd\u0cb0 \u0ca6\u0cc8\u0ca4\u0ccd\u0caf\u0cb0 \u0c97\u0ca3\u0ccd\u0ca1 \u0c95\u0cb0\u0cc1\u0ca3 \u0cb8\u0cae\u0cc1\u0ca6\u0ccd\u0cb0 \u0ca4\u0cbf\u0cae\u0cbf\u0cb0\u0c9a\u0ca3\u0ccd\u0ca1 \u0cb0\u0ccc\u0ca6\u0ccd\u0cb0\u0ca6\u0cbf <br/> \u0c85\u0cb8\u0cc1\u0cb0\u0cb0 \u0cae\u0cb0\u0ccd\u0ca6\u0cbf\u0cb8\u0cbf \u0cb2\u0ccb\u0c95\u0ccb\u0caa\u0ca6\u0ccd\u0cb0\u0cb5 \u0cac\u0cbf\u0ca1\u0cbf\u0cb8\u0cbf\u0ca6 \u0cb0\u0cc1\u0ca6\u0ccd\u0cb0\u0cbe\u0ca8\u0ccd\u0ca4\u0cb0\u0ccd\u0c97\u0ca4 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 7", "T": "\u0cad\u0cc2\u0ca4 \u0c97\u0ccd\u0cb0\u0cb9\u0cbe\u0ca6\u0cbf\u0c97\u0cb3\u0cc1 \u0cb8\u0ccb\u0c95\u0cbf\u0ca6 \u0cad\u0cc0\u0ca4\u0cbf \u0c9c\u0ccd\u0cb5\u0cb0\u0cbe\u0ca6\u0cbf\u0c97\u0cb3\u0cc1 \u0c9a\u0cc7\u0ca4\u0ccb <br/> \u0cae\u0cc1\u0c96 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8 \u0ca8\u0ccb\u0ca1\u0cb2\u0cc1 \u0c87\u0ca8\u0ccd\u0ca8\u0cbe \u0caa\u0cbe\u0ca6\u0c95\u0cb5\u0cb3\u0cbf\u0ca6\u0cc1 \u0caf\u0cbe\u0ca4\u0ca8\u0cc6 \u0c95\u0cb3\u0cc6\u0cb5\u0cc1\u0ca6\u0cc1"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 8", "T": "\u0cb2\u0c95\u0ccd\u0cb7\u0ccd\u0cae\u0cc0 \u0cb8\u0cb9\u0cbf\u0ca4\u0cb5\u0cbe\u0c97\u0cbf \u0cad\u0c95\u0ccd\u0ca4\u0cb0 \u0cb0\u0c95\u0ccd\u0cb7\u0cbf\u0cb8\u0cc1\u0cb5\u0cc1\u0ca6\u0c95\u0cbe\u0c97\u0cbf \u0cb2\u0c95\u0ccd\u0cb7\u0ca3 <br/> \u0caf\u0cc1\u0c95\u0ccd\u0ca4\u0ca6\u0cbf \u0cac\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca6\u0cc6 \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb2\u0c95\u0ccd\u0cb7\u0cc1\u0cae\u0cbf\u0ca8\u0cb0\u0cb8\u0cbf\u0cae\u0ccd\u0cb9 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8 \u0c9a\u0cb0\u0cbf\u0ca4\u0ccd\u0cb0\u0cc6\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 9", "T": "\u0c95\u0cb0\u0cbf\u0c97\u0cbf\u0cb0\u0cbf \u0cb8\u0cb9\u0cbf\u0ca4\u0cb5\u0cbe\u0c97\u0cbf \u0cad\u0c95\u0ccd\u0ca4\u0cb0 \u0ca6\u0cc1\u0cb0\u0cbf\u0ca4\u0ca6\u0ca8\u0ccd\u0ca4\u0cbf\u0cb8\u0cbf\u0cae\u0ccd\u0cb9 \u0caa\u0cb0\u0cbf\u0caa\u0cbe\u0cb2\u0cbf\u0cb8\u0cc1 <br/> \u0ca8\u0cbe \u0cae\u0cb0\u0cc6\u0cb9\u0cca\u0c95\u0ccd\u0c95\u0cc6\u0ca8\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8 \u0c97\u0cc1\u0cb0\u0cc1 \u0caa\u0cc1\u0cb0\u0ca8\u0ccd\u0ca6\u0cb0 \u0cb5\u0cbf\u0c9f\u0ccd\u0c9f\u0cb2 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0ca8\u0cc1"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d4d\u0d2e\u0d30\u0d3f\u0d38\u0d41\u0d35 \u0d1c\u0d28\u0d15\u0d46\u0d32\u0d4d\u0d32 \u0d2d\u0d35\u0d2d\u0d2f \u0d2a\u0d30\u0d3f\u0d24\u0d3e\u0d2a\u0d17\u0d33\u0d3f\u0d32\u0d4d\u0d32"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d30\u0d23\u0d3e\u0d17\u0d24 \u0d1c\u0d28 \u0d35\u0d24\u0d4d\u0d38\u0d32\u0d28\u0d46\u0d28\u0d3f\u0d38\u0d3f\u0d26 \u0d15\u0d30\u0d3f\u0d17\u0d3f\u0d30\u0d3f\u0d26\u0d41\u0d30\u0d4d\u0d17\u0d26 \u0d28\u0d30\u0d39\u0d30\u0d3f \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d28\u0d41"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d2a\u0d42\u0d30\u0d4d\u0d35 \u0d38\u0d41\u0d15\u0d4d\u0d30\u0d4d\u0d24\u0d26\u0d3f\u0d28\u0d4d\u0d26 \u0d38\u0d41\u0d1c\u0d28\u0d15\u0d46 \u0d24\u0d4b\u0d30\u0d4d\u0d35\u0d46 \u0d28\u0d40 \u0d2e\u0d41\u0d26\u0d26\u0d3f\u0d28\u0d4d\u0d26\u0d38\u0d30\u0d4d\u0d35 <br/> \u0d15\u0d3e\u0d32\u0d26\u0d4a\u0d33\u0d41 \u0d2e\u0d39\u0d26\u0d3e\u0d28\u0d28\u0d4d\u0d26\u0d26\u0d3f \u0d26\u0d41\u0d30\u0d4d\u0d35\u0d3e\u0d38\u0d30\u0d41 \u0d2a\u0d42\u0d1c\u0d3f\u0d2a \u0d2e\u0d42\u0d30\u0d41\u0d24\u0d3f \u0d28\u0d3f\u0d28\u0d4d\u0d28"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d15\u0d3e\u0d36\u0d3f \u0d24\u0d40\u0d30\u0d4d\u0d24\u0d4d\u0d24\u0d26\u0d3f\u0d28\u0d4d\u0d26 \u0d1a\u0d15\u0d4d\u0d30 \u0d2a\u0d30\u0d3e\u0d36\u0d30 \u0d24\u0d40\u0d30\u0d4d\u0d24\u0d4d\u0d24\u0d26\u0d3f\u0d28\u0d4d\u0d26 \u0d32\u0d47\u0d38\u0d3e\u0d26 <br/> \u0d2e\u0d3e\u0d30\u0d41\u0d24 \u0d2a\u0d3e\u0d26\u0d24\u0d40\u0d30\u0d4d\u0d24\u0d4d\u0d24 \u0d28\u0d30 \u0d15\u0d47\u0d38\u0d30\u0d3f \u0d24\u0d40\u0d30\u0d4d\u0d24\u0d4d\u0d24\u0d26\u0d3f\u0d28\u0d4d\u0d26\u0d4a\u0d2a\u0d4d\u0d2a\u0d41\u0d35 \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d28\u0d41"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d28\u0d40\u0d28\u0d3f\u0d39 \u0d15\u0d4d\u0d37\u0d47\u0d24\u0d4d\u0d30\u0d26\u0d4a\u0d33\u0d41 \u0d38\u0d47\u0d30\u0d3f\u0d39 \u0d35\u0d3e\u0d28\u0d30 \u0d2a\u0d15\u0d4d\u0d37\u0d3f\u0d17\u0d4d\u0d33\u0d41 \u0d2e\u0d28\u0d35\u0d3e\u0d26\u0d3f <br/> \u0d2c\u0d39\u0d41\u0d2a\u0d4d\u0d30\u0d3e\u0d23\u0d3f \u0d38\u0d2e\u0d42\u0d39\u0d17\u0d46 \u0d0f\u0d28\u0d41 \u0d2a\u0d41\u0d23\u0d4d\u0d2f \u0d2c\u0d32\u0d35\u0d3f\u0d30\u0d41\u0d35\u0d41\u0d26\u0d4a \u0d24\u0d3f\u0d32\u0d40\u0d2f\u0d26\u0d41"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d05\u0d24\u0d4d\u0d24\u0d3f\u0d2f \u0d2e\u0d30\u0d26\u0d32\u0d4d\u0d32\u0d3f \u0d15\u0d3e\u0d07\u0d17\u0d33\u0d41 \u0d39\u0d24\u0d4d\u0d24\u0d3f\u0d30\u0d41\u0d35\u0d28\u0d4d\u0d26\u0d26\u0d32\u0d3f \u0d2e\u0d4a\u0d24\u0d4d\u0d24\u0d2e\u0d4a\u0d24\u0d4d\u0d24\u0d35\u0d3e\u0d26 <br/> \u0d2c\u0d4d\u0d30\u0d39\u0d4d\u0d2e\u0d3e\u0d23\u0d4d\u0d21 \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d4a\u0d33\u0d41 \u0d39\u0d24\u0d4d\u0d24\u0d3f\u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3f\u0d2a\u0d4d\u0d2a\u0d41\u0d26\u0d28\u0d28\u0d4d\u0d24 \u0d15\u0d4b\u0d1f\u0d3f\u0d2f\u0d46\u0d28\u0d4d\u0d26\u0d41"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 5", "T": "\u0d05\u0d21\u0d35\u0d3f\u0d2f \u0d2e\u0d4d\u0d30\u0d4d\u0d17\u0d17\u0d33\u0d3f\u0d17\u0d46 \u0d2c\u0d39\u0d41 \u0d2c\u0d17\u0d46 \u0d17\u0d3f\u0d21\u0d32\u0d26\u0d46 \u0d2e\u0d30\u0d17\u0d33\u0d3f\u0d17\u0d46 \u0d12\u0d21\u0d46\u0d2f <br/> \u0d28\u0d40\u0d28\u0d32\u0d4d\u0d32\u0d26\u0d46 \u0d05\u0d28\u0d4d\u0d28\u0d4b\u0d26\u0d15\u0d17\u0d33 \u0d15\u0d4a\u0d21\u0d41\u0d35\u0d30\u0d46 \u0d2e\u0d28\u0d41\u0d1c\u0d30\u0d41 \u0d28\u0d40 \u0d17\u0d24\u0d3f \u0d38\u0d32\u0d39\u0d46\u0d28\u0d4d\u0d26\u0d41"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 6", "T": "\u0d15\u0d4d\u0d37\u0d24\u0d4d\u0d30 \u0d26\u0d48\u0d24\u0d4d\u0d2f\u0d30 \u0d17\u0d23\u0d4d\u0d21 \u0d15\u0d30\u0d41\u0d23 \u0d38\u0d2e\u0d41\u0d26\u0d4d\u0d30 \u0d24\u0d3f\u0d2e\u0d3f\u0d30\u0d1a\u0d23\u0d4d\u0d21 \u0d30\u0d4c\u0d26\u0d4d\u0d30\u0d26\u0d3f <br/> \u0d05\u0d38\u0d41\u0d30\u0d30 \u0d2e\u0d30\u0d4d\u0d26\u0d3f\u0d38\u0d3f \u0d32\u0d4b\u0d15\u0d4b\u0d2a\u0d26\u0d4d\u0d30\u0d35 \u0d2c\u0d3f\u0d21\u0d3f\u0d38\u0d3f\u0d26 \u0d30\u0d41\u0d26\u0d4d\u0d30\u0d3e\u0d28\u0d4d\u0d24\u0d30\u0d4d\u0d17\u0d24 \u0d28\u0d3f\u0d