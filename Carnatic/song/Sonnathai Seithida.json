u0c31\u0c48 \u0c15\u0c26\u0c26\u0c3f\u0c21\u0c32\u0c3e\u0c2e\u0c3e <br/> (\u0c30\u0c3e\u0c17\u0c3e: \u0c35\u0c28\u0c4d\u0c26\u0c28\u0c27\u0c3e\u0c30\u0c3f\u0c23\u0c3f)"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2e\u0c23\u0c4d\u0c23\u0c41\u0c2e\u0c4d \u0c2a\u0c4a\u0c28\u0c4d\u0c28\u0c41\u0c2e\u0c4d \u0c07\u0c28\u0c4d\u0c27 \u0c2e\u0c3e\u0c2f \u0c32\u0c4b\u0c17\u0c2e\u0c4d \u0c0e\u0c32\u0c4d\u0c32\u0c3e\u0c2e\u0c4d <br/> \u0c2e\u0c3e\u0c27\u0c35\u0c28\u0c4d \u0c2e\u0c2f\u0c2e\u0c3e\u0c17\u0c3f \u0c2e\u0c25\u0c3f\u0c2f\u0c2f\u0c4d \u0c2e\u0c2f\u0c15\u0c4d\u0c15\u0c41\u0c25\u0c21\u0c3f (\u0c38\u0c4a\u0c28\u0c4d\u0c28\u0c25\u0c48) <br/> (\u0c30\u0c3e\u0c17\u0c3e: \u0c35\u0c28\u0c4d\u0c26\u0c28\u0c27\u0c3e\u0c30\u0c3f\u0c23\u0c3f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c25\u0c28\u0c4d\u0c28\u0c3f\u0c32\u0c48 \u0c25\u0c21\u0c41\u0c2e\u0c3e\u0c31\u0c3f \u0c24\u0c35\u0c3f\u0c15\u0c4d\u0c15\u0c41\u0c31\u0c47\u0c28\u0c21\u0c3f <br/> \u0c15\u0c23\u0c4d\u0c23\u0c28\u0c4d \u0c15\u0c23\u0c4d\u0c23\u0c28\u0c4d \u0c0e\u0c28\u0c4d\u0c31\u0c41 \u0c15\u0c26\u0c30\u0c41\u0c17\u0c3f\u0c31\u0c47\u0c28\u0c21\u0c3f <br/> \u0c35\u0c46\u0c23\u0c4d\u0c23\u0c3f\u0c32\u0c35\u0c41 \u0c35\u0c3f\u0c34\u0c41\u0c2e\u0c4d \u0c35\u0c46\u0c1f\u0c4d\u0c1f \u0c35\u0c46\u0c33\u0c3f \u0c25\u0c28\u0c4d\u0c28\u0c3f\u0c32\u0c4d <br/> \u0c05\u0c28\u0c4d\u0c28\u0c35\u0c28\u0c46\u0c28\u0c15\u0c4a\u0c30\u0c41 \u0c06\u0c38\u0c48 \u0c25\u0c28\u0c4d\u0c26\u0c3e\u0c30\u0c21\u0c3f (\u0c38\u0c4a\u0c28\u0c4d\u0c28\u0c25\u0c48) <br/> (\u0c30\u0c3e\u0c17\u0c3e: \u0c1c\u0c2f\u0c28\u0c4d\u0c24\u0c36\u0c4d\u0c30\u0c40)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c15\u0c28\u0c4d\u0c28\u0c3f \u0c2e\u0c3e\u0c21\u0c2e\u0c4d \u0c25\u0c28\u0c4d\u0c28\u0c3f\u0c32\u0c4d \u0c15\u0c3e\u0c35\u0c32\u0c4d\u0c17\u0c33\u0c48 \u0c2e\u0c40\u0c31\u0c3f <br/> \u0c0e\u0c28\u0c4d\u0c28 \u0c25\u0c41\u0c23\u0c3f\u0c1b\u0c32\u0c41\u0c21\u0c28\u0c4d \u0c0e\u0c28\u0c4d\u0c28\u0c3f\u0c21\u0c2e\u0c4d \u0c35\u0c28\u0c4d\u0c26\u0c3e\u0c30\u0c21\u0c3f <br/> \u0c15\u0c23\u0c4d\u0c2e\u0c23\u0c3f\u0c2f\u0c47 \u0c09\u0c28\u0c4d\u0c28\u0c48 \u0c15\u0c3e\u0c23\u0c35\u0c47 \u0c35\u0c28\u0c4d\u0c26\u0c47\u0c28\u0c3f\u0c19\u0c4d\u0c17\u0c41 <br/> \u0c2e\u0c3f\u0c28\u0c4d\u0c28\u0c4a\u0c33\u0c3f\u0c2f\u0c47 \u0c09\u0c28\u0c4d\u0c27\u0c28\u0c4d \u0c2e\u0c47\u0c28\u0c3f \u0c0e\u0c28\u0c4d\u0c31\u0c3e\u0c30\u0c21\u0c3f (\u0c38\u0c4a\u0c28\u0c4d\u0c28\u0c25\u0c48) <br/> (\u0c30\u0c3e\u0c17\u0c3e: \u0c26\u0c47\u0c35\u0c2e\u0c28\u0c4b\u0c39\u0c30\u0c3f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c0e\u0c28\u0c4d\u0c28\u0c46\u0c28\u0c2e\u0c4a \u0c38\u0c4a\u0c32\u0c4d\u0c32\u0c3f \u0c0e\u0c28\u0c4d\u0c28\u0c48 \u0c2e\u0c2f\u0c15\u0c4d\u0c15\u0c3f \u0c35\u0c3f\u0c1f\u0c4d\u0c1f\u0c41 <br/> \u0c05\u0c28\u0c4d\u0c28\u0c2e\u0c4d \u0c2a\u0c3e\u0c32\u0c41\u0c2e\u0c4d \u0c35\u0c46\u0c31\u0c41\u0c15\u0c4d\u0c15 \u0c05\u0c32\u0c48\u0c2f \u0c35\u0c48\u0c24\u0c4d\u0c24\u0c3e\u0c30\u0c21\u0c3f <br/> \u0c2e\u0c28\u0c4d\u0c28\u0c35\u0c28\u0c4d\u0c27 \u0c35\u0c30\u0c4d\u0c15\u0c3f \u0c2e\u0c48\u0c28\u0c4d\u0c27\u0c28\u0c4d \u0c17\u0c4b\u0c2a\u0c3e\u0c32\u0c28\u0c4d <br/> \u0c15\u0c28\u0c4d\u0c28\u0c2e\u0c4d \u0c35\u0c48\u0c24\u0c4d\u0c25\u0c41 \u0c0e\u0c28\u0c4d\u0c27\u0c28\u0c4d \u0c15\u0c30\u0c41\u0c24\u0c4d\u0c25\u0c3f\u0c32\u0c4d \u0c2a\u0c41\u0c17\u0c41\u0c28\u0c4d\u0c25\u0c3e\u0c30\u0c21\u0c3f (\u0c38\u0c4a\u0c28\u0c4d\u0c28\u0c25\u0c48) <br/> (\u0c30\u0c3e\u0c17\u0c3e: \u0c2e\u0c3e\u0c23\u0c4d\u0c21\u0c4d)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0cca\u0ca8\u0ccd\u0ca8\u0ca5\u0cc8 \u0cb8\u0cc6\u0caf\u0ccd\u0ca5\u0cbf\u0ca1 \u0cb8\u0cbe\u0cb9\u0cb8\u0cae\u0cbe \u0c95\u0cbf\u0cb3\u0cbf\u0caf\u0cc7 <br/> \u0c95\u0ca3\u0ccd\u0ca3\u0ca8\u0cc6\u0ca8\u0cc1\u0cae\u0ccd \u0cae\u0cc1\u0cb1\u0cc8 \u0c95\u0ca6\u0ca6\u0cbf\u0ca1\u0cb2\u0cbe\u0cae\u0cbe <br/> (\u0cb0\u0cbe\u0c97\u0cbe: \u0cb5\u0ca8\u0ccd\u0ca6\u0ca8\u0ca7\u0cbe\u0cb0\u0cbf\u0ca3\u0cbf)"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0ca3\u0ccd\u0ca3\u0cc1\u0cae\u0ccd \u0caa\u0cca\u0ca8\u0ccd\u0ca8\u0cc1\u0cae\u0ccd \u0c87\u0ca8\u0ccd\u0ca7 \u0cae\u0cbe\u0caf \u0cb2\u0ccb\u0c97\u0cae\u0ccd \u0c8e\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd <br/> \u0cae\u0cbe\u0ca7\u0cb5\u0ca8\u0ccd \u0cae\u0caf\u0cae\u0cbe\u0c97\u0cbf \u0cae\u0ca5\u0cbf\u0caf\u0caf\u0ccd \u0cae\u0caf\u0c95\u0ccd\u0c95\u0cc1\u0ca5\u0ca1\u0cbf (\u0cb8\u0cca\u0ca8\u0ccd\u0ca8\u0ca5\u0cc8) <br/> (\u0cb0\u0cbe\u0c97\u0cbe: \u0cb5\u0ca8\u0ccd\u0ca6\u0ca8\u0ca7\u0cbe\u0cb0\u0cbf\u0ca3\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca5\u0ca8\u0ccd\u0ca8\u0cbf\u0cb2\u0cc8 \u0ca5\u0ca1\u0cc1\u0cae\u0cbe\u0cb1\u0cbf \u0ca4\u0cb5\u0cbf\u0c95\u0ccd\u0c95\u0cc1\u0cb1\u0cc7\u0ca8\u0ca1\u0cbf <br/> \u0c95\u0ca3\u0ccd\u0ca3\u0ca8\u0ccd \u0c95\u0ca3\u0ccd\u0ca3\u0ca8\u0ccd \u0c8e\u0ca8\u0ccd\u0cb1\u0cc1 \u0c95\u0ca6\u0cb0\u0cc1\u0c97\u0cbf\u0cb1\u0cc7\u0ca8\u0ca1\u0cbf <br/> \u0cb5\u0cc6\u0ca3\u0ccd\u0ca3\u0cbf\u0cb2\u0cb5\u0cc1 \u0cb5\u0cbf\u0cb3\u0cc1\u0cae\u0ccd \u0cb5\u0cc6\u0c9f\u0ccd\u0c9f \u0cb5\u0cc6\u0cb3\u0cbf \u0ca5\u0ca8\u0ccd\u0ca8\u0cbf\u0cb2\u0ccd <br/> \u0c85\u0ca8\u0ccd\u0ca8\u0cb5\u0ca8\u0cc6\u0ca8\u0c95\u0cca\u0cb0\u0cc1 \u0c86\u0cb8\u0cc8 \u0ca5\u0ca8\u0ccd\u0ca6\u0cbe\u0cb0\u0ca1\u0cbf (\u0cb8\u0cca\u0ca8\u0ccd\u0ca8\u0ca5\u0cc8) <br/> (\u0cb0\u0cbe\u0c97\u0cbe: \u0c9c\u0caf\u0ca8\u0ccd\u0ca4\u0cb6\u0ccd\u0cb0\u0cc0)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c95\u0ca8\u0ccd\u0ca8\u0cbf \u0cae\u0cbe\u0ca1\u0cae\u0ccd \u0ca5\u0ca8\u0ccd\u0ca8\u0cbf\u0cb2\u0ccd \u0c95\u0cbe\u0cb5\u0cb2\u0ccd\u0c97\u0cb3\u0cc8 \u0cae\u0cc0\u0cb1\u0cbf <br/> \u0c8e\u0ca8\u0ccd\u0ca8 \u0ca5\u0cc1\u0ca3\u0cbf\u0c9b\u0cb2\u0cc1\u0ca1\u0ca8\u0ccd \u0c8e\u0ca8\u0ccd\u0ca8\u0cbf\u0ca1\u0cae\u0ccd \u0cb5\u0ca8\u0ccd\u0ca6\u0cbe\u0cb0\u0ca1\u0cbf <br/> \u0c95\u0ca3\u0ccd\u0cae\u0ca3\u0cbf\u0caf\u0cc7 \u0c89\u0ca8\u0ccd\u0ca8\u0cc8 \u0c95\u0cbe\u0ca3\u0cb5\u0cc7 \u0cb5\u0ca8\u0ccd\u0ca6\u0cc7\u0ca8\u0cbf\u0c99\u0ccd\u0c97\u0cc1 <br/> \u0cae\u0cbf\u0ca8\u0ccd\u0ca8\u0cca\u0cb3\u0cbf\u0caf\u0cc7 \u0c89\u0ca8\u0ccd\u0ca7\u0ca8\u0ccd \u0cae\u0cc7\u0ca8\u0cbf \u0c8e\u0ca8\u0ccd\u0cb1\u0cbe\u0cb0\u0ca1\u0cbf (\u0cb8\u0cca\u0ca8\u0ccd\u0ca8\u0ca5\u0cc8) <br/> (\u0cb0\u0cbe\u0c97\u0cbe: \u0ca6\u0cc7\u0cb5\u0cae\u0ca8\u0ccb\u0cb9\u0cb0\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c8e\u0ca8\u0ccd\u0ca8\u0cc6\u0ca8\u0cae\u0cca \u0cb8\u0cca\u0cb2\u0ccd\u0cb2\u0cbf \u0c8e\u0ca8\u0ccd\u0ca8\u0cc8 \u0cae\u0caf\u0c95\u0ccd\u0c95\u0cbf \u0cb5\u0cbf\u0c9f\u0ccd\u0c9f\u0cc1 <br/> \u0c85\u0ca8\u0ccd\u0ca8\u0cae\u0ccd \u0caa\u0cbe\u0cb2\u0cc1\u0cae\u0ccd \u0cb5\u0cc6\u0cb1\u0cc1\u0c95\u0ccd\u0c95 \u0c85\u0cb2\u0cc8\u0caf \u0cb5\u0cc8\u0ca4\u0ccd\u0ca4\u0cbe\u0cb0\u0ca1\u0cbf <br/> \u0cae\u0ca8\u0ccd\u0ca8\u0cb5\u0ca8\u0ccd\u0ca7 \u0cb5\u0cb0\u0ccd\u0c95\u0cbf \u0cae\u0cc8\u0ca8\u0ccd\u0ca7\u0ca8\u0ccd \u0c97\u0ccb\u0caa\u0cbe\u0cb2\u0ca8\u0ccd <br/> \u0c95\u0ca8\u0ccd\u0ca8\u0cae\u0ccd \u0cb5\u0cc8\u0ca4\u0ccd\u0ca5\u0cc1 \u0c8e\u0ca8\u0ccd\u0ca7\u0ca8\u0ccd \u0c95\u0cb0\u0cc1\u0ca4\u0ccd\u0ca5\u0cbf\u0cb2\u0ccd \u0caa\u0cc1\u0c97\u0cc1\u0ca8\u0ccd\u0ca5\u0cbe\u0cb0\u0ca1\u0cbf (\u0cb8\u0cca\u0ca8\u0ccd\u0ca8\u0ca5\u0cc8) <br/> (\u0cb0\u0cbe\u0c97\u0cbe: \u0cae\u0cbe\u0ca3\u0ccd\u0ca1\u0ccd)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d4a\u0d28\u0d4d\u0d28\u0d25\u0d48 \u0d38\u0d46\u0d2f\u0d4d\u0d25\u0d3f\u0d21 \u0d38\u0d3e\u0d39\u0d38\u0d2e\u0d3e \u0d15\u0d3f\u0d33\u0d3f\u0d2f\u0d47 <br/> \u0d15\u0d23\u0d4d\u0d23\u0d28\u0d46\u0d28\u0d41\u0d2e\u0d4d \u0d2e\u0d41\u0d31\u0d48 \u0d15\u0d26\u0d26\u0d3f\u0d21\u0d32\u0d3e\u0d2e\u0d3e <br/> (\u0d30\u0d3e\u0d17\u0d3e: \u0d35\u0d28\u0d4d\u0d26\u0d28\u0d27\u0d3e\u0d30\u0d3f\u0d23\u0d3f)"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d23\u0d4d\u0d23\u0d41\u0d2e\u0d4d \u0d2a\u0d4a\u0d28\u0d4d\u0d28\u0d41\u0d2e\u0d4d \u0d07\u0d28\u0d4d\u0d27 \u0d2e\u0d3e\u0d2f \u0d32\u0d4b\u0d17\u0d2e\u0d4d \u0d0e\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d <br/> \u0d2e\u0d3e\u0d27\u0d35\u0d28\u0d4d \u0d2e\u0d2f\u0d2e\u0d3e\u0d17\u0d3f \u0d2e\u0d25\u0d3f\u0d2f\u0d2f\u0d4d \u0d2e\u0d2f\u0d15\u0d4d\u0d15\u0d41\u0d25\u0d21\u0d3f (\u0d38\u0d4a\u0d28\u0d4d\u0d28\u0d25\u0d48) <br/> (\u0d30\u0d3e\u0d17\u0d3e: \u0d35\u0d28\u0d4d\u0d26\u0d28\u0d27\u0d3e\u0d30\u0d3f\u0d23\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d25\u0d28\u0d4d\u0d28\u0d3f\u0d32\u0d48 \u0d25\u0d21\u0d41\u0d2e\u0d3e\u0d31\u0d3f \u0d24\u0d35\u0d3f\u0d15\u0d4d\u0d15\u0d41\u0d31\u0d47\u0d28\u0d21\u0d3f <br/> \u0d15\u0d23\u0d4d\u0d23\u0d28\u0d4d \u0d15\u0d23\u0d4d\u0d23\u0d28\u0d4d \u0d0e\u0d28\u0d4d\u0d31\u0d41 \u0d15\u0d26\u0d30\u0d41\u0d17\u0d3f\u0d31\u0d47\u0d28\u0d21\u0d3f <br/> \u0d35\u0d46\u0d23\u0d4d\u0d23\u0d3f\u0d32\u0d35\u0d41 \u0d35\u0d3f\u0d34\u0d41\u0d2e\u0d4d \u0d35\u0d46\u0d1f\u0d4d\u0d1f \u0d35\u0d46\u0d33\u0d3f \u0d25\u0d28\u0d4d\u0d28\u0d3f\u0d32\u0d4d <br/> \u0d05\u0d28\u0d4d\u0d28\u0d35\u0d28\u0d46\u0d28\u0d15\u0d4a\u0d30\u0d41 \u0d06\u0d38\u0d48 \u0d25\u0d28\u0d4d\u0d26\u0d3e\u0d30\u0d21\u0d3f (\u0d38\u0d4a\u0d28\u0d4d\u0d28\u0d25\u0d48) <br/> (\u0d30\u0d3e\u0d17\u0d3e: \u0d1c\u0d2f\u0d28\u0d4d\u0d24\u0d36\u0d4d\u0d30\u0d40)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d15\u0d28\u0d4d\u0d28\u0d3f \u0d2e\u0d3e\u0d21\u0d2e\u0d4d \u0d25\u0d28\u0d4d\u0d28\u0d3f\u0d32\u0d4d \u0d15\u0d3e\u0d35\u0d32\u0d4d\u0d17\u0d33\u0d48 \u0d2e\u0d40\u0d31\u0d3f <br/> \u0d0e\u0d28\u0d4d\u0d28 \u0d25\u0d41\u0d23\u0d3f\u0d1b\u0d32\u0d41\u0d21\u0d28\u0d4d \u0d0e\u0d28\u0d4d\u0d28\u0d3f\u0d21\u0d2e\u0d4d \u0d35\u0d28\u0d4d\u0d26\u0d3e\u0d30\u0d21\u0d3f <br/> \u0d15\u0d23\u0d4d\u0d2e\u0d23\u0d3f\u0d2f\u0d47 \u0d09\u0d28\u0d4d\u0d28\u0d48 \u0d15\u0d3e\u0d23\u0d35\u0d47 \u0d35\u0d28\u0d4d\u0d26\u0d47\u0d28\u0d3f\u0d19\u0d4d\u0d17\u0d41 <br/> \u0d2e\u0d3f\u0d28\u0d4d\u0d28\u0d4a\u0d33\u0d3f\u0d2f\u0d47 \u0d09\u0d28\u0d4d\u0d27\u0d28\u0d4d \u0d2e\u0d47\u0d28\u0d3f \u0d0e\u0d28\u0d4d\u0d31\u0d3e\u0d30\u0d21\u0d3f (\u0d38\u0d4a\u0d28\u0d4d\u0d28\u0d25\u0d48) <br/> (\u0d30\u0d3e\u0d17\u0d3e: \u0d26\u0d47\u0d35\u0d2e\u0d28\u0d4b\u0d39\u0d30\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d0e\u0d28\u0d4d\u0d28\u0d46\u0d28\u0d2e\u0d4a \u0d38\u0d4a\u0d32\u0d4d\u0d32\u0d3f \u0d0e\u0d28\u0d4d\u0d28\u0d48 \u0d2e\u0d2f\u0d15\u0d4d\u0d15\u0d3f \u0d35\u0d3f\u0d1f\u0d4d\u0d1f\u0d41 <br/> \u0d05\u0d28\u0d4d\u0d28\u0d2e\u0d4d \u0d2a\u0d3e\u0d32\u0d41\u0d2e\u0d4d \u0d35\u0d46\u0d31\u0d41\u0d15\u0d4d\u0d15 \u0d05\u0d32\u0d48\u0d2f \u0d35\u0d48\u0d24\u0d4d\u0d24\u0d3e\u0d30\u0d21\u0d3f <br/> \u0d2e\u0d28\u0d4d\u0d28\u0d35\u0d28\u0d4d\u0d27 \u0d35\u0d30\u0d4d\u0d15\u0d3f \u0d2e\u0d48\u0d28\u0d4d\u0d27\u0d28\u0d4d \u0d17\u0d4b\u0d2a\u0d3e\u0d32\u0d28\u0d4d <br/> \u0d15\u0d28\u0d4d\u0d28\u0d2e\u0d4d \u0d35\u0d48\u0d24\u0d4d\u0d25\u0d41 \u0d0e\u0d28\u0d4d\u0d27\u0d28\u0d4d \u0d15\u0d30\u0d41\u0d24\u0d4d\u0d25\u0d3f\u0d32\u0d4d \u0d2a\u0d41\u0d17\u0d41\u0d28\u0d4d\u0d25\u0d3e\u0d30\u0d21\u0d3f (\u0d38\u0d4a\u0d28\u0d4d\u0d28\u0d25\u0d48) <br/> (\u0d30\u0d3e\u0d17\u0d3e: \u0d2e\u0d3e\u0d23\u0d4d\u0d21\u0d4d)"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0938\u094b\u0928\u094d\u0928\u0925\u0948 \u0938\u0947\u092f\u094d\u0925\u093f\u0921 \u0938\u093e\u0939\u0938\u092e\u093e \u0915\u093f\u0932\u093f\u092f\u0947 <br/> \u0915\u0923\u094d\u0923\u0928\u0947\u0928\u0941\u092e\u094d \u092e\u0941\u0930\u0948 \u0915\u0926\u0926\u093f\u0921\u0932\u093e\u092e\u093e <br/> (\u0930\u093e\u0917\u093e: \u0935\u0928\u094d\u0926\u0928\u0927\u093e\u0930\u093f\u0923\u093f)"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092e\u0923\u094d\u0923\u0941\u092e\u094d \u092a\u094b\u0928\u094d\u0928\u0941\u092e\u094d \u0907\u0928\u094d\u0927 \u092e\u093e\u092f \u0932\u094b\u0917\u092e\u094d \u090f\u0932\u094d\u0932\u093e\u092e\u094d <br/> \u092e\u093e\u0927\u0935\u0928\u094d \u092e\u092f\u092e\u093e\u0917\u093f \u092e\u0925\u093f\u092f\u092f\u094d \u092e\u092f\u0915\u094d\u0915\u0941\u0925\u0921\u093f (\u0938\u094b\u0928\u094d\u0928\u0925\u0948) <br/> (\u0930\u093e\u0917\u093e: \u0935\u0928\u094d\u0926\u0928\u0927\u093e\u0930\u093f\u0923\u093f)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0925\u0928\u094d\u0928\u093f\u0932\u0948 \u0925\u0921\u0941\u092e\u093e\u0930\u093f \u0924\u0935\u093f\u0915\u094d\u0915\u0941\u0930\u0947\u0928\u0921\u093f <br/> \u0915\u0923\u094d\u0923\u0928\u094d \u0915\u0923\u094d\u0923\u0928\u094d \u090f\u0928\u094d\u0930\u0941 \u0915\u0926\u0930\u0941\u0917\u093f\u0930\u0947\u0928\u0921\u093f <br/> \u0935\u0947\u0923\u094d\u0923\u093f\u0932\u0935\u0941 \u0935\u093f\u0932\u0941\u092e\u094d \u0935\u0947\u091f\u094d\u091f \u0935\u0947\u0932\u093f \u0925\u0928\u094d\u0928\u093f\u0932\u094d <br/> \u0905\u0928\u094d\u0928\u0935\u0928\u0947\u0928\u0915\u094b\u0930\u0941 \u0906\u0938\u0948 \u0925\u0928\u094d\u0926\u093e\u0930\u0921\u093f (\u0938\u094b\u0928\u094d\u0928\u0925\u0948) <br/> (\u0930\u093e\u0917\u093e: \u091c\u092f\u0928\u094d\u0924\u0936\u094d\u0930\u0940)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u0915\u0928\u094d\u0928\u093f \u092e\u093e\u0921\u092e\u094d \u0925\u0928\u094d\u0928\u093f\u0932\u094d \u0915\u093e\u0935\u0932\u094d\u0917\u0932\u0948 \u092e\u0940\u0930\u093f <br/> \u090f\u0928\u094d\u0928 \u0925\u0941\u0923\u093f\u091b\u0932\u0941\u0921\u0928\u094d \u090f\u0928\u094d\u0928\u093f\u0921\u092e\u094d \u0935\u0928\u094d\u0926\u093e\u0930\u0921\u093f <br/> \u0915\u0923\u094d\u092e\u0923\u093f\u092f\u0947 \u0909\u0928\u094d\u0928\u0948 \u0915\u093e\u0923\u0935\u0947 \u0935\u0928\u094d\u0926\u0947\u0928\u093f\u0919\u094d\u0917\u0941 <br/> \u092e\u093f\u0928\u094d\u0928\u094b\u0932\u093f\u092f\u0947 \u0909\u0928\u094d\u0927\u0928\u094d \u092e\u0947\u0928\u093f \u090f\u0928\u094d\u0930\u093e\u0930\u0921\u093f (\u0938\u094b\u0928\u094d\u0928\u0925\u094