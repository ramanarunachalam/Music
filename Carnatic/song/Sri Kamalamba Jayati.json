u0bbf\u0baa\u0bc1\u0bb0 \u0b9a\u0bc1\u0ba8\u0bcd\u0ba4\u0bb0\u0bc0\u0bae\u0bcd <br/> \u0bb0\u0bbe\u0b9c \u0bb0\u0bbe\u0b9c\u0bc7\u0bb6\u0bcd\u0bb5\u0bb0\u0bc0\u0bae\u0bcd \u0bb6\u0bcd\u0bb0\u0bc0-\u0b95\u0bb0 \u0b9a\u0bb0\u0bcd\u0bb5\u0bbe\u0ba9\u0ba8\u0bcd\u0ba4-\u0bae\u0baf - <br/> \u0b9a\u0b95\u0bcd\u0bb0 \u0bb5\u0bbe\u0b9a\u0bbf\u0ba9\u0bc0\u0bae\u0bcd \u0b9a\u0bc1\u0bb5\u0bbe\u0b9a\u0bbf\u0ba9\u0bc0\u0bae\u0bcd \u0b9a\u0bbf\u0ba8\u0bcd\u0ba4\u0baf\u0bc7\u0b85\u0bb9\u0bae\u0bcd"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0ba4\u0bbf\u0bb5\u0bbe\u0b95\u0bb0 \u0b9a\u0bc0\u0ba4 \u0b95\u0bbf\u0bb0\u0ba3 \u0baa\u0bbe\u0bb5\u0b95\u0bbe\u0ba4\u0bbf \u0bb5\u0bbf\u0b95\u0bbe\u0b9a-\u0b95\u0bb0\u0baf\u0bbe <br/> \u0baa\u0bc0-\u0b95\u0bb0 \u0ba4\u0bbe\u0baa-\u0ba4\u0bcd\u0bb0\u0baf\u0bbe\u0ba4\u0bbf \u0baa\u0bc7\u0ba4\u0ba9 \u0ba4\u0bc1\u0bb0\u0bc0\u0ba3-\u0ba4\u0bb0\u0baf\u0bbe <br/> \u0baa\u0bbe\u0b95 \u0bb0\u0bbf\u0baa\u0bc1 \u0baa\u0bcd\u0bb0\u0bae\u0bc1\u0b95\u0bbe\u0ba4\u0bbf \u0baa\u0bcd\u0bb0\u0bbe\u0bb0\u0bcd\u0ba4\u0bbf\u0ba4 \u0b9a\u0bc1-\u0b95\u0bb3\u0bc7\u0baa\u0bb0\u0baf\u0bbe <br/> \u0baa\u0bcd\u0bb0\u0bbe\u0b95\u0b9f\u0bcd\u0baf \u0baa\u0bb0\u0bbe-\u0baa\u0bb0\u0baf\u0bbe \u0baa\u0bbe\u0bb2\u0bbf\u0ba4\u0bcb-\u0ba4\u0baf\u0bbe-\u0b95\u0bb0\u0baf\u0bbe"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0bb6\u0bcd\u0bb0\u0bc0 \u0bae\u0bbe\u0ba4\u0bcd\u0bb0\u0bc7 \u0ba8\u0bae\u0bb8\u0bcd\u0ba4\u0bc7 \u0b9a\u0bbf\u0ba9\u0bcd\u0bae\u0bbe\u0ba4\u0bcd\u0bb0\u0bc7 <br/> \u0b9a\u0bc7\u0bb5\u0bbf\u0ba4 \u0bb0\u0bae\u0bbe \u0bb9\u0bb0\u0bc0\u0b9a \u0bb5\u0bbf\u0ba4\u0bbe\u0ba4\u0bcd\u0bb0\u0bc7 <br/> \u0bb5\u0bbe\u0bae\u0bbe\u0ba4\u0bbf \u0b9a\u0b95\u0bcd\u0ba4\u0bbf \u0baa\u0bc2\u0b9c\u0bbf\u0ba4 \u0baa\u0bb0 \u0ba4\u0bc7\u0bb5\u0ba4\u0bbe\u0baf\u0bbe\u0bb9 \u0b9a\u0b95\u0bb2\u0bae\u0bcd \u0b9c\u0bbe\u0ba4\u0bae\u0bcd <br/> \u0b95\u0bbe\u0bae\u0bbe\u0ba4\u0bbf \u0ba4\u0bcd\u0bb5\u0bbe\u0ba4\u0b9a\u0baa\u0bbf\u0bb0\u0bc1\u0baa\u0bbe\u0b9a\u0bbf\u0ba4 - <br/> \u0b95\u0bbe\u0ba4\u0bbf \u0bb9\u0bbe\u0ba4\u0bbf \u0b9a\u0bbe\u0ba4\u0bbf \u0bae\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 \u0bb0\u0bc2\u0baa\u0bbf\u0ba3\u0bcd\u0baf\u0bbe\u0bb9 <br/> \u0baa\u0bcd\u0bb0\u0bc7\u0bae\u0bbe\u0bb8\u0bcd\u0baa\u0ba4 \u0b9a\u0bbf\u0bb5 \u0b95\u0bc1\u0bb0\u0bc1 \u0b95\u0bc1\u0bb9 \u0b9c\u0ba9\u0ba9\u0bcd\u0baf\u0bbe\u0bae\u0bcd - <br/> \u0baa\u0bcd\u0bb0\u0bc0\u0ba4\u0bbf \u0baf\u0bc1\u0b95\u0bcd\u0ba4 \u0bae\u0b9a\u0bcd\u0b9a\u0bbf\u0ba4\u0bcd\u0ba4\u0bae\u0bcd \u0bb5\u0bbf\u0bb2\u0baf\u0ba4\u0bc1"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0baa\u0bcd\u0bb0\u0bb9\u0bcd\u0bae-\u0bae\u0baf \u0baa\u0bcd\u0bb0\u0b95\u0bbe\u0b9a\u0bbf\u0ba9\u0bc0 \u0ba8\u0bbe\u0bae \u0bb0\u0bc2\u0baa \u0bb5\u0bbf\u0bae\u0bb0\u0bcd\u0b9a\u0bbf\u0ba9\u0bc0 <br/> \u0b95\u0bbe\u0bae \u0b95\u0bb2\u0bbe \u0baa\u0bcd\u0bb0\u0ba4\u0bb0\u0bcd\u0b9a\u0bbf\u0ba9\u0bc0 \u0b9a\u0bbe\u0bae\u0bb0\u0bb8\u0bcd\u0baf \u0ba8\u0bbf\u0ba4\u0bb0\u0bcd\u0b9a\u0bbf\u0ba9\u0bc0"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c4d\u0c30\u0c40 \u0c15\u0c2e\u0c32\u0c3e\u0c2e\u0c4d\u0c2c\u0c3e \u0c1c\u0c2f\u0c24\u0c3f \u0c05\u0c2e\u0c4d\u0c2c\u0c3e <br/> \u0c36\u0c4d\u0c30\u0c40 \u0c15\u0c2e\u0c32\u0c3e\u0c2e\u0c4d\u0c2c\u0c3e \u0c1c\u0c2f\u0c24\u0c3f \u0c1c\u0c17\u0c26\u0c2e\u0c4d\u0c2c\u0c3e <br/> \u0c36\u0c4d\u0c30\u0c40 \u0c15\u0c2e\u0c32\u0c3e\u0c2e\u0c4d\u0c2c\u0c3e \u0c1c\u0c2f\u0c24\u0c3f <br/> \u0c36\u0c43\u0c02\u0c17\u0c3e\u0c30 \u0c30\u0c38 \u0c15\u0c26\u0c2e\u0c4d\u0c2c\u0c3e \u0c2e\u0c26\u0c2e\u0c4d\u0c2c\u0c3e <br/> \u0c36\u0c4d\u0c30\u0c40 \u0c15\u0c2e\u0c32\u0c3e\u0c2e\u0c4d\u0c2c\u0c3e \u0c1c\u0c2f\u0c24\u0c3f <br/> \u0c1a\u0c3f\u0c26\u0c4d\u0c2c\u0c3f\u0c2e\u0c4d\u0c2c \u0c2a\u0c4d\u0c30\u0c24\u0c3f-\u0c2c\u0c3f\u0c2e\u0c4d\u0c2c\u0c47\u0c28\u0c4d\u0c26\u0c41 \u0c2c\u0c3f\u0c2e\u0c4d\u0c2c\u0c3e <br/> \u0c36\u0c4d\u0c30\u0c40 \u0c15\u0c2e\u0c32\u0c3e\u0c2e\u0c4d\u0c2c\u0c3e \u0c1c\u0c2f\u0c24\u0c3f"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c36\u0c4d\u0c30\u0c40 \u0c2a\u0c41\u0c30 \u0c2c\u0c3f\u0c28\u0c4d\u0c26\u0c41 \u0c2e\u0c27\u0c4d\u0c2f\u0c38\u0c4d\u0c25 \u0c1a\u0c3f\u0c28\u0c4d\u0c24\u0c3e\u0c2e\u0c23\u0c3f \u0c2e\u0c28\u0c4d\u0c26\u0c3f\u0c30\u0c38\u0c4d\u0c25 - <br/> \u0c36\u0c3f\u0c35\u0c3e\u0c15\u0c3e\u0c30 \u0c2e\u0c28\u0c4d\u0c1a \u0c38\u0c4d\u0c25\u0c3f\u0c24 \u0c36\u0c3f\u0c35 \u0c15\u0c3e\u0c2e\u0c47\u0c36\u0c3e\u0c02\u0c15\u0c38\u0c4d\u0c25\u0c3e"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c42\u0c15\u0c30\u0c3e\u0c28\u0c28\u0c3e\u0c26\u0c4d\u0c2f\u0c30\u0c4d\u0c1a\u0c3f\u0c24 \u0c2e\u0c39\u0c3e \u0c24\u0c4d\u0c30\u0c3f\u0c2a\u0c41\u0c30 \u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30\u0c40\u0c02 <br/> \u0c30\u0c3e\u0c1c \u0c30\u0c3e\u0c1c\u0c47\u0c36\u0c4d\u0c35\u0c30\u0c40\u0c02 \u0c36\u0c4d\u0c30\u0c40-\u0c15\u0c30 \u0c38\u0c30\u0c4d\u0c35\u0c3e\u0c28\u0c28\u0c4d\u0c26-\u0c2e\u0c2f - <br/> \u0c1a\u0c15\u0c4d\u0c30 \u0c35\u0c3e\u0c38\u0c3f\u0c28\u0c40\u0c02 \u0c38\u0c41\u0c35\u0c3e\u0c38\u0c3f\u0c28\u0c40\u0c02 \u0c1a\u0c3f\u0c28\u0c4d\u0c24\u0c2f\u0c47\u0c05\u0c39\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c26\u0c3f\u0c35\u0c3e\u0c15\u0c30 \u0c36\u0c40\u0c24 \u0c15\u0c3f\u0c30\u0c23 \u0c2a\u0c3e\u0c35\u0c15\u0c3e\u0c26\u0c3f \u0c35\u0c3f\u0c15\u0c3e\u0c38-\u0c15\u0c30\u0c2f\u0c3e <br/> \u0c2d\u0c40-\u0c15\u0c30 \u0c24\u0c3e\u0c2a-\u0c24\u0c4d\u0c30\u0c2f\u0c3e\u0c26\u0c3f \u0c2d\u0c47\u0c26\u0c28 \u0c27\u0c41\u0c30\u0c40\u0c23-\u0c24\u0c30\u0c2f\u0c3e <br/> \u0c2a\u0c3e\u0c15 \u0c30\u0c3f\u0c2a\u0c41 \u0c2a\u0c4d\u0c30\u0c2e\u0c41\u0c16\u0c3e\u0c26\u0c3f \u0c2a\u0c4d\u0c30\u0c3e\u0c30\u0c4d\u0c25\u0c3f\u0c24 \u0c38\u0c41-\u0c15\u0c33\u0c47\u0c2c\u0c30\u0c2f\u0c3e <br/> \u0c2a\u0c4d\u0c30\u0c3e\u0c15\u0c1f\u0c4d\u0c2f \u0c2a\u0c30\u0c3e-\u0c2a\u0c30\u0c2f\u0c3e \u0c2a\u0c3e\u0c32\u0c3f\u0c24\u0c4b-\u0c26\u0c2f\u0c3e-\u0c15\u0c30\u0c2f\u0c3e"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c36\u0c4d\u0c30\u0c40 \u0c2e\u0c3e\u0c24\u0c4d\u0c30\u0c47 \u0c28\u0c2e\u0c38\u0c4d\u0c24\u0c47 \u0c1a\u0c3f\u0c28\u0c4d\u0c2e\u0c3e\u0c24\u0c4d\u0c30\u0c47 <br/> \u0c38\u0c47\u0c35\u0c3f\u0c24 \u0c30\u0c2e\u0c3e \u0c39\u0c30\u0c40\u0c36 \u0c35\u0c3f\u0c27\u0c3e\u0c24\u0c4d\u0c30\u0c47 <br/> \u0c35\u0c3e\u0c2e\u0c3e\u0c26\u0c3f \u0c36\u0c15\u0c4d\u0c24\u0c3f \u0c2a\u0c42\u0c1c\u0c3f\u0c24 \u0c2a\u0c30 \u0c26\u0c47\u0c35\u0c24\u0c3e\u0c2f\u0c3e\u0c03 \u0c38\u0c15\u0c32\u0c02 \u0c1c\u0c3e\u0c24\u0c2e\u0c4d <br/> \u0c15\u0c3e\u0c2e\u0c3e\u0c26\u0c3f \u0c26\u0c4d\u0c35\u0c3e\u0c26\u0c36\u0c2d\u0c3f\u0c30\u0c41\u0c2a\u0c3e\u0c38\u0c3f\u0c24 - <br/> \u0c15\u0c3e\u0c26\u0c3f \u0c39\u0c3e\u0c26\u0c3f \u0c38\u0c3e\u0c26\u0c3f \u0c2e\u0c28\u0c4d\u0c24\u0c4d\u0c30 \u0c30\u0c42\u0c2a\u0c3f\u0c23\u0c4d\u0c2f\u0c3e\u0c03 <br/> \u0c2a\u0c4d\u0c30\u0c47\u0c2e\u0c3e\u0c38\u0c4d\u0c2a\u0c26 \u0c36\u0c3f\u0c35 \u0c17\u0c41\u0c30\u0c41 \u0c17\u0c41\u0c39 \u0c1c\u0c28\u0c28\u0c4d\u0c2f\u0c3e\u0c02 - <br/> \u0c2a\u0c4d\u0c30\u0c40\u0c24\u0c3f \u0c2f\u0c41\u0c15\u0c4d\u0c24 \u0c2e\u0c1a\u0c4d\u0c1a\u0c3f\u0c24\u0c4d\u0c24\u0c02 \u0c35\u0c3f\u0c32\u0c2f\u0c24\u0c41"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c2c\u0c4d\u0c30\u0c39\u0c4d\u0c2e-\u0c2e\u0c2f \u0c2a\u0c4d\u0c30\u0c15\u0c3e\u0c36\u0c3f\u0c28\u0c40 \u0c28\u0c3e\u0c2e \u0c30\u0c42\u0c2a \u0c35\u0c3f\u0c2e\u0c30\u0c4d\u0c36\u0c3f\u0c28\u0c40 <br/> \u0c15\u0c3e\u0c2e \u0c15\u0c32\u0c3e \u0c2a\u0c4d\u0c30\u0c26\u0c30\u0c4d\u0c36\u0c3f\u0c28\u0c40 \u0c38\u0c3e\u0c2e\u0c30\u0c38\u0c4d\u0c2f \u0c28\u0c3f\u0c26\u0c30\u0c4d\u0c36\u0c3f\u0c28\u0c40"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cae\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cbe \u0c9c\u0caf\u0ca4\u0cbf \u0c85\u0cae\u0ccd\u0cac\u0cbe <br/> \u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cae\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cbe \u0c9c\u0caf\u0ca4\u0cbf \u0c9c\u0c97\u0ca6\u0cae\u0ccd\u0cac\u0cbe <br/> \u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cae\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cbe \u0c9c\u0caf\u0ca4\u0cbf <br/> \u0cb6\u0cc3\u0c82\u0c97\u0cbe\u0cb0 \u0cb0\u0cb8 \u0c95\u0ca6\u0cae\u0ccd\u0cac\u0cbe \u0cae\u0ca6\u0cae\u0ccd\u0cac\u0cbe <br/> \u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cae\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cbe \u0c9c\u0caf\u0ca4\u0cbf <br/> \u0c9a\u0cbf\u0ca6\u0ccd\u0cac\u0cbf\u0cae\u0ccd\u0cac \u0caa\u0ccd\u0cb0\u0ca4\u0cbf-\u0cac\u0cbf\u0cae\u0ccd\u0cac\u0cc7\u0ca8\u0ccd\u0ca6\u0cc1 \u0cac\u0cbf\u0cae\u0ccd\u0cac\u0cbe <br/> \u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cae\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cbe \u0c9c\u0caf\u0ca4\u0cbf"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0caa\u0cc1\u0cb0 \u0cac\u0cbf\u0ca8\u0ccd\u0ca6\u0cc1 \u0cae\u0ca7\u0ccd\u0caf\u0cb8\u0ccd\u0ca5 \u0c9a\u0cbf\u0ca8\u0ccd\u0ca4\u0cbe\u0cae\u0ca3\u0cbf \u0cae\u0ca8\u0ccd\u0ca6\u0cbf\u0cb0\u0cb8\u0ccd\u0ca5 - <br/> \u0cb6\u0cbf\u0cb5\u0cbe\u0c95\u0cbe\u0cb0 \u0cae\u0ca8\u0ccd\u0c9a \u0cb8\u0ccd\u0ca5\u0cbf\u0ca4 \u0cb6\u0cbf\u0cb5 \u0c95\u0cbe\u0cae\u0cc7\u0cb6\u0cbe\u0c82\u0c95\u0cb8\u0ccd\u0ca5\u0cbe"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0cc2\u0c95\u0cb0\u0cbe\u0ca8\u0ca8\u0cbe\u0ca6\u0ccd\u0caf\u0cb0\u0ccd\u0c9a\u0cbf\u0ca4 \u0cae\u0cb9\u0cbe \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0 \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cc0\u0c82 <br/> \u0cb0\u0cbe\u0c9c \u0cb0\u0cbe\u0c9c\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0\u0cc0\u0c82 \u0cb6\u0ccd\u0cb0\u0cc0-\u0c95\u0cb0 \u0cb8\u0cb0\u0ccd\u0cb5\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6-\u0cae\u0caf - <br/> \u0c9a\u0c95\u0ccd\u0cb0 \u0cb5\u0cbe\u0cb8\u0cbf\u0ca8\u0cc0\u0c82 \u0cb8\u0cc1\u0cb5\u0cbe\u0cb8\u0cbf\u0ca8\u0cc0\u0c82 \u0c9a\u0cbf\u0ca8\u0ccd\u0ca4\u0caf\u0cc7\u0c85\u0cb9\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0ca6\u0cbf\u0cb5\u0cbe\u0c95\u0cb0 \u0cb6\u0cc0\u0ca4 \u0c95\u0cbf\u0cb0\u0ca3 \u0caa\u0cbe\u0cb5\u0c95\u0cbe\u0ca6\u0cbf \u0cb5\u0cbf\u0c95\u0cbe\u0cb8-\u0c95\u0cb0\u0caf\u0cbe <br/> \u0cad\u0cc0-\u0c95\u0cb0 \u0ca4\u0cbe\u0caa-\u0ca4\u0ccd\u0cb0\u0caf\u0cbe\u0ca6\u0cbf \u0cad\u0cc7\u0ca6\u0ca8 \u0ca7\u0cc1\u0cb0\u0cc0\u0ca3-\u0ca4\u0cb0\u0caf\u0cbe <br/> \u0caa\u0cbe\u0c95 \u0cb0\u0cbf\u0caa\u0cc1 \u0caa\u0ccd\u0cb0\u0cae\u0cc1\u0c96\u0cbe\u0ca6\u0cbf \u0caa\u0ccd\u0cb0\u0cbe\u0cb0\u0ccd\u0ca5\u0cbf\u0ca4 \u0cb8\u0cc1-\u0c95\u0cb3\u0cc7\u0cac\u0cb0\u0caf\u0cbe <br/> \u0caa\u0ccd\u0cb0\u0cbe\u0c95\u0c9f\u0ccd\u0caf \u0caa\u0cb0\u0cbe-\u0caa\u0cb0\u0caf\u0cbe \u0caa\u0cbe\u0cb2\u0cbf\u0ca4\u0ccb-\u0ca6\u0caf\u0cbe-\u0c95\u0cb0\u0caf\u0cbe"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0cae\u0cbe\u0ca4\u0ccd\u0cb0\u0cc7 \u0ca8\u0cae\u0cb8\u0ccd\u0ca4\u0cc7 \u0c9a\u0cbf\u0ca8\u0ccd\u0cae\u0cbe\u0ca4\u0ccd\u0cb0\u0cc7 <br/> \u0cb8\u0cc7\u0cb5\u0cbf\u0ca4 \u0cb0\u0cae\u0cbe \u0cb9\u0cb0\u0cc0\u0cb6 \u0cb5\u0cbf\u0ca7\u0cbe\u0ca4\u0ccd\u0cb0\u0cc7 <br/> \u0cb5\u0cbe\u0cae\u0cbe\u0ca6\u0cbf \u0cb6\u0c95\u0ccd\u0ca4\u0cbf \u0caa\u0cc2\u0c9c\u0cbf\u0ca4 \u0caa\u0cb0 \u0ca6\u0cc7\u0cb5\u0ca4\u0cbe\u0caf\u0cbe\u0c83 \u0cb8\u0c95\u0cb2\u0c82 \u0c9c\u0cbe\u0ca4\u0cae\u0ccd <br/> \u0c95\u0cbe\u0cae\u0cbe\u0ca6\u0cbf \u0ca6\u0ccd\u0cb5\u0cbe\u0ca6\u0cb6\u0cad\u0cbf\u0cb0\u0cc1\u0caa\u0cbe\u0cb8\u0cbf\u0ca4 - <br/> \u0c95\u0cbe\u0ca6\u0cbf \u0cb9\u0cbe\u0ca6\u0cbf \u0cb8\u0cbe\u0ca6\u0cbf \u0cae\u0ca8\u0ccd\u0ca4\u0ccd\u0cb0 \u0cb0\u0cc2\u0caa\u0cbf\u0ca3\u0ccd\u0caf\u0cbe\u0c83 <br/> \u0caa\u0ccd\u0cb0\u0cc7\u0cae\u0cbe\u0cb8\u0ccd\u0caa\u0ca6 \u0cb6\u0cbf\u0cb5 \u0c97\u0cc1\u0cb0\u0cc1 \u0c97\u0cc1\u0cb9 \u0c9c\u0ca8\u0ca8\u0ccd\u0caf\u0cbe\u0c82 - <br/> \u0caa\u0ccd\u0cb0\u0cc0\u0ca4\u0cbf \u0caf\u0cc1\u0c95\u0ccd\u0ca4 \u0cae\u0c9a\u0ccd\u0c9a\u0cbf\u0ca4\u0ccd\u0ca4\u0c82 \u0cb5\u0cbf\u0cb2\u0caf\u0ca4\u0cc1"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cac\u0ccd\u0cb0\u0cb9\u0ccd\u0cae-\u0cae\u0caf \u0caa\u0ccd\u0cb0\u0c95\u0cbe\u0cb6\u0cbf\u0ca8\u0cc0 \u0ca8\u0cbe\u0cae \u0cb0\u0cc2\u0caa \u0cb5\u0cbf\u0cae\u0cb0\u0ccd\u0cb6\u0cbf\u0ca8\u0cc0 <br/> \u0c95\u0cbe\u0cae \u0c95\u0cb2\u0cbe \u0caa\u0ccd\u0cb0\u0ca6\u0cb0\u0ccd\u0cb6\u0cbf\u0ca8\u0cc0 \u0cb8\u0cbe\u0cae\u0cb0\u0cb8\u0ccd\u0caf \u0ca8\u0cbf\u0ca6\u0cb0\u0ccd\u0cb6\u0cbf\u0ca8\u0cc0"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d2e\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d3e \u0d1c\u0d2f\u0d24\u0d3f \u0d05\u0d2e\u0d4d\u0d2c\u0d3e <br/> \u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d2e\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d3e \u0d1c\u0d2f\u0d24\u0d3f \u0d1c\u0d17\u0d26\u0d2e\u0d4d\u0d2c\u0d3e <br/> \u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d2e\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d3e \u0d1c\u0d2f\u0d24\u0d3f <br/> \u0d36\u0d43\u0d02\u0d17\u0d3e\u0d30 \u0d30\u0d38 \u0d15\u0d26\u0d2e\u0d4d\u0d2c\u0d3e \u0d2e\u0d26\u0d2e\u0d4d\u0d2c\u0d3e <br/> \u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d2e\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d3e \u0d1c\u0d2f\u0d24\u0d3f <br/> \u0d1a\u0d3f\u0d26\u0d4d\u0d2c\u0d3f\u0d2e\u0d4d\u0d2c \u0d2a\u0d4d\u0d30\u0d24\u0d3f-\u0d2c\u0d3f\u0d2e\u0d4d\u0d2c\u0d47\u0d28\u0d4d\u0d26\u0d41 \u0d2c\u0d3f\u0d2e\u0d4d\u0d2c\u0d3e <br/> \u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d2e\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d3e \u0d1c\u0d2f\u0d24\u0d3f"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d36\u0d4d\u0d30\u0d40 \u0d2a\u0d41\u0d30 \u0d2c\u0d3f\u0d28\u0d4d\u0d26\u0d41 \u0d2e\u0d27\u0d4d\u0d2f\u0d38\u0d4d\u0d25 \u0d1a\u0d3f\u0d28\u0d4d\u0d24\u0d3e\u0d2e\u0d23\u0d3f \u0d2e\u0d28\u0d4d\u0d26\u0d3f\u0d30\u0d38\u0d4d\u0d25 - <br/> \u0d36\u0d3f\u0d35\u0d3e\u0d15\u0d3e\u0d30 \u0d2e\u0d28\u0d4d\u0d1a \u0d38\u0d4d\u0d25\u0d3f\u0d24 \u0d36\u0d3f\u0d35 \u0d15\u0d3e\u0d2e\u0d47\u0d36\u0d3e\u0d02\u0d15\u0d38\u0d4d\u0d25\u0d3e"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d42\u0d15\u0d30\u0d3e\u0d28\u0d28\u0d3e\u0d26\u0d4d\u0d2f\u0d30\u0d4d\u0d1a\u0d3f\u0d24 \u0d2e\u0d39\u0d3e \u0d24\u0d4d\u0d30\u0d3f\u0d2a\u0d41\u0d30 \u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d40\u0d02 <br/> \u0d30\u0d3e\u0d1c \u0d30\u0d3e\u0d1c\u0d47\u0d36\u0d4d\u0d35\u0d30\u0d40\u0d02 \u0d36\u0d4d\u0d30\u0d40-\u0d15\u0d30 \u0d38\u0d30\u0d4d\u0d35\u0d3e\u0d28\u0d28\u0d4d\u0d26-\u0d2e\u0d2f - <br/> \u0d1a\u0d15\u0d4d\u0d30 \u0d35\u0d3e\u0d38\u0d3f\u0d28\u0d40\u0d02 \u0d38\u0d41\u0d35\u0d3e\u0d38\u0d3f\u0d28\u0d40\u0d02 \u0d1a\u0d3f\u0d28\u0d4d\u0d24\u0d2f\u0d47\u0d05\u0d39\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d26\u0d3f\u0d35\u0d3e\u0d15\u0d30 \u0d36\u0d40\u0d24 \u0d15\u0d3f\u0d30\u0d23 \u0d2a\u0d3e\u0d35\u0d15\u0d3e\u0d26\u0d3f \u0d35\u0d3f\u0d15\u0d3e\u0d38-\u0d15\u0d30\u0d2f\u0d3e <br/> \u0d2d\u0d40-\u0d15\u0d30 \u0d24\u0d3e\u0d2a-\u0d24\u0d4d\u0d30\u0d2f\u0d3e\u0d26\u0d3f \u0d2d\u0d47\u0d26\u0d28 \u0d27\u0d41\u0d30\u0d40\u0d23-\u0d24\u0d30\u0d2f\u0d3e <br/> \u0d2a\u0d3e\u0d15 \u0d30\u0d3f\u0d2a\u0d41 \u0d2a\u0d4d\u0d30\u0d2e\u0d41\u0d16\u0d3e\u0d26\u0d3f \u0d2a\u0d4d\u0d30\u0d3e\u0d30\u0d4d\u0d25\u0d3f\u0d24 \u0d38\u0d41-\u0d15\u0d33\u0d47\u0d2c\u0d30\u0d2f\u0d3e <br/> \u0d2a\u0d4d\u0d30\u0d3e\u0d15\u0d1f\u0d4d\u0d2f \u0d2a\u0d30\u0d3e-\u0d2a\u0d30\u0d2f\u0d3e \u0d2a\u0d3e\u0d32\u0d3f\u0d24\u0d4b-\u0d26\u0d2f\u0d3e-\u0d15\u0d30\u0d2f\u0d3e"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d36\u0d4d\u0d30\u0d40 \u0d2e\u0d3e\u0d24\u0d4d\u0d30\u0d47 \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47 \u0d1a\u0d3f\u0d28\u0d4d\u0d2e\u0d3e\u0d24\u0d4d\u0d30\u0d47 <br/> \u0d38\u0d47\u0d35\u0d3f\u0d24 \u0d30\u0d2e\u0d3e \u0d39\u0d30\u0d40\u0d36 \u0d35\u0d3f\u0d27\u0d3e\u0d24\u0d4d\u0d30\u0d47 <br/> \u0d35\u0d3e\u0d2e\u0d3e\u0d26\u0d3f \u0d36\u0d15\u0d4d\u0d24\u0d3f \u0d2a\u0d42\u0d1c\u0d3f\u0d24 \u0d2a\u0d30 \u0d26\u0d47\u0d35\u0d24\u0d3e\u0d2f\u0d3e\u0d03 \u0d38\u0d15\u0d32\u0d02 \u0d1c\u0d3e\u0d24\u0d2e\u0d4d <br/> \u0d15\u0d3e\u0d2e\u0d3e\u0d26\u0d3f \u0d26\u0d4d\u0d35\u0d3e\u0d26\u0d36\u0d2d\u0d3f\u0d30\u0d41\u0d2a\u0d3e\u0d38\u0d3f\u0d24 - <br/> \u0d15\u0d3e\u0d26\u0d3f \u0d39\u0d3e\u0d26\u0d3f \u0d38\u0d3e\u0d26\u0d3f \u0d2e\u0d28\u0d4d\u0d24\u0d4d\u0d30 \u0d30\u0d42\u0d2a\u0d3f\u0d23\u0d4d\u0d2f\u0d3e\u0d03 <br/> \u0d2a\u0d4d\u0d30\u0d47\u0d2e\u0d3e\u0d38\u0d4d\u0d2a\u0d26 \u0d36\u0d3f\u0d35 \u0d17\u0d41\u0d30\u0d41 \u0d17\u0d41\u0d39 \u0d1c\u0d28\u0d28\u0d4d\u0d2f\u0d3e\u0d02 - <br/> \u0d2a\u0d4d\u0d30\u0d40\u0d24\u0d3f \u0d2f\u0d41\u0d15\u0d4d\u0d24 \u0d2e\u0d1a\u0d4d\u0d1a\u0d3f\u0d24\u0d4d\u0d24\u0d02 \u0d35\u0d3f\u0d32\u0d2f\u0d24\u0d41"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d2c\u0d4d\u0d30\u0d39\u0d4d\u0d2e-\u0d2e\u0d2f \u0d2a\u0d4d\u0d30\u0d15\u0d3e\u0d36\u0d3f\u0d28\u0d40 \u0d28\u0d3e\u0d2e \u0d30\u0d42\u0d2a \u0d35\u0d3f\u0d2e\u0d30\u0d4d\u0d36\u0d3f\u0d28\u0d40 <br/> \u0d15\u0d3e\u0d2e \u0d15\u0d32\u0d3e \u0d2a\u0d4d\u0d30\u0d26\u0d30\u0d4d\u0d36\u0d3f\u0d28\u0d40 \u0d38\u0d3e\u0d2e\u0d30\u0d38\u0d4d\u0d2f \u0d28\u0d3f\u0d26\u0d30\u0d4d\u0d36\u0d3f\u0d28\u0d40"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u094d\u0930\u0940 \u0915\u092e\u0932\u093e\u092e\u094d\u092c\u093e \u091c\u092f\u0924\u093f \u0905\u092e\u094d\u092c\u093e <br/> \u0936\u094d\u0930\u0940 \u0915\u092e\u0932\u093e\u092e\u094d\u092c\u093e \u091c\u092f\u0924\u093f \u091c\u0917\u0926\u092e\u094d\u092c\u093e <br/> \u0936\u094d\u0930\u0940 \u0915\u092e\u0932\u093e\u092e\u094d\u092c\u093e \u091c\u092f\u0924\u093f <br/> \u0936\u0943\u0902\u0917\u093e\u0930 \u0930\u0938 \u0915\u0926\u092e\u094d\u092c\u093e \u092e\u0926\u092e\u094d\u092c\u093e <br/> \u0936\u094d\u0930\u0940 \u0915\u092e\u0932\u093e\u092e\u094d\u092c\u093e \u091c\u092f\u0924\u093f <br/> \u091a\u093f\u0926\u094d\u092c\u093f\u092e\u094d\u092c \u092a\u094d\u0930\u0924\u093f-\u092c\u093f\u092e\u094d\u092c\u0947\u0928\u094d\u0926\u0941 \u092c\u093f\u092e\u094d\u092c\u093e <br/> \u0936\u094d\u0930\u0940 \u0915\u092e\u0932\u093e\u092e\u094d\u092c\u093e \u091c\u092f\u0924\u093f"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0936\u094d\u0930\u0940 \u092a\u0941\u0930 \u092c\u093f\u0928\u094d\u0926\u0941 \u092e\u0927\u094d\u092f\u0938\u094d\u0925 \u091a\u093f\u0928\u094d\u0924\u093e\u092e\u0923\u093f \u092e\u0928\u094d\u0926\u093f\u0930\u0938\u094d\u0925 - <br/> \u0936\u093f\u0935\u093e\u0915\u093e\u0930 \u092e\u0928\u094d\u091a \u0938\u094d\u0925\u093f\u0924 \u0936\u093f\u0935 \u0915\u093e\u092e\u0947\u0936\u093e\u0902\u0915\u0938\u094d\u0925\u093e"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0938\u0942\u0915\u0930\u093e\u0928\u0928\u093e\u0926\u094d\u092f\u0930\u094d\u091a\u093f\u0924 \u092e\u0939\u093e \u0924\u094d\u0930\u093f\u092a\u0941\u0930 \u0938\u0941\u0928\u094d\u0926\u0930\u0940\u0902 <br/> \u0930\u093e\u091c \u0930\u093e\u091c\u0947\u0936\u094d\u0935\u0930\u0940\u0902 \u0936\u094d\u0930\u0940-\u0915\u0930 \u0938\u0930\u094d\u0935\u093e\u0928\u0928\u094d\u0926-\u092e\u092f - <br/> \u091a\u0915\u094d\u0930 \u0935\u093e\u0938\u093f\u0928\u0940\u0902 \u0938\u0941\u0935\u093e\u0938\u093f\u0928\u0940\u0902 \u091a\u093f\u0928\u094d\u0924\u092f\u0947\u0905\u0939\u092e\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0926\u093f\u0935\u093e\u0915\u0930 \u0936\u0940\u0924 \u0915\u093f\u0930\u0923 \u092a\u093e\u0935\u0915\u093e\u0926\u093f \u0935\u093f\u0915\u093e\u0938-\u0915\u0930\u092f\u093e <br/> \u092d\u0940-\u0915\u0930 \u0924\u093e\u092a-\u0924\u094d\u0930\u092f\u093e\u0926\u093f \u092d\u0947\u0926\u0928 \u0927\u0941\u0930\u0940\u0923-\u0924\u0930\u092f\u093e <br/> \u092a\u093e\u0915 \u0930\u093f\u092a\u0941 \u092a\u094d\u0930\u092e\u0941\u0916\u093e\u0926\u093f \u092a\u094d\u0930\u093e\u0930\u094d\u0925\u093f\u0924 \u0938\u0941-\u0915\u0932\u0947\u092c\u0930\u092f\u093e <br/> \u092a\u094d\u0930\u093e\u0915\u091f\u094d\u092f \u092a\u0930\u093e-\u092a\u0930\u092f\u093e \u092a\u093e\u0932\u093f\u0924\u094b-\u0926\u092f\u093e-\u0915\u0930\u092f\u093e"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0936\u094d\u0930\u0940 \u092e\u093e\u0924\u094d\u0930\u0947 \u0928\u092e\u0938\u094d\u0924\u0947 \u091a\u093f\u0928\u094d\u092e\u093e\u0924\u094d\u0930\u0947 <br/> \u0938\u0947\u0935\u093f\u0924 \u0930\u092e\u093e \u0939\u0930\u0940\u0936 \u0935\u093f\u0927\u093e\u0924\u094d\u0930\u0947 <br/> \u0935\u093e\u092e\u093e\u0926\u093f \u0936\u0915\u094d\u0924\u093f \u092a\u0942\u091c\u093f\u0924 \u092a\u0930 \u0926\u0947\u0935\u0924\u093e\u092f\u093e\u0903 \u0938\u0915\u0932\u0902 \u091c\u093e\u0924\u092e\u094d <br/> \u0915\u093e\u092e\u093e\u0926\u093f \u0926\u094d\u0935\u093e\u0926\u0936\u092d\u093f\u0930\u0941\u092a\u093e\u0938\u093f\u0924 - <br/> \u0915\u093e\u0926\u093f \u0939\u093e\u0926\u093f \u0938\u093e\u0926\u093f \u092e\u0928\u094d\u0924\u094d\u0930 \u0930\u0942\u092a\u093f\u0923\u094d\u092f\u093e\u0903 <br/> \u092a\u094d\u0930\u0947\u092e\u093e\u0938\u094d\u092a\u0926 \u0936\u093f\u0935 \u0917\u0941\u0930\u0941 \u0917\u0941\u0939 \u091c\u0928\u0928\u094d\u092f\u093e\u0902 - <br/> \u092a\u094d\u0930\u0940\u0924\u093f \u092f\u0941\u0915\u094d\u0924 \u092e\u091a\u094d\u091a\u093f\u0924\u094d\u0924\u0902 \u0935\u093f\u0932\u092f\u0924\u0941"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u092c\u094d\u0930\u0939\u094d\u092e-\u092e\u092f \u092a\u094d\u0930\u0915\u093e\u0936\u093f\u0928\u0940 \u0928\u093e\u092e \u0930\u0942\u092a \u0935\u093f\u092e\u0930\u094d\u0936\u093f\u0928\u0940 <br/> \u0915\u093e\u092e \u0915\u0932\u093e \u092a\u094d\u0930\u0926\u0930\u094d\u0936\u093f\u0928\u0940 \u0938\u093e\u092e\u0930\u0938\u094d\u092f \u0928\u093f\u0926\u0930\u094d\u0936\u093f\u0928\u0940"}]}], "L": "l", "lyricsref": [{"links": [{"N": "guru-guha.blogspot.com", "L": "http://guru-guha.blogspot.com/2007/09/dikshitar-kriti-sri-kamalaambaa-jayati.html"}, {"N": "www.medieval.org", "L": "http://www.medieval.org/music/world/carnatic/lyrics/srao/kamala.html#ahiri"}, {"N": "www.shivkumar.org", "L": "http://www.shivkumar.org/music/kamalamba-jayati-ahiri.htm"}]}], "songs": [{"I": "2tF2qMAqLsA", "T": 21, "S": 798, "R": 75, "C": 2, "A": 301, "D": "9:56", "V": "454", "J": "m"}, {"I": "fVkOA7lr94M", "T": 21, "S": 798, "R": 75, "C": 2, "A": 101, "D": "12:20", "V": "4,606", "J": "m"}, {"I": "hrfPl5MwE_c", "T": 21, "S": 798, "R": 75, "C": 2, "A": 101, "D": "24:47", "V": "4,212", "J": "m"}, {"I": "SAFsiQMjY1c&start=0&end=3", "T": 21, "S": 798, "R": 75, "C": 2, "A": 101, "D": "0:00", "V": "0", "J": "m"}, {"I": "Wry2g63P7K8&start=0&end=6", "T": 21, "S": 798, "R": 75, "C": 2, "A": 101, "D": "0:00", "V": "0", "J": "s"}, {"I": "iFbslvB_Vc4&start=1268&end=1742", "T": 21, "S": 798, "R": 75, "C": 2, "A": 334, "D": "7:54", "V": "209", "J": "m"}, {"I": "6B0nNDqcPPM", "T": 21, "S": 798, "R": 75, "C": 2, "A": 334, "D": "8:02", "V": "203", "J": "m"}, {"I": "lBLywn2Oots", "T": 21, "S": 798, "R": 75, "C": 2, "A": 334, "D": "7:55", "V": "106", "J": "m"}, {"I": "o2Amfh7bUKk&start=2827&end=3550", "T": 21, "S": 798, "R": 75, "C": 2, "A": 401, "D": "12:03", "V": "3,490", "J": "m"}, {"I": "6x5yafVSY1g", "T": 21, "S": 798, "R": 75, "C": 2, "A": 401, "D": "14:05", "V": "46", "J": "m"}, {"I": "nt4BpDEdOmo", "T": 21, "S": 798, "R": 75, "C": 2, "A": 36, "D": "11:34", "