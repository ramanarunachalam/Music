c23\u0c3e\u0c2f\u0c3e\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c39\u0c4d\u0c30\u0c40\u0c02\u0c15\u0c3e\u0c30 \u0c35\u0c3f\u0c2a\u0c3f\u0c28 \u0c39\u0c30\u0c3f\u0c23\u0c4d\u0c2f\u0c3e\u0c02 \u0c39\u0c4d\u0c30\u0c40\u0c02\u0c15\u0c3e\u0c30 \u0c38\u0c41-\u0c36\u0c30\u0c40\u0c30\u0c3f\u0c23\u0c4d\u0c2f\u0c3e\u0c02 <br/> \u0c39\u0c4d\u0c30\u0c40\u0c02\u0c15\u0c3e\u0c30 \u0c24\u0c30\u0c41 \u0c2e\u0c28\u0c4d\u0c1c\u0c30\u0c4d\u0c2f\u0c3e\u0c02 \u0c39\u0c4d\u0c30\u0c40\u0c02\u0c15\u0c3e\u0c30\u0c47\u0c36\u0c4d\u0c35\u0c30\u0c4d\u0c2f\u0c3e\u0c02 \u0c17\u0c4c\u0c30\u0c4d\u0c2f\u0c3e\u0c2e\u0c4d"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c36\u0c30\u0c40\u0c30 \u0c24\u0c4d\u0c30\u0c2f \u0c35\u0c3f\u0c32\u0c15\u0c4d\u0c37\u0c23 \u0c38\u0c41\u0c16-\u0c24\u0c30 \u0c38\u0c4d\u0c35\u0c3e\u0c24\u0c4d\u0c2e\u0c3e\u0c28\u0c41\u0c2d\u0c4b\u0c17\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c02 <br/> \u0c35\u0c3f\u0c30\u0c3f\u0c28\u0c4d\u0c1a\u0c3f \u0c39\u0c30\u0c40\u0c36\u0c3e\u0c28 \u0c39\u0c30\u0c3f-\u0c39\u0c2f \u0c35\u0c47\u0c26\u0c3f\u0c24 \u0c30\u0c39\u0c38\u0c4d\u0c2f \u0c2f\u0c4b\u0c17\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c2e\u0c4d <br/> \u0c2a\u0c30\u0c3e\u0c26\u0c3f \u0c35\u0c3e\u0c17\u0c4d\u0c26\u0c47\u0c35\u0c24\u0c3e \u0c30\u0c42\u0c2a \u0c35\u0c36\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c26\u0c3f \u0c35\u0c3f\u0c2d\u0c3e\u0c17\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c02 <br/> \u0c1a\u0c30\u0c3e\u0c24\u0c4d\u0c2e\u0c15 \u0c38\u0c30\u0c4d\u0c35 \u0c30\u0c4b\u0c17 \u0c39\u0c30 \u0c28\u0c3f\u0c30\u0c3e\u0c2e\u0c2f \u0c30\u0c3e\u0c1c \u0c2f\u0c4b\u0c17\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c15\u0c30 \u0c27\u0c43\u0c24 \u0c35\u0c40\u0c23\u0c3e \u0c35\u0c3e\u0c26\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c02 \u0c15\u0c2e\u0c32\u0c3e \u0c28\u0c17\u0c30 \u0c35\u0c3f\u0c28\u0c4b\u0c26\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c02 <br/> \u0c38\u0c41\u0c30 \u0c28\u0c30 \u0c2e\u0c41\u0c28\u0c3f \u0c1c\u0c28 \u0c2e\u0c4b\u0c26\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c02 \u0c17\u0c41\u0c30\u0c41 \u0c17\u0c41\u0c39 \u0c35\u0c30 \u0c2a\u0c4d\u0c30\u0c38\u0c3e\u0c26\u0c3f\u0c28\u0c4d\u0c2f\u0c3e\u0c2e\u0c4d"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cae\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cbe\u0caf\u0cbe\u0c82 \u0cad\u0c95\u0ccd\u0ca4\u0cbf\u0c82 \u0c95\u0cb0\u0ccb\u0cae\u0cbf <br/> \u0cb6\u0ccd\u0cb0\u0cbf\u0ca4 \u0c95\u0cb2\u0ccd\u0caa \u0cb5\u0cbe\u0c9f\u0cbf\u0c95\u0cbe\u0caf\u0cbe\u0c82 \u0c9a\u0ca3\u0ccd\u0ca1\u0cbf\u0c95\u0cbe\u0caf\u0cbe\u0c82 \u0c9c\u0c97\u0ca6\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cbe\u0caf\u0cbe\u0cae\u0ccd"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb0\u0cbe\u0c95\u0cbe \u0c9a\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0cb5\u0ca6\u0ca8\u0cbe\u0caf\u0cbe\u0c82 \u0cb0\u0cbe\u0c9c\u0cc0\u0cb5 \u0ca8\u0caf\u0ca8\u0cbe\u0caf\u0cbe\u0c82 <br/> \u0caa\u0cbe\u0c95\u0cbe\u0cb0\u0cbf \u0ca8\u0cc1\u0ca4 \u0c9a\u0cb0\u0ca3\u0cbe\u0caf\u0cbe\u0c82 \u0c86\u0c95\u0cbe\u0cb6\u0cbe\u0ca6\u0cbf \u0c95\u0cbf\u0cb0\u0ca3\u0cbe\u0caf\u0cbe\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cb9\u0ccd\u0cb0\u0cc0\u0c82\u0c95\u0cbe\u0cb0 \u0cb5\u0cbf\u0caa\u0cbf\u0ca8 \u0cb9\u0cb0\u0cbf\u0ca3\u0ccd\u0caf\u0cbe\u0c82 \u0cb9\u0ccd\u0cb0\u0cc0\u0c82\u0c95\u0cbe\u0cb0 \u0cb8\u0cc1-\u0cb6\u0cb0\u0cc0\u0cb0\u0cbf\u0ca3\u0ccd\u0caf\u0cbe\u0c82 <br/> \u0cb9\u0ccd\u0cb0\u0cc0\u0c82\u0c95\u0cbe\u0cb0 \u0ca4\u0cb0\u0cc1 \u0cae\u0ca8\u0ccd\u0c9c\u0cb0\u0ccd\u0caf\u0cbe\u0c82 \u0cb9\u0ccd\u0cb0\u0cc0\u0c82\u0c95\u0cbe\u0cb0\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0\u0ccd\u0caf\u0cbe\u0c82 \u0c97\u0ccc\u0cb0\u0ccd\u0caf\u0cbe\u0cae\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cb6\u0cb0\u0cc0\u0cb0 \u0ca4\u0ccd\u0cb0\u0caf \u0cb5\u0cbf\u0cb2\u0c95\u0ccd\u0cb7\u0ca3 \u0cb8\u0cc1\u0c96-\u0ca4\u0cb0 \u0cb8\u0ccd\u0cb5\u0cbe\u0ca4\u0ccd\u0cae\u0cbe\u0ca8\u0cc1\u0cad\u0ccb\u0c97\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0c82 <br/> \u0cb5\u0cbf\u0cb0\u0cbf\u0ca8\u0ccd\u0c9a\u0cbf \u0cb9\u0cb0\u0cc0\u0cb6\u0cbe\u0ca8 \u0cb9\u0cb0\u0cbf-\u0cb9\u0caf \u0cb5\u0cc7\u0ca6\u0cbf\u0ca4 \u0cb0\u0cb9\u0cb8\u0ccd\u0caf \u0caf\u0ccb\u0c97\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0cae\u0ccd <br/> \u0caa\u0cb0\u0cbe\u0ca6\u0cbf \u0cb5\u0cbe\u0c97\u0ccd\u0ca6\u0cc7\u0cb5\u0ca4\u0cbe \u0cb0\u0cc2\u0caa \u0cb5\u0cb6\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0ca6\u0cbf \u0cb5\u0cbf\u0cad\u0cbe\u0c97\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0c82 <br/> \u0c9a\u0cb0\u0cbe\u0ca4\u0ccd\u0cae\u0c95 \u0cb8\u0cb0\u0ccd\u0cb5 \u0cb0\u0ccb\u0c97 \u0cb9\u0cb0 \u0ca8\u0cbf\u0cb0\u0cbe\u0cae\u0caf \u0cb0\u0cbe\u0c9c \u0caf\u0ccb\u0c97\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0cae\u0ccd"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c95\u0cb0 \u0ca7\u0cc3\u0ca4 \u0cb5\u0cc0\u0ca3\u0cbe \u0cb5\u0cbe\u0ca6\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0c82 \u0c95\u0cae\u0cb2\u0cbe \u0ca8\u0c97\u0cb0 \u0cb5\u0cbf\u0ca8\u0ccb\u0ca6\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0c82 <br/> \u0cb8\u0cc1\u0cb0 \u0ca8\u0cb0 \u0cae\u0cc1\u0ca8\u0cbf \u0c9c\u0ca8 \u0cae\u0ccb\u0ca6\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0c82 \u0c97\u0cc1\u0cb0\u0cc1 \u0c97\u0cc1\u0cb9 \u0cb5\u0cb0 \u0caa\u0ccd\u0cb0\u0cb8\u0cbe\u0ca6\u0cbf\u0ca8\u0ccd\u0caf\u0cbe\u0cae\u0ccd"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d2e\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d3f\u0d15\u0d3e\u0d2f\u0d3e\u0d02 \u0d2d\u0d15\u0d4d\u0d24\u0d3f\u0d02 \u0d15\u0d30\u0d4b\u0d2e\u0d3f <br/> \u0d36\u0d4d\u0d30\u0d3f\u0d24 \u0d15\u0d32\u0d4d\u0d2a \u0d35\u0d3e\u0d1f\u0d3f\u0d15\u0d3e\u0d2f\u0d3e\u0d02 \u0d1a\u0d23\u0d4d\u0d21\u0d3f\u0d15\u0d3e\u0d2f\u0d3e\u0d02 \u0d1c\u0d17\u0d26\u0d2e\u0d4d\u0d2c\u0d3f\u0d15\u0d3e\u0d2f\u0d3e\u0d2e\u0d4d"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d30\u0d3e\u0d15\u0d3e \u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30 \u0d35\u0d26\u0d28\u0d3e\u0d2f\u0d3e\u0d02 \u0d30\u0d3e\u0d1c\u0d40\u0d35 \u0d28\u0d2f\u0d28\u0d3e\u0d2f\u0d3e\u0d02 <br/> \u0d2a\u0d3e\u0d15\u0d3e\u0d30\u0d3f \u0d28\u0d41\u0d24 \u0d1a\u0d30\u0d23\u0d3e\u0d2f\u0d3e\u0d02 \u0d06\u0d15\u0d3e\u0d36\u0d3e\u0d26\u0d3f \u0d15\u0d3f\u0d30\u0d23\u0d3e\u0d2f\u0d3e\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d39\u0d4d\u0d30\u0d40\u0d02\u0d15\u0d3e\u0d30 \u0d35\u0d3f\u0d2a\u0d3f\u0d28 \u0d39\u0d30\u0d3f\u0d23\u0d4d\u0d2f\u0d3e\u0d02 \u0d39\u0d4d\u0d30\u0d40\u0d02\u0d15\u0d3e\u0d30 \u0d38\u0d41-\u0d36\u0d30\u0d40\u0d30\u0d3f\u0d23\u0d4d\u0d2f\u0d3e\u0d02 <br/> \u0d39\u0d4d\u0d30\u0d40\u0d02\u0d15\u0d3e\u0d30 \u0d24\u0d30\u0d41 \u0d2e\u0d28\u0d4d\u0d1c\u0d30\u0d4d\u0d2f\u0d3e\u0d02 \u0d39\u0d4d\u0d30\u0d40\u0d02\u0d15\u0d3e\u0d30\u0d47\u0d36\u0d4d\u0d35\u0d30\u0d4d\u0d2f\u0d3e\u0d02 \u0d17\u0d4c\u0d30\u0d4d\u0d2f\u0d3e\u0d2e\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d36\u0d30\u0d40\u0d30 \u0d24\u0d4d\u0d30\u0d2f \u0d35\u0d3f\u0d32\u0d15\u0d4d\u0d37\u0d23 \u0d38\u0d41\u0d16-\u0d24\u0d30 \u0d38\u0d4d\u0d35\u0d3e\u0d24\u0d4d\u0d2e\u0d3e\u0d28\u0d41\u0d2d\u0d4b\u0d17\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d02 <br/> \u0d35\u0d3f\u0d30\u0d3f\u0d28\u0d4d\u0d1a\u0d3f \u0d39\u0d30\u0d40\u0d36\u0d3e\u0d28 \u0d39\u0d30\u0d3f-\u0d39\u0d2f \u0d35\u0d47\u0d26\u0d3f\u0d24 \u0d30\u0d39\u0d38\u0d4d\u0d2f \u0d2f\u0d4b\u0d17\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d2e\u0d4d <br/> \u0d2a\u0d30\u0d3e\u0d26\u0d3f \u0d35\u0d3e\u0d17\u0d4d\u0d26\u0d47\u0d35\u0d24\u0d3e \u0d30\u0d42\u0d2a \u0d35\u0d36\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d26\u0d3f \u0d35\u0d3f\u0d2d\u0d3e\u0d17\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d02 <br/> \u0d1a\u0d30\u0d3e\u0d24\u0d4d\u0d2e\u0d15 \u0d38\u0d30\u0d4d\u0d35 \u0d30\u0d4b\u0d17 \u0d39\u0d30 \u0d28\u0d3f\u0d30\u0d3e\u0d2e\u0d2f \u0d30\u0d3e\u0d1c \u0d2f\u0d4b\u0d17\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d2e\u0d4d"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d15\u0d30 \u0d27\u0d43\u0d24 \u0d35\u0d40\u0d23\u0d3e \u0d35\u0d3e\u0d26\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d02 \u0d15\u0d2e\u0d32\u0d3e \u0d28\u0d17\u0d30 \u0d35\u0d3f\u0d28\u0d4b\u0d26\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d02 <br/> \u0d38\u0d41\u0d30 \u0d28\u0d30 \u0d2e\u0d41\u0d28\u0d3f \u0d1c\u0d28 \u0d2e\u0d4b\u0d26\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d02 \u0d17\u0d41\u0d30\u0d41 \u0d17\u0d41\u0d39 \u0d35\u0d30 \u0d2a\u0d4d\u0d30\u0d38\u0d3e\u0d26\u0d3f\u0d28\u0d4d\u0d2f\u0d3e\u0d2e\u0d4d"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u094d\u0930\u0940 \u0915\u092e\u0932\u093e\u092e\u094d\u092c\u093f\u0915\u093e\u092f\u093e\u0902 \u092d\u0915\u094d\u0924\u093f\u0902 \u0915\u0930\u094b\u092e\u093f <br/> \u0936\u094d\u0930\u093f\u0924 \u0915\u0932\u094d\u092a \u0935\u093e\u091f\u093f\u0915\u093e\u092f\u093e\u0902 \u091a\u0923\u094d\u0921\u093f\u0915\u093e\u092f\u093e\u0902 \u091c\u0917\u0926\u092e\u094d\u092c\u093f\u0915\u093e\u092f\u093e\u092e\u094d"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0930\u093e\u0915\u093e \u091a\u0928\u094d\u0926\u094d\u0930 \u0935\u0926\u0928\u093e\u092f\u093e\u0902 \u0930\u093e\u091c\u0940\u0935 \u0928\u092f\u0928\u093e\u092f\u093e\u0902 <br/> \u092a\u093e\u0915\u093e\u0930\u093f \u0928\u0941\u0924 \u091a\u0930\u0923\u093e\u092f\u093e\u0902 \u0906\u0915\u093e\u0936\u093e\u0926\u093f \u0915\u093f\u0930\u0923\u093e\u092f\u093e\u092e\u094d"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0939\u094d\u0930\u0940\u0902\u0915\u093e\u0930 \u0935\u093f\u092a\u093f\u0928 \u0939\u0930\u093f\u0923\u094d\u092f\u093e\u0902 \u0939\u094d\u0930\u0940\u0902\u09