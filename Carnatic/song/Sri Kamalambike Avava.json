0bcb\u0b95 \u0b9c\u0ba9\u0ba9\u0bbf \u0baa\u0b95 \u0bae\u0bbe\u0bb2\u0bbf\u0ba9\u0bbf \u0b9a\u0b95\u0bcd\u0bb0\u0bc1\u0ba4\u0bcd-"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b86\u0bb2\u0bcb\u0b95\u0baf \u0bae\u0bbe\u0bae\u0bcd \u0b9a\u0bb0\u0bcd\u0bb5 \u0b9a\u0bbf\u0ba4\u0bcd\u0ba4\u0bbf-\u0baa\u0bcd\u0bb0\u0ba4\u0bbe\u0baf\u0bbf\u0b95\u0bc7 <br/> \u0ba4\u0bcd\u0bb0\u0bbf\u0baa\u0bc1\u0bb0\u0bbe\u0bae\u0bcd\u0baa\u0bbf\u0b95\u0bc7 \u0baa\u0bbe\u0bb2\u0bbe\u0bae\u0bcd\u0baa\u0bbf\u0b95\u0bc7"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0b9a\u0ba8\u0bcd\u0ba4\u0baa\u0bcd\u0ba4 \u0bb9\u0bc7\u0bae \u0b9a\u0ba9\u0bcd\u0ba9\u0bbf\u0baa \u0ba4\u0bc7\u0bb9\u0bc7 <br/> \u0b9a\u0ba4\u0bbe(\u0b85)\u0b95\u0ba3\u0bcd\u0b9f\u0bc8\u0b95 \u0bb0\u0b9a \u0baa\u0bcd\u0bb0\u0bb5\u0bbe\u0bb9\u0bc7 <br/> \u0b9a\u0ba8\u0bcd\u0ba4\u0bbe\u0baa \u0bb9\u0bb0 \u0ba4\u0bcd\u0bb0\u0bbf\u0b95\u0bcb\u0ba3 \u0b95\u0bc7\u0bb9\u0bc7 <br/> \u0b9a\u0b95\u0bbe\u0bae\u0bc7\u0bb6\u0bcd\u0bb5\u0bb0\u0bbf \u0b9a\u0b95\u0bcd\u0ba4\u0bbf \u0b9a\u0bae\u0bc2\u0bb9\u0bc7 <br/> \u0b9a\u0ba8\u0bcd\u0ba4\u0ba4\u0bae\u0bcd \u0bae\u0bc1\u0b95\u0bcd\u0ba4\u0bbf \u0b95\u0ba3\u0bcd\u0b9f\u0bbe \u0bae\u0ba3\u0bbf - <br/> \u0b95\u0bcb\u0bb7\u0bbe\u0baf\u0bae\u0bbe\u0ba9 \u0b95\u0bb5\u0bbe\u0b9f \u0ba4\u0bcd\u0bb5\u0bbe\u0bb0\u0bc7 <br/> \u0b85\u0ba9\u0ba8\u0bcd\u0ba4 \u0b95\u0bc1\u0bb0\u0bc1 \u0b95\u0bc1\u0bb9 \u0bb5\u0bbf\u0ba4\u0bbf\u0ba4\u0bc7 <br/> \u0b95\u0bb0\u0bbe\u0b99\u0bcd\u0b95\u0bc1\u0bb2\u0bbf \u0ba8\u0b95\u0bcb\u0ba4\u0baf \u0bb5\u0bbf\u0bb7\u0bcd\u0ba3\u0bc1 \u0ba4\u0b9a\u0bbe\u0bb5\u0ba4\u0bbe\u0bb0\u0bc7"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b85\u0ba8\u0bcd\u0ba4\u0bb9\u0b95\u0bb0\u0ba3\u0bc7\u0b95\u0bcd\u0bb7\u0bc1 \u0b95\u0bbe\u0bb0\u0bcd\u0bae\u0bc1\u0b95 \u0b9a\u0baa\u0bcd\u0ba4\u0bbe\u0ba4\u0bbf - <br/> \u0baa\u0ba9\u0bcd\u0b9a \u0ba4\u0ba9\u0bcd\u0bae\u0bbe\u0ba4\u0bcd\u0bb0 \u0bb5\u0bbf\u0b9a\u0bbf\u0b95\u0bbe(\u0b85)\u0ba4\u0bcd\u0baf\u0ba8\u0bcd\u0ba4 - <br/> \u0bb0\u0bbe\u0b95 \u0baa\u0bbe\u0b9a \u0ba4\u0bcd\u0bb5\u0bc7\u0bb7\u0bbe\u0ba9\u0bcd\u0b95\u0bc1\u0b9a \u0ba4\u0bb0 - <br/> \u0b95\u0bb0\u0bc7(\u0b85)\u0ba4\u0bbf \u0bb0\u0bb9\u0bb8\u0bcd\u0baf \u0baf\u0bcb\u0b95\u0bbf\u0ba9\u0bc0 \u0baa\u0bb0\u0bc7"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c4d\u0c30\u0c40 \u0c15\u0c2e\u0c32\u0c3e\u0c2e\u0c4d\u0c2c\u0c3f\u0c15\u0c47 \u0c05\u0c35\u0c3e\u0c35 <br/> \u0c36\u0c3f\u0c35\u0c47 \u0c15\u0c30 \u0c27\u0c43\u0c24 \u0c36\u0c41\u0c15 \u0c36\u0c3e\u0c30\u0c3f\u0c15\u0c47"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c32\u0c4b\u0c15 \u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c3f \u0c15\u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c3f \u0c36\u0c42\u0c32\u0c3f\u0c28\u0c3f <br/> \u0c32\u0c4b\u0c15 \u0c1c\u0c28\u0c28\u0c3f \u0c2d\u0c17 \u0c2e\u0c3e\u0c32\u0c3f\u0c28\u0c3f \u0c38\u0c15\u0c43\u0c26\u0c4d-"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c06\u0c32\u0c4b\u0c15\u0c2f \u0c2e\u0c3e\u0c02 \u0c38\u0c30\u0c4d\u0c35 \u0c38\u0c3f\u0c26\u0c4d\u0c27\u0c3f-\u0c2a\u0c4d\u0c30\u0c26\u0c3e\u0c2f\u0c3f\u0c15\u0c47 <br/> \u0c24\u0c4d\u0c30\u0c3f\u0c2a\u0c41\u0c30\u0c3e\u0c2e\u0c4d\u0c2c\u0c3f\u0c15\u0c47 \u0c2c\u0c3e\u0c32\u0c3e\u0c2e\u0c4d\u0c2c\u0c3f\u0c15\u0c47"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c38\u0c28\u0c4d\u0c24\u0c2a\u0c4d\u0c24 \u0c39\u0c47\u0c2e \u0c38\u0c28\u0c4d\u0c28\u0c3f\u0c2d \u0c26\u0c47\u0c39\u0c47 <br/> \u0c38\u0c26\u0c3e(\u0c05)\u0c16\u0c23\u0c4d\u0c21\u0c48\u0c15 \u0c30\u0c38 \u0c2a\u0c4d\u0c30\u0c35\u0c3e\u0c39\u0c47 <br/> \u0c38\u0c28\u0c4d\u0c24\u0c3e\u0c2a \u0c39\u0c30 \u0c24\u0c4d\u0c30\u0c3f\u0c15\u0c4b\u0c23 \u0c17\u0c47\u0c39\u0c47 <br/> \u0c38\u0c15\u0c3e\u0c2e\u0c47\u0c36\u0c4d\u0c35\u0c30\u0c3f \u0c36\u0c15\u0c4d\u0c24\u0c3f \u0c38\u0c2e\u0c42\u0c39\u0c47 <br/> \u0c38\u0c28\u0c4d\u0c24\u0c24\u0c02 \u0c2e\u0c41\u0c15\u0c4d\u0c24\u0c3f \u0c18\u0c23\u0c4d\u0c1f\u0c3e \u0c2e\u0c23\u0c3f - <br/> \u0c18\u0c4b\u0c37\u0c3e\u0c2f\u0c2e\u0c3e\u0c28 \u0c15\u0c35\u0c3e\u0c1f \u0c26\u0c4d\u0c35\u0c3e\u0c30\u0c47 <br/> \u0c05\u0c28\u0c28\u0c4d\u0c24 \u0c17\u0c41\u0c30\u0c41 \u0c17\u0c41\u0c39 \u0c35\u0c3f\u0c26\u0c3f\u0c24\u0c47 <br/> \u0c15\u0c30\u0c3e\u0c19\u0c4d\u0c17\u0c41\u0c32\u0c3f \u0c28\u0c16\u0c4b\u0c26\u0c2f \u0c35\u0c3f\u0c37\u0c4d\u0c23\u0c41 \u0c26\u0c36\u0c3e\u0c35\u0c24\u0c3e\u0c30\u0c47"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c05\u0c28\u0c4d\u0c24\u0c03\u0c15\u0c30\u0c23\u0c47\u0c15\u0c4d\u0c37\u0c41 \u0c15\u0c3e\u0c30\u0c4d\u0c2e\u0c41\u0c15 \u0c36\u0c2c\u0c4d\u0c26\u0c3e\u0c26\u0c3f - <br/> \u0c2a\u0c28\u0c4d\u0c1a \u0c24\u0c28\u0c4d\u0c2e\u0c3e\u0c24\u0c4d\u0c30 \u0c35\u0c3f\u0c36\u0c3f\u0c16\u0c3e(\u0c05)\u0c24\u0c4d\u0c2f\u0c28\u0c4d\u0c24 - <br/> \u0c30\u0c3e\u0c17 \u0c2a\u0c3e\u0c36 \u0c26\u0c4d\u0c35\u0c47\u0c37\u0c3e\u0c28\u0c4d\u0c15\u0c41\u0c36 \u0c27\u0c30 - <br/> \u0c15\u0c30\u0c47(\u0c05)\u0c24\u0c3f \u0c30\u0c39\u0c38\u0c4d\u0c2f \u0c2f\u0c4b\u0c17\u0c3f\u0c28\u0c40 \u0c2a\u0c30\u0c47"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cae\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cc7 \u0c85\u0cb5\u0cbe\u0cb5 <br/> \u0cb6\u0cbf\u0cb5\u0cc7 \u0c95\u0cb0 \u0ca7\u0cc3\u0ca4 \u0cb6\u0cc1\u0c95 \u0cb6\u0cbe\u0cb0\u0cbf\u0c95\u0cc7"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb2\u0ccb\u0c95 \u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0cbf \u0c95\u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0cbf \u0cb6\u0cc2\u0cb2\u0cbf\u0ca8\u0cbf <br/> \u0cb2\u0ccb\u0c95 \u0c9c\u0ca8\u0ca8\u0cbf \u0cad\u0c97 \u0cae\u0cbe\u0cb2\u0cbf\u0ca8\u0cbf \u0cb8\u0c95\u0cc3\u0ca6\u0ccd-"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c86\u0cb2\u0ccb\u0c95\u0caf \u0cae\u0cbe\u0c82 \u0cb8\u0cb0\u0ccd\u0cb5 \u0cb8\u0cbf\u0ca6\u0ccd\u0ca7\u0cbf-\u0caa\u0ccd\u0cb0\u0ca6\u0cbe\u0caf\u0cbf\u0c95\u0cc7 <br/> \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0\u0cbe\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cc7 \u0cac\u0cbe\u0cb2\u0cbe\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cb8\u0ca8\u0ccd\u0ca4\u0caa\u0ccd\u0ca4 \u0cb9\u0cc7\u0cae \u0cb8\u0ca8\u0ccd\u0ca8\u0cbf\u0cad \u0ca6\u0cc7\u0cb9\u0cc7 <br/> \u0cb8\u0ca6\u0cbe(\u0c85)\u0c96\u0ca3\u0ccd\u0ca1\u0cc8\u0c95 \u0cb0\u0cb8 \u0caa\u0ccd\u0cb0\u0cb5\u0cbe\u0cb9\u0cc7 <br/> \u0cb8\u0ca8\u0ccd\u0ca4\u0cbe\u0caa \u0cb9\u0cb0 \u0ca4\u0ccd\u0cb0\u0cbf\u0c95\u0ccb\u0ca3 \u0c97\u0cc7\u0cb9\u0cc7 <br/> \u0cb8\u0c95\u0cbe\u0cae\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0\u0cbf \u0cb6\u0c95\u0ccd\u0ca4\u0cbf \u0cb8\u0cae\u0cc2\u0cb9\u0cc7 <br/> \u0cb8\u0ca8\u0ccd\u0ca4\u0ca4\u0c82 \u0cae\u0cc1\u0c95\u0ccd\u0ca4\u0cbf \u0c98\u0ca3\u0ccd\u0c9f\u0cbe \u0cae\u0ca3\u0cbf - <br/> \u0c98\u0ccb\u0cb7\u0cbe\u0caf\u0cae\u0cbe\u0ca8 \u0c95\u0cb5\u0cbe\u0c9f \u0ca6\u0ccd\u0cb5\u0cbe\u0cb0\u0cc7 <br/> \u0c85\u0ca8\u0ca8\u0ccd\u0ca4 \u0c97\u0cc1\u0cb0\u0cc1 \u0c97\u0cc1\u0cb9 \u0cb5\u0cbf\u0ca6\u0cbf\u0ca4\u0cc7 <br/> \u0c95\u0cb0\u0cbe\u0c99\u0ccd\u0c97\u0cc1\u0cb2\u0cbf \u0ca8\u0c96\u0ccb\u0ca6\u0caf \u0cb5\u0cbf\u0cb7\u0ccd\u0ca3\u0cc1 \u0ca6\u0cb6\u0cbe\u0cb5\u0ca4\u0cbe\u0cb0\u0cc7"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c85\u0ca8\u0ccd\u0ca4\u0c83\u0c95\u0cb0\u0ca3\u0cc7\u0c95\u0ccd\u0cb7\u0cc1 \u0c95\u0cbe\u0cb0\u0ccd\u0cae\u0cc1\u0c95 \u0cb6\u0cac\u0ccd\u0ca6\u0cbe\u0ca6\u0cbf - <br/> \u0caa\u0ca8\u0ccd\u0c9a \u0ca4\u0ca8\u0ccd\u0cae\u0cbe\u0ca4\u0ccd\u0cb0 \u0cb5\u0cbf\u0cb6\u0cbf\u0c96\u0cbe(\u0c85)\u0ca4\u0ccd\u0caf\u0ca8\u0ccd\u0ca4 - <br/> \u0cb0\u0cbe\u0c97 \u0caa\u0cbe\u0cb6 \u0ca6\u0ccd\u0cb5\u0cc7\u0cb7\u0cbe\u0ca8\u0ccd\u0c95\u0cc1\u0cb6 \u0ca7\u0cb0 - <br/> \u0c95\u0cb0\u0cc7(\u0c85)\u0ca4\u0cbf \u0cb0\u0cb9\u0cb8\u0ccd\u0caf \u0caf\u0ccb\u0c97\u0cbf\u0ca8\u0cc0 \u0caa\u0cb0\u0cc7"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d2e\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d3f\u0d15\u0d47 \u0d05\u0d35\u0d3e\u0d35 <br/> \u0d36\u0d3f\u0d35\u0d47 \u0d15\u0d30 \u0d27\u0d43\u0d24 \u0d36\u0d41\u0d15 \u0d36\u0d3e\u0d30\u0d3f\u0d15\u0d47"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d32\u0d4b\u0d15 \u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d3f \u0d15\u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d3f \u0d36\u0d42\u0d32\u0d3f\u0d28\u0d3f <br/> \u0d32\u0d4b\u0d15 \u0d1c\u0d28\u0d28\u0d3f \u0d2d\u0d17 \u0d2e\u0d3e\u0d32\u0d3f\u0d28\u0d3f \u0d38\u0d15\u0d43\u0d26\u0d4d-"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d06\u0d32\u0d4b\u0d15\u0d2f \u0d2e\u0d3e\u0d02 \u0d38\u0d30\u0d4d\u0d35 \u0d38\u0d3f\u0d26\u0d4d\u0d27\u0d3f-\u0d2a\u0d4d\u0d30\u0d26\u0d3e\u0d2f\u0d3f\u0d15\u0d47 <br/> \u0d24\u0d4d\u0d30\u0d3f\u0d2a\u0d41\u0d30\u0d3e\u0d2e\u0d4d\u0d2c\u0d3f\u0d15\u0d47 \u0d2c\u0d3e\u0d32\u0d3e\u0d2e\u0d4d\u0d2c\u0d3f\u0d15\u0d47"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d38\u0d28\u0d4d\u0d24\u0d2a\u0d4d\u0d24 \u0d39\u0d47\u0d2e \u0d38\u0d28\u0d4d\u0d28\u0d3f\u0d2d \u0d26\u0d47\u0d39\u0d47 <br/> \u0d38\u0d26\u0d3e(\u0d05)\u0d16\u0d23\u0d4d\u0d21\u0d48\u0d15 \u0d30\u0d38 \u0d2a\u0d4d\u0d30\u0d35\u0d3e\u0d39\u0d47 <br/> \u0d38\u0d28\u0d4d\u0d24\u0d3e\u0d2a \u0d39\u0d30 \u0d24\u0d4d\u0d30\u0d3f\u0d15\u0d4b\u0d23 \u0d17\u0d47\u0d39\u0d47 <br/> \u0d38\u0d15\u0d3e\u0d2e\u0d47\u0d36\u0d4d\u0d35\u0d30\u0d3f \u0d36\u0d15\u0d4d\u0d24\u0d3f \u0d38\u0d2e\u0d42\u0d39\u0d47 <br/> \u0d38\u0d28\u0d4d\u0d24\u0d24\u0d02 \u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3f \u0d18\u0d23\u0d4d\u0d1f\u0d3e \u0d2e\u0d23\u0d3f - <br/> \u0d18\u0d4b\u0d37\u0d3e\u0d2f\u0d2e\u0d3e\u0d28 \u0d15\u0d35\u0d3e\u0d1f \u0d26\u0d4d\u0d35\u0d3e\u0d30\u0d47 <br/> \u0d05\u0d28\u0d28\u0d4d\u0d24 \u0d17\u0d41\u0d30\u0d41 \u0d17\u0d41\u0d39 \u0d35\u0d3f\u0d26\u0d3f\u0d24\u0d47 <br/> \u0d15\u0d30\u0d3e\u0d19\u0d4d\u0d17\u0d41\u0d32\u0d3f \u0d28\u0d16\u0d4b\u0d26\u0d2f \u0d35\u0d3f\u0d37\u0d4d\u0d23\u0d41 \u0d26\u0d36\u0d3e\u0d35\u0d24\u0d3e\u0d30\u0d47"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d05\u0d28\u0d4d\u0d24\u0d03\u0d15\u0d30\u0d23\u0d47\u0d15\u0d4d\u0d37\u0d41 \u0d15\u0d3e\u0d30\u0d4d\u0d2e\u0d41\u0d15 \u0d36\u0d2c\u0d4d\u0d26\u0d3e\u0d26\u0d3f - <br/> \u0d2a\u0d28\u0d4d\u0d1a \u0d24\u0d28\u0d4d\u0d2e\u0d3e\u0d24\u0d4d\u0d30 \u0d35\u0d3f\u0d36\u0d3f\u0d16\u0d3e(\u0d05)\u0d24\u0d4d\u0d2f\u0d28\u0d4d\u0d24 - <br/> \u0d30\u0d3e\u0d17 \u0d2a\u0d3e\u0d36 \u0d26\u0d4d\u0d35\u0d47\u0d37\u0d3e\u0d28\u0d4d\u0d15\u0d41\u0d36 \u0d27\u0d30 - <br/> \u0d15\u0d30\u0d47(\u0d05)\u0d24\u0d3f \u0d30\u0d39\u0d38\u0d4d\u0d2f \u0d2f\u0d4b\u0d17\u0d3f\u0d28\u0d40 \u0d2a\u0d30\u0d47"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u094d\u0930\u0940 \u0915\u092e\u0932\u093e\u092e\u094d\u092c\u093f\u0915\u0947 \u0905\u0935\u093e\u0935 <br/> \u0936\u093f\u0935\u0947 \u0915\u0930 \u0927\u0943\u0924 \u0936\u0941\u0915 \u0936\u093e\u0930\u093f\u0915\u0947"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0932\u094b\u0915 \u092a\u093e\u0932\u093f\u0928\u093f \u0915\u092a\u093e\u0932\u093f\u0928\u093f \u0936\u0942\u0932\u093f\u0928\u093f <br/> \u0932\u094b\u0915 \u091c\u0928\u0928\u093f \u092d\u0917 \u092e\u093e\u0932\u093f\u0928\u093f \u0938\u0915\u0943\u0926\u094d-"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0906\u0932\u094b\u0915\u092f \u092e\u093e\u0902 \u0938\u0930\u094d\u0935 \u0938\u093f\u0926\u094d\u0927\u093f-\u092a\u094d\u0930\u0926\u093e\u092f\u093f\u0915\u0947 <br/> \u0924\u094d\u0930\u093f\u092a\u0941\u0930\u093e\u092e\u094d\u092c\u093f\u0915\u0947 \u092c\u093e\u0932\u093e\u092e\u094d\u092c\u093f\u0915\u0947"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0938\u0928\u094d\u0924\u092a\u094d\u0924 \u0939\u0947\u092e \u0938\u0928\u094d\u0928\u093f\u092d \u0926\u0947\u0939\u0947 <br/> \u0938\u0926\u093e(\u0905)\u0916\u0923\u094d\u0921\u0948\u0915 \u0930\u0938 \u092a\u094d\u0930\u0935\u093e\u0939\u0947 <br/> \u0938\u0928\u094d\u0924\u093e\u092a \u0939\u0930 \u0924\u094d\u0930\u093f\u0915\u094b\u0923 \u0917\u0947\u0939\u0947 <br/> \u0938\u0915\u093e\u092e\u0947\u0936\u094d\u0935\u0930\u093f \u0936\u0915\u094d\u0924\u093f \u0938\u092e\u0942\u0939\u0947 <br/> \u0938\u0928\u094d\u0924\u0924\u0902 \u092e\u0941\u0915\u094d\u0924\u093f \u0918\u0923\u094d\u091f\u093e \u092e\u0923\u093f - <br/> \u0918\u094b\u0937\u093e\u092f\u092e\u093e\u0928 \u0915\u0935\u093e\u091f \u0926\u094d\u0935\u093e\u0930\u0947 <br/> \u0905\u0928\u0928\u094d\u0924 \u0917\u0941\u0930\u0941 \u0917\u0941\u0939 \u0935\u093f\u0926\u093f\u0924\u0947 <br/> \u0915\u0930\u093e\u0919\u094d\u0917\u0941\u0932\u093f \u0928\u0916\u094b\u0926\u092f \u0935\u093f\u0937\u094d\u0923\u0941 \u0926\u0936\u093e\u0935\u0924\u093e\u0930\u0947"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0905\u0928\u094d\u0924\u0903\u0915\u0930\u0923\u0947\u0915\u094d\u0937\u0941 \u0915\u093e\u0930\u094d\u092e\u0941\u0915 \u0936\u092c\u094d\u0926\u093e\u0926\u093f - <br/> \u092a\u0928\u094d\u091a \u0924\u0928\u094d\u092e\u093e\u0924\u094d\u0930 \u0935\u093f\u0936\u093f\u0916\u093e(\u0905)\u0924\u094d\u092f\u0928\u094d\u0924 - <br/> \u0930\u093e\u0917 \u092a\u093e\u0936 \u0926\u094d\u0935\u0947\u0937\u093e\u0928\u094d\u0915\u0941\u0936 \u0927\u0930 - <br/> \u0915\u0930\u0947(\u0905)\u0924\u093f \u0930\u0939\u0938\u094d\u092f \u092f\u094b\u0917\u093f\u0928\u0940 \u092a\u0930\u0947"}]}],