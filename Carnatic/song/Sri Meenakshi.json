bcd\u0bb5\u0bb0 \u0bae\u0bcb\u0bb9\u0bbf\u0ba4\u0bbe\u0b95\u0bbe\u0bb0\u0bc7 <br/> \u0b95\u0bbe\u0bae\u0bbf\u0ba4 \u0baa\u0bb2 \u0baa\u0bcd\u0bb0\u0ba4\u0bbe\u0baf\u0b95 \u0b95\u0ba4\u0bae\u0bcd\u0baa \u0bb5\u0bbf\u0baa\u0bbf\u0ba9\u0bbe\u0b95\u0bbe\u0bb0\u0bc7 <br/> \u0baa\u0bc2\u0bae\u0bbf \u0baa\u0bbe\u0bb2 \u0ba8\u0bc1\u0ba4 \u0b95\u0bc1\u0bb0\u0bc1 \u0b95\u0bc1\u0bb9 \u0baa\u0bc2\u0b9c\u0bbf\u0ba4 \u0bae\u0bc1\u0b95\u0bcd\u0ba4\u0bbe \u0bb9\u0bbe\u0bb0\u0bc7"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0b95\u0bcb\u0bae\u0bb2-\u0ba4\u0bb0 \u0baa\u0ba4 \u0baf\u0bc1\u0b95\u0bb3\u0bc7 <br/> \u0b95\u0bc1\u0b9a\u0bcd\u0b9a \u0b9c\u0baf \u0bb8\u0bcd\u0ba4\u0ba9 \u0baf\u0bc1\u0b95\u0bb3\u0bc7 <br/> \u0ba4\u0bbe\u0bae\u0bb0\u0b9a \u0b9a\u0bb0\u0ba3 \u0baf\u0bc1\u0b95\u0bb3\u0bc7 <br/> \u0ba4\u0ba4\u0bcd\u0ba4\u0bcd\u0bb5\u0bae\u0bcd \u0baa\u0ba4\u0bbe\u0bb0\u0bcd\u0ba4 \u0baf\u0bc1\u0b95\u0bb3\u0bc7"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c4d\u0c30\u0c40 \u0c2e\u0c40\u0c28\u0c3e\u0c15\u0c4d\u0c37\u0c3f \u0c17\u0c4c\u0c30\u0c3f \u0c30\u0c3e\u0c1c \u0c36\u0c4d\u0c2f\u0c3e\u0c2e\u0c33\u0c47 <br/> \u0c06\u0c36\u0c4d\u0c30\u0c3f\u0c24 \u0c30\u0c15\u0c4d\u0c37\u0c23 \u0c27\u0c41\u0c30\u0c40\u0c23-\u0c24\u0c30 - <br/> \u0c32\u0c18\u0c41 \u0c36\u0c4d\u0c2f\u0c3e\u0c2e\u0c33\u0c47 \u0c38\u0c15\u0c33\u0c47 <br/> \u0c36\u0c43\u0c02\u0c17\u0c3e\u0c30 \u0c15\u0c3e\u0c2e \u0c15\u0c32\u0c47 \u0c36\u0c4d\u0c30\u0c3f\u0c2f\u0c02 \u0c26\u0c47\u0c39\u0c3f \u0c2e\u0c3e\u0c02 \u0c2a\u0c3e\u0c39\u0c3f"}, {"M": "\u0c38\u0c2e\u0c38\u0c4d\u0c24\u0c3f \u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c2d\u0c42\u0c2e\u0c3f \u0c1c\u0c32\u0c3e\u0c17\u0c4d\u0c28\u0c4d\u0c2f\u0c28\u0c3f\u0c32 \u0c17\u0c17\u0c28 \u0c2d\u0c42\u0c24 \u0c2a\u0c28\u0c4d\u0c1a\u0c15\u0c3e\u0c27\u0c3e\u0c30\u0c47 <br/> \u0c38\u0c4d\u0c35\u0c3e\u0c2e\u0c3f\u0c28\u0c3f \u0c38\u0c4b\u0c2e \u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30\u0c47\u0c36\u0c4d\u0c35\u0c30 \u0c2e\u0c4b\u0c39\u0c3f\u0c24\u0c3e\u0c15\u0c3e\u0c30\u0c47 <br/> \u0c15\u0c3e\u0c2e\u0c3f\u0c24 \u0c2b\u0c32 \u0c2a\u0c4d\u0c30\u0c26\u0c3e\u0c2f\u0c15 \u0c15\u0c26\u0c2e\u0c4d\u0c2c \u0c35\u0c3f\u0c2a\u0c3f\u0c28\u0c3e\u0c17\u0c3e\u0c30\u0c47 <br/> \u0c2d\u0c42\u0c2e\u0c3f \u0c2a\u0c3e\u0c32 \u0c28\u0c41\u0c24 \u0c17\u0c41\u0c30\u0c41 \u0c17\u0c41\u0c39 \u0c2a\u0c42\u0c1c\u0c3f\u0c24 \u0c2e\u0c41\u0c15\u0c4d\u0c24\u0c3e \u0c39\u0c3e\u0c30\u0c47"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c15\u0c4b\u0c2e\u0c32-\u0c24\u0c30 \u0c2a\u0c26 \u0c2f\u0c41\u0c17\u0c33\u0c47 <br/> \u0c17\u0c41\u0c1a\u0c4d\u0c1b \u0c1c\u0c2f \u0c38\u0c4d\u0c24\u0c28 \u0c2f\u0c41\u0c17\u0c33\u0c47 <br/> \u0c24\u0c3e\u0c2e\u0c30\u0c38 \u0c1a\u0c30\u0c23 \u0c2f\u0c41\u0c17\u0c33\u0c47 <br/> \u0c24\u0c24\u0c4d\u0c24\u0c4d\u0c35\u0c02 \u0c2a\u0c26\u0c3e\u0c30\u0c4d\u0c25 \u0c2f\u0c41\u0c17\u0c33\u0c47"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0cae\u0cc0\u0ca8\u0cbe\u0c95\u0ccd\u0cb7\u0cbf \u0c97\u0ccc\u0cb0\u0cbf \u0cb0\u0cbe\u0c9c \u0cb6\u0ccd\u0caf\u0cbe\u0cae\u0cb3\u0cc7 <br/> \u0c86\u0cb6\u0ccd\u0cb0\u0cbf\u0ca4 \u0cb0\u0c95\u0ccd\u0cb7\u0ca3 \u0ca7\u0cc1\u0cb0\u0cc0\u0ca3-\u0ca4\u0cb0 - <br/> \u0cb2\u0c98\u0cc1 \u0cb6\u0ccd\u0caf\u0cbe\u0cae\u0cb3\u0cc7 \u0cb8\u0c95\u0cb3\u0cc7 <br/> \u0cb6\u0cc3\u0c82\u0c97\u0cbe\u0cb0 \u0c95\u0cbe\u0cae \u0c95\u0cb2\u0cc7 \u0cb6\u0ccd\u0cb0\u0cbf\u0caf\u0c82 \u0ca6\u0cc7\u0cb9\u0cbf \u0cae\u0cbe\u0c82 \u0caa\u0cbe\u0cb9\u0cbf"}, {"M": "\u0cb8\u0cae\u0cb8\u0ccd\u0ca4\u0cbf \u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cad\u0cc2\u0cae\u0cbf \u0c9c\u0cb2\u0cbe\u0c97\u0ccd\u0ca8\u0ccd\u0caf\u0ca8\u0cbf\u0cb2 \u0c97\u0c97\u0ca8 \u0cad\u0cc2\u0ca4 \u0caa\u0ca8\u0ccd\u0c9a\u0c95\u0cbe\u0ca7\u0cbe\u0cb0\u0cc7 <br/> \u0cb8\u0ccd\u0cb5\u0cbe\u0cae\u0cbf\u0ca8\u0cbf \u0cb8\u0ccb\u0cae \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0 \u0cae\u0ccb\u0cb9\u0cbf\u0ca4\u0cbe\u0c95\u0cbe\u0cb0\u0cc7 <br/> \u0c95\u0cbe\u0cae\u0cbf\u0ca4 \u0cab\u0cb2 \u0caa\u0ccd\u0cb0\u0ca6\u0cbe\u0caf\u0c95 \u0c95\u0ca6\u0cae\u0ccd\u0cac \u0cb5\u0cbf\u0caa\u0cbf\u0ca8\u0cbe\u0c97\u0cbe\u0cb0\u0cc7 <br/> \u0cad\u0cc2\u0cae\u0cbf \u0caa\u0cbe\u0cb2 \u0ca8\u0cc1\u0ca4 \u0c97\u0cc1\u0cb0\u0cc1 \u0c97\u0cc1\u0cb9 \u0caa\u0cc2\u0c9c\u0cbf\u0ca4 \u0cae\u0cc1\u0c95\u0ccd\u0ca4\u0cbe \u0cb9\u0cbe\u0cb0\u0cc7"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c95\u0ccb\u0cae\u0cb2-\u0ca4\u0cb0 \u0caa\u0ca6 \u0caf\u0cc1\u0c97\u0cb3\u0cc7 <br/> \u0c97\u0cc1\u0c9a\u0ccd\u0c9b \u0c9c\u0caf \u0cb8\u0ccd\u0ca4\u0ca8 \u0caf\u0cc1\u0c97\u0cb3\u0cc7 <br/> \u0ca4\u0cbe\u0cae\u0cb0\u0cb8 \u0c9a\u0cb0\u0ca3 \u0caf\u0cc1\u0c97\u0cb3\u0cc7 <br/> \u0ca4\u0ca4\u0ccd\u0ca4\u0ccd\u0cb5\u0c82 \u0caa\u0ca6\u0cbe\u0cb0\u0ccd\u0ca5 \u0caf\u0cc1\u0c97\u0cb3\u0cc7"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d4d\u0d30\u0d40 \u0d2e\u0d40\u0d28\u0d3e\u0d15\u0d4d\u0d37\u0d3f \u0d17\u0d4c\u0d30\u0d3f \u0d30\u0d3e\u0d1c \u0d36\u0d4d\u0d2f\u0d3e\u0d2e\u0d33\u0d47 <br/> \u0d06\u0d36\u0d4d\u0d30\u0d3f\u0d24 \u0d30\u0d15\u0d4d\u0d37\u0d23 \u0d27\u0d41\u0d30\u0d40\u0d23-\u0d24\u0d30 - <br/> \u0d32\u0d18\u0d41 \u0d36\u0d4d\u0d2f\u0d3e\u0d2e\u0d33\u0d47 \u0d38\u0d15\u0d33\u0d47 <br/> \u0d36\u0d43\u0d02\u0d17\u0d3e\u0d30 \u0d15\u0d3e\u0d2e \u0d15\u0d32\u0d47 \u0d36\u0d4d\u0d30\u0d3f\u0d2f\u0d02 \u0d26\u0d47\u0d39\u0d3f \u0d2e\u0d3e\u0d02 \u0d2a\u0d3e\u0d39\u0d3f"}, {"M": "\u0d38\u0d2e\u0d38\u0d4d\u0d24\u0d3f \u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d2d\u0d42\u0d2e\u0d3f \u0d1c\u0d32\u0d3e\u0d17\u0d4d\u0d28\u0d4d\u0d2f\u0d28\u0d3f\u0d32 \u0d17\u0d17\u0d28 \u0d2d\u0d42\u0d24 \u0d2a\u0d28\u0d4d\u0d1a\u0d15\u0d3e\u0d27\u0d3e\u0d30\u0d47 <br/> \u0d38\u0d4d\u0d35\u0d3e\u0d2e\u0d3f\u0d28\u0d3f \u0d38\u0d4b\u0d2e \u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d47\u0d36\u0d4d\u0d35\u0d30 \u0d2e\u0d4b\u0d39\u0d3f\u0d24\u0d3e\u0d15\u0d3e\u0d30\u0d47 <br/> \u0d15\u0d3e\u0d2e\u0d3f\u0d24 \u0d2b\u0d32 \u0d2a\u0d4d\u0d30\u0d26\u0d3e\u0d2f\u0d15 \u0d15\u0d26\u0d2e\u0d4d\u0d2c \u0d35\u0d3f\u0d2a\u0d3f\u0d28\u0d3e\u0d17\u0d3e\u0d30\u0d47 <br/> \u0d2d\u0d42\u0d2e\u0d3f \u0d2a\u0d3e\u0d32 \u0d28\u0d41\u0d24 \u0d17\u0d41\u0d30\u0d41 \u0d17\u0d41\u0d39 \u0d2a\u0d42\u0d1c\u0d3f\u0d24 \u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3e \u0d39\u0d3e\u0d30\u0d47"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d15\u0d4b\u0d2e\u0d32-\u0d24\u0d30 \u0d2a\u0d26 \u0d2f\u0d41\u0d17\u0d33\u0d47 <br/> \u0d17\u0d41\u0d1a\u0d4d\u0d1b \u0d1c\u0d2f \u0d38\u0d4d\u0d24\u0d28 \u0d2f\u0d41\u0d17\u0d33\u0d47 <br/> \u0d24\u0d3e\u0d2e\u0d30\u0d38 \u0d1a\u0d30\u0d23 \u0d2f\u0d41\u0d17\u0d33\u0d47 <br/> \u0d24\u0d24\u0d4d\u0d24\u0d4d\u0d35\u0d02 \u0d2a\u0d26\u0d3e\u0d30\u0d4d\u0d25 \u0d2f\u0d41\u0d17\u0d33\u0d47"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u094d\u0930\u0940 \u092e\u0940\u0928\u093e\u0915\u094d\u0937\u093f \u0917\u094c\u0930\u093f \u0930\u093e\u091c \u0936\u094d\u092f\u093e\u092e\u0932\u0947 <br/> \u0906\u0936\u094d\u0930\u093f\u0924 \u0930\u0915\u094d\u0937\u0923 \u0927\u0941\u0930\u0940\u0923-\u0924\u0930 - <br/> \u0932\u0918\u0941 \u0936\u094d\u092f\u093e\u092e\u0932\u0947 \u0938\u0915\u0932\u0947 <br/> \u0936\u0943\u0902\u0917\u093e\u0930 \u0915\u093e\u092e \u0915\u0932\u0947 \u0936\u094d\u0930\u093f\u092f\u0902 \u0926\u0947\u0939\u093f \u092e\u093e\u0902 \u092a\u093e\u0939\u093f"}, {"M": "\u0938\u092e\u0938\u094d\u0924\u093f \u091a\u0930\u0923\u092e\u094d", "T": "\u092d\u0942\u092e\u093f \u091c\u0932\u093e\u0917\u094d\u0928\u094d\u092f\u0928\u093f\u0932 \u0917\u0917\u0928 \u092d\u0942\u0924 \u092a\u0928\u094d\u091a\u0915\u093e\u0927\u093e\u0930\u0947 <br/> \u0938\u094d\u0935\u093e\u092e\u093f\u0928\u093f \u0938\u094b\u092e \u0938\u0941\u0928\u094d\u0926\u0930\u0947\u0936\u094d\u0935\u0930 \u092e\u094b\u0939\u093f\u0924\u093e\u0915\u093e\u0930\u0947 <br/> \u0915\u093e\u092e\u093f\u0924 \u092b\u0932 \u092a\u094d\u0930\u0926\u093e\u092f\u0915 \u0915\u0926\u092e\u094d\u092c \u0935\u093f\u092a\u093f\u0928\u093e\u0917\u093e\u0930\u0947 <br/> \u092d\u0942\u092e\u093f \u092a\u093e\u0932 \u0928\u0941\u0924 \u0917\u0941\u0930\u0941 \u0917\u0941\u0939 \u092a\u0942\u091c\u093f\u0924 \u092e\u0941\u0915\u094d\u0924\u093e \u0939\u093e\u0930\u0947"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0915\u094b\u092e\u0932-\u0924\u0930 \u092a\u0926 \u092f\u0941\u0917\u0932\u0947 <br/> \u0917\u0941\u091a\u094d\u091b \u091c\u092f \u0938\u094d\u0924\u0928 \u092f\u0941\u0917\u0932\u0947 <br/> \u0924\u093e\u092e\u0930\u0938 \u091a\u0930\u0923 \u092f\u0941\u0917\u0932\u0947 <br/> \u0924\u0924\u094d\u0924\u094d\u0935\u0902 \u092a\u0926\u093e\u0930\u094d\u0925 \u092f\u0941\u0917\u0932\u0947"}]}], "L": "l", "lyricsref": [{"links": [{"N": "guru-guha.blogspot.com", "L": "http://guru-guha.blogspot.com/2008/08/dikshitar-kriti-sri-meenakshi-gauri.html"}, {"N": "karnatik.com", "L": "https://karnatik.com/c5950.shtml"}]}]}