u0bc1\u0bb0\u0bc1 \u0b9a\u0bc1\u0bb0\u0bae\u0bc1\u0ba4\u0bbf\u0ba4\u0bc7 \u0b9a\u0bbe\u0bb5\u0bbf\u0ba4\u0bcd\u0baf \u0bb5\u0bbf\u0ba4\u0bbf\u0ba4\u0bc7 \u0bb9\u0bbe\u0bb2\u0bb8\u0bcd\u0baf\u0ba9\u0b95\u0bb0 \u0bb8\u0bcd\u0ba4\u0bbf\u0ba4\u0bc7 \u0bb9\u0bc7 \u0bb2\u0bb2\u0bbf\u0ba4\u0bc7"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0bb0\u0bae\u0bbe \u0bb5\u0bc0\u0ba3\u0bbe\u0baa\u0bbe\u0ba3\u0bbf \u0baa\u0bcb\u0bb7\u0ba3 \u0b9a\u0ba8\u0bcd\u0ba4\u0bcb\u0bb7\u0ba3\u0bbf \u0bb0\u0b9c\u0ba4 \u0b9a\u0bbe\u0ba4\u0b95 \u0b86\u0bae\u0bcd\u0bb0\u0b9a \u0baa\u0bb5\u0bbf\u0b95\u0bbe\u0bb0\u0bbf\u0ba3\u0bbf <br/> \u0bb0\u0bbe\u0b95\u0bc7\u0ba8\u0bcd\u0ba4\u0bc1 \u0bb5\u0ba4\u0ba9\u0bbf \u0bb0\u0bb5\u0bbf \u0b9a\u0b9a\u0bbf \u0bb5\u0b9a\u0bbf \u0ba8\u0baf\u0ba9\u0bbf \u0bb0\u0bbe\u0b95 \u0b95\u0bbe\u0bae\u0bb5\u0bb0\u0bcd\u0ba4\u0bbf\u0ba9\u0bbf \u0bb9\u0bb0\u0bbf \u0bb5\u0bbf\u0b9f\u0bae\u0bcd\u0baa\u0bbf\u0ba9\u0bbf <br/> \u0bb6\u0bcd\u0bb0\u0bc0 \u0b9a\u0b95\u0bcd\u0bb0 \u0bb5\u0bbe\u0b9a\u0bbf\u0ba9\u0bbf \u0b85\u0bae\u0bcd\u0baa \u0b9a\u0bc1\u0bb9\u0bbe\u0b9a\u0bbf\u0ba9\u0bbf \u0bb9\u0bae\u0bcd\u0b9a\u0bbf\u0ba9\u0bbf \u0bb6\u0bcd\u0bb0\u0bc0\u0bb5\u0ba4\u0bcd\u0b9a \u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bbf \u0bb6\u0bcd\u0bb0\u0bcd\u0ba4\u0bbf \u0bb5\u0bbf\u0bb2\u0bbe\u0b9a\u0bbf\u0ba9\u0bbf"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0ba4\u0bbe\u0bb0\u0ba3 \u0ba4\u0bb0\u0ba3 \u0ba8\u0bbf\u0baa\u0bc1\u0ba3\u0bbf \u0ba4\u0bbe\u0baa\u0ba4\u0bcd\u0bb0\u0baf \u0b95\u0bbe\u0bb0\u0bbf\u0ba9\u0bcd\u0ba3\u0bbf \u0b9a\u0ba9\u0bcd\u0ba9\u0bc1\u0ba4 \u0b95\u0bb2\u0bbe\u0b9a\u0bbf\u0ba9\u0bbf \u0b9a\u0bb9\u0bb5\u0bbe\u0b9a\u0bbf\u0ba9\u0bbf \u0b9a\u0bc1\u0bb9\u0bbe\u0b9a\u0bbf\u0ba9\u0bbf"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c4d\u0c30\u0c40 \u0c36\u0c3e\u0c32\u0c3f\u0c35\u0c3e\u0c1f\u0c3f\u0c36 \u0c28\u0c3e\u0c2f\u0c3f\u0c15\u0c47 \u0c05\u0c2e\u0c4d\u0c2c \u0c36\u0c4d\u0c30\u0c4d\u0c24\u0c1c\u0c28 \u0c2b\u0c32\u0c26\u0c3e\u0c2f\u0c3f\u0c15\u0c47 \u0c1c\u0c17\u0c26\u0c2e\u0c4d\u0c2c\u0c3f\u0c15\u0c47"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c4d\u0c30\u0c40\u0c36\u0c28\u0c41\u0c24\u0c47 \u0c17\u0c3f\u0c30\u0c40\u0c36 \u0c38\u0c41\u0c24\u0c47 \u0c36\u0c3f\u0c35 \u0c38\u0c39\u0c3f\u0c24\u0c47 \u0c28\u0c3f\u0c1c\u0c2d\u0c15\u0c4d\u0c24 \u0c39\u0c3f\u0c24\u0c47 \u0c39\u0c47\u0c2e\u0c32\u0c24\u0c47"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c39\u0c30\u0c17\u0c41\u0c30\u0c41 \u0c38\u0c41\u0c30\u0c2e\u0c41\u0c26\u0c3f\u0c26\u0c47 \u0c38\u0c3e\u0c35\u0c3f\u0c26\u0c4d\u0c2f \u0c35\u0c3f\u0c27\u0c3f\u0c24\u0c47 \u0c39\u0c3e\u0c32\u0c38\u0c4d\u0c2f\u0c28\u0c15\u0c30 \u0c38\u0c4d\u0c25\u0c3f\u0c24\u0c47 \u0c39\u0c47 \u0c32\u0c32\u0c3f\u0c24\u0c47"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c30\u0c2e\u0c3e \u0c35\u0c40\u0c23\u0c3e\u0c2a\u0c3e\u0c23\u0c3f \u0c2a\u0c4b\u0c37\u0c23 \u0c38\u0c28\u0c4d\u0c24\u0c4b\u0c37\u0c23\u0c3f \u0c30\u0c1c\u0c24 \u0c38\u0c3e\u0c27\u0c15 \u0c06\u0c2e\u0c4d\u0c30\u0c38 \u0c2d\u0c35\u0c3f\u0c15\u0c3e\u0c30\u0c3f\u0c23\u0c3f <br/> \u0c30\u0c3e\u0c15\u0c47\u0c28\u0c4d\u0c26\u0c41 \u0c35\u0c26\u0c28\u0c3f \u0c30\u0c35\u0c3f \u0c36\u0c36\u0c3f \u0c35\u0c36\u0c3f \u0c28\u0c2f\u0c28\u0c3f \u0c30\u0c3e\u0c17 \u0c15\u0c3e\u0c2e\u0c35\u0c30\u0c4d\u0c27\u0c3f\u0c28\u0c3f \u0c39\u0c30\u0c3f \u0c35\u0c3f\u0c1f\u0c2e\u0c4d\u0c2c\u0c3f\u0c28\u0c3f <br/> \u0c36\u0c4d\u0c30\u0c40 \u0c1a\u0c15\u0c4d\u0c30 \u0c35\u0c3e\u0c38\u0c3f\u0c28\u0c3f \u0c05\u0c2e\u0c4d\u0c2c \u0c38\u0c41\u0c39\u0c3e\u0c38\u0c3f\u0c28\u0c3f \u0c39\u0c2e\u0c4d\u0c38\u0c3f\u0c28\u0c3f \u0c36\u0c4d\u0c30\u0c40\u0c35\u0c24\u0c4d\u0c38 \u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c3f \u0c36\u0c4d\u0c30\u0c4d\u0c24\u0c3f \u0c35\u0c3f\u0c32\u0c3e\u0c38\u0c3f\u0c28\u0c3f"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c24\u0c3e\u0c30\u0c23 \u0c24\u0c30\u0c23 \u0c28\u0c3f\u0c2a\u0c41\u0c23\u0c3f \u0c24\u0c3e\u0c2a\u0c24\u0c4d\u0c30\u0c2f \u0c15\u0c3e\u0c30\u0c3f\u0c28\u0c4d\u0c23\u0c3f \u0c38\u0c28\u0c4d\u0c28\u0c41\u0c24 \u0c15\u0c32\u0c3e\u0c38\u0c3f\u0c28\u0c3f \u0c38\u0c39\u0c35\u0c3e\u0c38\u0c3f\u0c28\u0c3f \u0c38\u0c41\u0c39\u0c3e\u0c38\u0c3f\u0c28\u0c3f"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0cb6\u0cbe\u0cb2\u0cbf\u0cb5\u0cbe\u0c9f\u0cbf\u0cb6 \u0ca8\u0cbe\u0caf\u0cbf\u0c95\u0cc7 \u0c85\u0cae\u0ccd\u0cac \u0cb6\u0ccd\u0cb0\u0ccd\u0ca4\u0c9c\u0ca8 \u0cab\u0cb2\u0ca6\u0cbe\u0caf\u0cbf\u0c95\u0cc7 \u0c9c\u0c97\u0ca6\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cc7"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0\u0cb6\u0ca8\u0cc1\u0ca4\u0cc7 \u0c97\u0cbf\u0cb0\u0cc0\u0cb6 \u0cb8\u0cc1\u0ca4\u0cc7 \u0cb6\u0cbf\u0cb5 \u0cb8\u0cb9\u0cbf\u0ca4\u0cc7 \u0ca8\u0cbf\u0c9c\u0cad\u0c95\u0ccd\u0ca4 \u0cb9\u0cbf\u0ca4\u0cc7 \u0cb9\u0cc7\u0cae\u0cb2\u0ca4\u0cc7"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cb9\u0cb0\u0c97\u0cc1\u0cb0\u0cc1 \u0cb8\u0cc1\u0cb0\u0cae\u0cc1\u0ca6\u0cbf\u0ca6\u0cc7 \u0cb8\u0cbe\u0cb5\u0cbf\u0ca6\u0ccd\u0caf \u0cb5\u0cbf\u0ca7\u0cbf\u0ca4\u0cc7 \u0cb9\u0cbe\u0cb2\u0cb8\u0ccd\u0caf\u0ca8\u0c95\u0cb0 \u0cb8\u0ccd\u0ca5\u0cbf\u0ca4\u0cc7 \u0cb9\u0cc7 \u0cb2\u0cb2\u0cbf\u0ca4\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cb0\u0cae\u0cbe \u0cb5\u0cc0\u0ca3\u0cbe\u0caa\u0cbe\u0ca3\u0cbf \u0caa\u0ccb\u0cb7\u0ca3 \u0cb8\u0ca8\u0ccd\u0ca4\u0ccb\u0cb7\u0ca3\u0cbf \u0cb0\u0c9c\u0ca4 \u0cb8\u0cbe\u0ca7\u0c95 \u0c86\u0cae\u0ccd\u0cb0\u0cb8 \u0cad\u0cb5\u0cbf\u0c95\u0cbe\u0cb0\u0cbf\u0ca3\u0cbf <br/> \u0cb0\u0cbe\u0c95\u0cc7\u0ca8\u0ccd\u0ca6\u0cc1 \u0cb5\u0ca6\u0ca8\u0cbf \u0cb0\u0cb5\u0cbf \u0cb6\u0cb6\u0cbf \u0cb5\