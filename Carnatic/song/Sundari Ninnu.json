4d\u0c26\u0c32\u0c41 \u0c2a\u0c32\u0c4d\u0c15\u0c28\u0c3e\u0c2f\u0c46 <br/> \u0c2d\u0c3e\u0c35\u0c3f\u0c28\u0c4d\u0c1a\u0c3f \u0c28\u0c40 \u0c2a\u0c3e\u0c26\u0c2e\u0c41\u0c28 \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c\u0c41 <br/> \u0c2d\u0c3e\u0c35\u0c41\u0c15\u0c2e\u0c28\u0c41\u0c15\u0c4a\u0c28\u0c28\u0c3e\u0c2f\u0c46 \u0c24\u0c4d\u0c30\u0c3f\u0c2a\u0c41\u0c30"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc1 \u0cb5\u0cb0\u0ccd\u0ca3\u0cbf\u0cae\u0ccd\u0caa \u0cac\u0ccd\u0cb0\u0cb9\u0ccd\u0cae\u0cbe\u0ca6\u0cbf <br/> \u0cb8\u0cc1\u0cb0\u0cb2\u0c95\u0cc8\u0ca8 \u0ca4\u0cb0\u0cae\u0cbe"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0cc1\u0ca8\u0ccd\u0ca6 \u0cb0\u0ca6\u0ca8\u0cc7 \u0cb8\u0ca8\u0ca8\u0ccd\u0ca6\u0ca8\u0cbe\u0ca6\u0cbf <br/> \u0cb5\u0ca8\u0ccd\u0ca6\u0cbf\u0ca4\u0cc7 \u0ca8\u0cc7\u0ca8\u0cc6\u0ca8\u0ccd\u0ca4 \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c95\u0cb2\u0c95\u0cb2\u0cae\u0ca8\u0cc1 \u0cae\u0cc1\u0c96 \u0c95\u0cb3\u0cb2\u0ca8\u0cc1 \u0c95\u0ca8\u0cbf <br/> \u0c95\u0cb2\u0cc1\u0cb5\u0cb2 \u0cb0\u0cbe\u0c9c\u0cc1 \u0cad\u0cc1\u0cb5\u0cbf\u0c95\u0cbf \u0cb0\u0cbe\u0ca1\u0cbe\u0caf\u0cc6 <br/> \u0c9a\u0cc6\u0cb2\u0c97\u0cc1 \u0ca8\u0cc0 \u0cb2\u0cbe\u0cb5\u0ca3\u0ccd\u0caf\u0cae\u0cc1\u0ca8\u0cc1 \u0c95\u0ca8\u0cbf <br/> \u0c85\u0cb2\u0ca8\u0cbe\u0ca1\u0cc7 \u0cb5\u0cb2 \u0cb0\u0cbe\u0c9c\u0cc1 \u0c95\u0cbe\u0ca8\u0c95 \u0caa\u0ccb\u0caf\u0cc6 <br/> \u0ca8\u0cbf\u0cb2\u0cc1\u0cb5\u0cb0\u0cae\u0c97\u0cc1 \u0ca8\u0cc0 \u0c97\u0cae\u0ccd\u0cad\u0cc0\u0cb0\u0cae\u0cc1\u0ca8\u0cc1 \u0c95\u0ca8\u0cbf <br/> \u0c9c\u0cb2 \u0cb0\u0cbe\u0c9c\u0cc1 \u0c9c\u0ca1 \u0cb5\u0cc7\u0cb7\u0cc1\u0ca1\u0cbe\u0caf\u0cc6 <br/> \u0cac\u0cb2\u0cae\u0cc8\u0ca8 \u0ca7\u0cc0\u0cb0\u0ca4\u0ccd\u0cb5\u0cae\u0cc1\u0ca8\u0cc1 \u0c95\u0ca8\u0cbf <br/> \u0c95\u0ca8\u0c95\u0cbe\u0c9a\u0cb2\u0cc1\u0ca1\u0cc1 \u0ca4\u0cbe \u0cb6\u0cbf\u0cb2 \u0cb0\u0cc2\u0caa\u0cc1\u0ca1\u0cbe\u0caf\u0cc6 \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c95\u0ca8\u0cc1\u0cb2\u0ca8\u0cc1 \u0c95\u0ca8\u0cbf \u0cb8\u0cbf\u0c97\u0ccd\u0c97\u0cc1\u0caa\u0ca1\u0cbf \u0c97\u0ca3\u0ccd\u0ca1\u0cc1 <br/> \u0cae\u0cc0\u0ca8\u0cc1\u0cb2\u0cc1 \u0cb5\u0ca8\u0ca7\u0cbf \u0cb5\u0cbe\u0cb8\u0cae\u0cc1 \u0cb8\u0cc7\u0caf\u0ca8\u0cbe\u0caf\u0cc6 <br/> \u0c9c\u0ca8\u0ca8\u0cbf \u0ca8\u0cc0 \u0c9a\u0cbf\u0cb0\u0cc1 \u0ca8\u0cb5\u0ccd\u0cb5\u0cc1 \u0c95\u0cbe\u0ca8\u0ccd\u0ca4\u0cbf \u0cb8\u0ccb\u0c95\u0cbf <br/> \u0cb6\u0cbf\u0cb5\u0cc1\u0ca1\u0ca8\u0cc1\u0caa\u0cae\u0cae\u0ccc \u0cb6\u0cc1\u0cad\u0ccd\u0cb0\u0cc1\u0ca1\u0cbe\u0caf\u0cc6 <br/> \u0cae\u0cbf\u0ca8\u0cc1\u0c95\u0cc1 \u0cb8\u0cca\u0cae\u0ccd\u0cae\u0cc1\u0cb2 \u0c9a\u0cc7\u0cb2 \u0c95\u0cbe\u0ca8\u0ccd\u0ca4\u0cc1\u0cb2 <br/> \u0cae\u0cc6\u0cb0\u0cc1\u0caa\u0cc1\u0cb2\u0cc1 \u0c95\u0ca8\u0cbf \u0ca8\u0cbf\u0cae\u0cbf\u0cb7\u0cae\u0cc1 \u0ca8\u0cbf\u0cb2\u0cc1\u0cb5\u0ca6\u0cbe\u0caf\u0cc6 <br/> \u0c95\u0ca8\u0c95\u0cbe\u0c82\u0c97\u0cbf \u0ca8\u0cc0 \u0cb8\u0ccd\u0cb5\u0cb0\u0cae\u0cc1\u0ca8\u0cc1 \u0cb5\u0cbf\u0ca8\u0cbf \u0cb5\u0cbe\u0ca3\u0cbf <br/> \u0cae\u0c97\u0ca8\u0cbf \u0c9c\u0cbf\u0cb9\u0ccd\u0cb5\u0ca8\u0cc1 \u0ca4\u0cbe \u0caa\u0cc2\u0ca8\u0cbf\u0c95\u0cbe\u0caf\u0cc6 \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0caa\u0cbe\u0cb5\u0ca8\u0cae\u0cc1 \u0c9c\u0cc7\u0caf\u0cc1 \u0cac\u0cbf\u0cb0\u0cc1\u0ca6\u0cc1\u0ca8\u0cbf \u0c95\u0ca8\u0cbf \u0cad\u0c95\u0ccd\u0ca4 <br/> \u0caa\u0cbe\u0caa\u0cae\u0cc1 \u0caa\u0cbe\u0cb0\u0cbf \u0caa\u0ccb\u0ca8\u0cbe\u0caf\u0cc6 <br/> \u0c8f \u0cb5\u0cc7\u0cb3 \u0ca8\u0cc0 \u0ca6\u0caf \u0c9a\u0cc7\u0ca4 \u0cb8\u0ca4\u0ccd-\u0c95\u0cb5\u0cc1\u0cb2\u0cc6\u0cb2\u0ccd\u0cb2 <br/> \u0c95\u0cbe\u0cb5\u0ccd\u0caf\u0cae\u0cc1\u0cb2\u0ca8\u0cc1 \u0cb8\u0cc7\u0caf\u0ca8\u0cbe\u0caf\u0cc6 <br/> \u0cae\u0cbe \u0cb5\u0cb0\u0cc1\u0ca8\u0cbf\u0c95\u0cbf \u0c9a\u0cc6\u0cb2\u0ccd\u0cb2\u0cb2\u0ca8\u0cbf \u0cb8\u0ca8\u0ccd\u0ca4\u0ccb\u0cb7\u0cae\u0cc1\u0ca8 <br/> \u0ca6\u0cc7\u0cb5\u0cbf \u0caa\u0cc6\u0ca6\u0ccd\u0ca6\u0cb2\u0cc1 \u0caa\u0cb2\u0ccd\u0c95\u0ca8\u0cbe\u0caf\u0cc6 <br/> \u0cad\u0cbe\u0cb5\u0cbf\u0ca8\u0ccd\u0c9a\u0cbf \u0ca8\u0cc0 \u0caa\u0cbe\u0ca6\u0cae\u0cc1\u0ca8 \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c\u0cc1 <br/> \u0cad\u0cbe\u0cb5\u0cc1\u0c95\u0cae\u0ca8\u0cc1\u0c95\u0cca\u0ca8\u0ca8\u0cbe\u0caf\u0cc6 \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3f \u0d28\u0d3f\u0d28\u0d4d\u0d28\u0d41 \u0d35\u0d30\u0d4d\u0d23\u0d3f\u0d2e\u0d4d\u0d2a \u0d2c\u0d4d\u0d30\u0d39\u0d4d\u0d2e\u0d3e\u0d26\u0d3f <br/> \u0d38\u0d41\u0d30\u0d32\u0d15\u0d48\u0d28 \u0d24\u0d30\u0d2e\u0d3e"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d15\u0d41\u0d28\u0d4d\u0d26 \u0d30\u0d26\u0d28\u0d47 \u0d38\u0d28\u0d28\u0d4d\u0d26\u0d28\u0d3e\u0d26\u0d3f <br/> \u0d35\u0d28\u0d4d\u0d26\u0d3f\u0d24\u0d47 \u0d28\u0d47\u0d28\u0d46\u0d28\u0d4d\u0d24 \u0d24\u0d4d\u0d30\u0d3f\u0d2a\u0d41\u0d30"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d15\u0d32\u0d15\u0d32\u0d2e\u0d28\u0d41 \u0d2e\u0d41\u0d16 \u0d15\u0d33\u0d32\u0d28\u0d41 \u0d15\u0d28\u0d3f <br/> \u0d15\u0d32\u0d41\u0d35\u0d32 \u0d30\u0d3e\u0d1c\u0d41 \u0d2d\u0d41\u0d35\u0d3f\u0d15\u0d3f \u0d30\u0d3e\u0d21\u0d3e\u0d2f\u0d46 <br/> \u0d1a\u0d46\u0d32\u0d17\u0d41 \u0d28\u0d40 \u0d32\u0d3e\u0d35\u0d23\u0d4d\u0d2f\u0d2e\u0d41\u0d28\u0d41 \u0d15\u0d28\u0d3f <br/> \u0d05\u0d32\u0d28\u0d3e\u0d21\u0d47 \u0d35\u0d32 \u0d30\u0d3e\u0d1c\u0d41 \u0d15\u0d3e\u0d28\u0d15 \u0d2a\u0d4b\u0d2f\u0d46 <br/> \u0d28\u0d3f\u0d32\u0d41\u0d35\u0d30\u0d2e\u0d17\u0d41 \u0d28\u0d40 \u0d17\u0d2e\u0d4d\u0d2d\u0d40\u0d30\u0d2e\u0d41\u0d28\u0d41 \u0d15\u0d28\u0d3f <br/> \u0d1c\u0d32 \u0d30\u0d3e\