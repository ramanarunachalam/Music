f\u0ba4\u0bcb\u0bb9\u0bae\u0bcd <br/> \u0b9a\u0b9a\u0bcd\u0b9a\u0bbf\u0ba4\u0bbe\u0ba9\u0ba8\u0bcd\u0ba4-\u0ba8\u0bbe\u0ba4\u0bbe\u0ba4\u0bcd\u0bae\u0ba9\u0bbe \u0bb6\u0bcd\u0bb0\u0bc0"}, {"M": "\u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0b9a\u0bcb\u0bae-\u0ba4\u0bbe\u0ba4\u0bc7\u0ba9 \u0bb5\u0bb2\u0bcd\u0bb2\u0bc0-\u0ba4\u0bc7\u0bb5- <br/> \u0b9a\u0bc7\u0ba9\u0bcb\u0baa\u0bc7\u0ba4-\u0bb5\u0bb0-\u0b95\u0bc1\u0bb0\u0bc1\u0b95\u0bc1\u0bb9\u0bc7\u0ba9"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0baa\u0bcd\u0bb0\u0ba3\u0bb5\u0bbe\u0bb0\u0bcd\u0ba4\u0bcd\u0ba4-\u0ba4\u0ba4\u0bcd\u0bb5\u0bcb\u0baa\u0ba4\u0bc7\u0b9a\u0bc7\u0ba9 <br/> \u0bae\u0ba3\u0bbf-\u0bae\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0\u0bcc\u0bb7\u0ba4-\u0baa\u0bcd\u0bb0\u0ba4-\u0ba8\u0bbf\u0baa\u0bc1\u0ba3\u0bc7\u0ba9 <br/> \u0b9a\u0ba9\u0b95\u0bbe\u0ba4\u0bbf-\u0b9a\u0ba9\u0bcd\u0ba9\u0bc1\u0ba4-\u0baa\u0bcd\u0bb0\u0baa\u0bbe\u0bb5\u0bc7\u0ba9 <br/> \u0ba4\u0bbf\u0ba9\u0b95\u0bb0-\u0b9a\u0b9a\u0bbf-\u0ba4\u0bc7\u0b9c\u0bb9\u0bcd-\u0baa\u0bcd\u0bb0\u0b95\u0bbe\u0b9a\u0bc7\u0ba9"}, {"M": "\u0bae\u0ba4\u0bcd\u0baf\u0bae\u0b95\u0bbe\u0bb2 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0bae\u0bbe\u0ba3\u0bbf\u0b95\u0bcd\u0baf-\u0bae\u0b95\u0bc1\u0b9f-\u0b95\u0bc1\u0ba3\u0bcd\u0b9f\u0bb2-\u0b9a\u0bcb\u0baa\u0bbf\u0ba4 <br/> \u0bae\u0bb0\u0b95\u0ba4-\u0bae\u0ba3\u0bbf\u0bae\u0baf-\u0bb5\u0b9c\u0bcd\u0bb0\u0bbe\u0baf\u0bc1\u0ba4\u0bc7\u0ba9 <br/> \u0ba8\u0bb5-\u0ba8\u0ba8\u0bcd\u0ba4\u0ba9\u0bbe\u0ba4\u0bcd\u0baf\u0bc1\u0baa\u0bbe\u0b9a\u0bbf\u0ba4-\u0b9a\u0bb0\u0bb5\u0ba3- <br/> \u0bae\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0-\u0bb8\u0bcd\u0bb5\u0bb0\u0bc2\u0baa\u0bc7\u0ba3 \u0bb5\u0bb0\u0bc7\u0ba3 \u0bb6\u0bcd\u0bb0\u0bc0"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c4d\u0c35\u0c3e\u0c2e\u0c3f\u0c28\u0c3e\u0c25\u0c47\u0c28 \u0c38\u0c2e\u0c4d\u0c30\u0c15\u0c4d\u0c37\u0c3f\u0c24\u0c4b\u0c39\u0c2e\u0c4d <br/> \u0c38\u0c1a\u0c4d\u0c1a\u0c3f\u0c26\u0c3e\u0c28\u0c28\u0c4d\u0c26-\u0c28\u0c3e\u0c25\u0c3e\u0c24\u0c4d\u0c2e\u0c28\u0c3e \u0c36\u0c4d\u0c30\u0c40"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c4b\u0c2e-\u0c24\u0c3e\u0c24\u0c47\u0c28 \u0c35\u0c32\u0c4d\u0c32\u0c40-\u0c26\u0c47\u0c35- <br/> \u0c38\u0c47\u0c28\u0c4b\u0c2a\u0c47\u0c24-\u0c35\u0c30-\u0c17\u0c41\u0c30\u0c41\u0c17\u0c41\u0c39\u0c47\u0c28"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c2a\u0c4d\u0c30\u0c23\u0c35\u0c3e\u0c30\u0c4d\u0c24\u0c4d\u0c25-\u0c24\u0c24\u0c4d\u0c35\u0c4b\u0c2a\u0c26\u0c47\u0c36\u0c47\u0c28 <br/> \u0c2e\u0c23\u0c3f-\u0c2e\u0c28\u0c4d\u0c24\u0c4d\u0c30\u0c4c\u0c37\u0c27-\u0c2a\u0c4d\u0c30\u0c26-\u0c28\u0c3f\u0c2a\u0c41\u0c23\u0c47\u0c28 <br/> \u0c38\u0c28\u0c15\u0c3e\u0c26\u0c3f-\u0c38\u0c28\u0c4d\u0c28\u0c41\u0c24-\u0c2a\u0c4d\u0c30\u0c2d\u0c3e\u0c35\u0c47\u0c28 <br/> \u0c26\u0c3f\u0c28\u0c15\u0c30-\u0c36\u0c36\u0c3f-\u0c24\u0c47\u0c1c\u0c39\u0c4d-\u0c2a\u0c4d\u0c30\u0c15\u0c3e\u0c36\u0c47\u0c28"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c2e\u0c3e\u0c23\u0c3f\u0c15\u0c4d\u0c2f-\u0c2e\u0c15\u0c41\u0c1f-\u0c15\u0c41\u0c23\u0c4d\u0c21\u0c32-\u0c36\u0c4b\u0c2d\u0c3f\u0c24 <br/> \u0c2e\u0c30\u0c15\u0c24-\u0c2e\u0c23\u0c3f\u0c2e\u0c2f-\u0c35\u0c1c\u0c4d\u0c30\u0c3e\u0c2f\u0c41\u0c27\u0c47\u0c28 <br/> \u0c28\u0c35-\u0c28\u0c28\u0c4d\u0c26\u0c28\u0c3e\u0c26\u0c4d\u0c2f\u0c41\u0c2a\u0c3e\u0c38\u0c3f\u0c24-\u0c36\u0c30\u0c35\u0c23- <br/> \u0c2e\u0c28\u0c4d\u0c24\u0c4d\u0c30-\u0c38\u0c4d\u0c35\u0c30\u0c42\u0c2a\u0c47\u0c23 \u0c35\u0c30\u0c47\u0c23 \u0c36\u0c4d\u0c30\u0c40"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0ccd\u0cb5\u0cbe\u0cae\u0cbf\u0ca8\u0cbe\u0ca5\u0cc7\u0ca8 \u0cb8\u0cae\u0ccd\u0cb0\u0c95\u0ccd\u0cb7\u0cbf\u0ca4\u0ccb\u0cb9\u0cae\u0ccd <br/> \u0cb8\u0c9a\u0ccd\u0c9a\u0cbf\u0ca6\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6-\u0ca8\u0cbe\u0ca5\u0cbe\u0ca4\u0ccd\u0cae\u0ca8\u0cbe \u0cb6\u0ccd\u0cb0\u0cc0"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0ccb\u0cae-\u0ca4\u0cbe\u0ca4\u0cc7\u0ca8 \u0cb5\u0cb2\u0ccd\u0cb2\u0cc0-\u0ca6\u0cc7\u0cb5- <br/> \u0cb8\u0cc7\u0ca8\u0ccb\u0caa\u0cc7\u0ca4-\u0cb5\u0cb0-\u0c97\u0cc1\u0cb0\u0cc1\u0c97\u0cc1\u0cb9\u0cc7\u0ca8"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0caa\u0ccd\u0cb0\u0ca3\u0cb5\u0cbe\u0cb0\u0ccd\u0ca4\u0ccd\u0ca5-\u0ca4\u0ca4\u0ccd\u0cb5\u0ccb\u0caa\u0ca6\u0cc7\u0cb6\u0cc7\u0ca8 <br/> \u0cae\u0ca3\u0cbf-\u0cae\u0ca8\u0ccd\u0ca4\u0ccd\u0cb0\u0ccc\u0cb7\u0ca7-\u0caa\u0ccd\u0cb0\u0ca6-\u0ca8\u0cbf\u0caa\u0cc1\u0ca3\u0cc7\u0ca8 <br/> \u0cb8\u0ca8\u0c95\u0cbe\u0ca6\u0cbf-\u0cb8\u0ca8\u0ccd\u0ca8\u0cc1\u0ca4-\u0caa\u0ccd\u0cb0\u0cad\u0cbe\u0cb5\u0cc7\u0ca8 <br/> \u0ca6\u0cbf\u0ca8\u0c95\u0cb0-\u0cb6\u0cb6\u0cbf-\u0ca4\u0cc7\u0c9c\u0cb9\u0ccd-\u0caa\u0ccd\u0cb0\u0c95\u0cbe\u0cb6\u0cc7\u0ca8"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cae\u0cbe\u0ca3\u0cbf\u0c95\u0ccd\u0caf-\u0cae\u0c95\u0cc1\u0c9f-\u0c95\u0cc1\u0ca3\u0ccd\u0ca1\u0cb2-\u0cb6\u0ccb\u0cad\u0cbf\u0ca4 <br/> \u0cae\u0cb0\u0c95\u0ca4-\u0cae\u0ca3\u0cbf\u0cae\u0caf-\u0cb5\u0c9c\u0ccd\u0cb0\u0cbe\u0caf\u0cc1\u0ca7\u0cc7\u0ca8 <br/> \u0ca8\u0cb5-\u0ca8\u0ca8\u0ccd\u0ca6\u0ca8\u0cbe\u0ca6\u0ccd\u0caf\u0cc1\u0caa\u0cbe\u0cb8\u0cbf\u0ca4-\u0cb6\u0cb0\u0cb5\u0ca3- <br/> \u0cae\u0ca8\u0ccd\u0ca4\u0ccd\u0cb0-\u0cb8\u0ccd\u0cb5\u0cb0\u0cc2\u0caa\u0cc7\u0ca3 \u0cb5\u0cb0\u0cc7\u0ca3 \u0cb6\u0ccd\u0cb0\u0cc0"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d4d\u0d35\u0d3e\u0d2e\u0d3f\u0d28\u0d3e\u0d25\u0d47\u0d28 \u0d38\u0d2e\u0d4d\u0d30\u0d15\u0d4d\u0d37\u0d3f\u0d24\u0d4b\u0d39\u0d2e\u0d4d <br/> \u0d38\u0d1a\u0d4d\u0d1a\u0d3f\u0d26\u0d3e\u0d28\u0d28\u0d4d\u0d26-\u0d28\u0d3e\u0d25\u0d3e\u0d24\u0d4d\u0d2e\u0d28\u0d3e \u0d36\u0d4d\u0d30\u0d40"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d4b\u0d2e-\u0d24\u0d3e\u0d24\u0d47\u0d28 \u0d35\u0d32\u0d4d\u0d32\u0d40-\u0d26\u0d47\u0d35- <br/> \u0d38\u0d47\u0d28\u0d4b\u0d2a\u0d47\u0d24-\u0d35\u0d30-\u0d17\u0d41\u0d30\u0d41\u0d17\u0d41\u0d39\u0d47\u0d28"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d2a\u0d4d\u0d30\u0d23\u0d35\u0d3e\u0d30\u0d4d\u0d24\u0d4d\u0d25-\u0d24\u0d24\u0d4d\u0d35\u0d4b\u0d2a\u0d26\u0d47\u0d36\u0d47\u0d28 <br/> \u0d2e\u0d23\u0d3f-\u0d2e\u0d28\u0d4d\u0d24\u0d4d\u0d30\u0d4c\u0d37\u0d27-\u0d2a\u0d4d\u0d30\u0d26-\u0d28\u0d3f\u0d2a\u0d41\u0d23\u0d47\u0d28 <br/> \u0d38\u0d28\u0d15\u0d3e\u0d26\u0d3f-\u0d38\u0d28\u0d4d\u0d28\u0d41\u0d24-\u0d2a\u0d4d\u0d30\u0d2d\u0d3e\u0d35\u0d47\u0d28 <br/> \u0d26\u0d3f\u0d28\u0d15\u0d30-\u0d36\u0d36\u0d3f-\u0d24\u0d47\u0d1c\u0d39\u0d4d-\u0d2a\u0d4d\u0d30\u0d15\u0d3e\u0d36\u0d47\u0d28"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d2e\u0d3e\u0d23\u0d3f\u0d15\u0d4d\u0d2f-\u0d2e\u0d15\u0d41\u0d1f-\u0d15\u0d41\u0d23\u0d4d\u0d21\u0d32-\u0d36\u0d4b\u0d2d\u0d3f\u0d24 <br/> \u0d2e\u0d30\u0d15\u0d24-\u0d2e\u0d23\u0d3f\u0d2e\u0d2f-\u0d35\u0d1c\u0d4d\u0d30\u0d3e\u0d2f\u0d41\u0d27\u0d47\u0d28 <br/> \u0d28\u0d35-\u0d28\u0d28\u0d4d\u0d26\u0d28\u0d3e\u0d26\u0d4d\u0d2f\u0d41\u0d2a\u0d3e\u0d38\u0d3f\u0d24-\u0d36\u0d30\u0d35\u0d23- <br/> \u0d2e\u0d28\u0d4d\u0d24\u0d4d\u0d30-\u0d38\u0d4d\u0d35\u0d30\u0d42\u0d2a\u0d47\u0d23 \u0d35\u0d30\u0d47\u0d23 \u0d36\u0d4d\u0d30\u0d40"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0938\u094d\u0935\u093e\u092e\u093f\u0928\u093e\u0925\u0947\u0928 \u0938\u092e\u094d\u0930\u0915\u094d\u0937\u093f\u0924\u094b\u0939\u092e\u094d <br/> \u0938\u091a\u094d\u091a\u093f\u0926\u093e\u0928\u0928\u094d\u0926-\u0928\u093e\u0925\u093e\u0924\u094d\u092e\u0928\u093e \u0936\u094d\u0930\u0940"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0938\u094b\u092e-\u0924\u093e\u0924\u0947\u0928 \u0935\u0932\u094d\u0932\u0940-\u0926\u0947\u0935- <br/> \u0938\u0947\u0928\u094b\u092a\u0947\u0924-\u0935\u0930-\u0917\u0941\u0930\u0941\u0917\u0941\u0939\u0947\u0928"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u092a\u094d\u0930\u0923\u0935\u093e\u0930\u094d\u0924\u094d\u0925-\u0924\u0924\u094d\u0935\u094b\u092a\u0926\u0947\u0936\u0947\u0928 <br/> \u092e\u0923\u093f-\u092e\u0928\u094d\u0924\u094d\u0930\u094c\u0937\u0927-\u092a\u094d\u0930\u0926-\u0928\u093f\u092a\u0941\u0923\u0947\u0928 <br/> \u0938\u0928\u0915\u093e\u0926\u093f-\u0938\u0928\u094d\u0928\u0941\u0924-\u092a\u094d\u0930\u092d\u093e\u0935\u0947\u0928 <br/> \u0926\u093f\u0928\u0915\u0930-\u0936\u0936\u093f-\u0924\u0947\u091c\u0939\u094d-\u092a\u094d\u0930\u0915\u093e\u0936\u0947\u0928"}, {"M": "\u092e\u0927\u094d\u092f\u092e\u0915\u093e\u0932 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u092e\u093e\u0923\u093f\u0915\u094d\u092f-\u092e\u0915\u0941\u091f-\u0915\u0941\u0923\u094d\u0921\u0932-\u0936\u094b\u092d\u093f\u0924 <br/> \u092e\u0930\u0915\u0924-\u092e\u0923\u093f\u092e\u092f-\u0935\u091c\u094d\u0930\u093e\u092f\u0941\u0927\u0947\u0928 <br/> \u0928\u0935-\u0928\u0928\u094d\u0926\u0928\u093e\u0926\u094d\u092f\u0941\u092a\u093e\u0938\u093f\u0924-\u0936\u0930\u0935\u0923- <br/> \u092e\u0928\u094d\u0924\u094d\u0930-\u0938\u094d\u0935\u0930\u0942\u092a\u0947\u0923 \u0935\u0930\u0947\u0923 \u0936\u094d\u0930\u0940"}]}], "L": "l", "lyricsref": [{"links": [{"N": "www.medieval.org", "L": "http://www.medieval.org/music/world/carnatic/lyrics/L.Ramakrishnan/svAminAthEna.txt"}, {"N": "guru-guha.blogspot.com", "L": "http://guru-guha.blogspot.com/2008/11/dikshitar-kriti-svaminathena-raga.html"}]}]}