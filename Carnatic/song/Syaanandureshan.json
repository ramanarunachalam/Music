0b9c\u0bbe\u0ba4\u0bae\u0bcd <br/> \u0ba4\u0bc1\u0bb0\u0bbf\u0ba4\u0bae\u0bcd \u0ba4\u0bc2\u0bb0\u0ba4\u0bcd\u0ba4\u0b95\u0ba4\u0bcd\u0ba4\u0bc0\u0b9f\u0bc1\u0bae\u0bc6\u0ba9\u0bcd\u0ba9\u0bc1 \u0bb6\u0bcd\u0bb0\u0bc1\u0ba4\u0bbf\u0b95\u0bb3\u0bc6\u0bb2\u0bcd\u0bb2\u0bbe\u0bae\u0bcd \u0bb5\u0bb0\u0ba4\u0bb2\u0bcb\u0b95\u0bc7 \u0ba4\u0bcd\u0bb0\u0bc1\u0b9f\u0bae\u0bcd \u0b95\u0ba4\u0bbf\u0b95\u0bcd\u0b95\u0bc1\u0ba9\u0bcd\u0ba9\u0bc1"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0ba8\u0bbf\u0ba9\u0bcd\u0ba9\u0b9f\u0bbf\u0bae\u0bb2\u0bb0\u0bbe\u0b9a\u0bc8\u0baf\u0bc7 \u0baa\u0bc2\u0ba3\u0bcd\u0b9f\u0bbf\u0b92\u0bb0\u0bc1 \u0ba4\u0bc7\u0bb9\u0bbf\u0b95\u0bb3\u0bcd\u0b95\u0bcd\u0b95\u0bc1 \u0bae\u0ba9\u0bcd\u0ba9\u0bbf\u0bb2\u0bbf\u0bb9 \u0b9a\u0bcb\u0b95\u0bb2\u0bc7\u0b9a\u0bae\u0bc1\u0ba3\u0bcd\u0b9f\u0bcb <br/> \u0b9a\u0ba9\u0bcd\u0ba9\u0ba4\u0bbf \u0b9a\u0bc6\u0b87\u0bb5\u0bcb\u0bb0\u0bcd\u0b95\u0bcd\u0b95\u0bc1 \u0b95\u0bb2\u0bcd\u0baa\u0b9a\u0bbe\u0bb2 \u0b9a\u0bae\u0ba9\u0bbe\u0bae\u0bcd \u0bb2\u0bcb\u0b95 \u0b9a\u0bbe\u0bb0\u0bcd\u0bb5\u0baa\u0bcc\u0bae \u0baa\u0bcd\u0bb9\u0b89\u0bae\u0bbf \u0bb0\u0bae\u0bbe\u0bb2\u0bcb\u0bb2"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0bae\u0bc0\u0ba9 \u0b95\u0b9a\u0bcd\u0b9a \u0baa\u0bbe\u0ba4\u0bbf\u0baf\u0bbe\u0bae\u0bcd \u0b9a\u0bb0\u0bc0\u0bb0\u0bae\u0bcd \u0baa\u0bc2\u0ba3\u0bcd\u0b9f\u0bc1 \u0bb5\u0bbf\u0baa\u0bc1\u0ba4\u0bb0\u0bcd\u0b95\u0bcd\u0b95\u0bcd-\u0b86\u0ba9\u0ba8\u0bcd\u0ba4\u0bae\u0bb0\u0bc1\u0bb3\u0bcd \u0b9a\u0bc6\u0baf\u0bcd\u0baf\u0bc1\u0bae\u0baa\u0bbe\u0bb0\u0bae\u0bcd <br/> \u0bb6\u0bcd\u0bb0\u0bc0\u0ba9\u0bbf\u0ba4\u0bc7 \u0bb5\u0bb0\u0bcd\u0b9c\u0bbf\u0ba4 \u0bae\u0ba4 \u0bb2\u0bcb\u0baa \u0b9a\u0bc7\u0bb5\u0bbf\u0ba4 \u0baa\u0bbe\u0ba4 \u0b9a\u0bbf\u0ba9\u0bcd\u0bae\u0baf\u0ba9\u0bbe\u0b95\u0bc1\u0bae\u0bcd \u0bb6\u0bcd\u0bb0\u0bc0 \u0baa\u0ba4\u0bcd\u0bae\u0ba9\u0bbe\u0baa"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c4d\u0c2f\u0c3e\u0c2