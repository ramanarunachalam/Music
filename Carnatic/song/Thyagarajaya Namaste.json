ccd\u0ca4\u0caf\u0cc7 <br/> \u0cae\u0cc1\u0c95\u0cc1\u0cb0 \u0cac\u0cbf\u0cae\u0ccd\u0cac \u0caa\u0ccd\u0cb0\u0ca4\u0cbf\u0cac\u0cbf\u0cae\u0ccd\u0cac\u0cbf\u0ca4 \u0cae\u0cc1\u0c96 \u0cb8\u0ccd\u0cab\u0cc2\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0caf\u0cc7 <br/> \u0cae\u0cc1\u0ca8\u0cbf \u0caa\u0c95\u0ccd\u0cb7\u0cbf \u0cae\u0cc3\u0c97 \u0c95\u0cc0\u0c9f\u0cbe\u0ca6\u0cbf \u0cae\u0cc2\u0c95\u0ccd\u0ca4\u0cbf \u0caa\u0ccd\u0cb0\u0ca6 \u0c95\u0cc0\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0caf\u0cc7"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cb8\u0c95\u0cb2\u0cbe\u0c97\u0cae \u0cae\u0ca8\u0ccd\u0ca4\u0ccd\u0cb0 \u0ca4\u0ca8\u0ccd\u0ca4\u0ccd\u0cb0 \u0cb8\u0cbe\u0cb0\u0c9c\u0ccd\u0c9e\u0cbe\u0ca8\u0cc1 \u0cb0\u0c95\u0ccd\u0ca4\u0caf\u0cc7 <br/> \u0c85\u0c95\u0ca4\u0cbe\u0ca6\u0cbf \u0ca4\u0ccd\u0cb0\u0cbf\u0cb0\u0cc7\u0c96\u0cbe\u0ca4\u0ccd\u0cae\u0c95\u0cbe\u0ca7\u0cbe\u0cb0 \u0caa\u0ccd\u0cb0\u0cb5\u0cc3\u0ca4\u0ccd\u0ca4\u0caf\u0cc7 <br/> \u0cb8\u0c95\u0cb3 \u0ca8\u0cbf\u0cb7\u0ccd\u0c95\u0cb3 \u0cb8\u0ccd\u0cb5\u0cb0\u0cc2\u0caa \u0cb8\u0c9a\u0ccd\u0c9a\u0cbf\u0ca4\u0ccd \u0cb8\u0cc1\u0c96 \u0cb5\u0ccd\u0caf\u0cbe\u0caa\u0ccd\u0ca4\u0caf\u0cc7 <br/> \u0cb5\u0cbf\u0c95\u0cb2\u0ccd\u0caa \u0cad\u0cc7\u0ca6\u0caf\u0cc1\u0c95\u0ccd\u0ca4\u0caf\u0cc7 \u0cb5\u0cbf\u0c9f\u0c82\u0c95 \u0cb0\u0cc2\u0caa \u0cb6\u0c95\u0ccd\u0ca4\u0caf\u0cc7"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d24\u0d4d\u0d2f\u0d3e\u0d17\u0d30\u0d3e\u0d1c\u0d3e\u0d2f \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47 \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47 <br/> \u0d15\u0d3e\u0d24\u0d4d\u0d2f\u0d3e\u0d2f\u0d28\u0d40\u0d2a\u0d24\u0d47 \u0d2a\u0d36\u0d41\u0d2a \u0d24\u0d47 \u0d38\u0d3f\u0d39\u0d4d\u0d2e\u0d3e\u0d38\u0d28\u0d2a\u0d24\u0d47"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d35\u0d3e\u0d17\u0d40\u0d36\u0d3e\u0d26\u0d4d\u0d2f\u0d16\u0d3f\u0d32 \u0d26\u0d47\u0d35 \u0d35\u0d28\u