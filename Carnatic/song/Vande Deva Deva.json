\u0bae\u0bcd \u0baa\u0bb0\u0bae \u0b9a\u0bbe\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0\u0bbe\u0bb0\u0bc1\u0ba3\u0bc0\u0bae\u0bbe\u0bb5\u0bb9\u0bae\u0bcd <br/> \u0ba4\u0bae\u0bbf\u0ba4\u0b95\u0bb2\u0b9a \u0b95\u0b9f\u0bbe\u0bb9\u0bcd\u0bb5\u0baf \u0ba4\u0bbe\u0ba9\u0bb5\u0bbe\u0b9a\u0bc1\u0bae\u0ba9\u0baa\u0bbe\u0baf\u0bae\u0bcd <br/> \u0b9a\u0bc1\u0bae\u0ba4\u0bc1\u0bb0\u0bbe\u0bb0\u0bb5 \u0bae\u0b9e\u0bcd\u0b9c\u0bc0\u0bb0 \u0b9a\u0bcb\u0baa\u0bbf\u0ba4\u0bae \u0ba8\u0bbe\u0bb0\u0ba4\u0bcd\u0ba9\u0bae\u0bcd"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0baa\u0bb6\u0bcd\u0baa \u0ba8\u0bbe\u0bb0\u0bc0\u0b9c\u0ba9 \u0bb0\u0b9a\u0bbf\u0ba4 \u0baa\u0bb0\u0bbf\u0b9a\u0bc1\u0bae\u0bcd\u0baa\u0ba9\u0bc7 \u0ba4\u0ba4\u0bc0\u0baf\u0bbe\u0b95\u0bcd\u0bb7\u0bbf <br/> \u0baa\u0bcd\u0bb0\u0bcd\u0b9a \u0ba8\u0bc0\u0bb2 \u0bae\u0bbe\u0ba9\u0bc1\u0bb7\u0bbe\u0b99\u0bcd\u0b95\u0bc7\u0ba3 \u0b9a\u0b9a \u0ba4\u0bb0\u0bbe\u0baf\u0bbf\u0ba4 \u0ba8\u0b95\u0bb0\u0bae\u0bcd <br/> \u0bb5\u0bbf\u0b9a\u0ba4 \u0b95\u0bbe\u0ba8\u0bcd\u0ba4\u0bbf\u0baf\u0bc1\u0ba4\u0bae\u0bcd \u0b95\u0bcb\u0b95\u0bc1\u0bb2 \u0bb5\u0bcd\u0bb0\u0bcd\u0ba8\u0bcd\u0ba4\u0bbe\u0bb5\u0ba9 \u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0b9a\u0bb0\u0ba3\u0bae\u0bcd <br/> \u0bb5\u0bbf\u0b9a\u0bb0\u0ba3 \u0b9c\u0ba9\u0bbe\u0ba4\u0bbf \u0baa\u0bbe\u0bb0 \u0bb5\u0bbf\u0b9a\u0b9a\u0ba9 \u0baa\u0bb0\u0bbe\u0baf\u0ba3\u0bae\u0bcd"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0baa\u0bb5\u0ba9\u0bbe\u0b9a\u0ba9 \u0bb5\u0bb0\u0b95\u0bbe\u0bb3\u0bc0\u0baf \u0baa\u0ba3 \u0ba4\u0b9f \u0bb5\u0bbf\u0bb9\u0bbf\u0ba4 \u0ba8\u0b9f\u0ba9\u0bae\u0bcd <br/> \u0baa\u0bc1\u0bb5\u0ba9 \u0bb0\u0bae\u0bcd\u0baf \u0bb5\u0bc7\u0ba3\u0bc1\u0bb5\u0bbe\u0ba4\u0ba9 \u0bb5\u0bbf\u0bb0\u0b9a\u0bbf\u0ba4 \u0bb5\u0bbf\u0bb5\u0bbf\u0ba4 \u0ba4\u0bbe\u0bb3\u0bae\u0bcd <br/> \u0b9a\u0bbf\u0bb5 \u0b9a\u0ba9\u0b95 \u0bb5\u0bbf\u0ba4\u0bbf \u0b9a\u0bc1\u0bb0\u0bc7\u0b9a \u0b9a\u0bc7\u0bb5\u0bcd\u0baf\u0bae\u0baa \u0bb5\u0bb0\u0bcd\u0b95\u0b95\u0bb0\u0bae\u0bcd <br/> \u0baa