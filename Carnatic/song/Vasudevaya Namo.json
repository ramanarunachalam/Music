u0c24\u0c3f \u0c1c\u0c28\u0c2e\u0c41\u0c16 \u0c28\u0c35\u0c26\u0c33 \u0c38\u0c4c\u0c30\u0c2d \u0c38\u0c19\u0c4d\u0c17 \u0c24\u0c41\u0c19\u0c4d\u0c17\u0c3e\u0c2f"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c36\u0c30\u0c26\u0c41\u0c24\u0c1e\u0c4d\u0c1a\u0c3f\u0c24 \u0c2a\u0c3e\u0c1f\u0c32\u0c40 \u0c26\u0c33 \u0c38\u0c4c\u0c30\u0c2a\u0c4d-\u0c2a\u0c30\u0c3f\u0c35\u0c3e\u0c26\u0c3f \u0c32\u0c4b\u0c1a\u0c28\u0c3e\u0c2f <br/> \u0c38\u0c15\u0c32 \u0c35\u0c47\u0c26 \u0c35\u0c47\u0c26\u0c3e\u0c28\u0c4d\u0c24\u0c3e\u0c1f\u0c35\u0c3f \u0c36\u0c30\u0c3f\u0c24\u0c3e\u0c2a\u0c3e\u0c35\u0c4d\u0c30\u0c4d\u0c24 \u0c2a\u0c3e\u0c26\u0c3e\u0c2e\u0c4d\u0c2c\u0c41\u0c1c\u0c3e\u0c2f <br/> \u0c2a\u0c30\u0c2e \u0c15\u0c30\u0c41\u0c23\u0c3e\u0c30\u0c38\u0c2e\u0c2f \u0c35\u0c3f\u0c15\u0c38\u0c3f\u0c24 \u0c2a\u0c19\u0c4d\u0c15\u0c47\u0c30\u0c41\u0c39 \u0c2a\u0c4d\u0c30\u0c38\u0c28\u0c4d\u0c28 \u0c35\u0c26\u0c28\u0c3e\u0c2f <br/> \u0c2d\u0c3e\u0c35\u0c30\u0c3e\u0c17 \u0c2e\u0c41\u0c30\u0c33\u0c40 \u0c30\u0c35 \u0c28\u0c3f\u0c28\u0c3e\u0c26 \u0c2d\u0c15\u0c4d\u0c24 \u0c2e\u0c28\u0c4b\u0c39\u0c30\u0c23\u0c3e\u0c2f \u0c15\u0c4d\u0c30\u0c4d\u0c37\u0c4d\u0c23\u0c3e\u0c2f"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c2a\u0c30\u0c3f\u0c2e\u0c33 \u0c09\u0c32\u0c2a \u0c32\u0c35\u0c19\u0c4d\u0c17 \u0c38\u0c26\u0c28\u0c3e\u0c2f \u0c2d\u0c3e\u0c17\u0c26\u0c47\u0c2f \u0c1c\u0c28 \u0c2e\u0c26\u0c28 \u0c2e\u0c26\u0c28\u0c3e\u0c2f <br/> \u0c2d\u0c41\u0c1c\u0c17 \u0c2b\u0c23\u0c2e\u0c23\u0c40 \u0c24\u0c30\u0c33 \u0c2a\u0c26\u0c3e\u0c19\u0c4d\u0c17\u0c41\u0c33\u0c3f \u0c2d\u0c42\u0c37\u0c23 \u0c28\u0c1f\u0c35\u0c30 \u0c35\u0c47\u0c37\u0c23\u0c3e\u0c2f \u0c2a\u0c30"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb5\u0cbe\u0cb8\u0cc1\u0ca6\u0cc7\u0cb5\u0cbe\u0caf \u0ca8\u0cae\u0ccb \u0ca8\u0cae\u0cb8\u0ccd\u0ca4\u0cc7 \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb8\u0cb0\u0ccd\u0cb5 \u0cad\u0cc2\u0ca4\u0c95\u0ccd\u0cb7\u0caf\u0cbe\u0caf"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c86\u0cb8\u0cc1\u0cb0 \u0cae\u0ca6 \u0cb9\u0cb0\u0ca3 \u0c95\u0ccd\u0cb0\u0ccd\u0cb7\u0ccd\u0ca3\u0cbe\u0caf \u0c85\u0ca8\u0ca8\u0ccd\u0ca4 \u0cb6\u0ca4\u0ccd\u0caf\u0cbe\u0caf \u0ca8\u0cae\u0ccb \u0ca8\u0cae\u0cb8\u0ccd\u0ca4\u0cc7 <br/> \u0cad\u0cbe\u0cb8\u0c95 \u0cad\u0cbe\u0cb8\u0c95\u0cbe\u0caf \u0cb5\u0ccd\u0cb0\u0c9c\u0cc7\u0cb6\u0cbe\u0caf \u0cad\u0cb5 \u0cad\u0caf \u0cb9\u0cb0\u0ca3 \u0c85\u0cb0\u0ccd\u0ca3 \u0c9a\u0cb0\u0ca3\u0cbe\u0caf"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0cb5\u0cbe\u0cb8\u0cbf\u0ca4 \u0c95\u0ca8\u0c95\u0cbe\u0cae\u0ccd\u0cac\u0cb0\u0cbe\u0caf \u0ca8\u0cbf\u0ca6\u0cc1\u0cb5\u0ca8 \u0cb5\u0cb0 \u0cb0\u0cb8 \u0cae\u0ca7\u0cc1\u0c95\u0cb0 \u0caa\u0ccd\u0cb0\u0cae\u0cc1\u0ca6\u0cbf\u0ca4 \u0cae\u0cc1\u0c96\u0cbe\u0caf <br/> \u0cb0\u0cbe\u0cb8 \u0cb2\u0ccb\u0c95 \u0cb5\u0ccd\u0cb0\u0c9c \u0caf\u0cc1\u0cb5\u0ca4\u0cbf \u0c9c\u0ca8\u0cae\u0cc1\u0c96 \u0ca8\u0cb5\u0ca6\u0cb3 \u0cb8\u0ccc\u0cb0\u0cad \u0cb8\u0c99\u0ccd\u0c97 \u0ca4\u0cc1\u0c99\u0ccd\u0c97\u0cbe\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cb6\u0cb0\u0ca6\u0cc1\u0ca4\u0c9e\u0ccd\u0c9a\u0cbf\u0ca4 \u0caa\u0cbe\u0c9f\u0cb2\u0cc0 \u0ca6\u0cb3 \u0cb8\u0ccc\u0cb0\u0caa\u0ccd-\u0caa\u0cb0\u0cbf\u0cb5\u0cbe\u0ca6\u0cbf \u0cb2\u0ccb\u0c9a\u0ca8\u0cbe\u0caf <br/> \u0cb8\u0c95\u0cb2 \u0cb5\u0cc7\u0ca6 \u0cb5\u0cc7\u0ca6\u0cbe\u0ca8\u0ccd\u0ca4\u0cbe\u0c9f\u0cb5\u0cbf \u0cb6\u0cb0\u0cbf\u0ca4\u0cbe\u0caa\u0cbe\u0cb5\u0ccd\u0cb0\u0ccd\u0ca4 \u0caa\u0cbe\u0ca6\u0cbe\u0cae\u0ccd\u0cac\u0cc1\u0c9c\u0cbe\u0caf <br/> \u0caa\u0cb0\u0cae \u0c95\u0cb0\u0cc1\u0ca3\u0cbe\u0cb0\u0cb8\u0cae\u0caf \u0cb5\u0cbf\u0c95\u0cb8\u0cbf\u0ca4 \u0caa\u0c99\u0ccd\u0c95\u0cc7\u0cb0\u0cc1\u0cb9 \u0caa\u0ccd\u0cb0\u0cb8\u0ca8\u0ccd\u0ca8 \u0cb5\u0ca6\u0ca8\u0cbe\u0caf <br/> \u0cad\u0cbe\u0cb5\u0cb0\u0cbe\u0c97 \u0cae\u0cc1\u0cb0\u0cb3\u0cc0 \u0cb0\u0cb5 \u0ca8\u0cbf\u0ca8\u0cbe\u0ca6 \u0cad\u0c95\u0ccd\u0ca4 \u0cae\u0ca8\u0ccb\u0cb9\u0cb0\u0ca3\u0cbe\u0caf \u0c95\u0ccd\u0cb0\u0ccd\u0cb7\u0ccd\u0ca3\u0cbe\u0caf"}, {"M": "\u0cae\u0ca7\u0ccd\u0caf\u0cae\u0c95\u0cbe\u0cb2 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0caa\u0cb0\u0cbf\u0cae\u0cb3 \u0c89\u0cb2\u0caa \u0cb2\u0cb5\u0c99\u0ccd\u0c97 \u0cb8\u0ca6\u0ca8\u0cbe\u0caf \u0cad\u0cbe\u0c97\u0ca6\u0cc7\u0caf \u0c9c\u0ca8 \u0cae\u0ca6\u0ca8 \u0cae\u0ca6\u0ca8\u0cbe\u0caf <br/> \u0cad\u0cc1\u0c9c\u0c97 \u0cab\u0ca3\u0cae\u0ca3\u0cc0 \u0ca4\u0cb0\u0cb3 \u0caa\u0ca6\u0cbe\u0c99\u0ccd\u0c97\u0cc1\u0cb3\u0cbf \u0cad\u0cc2\u0cb7\u0ca3 \u0ca8\u0c9f\u0cb5\u0cb0 \u0cb5\u0cc7\u0cb7\u0ca3\u0cbe\u0caf \u0caa\u0cb0"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d35\u0d3e\u0d38\u0d41\u0d26\u0d47\u0d35\u0d3e\u0d2f \u0d28\u0d2e\u0d4b \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47 \u0d36\u0d4d\u0d30\u0d40 \u0d38\u0d30\u0d4d\u0d35 \u0d2d\u0d42\u0d24\u0d15\u0d4d\u0d37\u0d2f\u0d3e\u0d2f"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d06\u0d38\u0d41\u0d30 \u0d2e\u0d26 \u0d39\u0d30\u0d23 \u0d15\u0d4d\u0d30\u0d4d\u0d37\u0d4d\u0d23\u0d3e\u0d2f \u0d05\u0d28\u0d28\u0d4d\u0d24 \u0d36\u0d24\u0d4d\u0d2f\u0d3e\u0d2f \u0d28\u0d2e\u0d4b \u0d28\u0d2e\u0d38\u0d4d\u0d24\u0d47 <br/> \u0d2d\u0d3e\u0d38\u0d15 \u0d2d\u0d3e\u0d38\u0d15\u0d3e\u0d2f \u0d35\u0d4d\u0d30\u0d1c\u0d47\u0d36\u0d3e\u0d2f \u0d2d\u0d35 \u0d2d\u0d2f \u0d39\u0d30\u0d23 \u0d05\u0d30\u0d4d\u0d23 \u0d1a\u0d30\u0d23\u0d3e\u0d2f"}, {"M": "\u0d2e\u0d27\u0d4d\u0d2f\u0d2e\u0d15\u0d3e\u0d32 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d35\u0d3e\u0d38\u0d3f\u0d24 \u0d15\u0d28\u0d15\u0d3e\u0d2e\u0d4d\u0d2c\u0d30\u0d3e\u0d2f \u0d28\u0d3f\u0d26\u0d41\u0d35\u0d28 \u0d35\u0d30 \u0d30\u0d38 \u0d2e\u0d27\u0d41\u0d15\u0d30 \u0d2a\u0d4d\u0d30\u0d2e\u0d41\u0d26\u0d3f\u0d24 \u0d2e\u0d41\u0d16\u0d3e\u0d2f <br/> \u0d30\u